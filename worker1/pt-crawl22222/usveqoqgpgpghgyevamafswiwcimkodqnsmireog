<tr style="height:1px;text-align:center"><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td colspan="2" rowspan="2" style="height:2em;width:2em"></td><td style="height:1em;width:1em"></td><td style="height:1em;border-bottom:1px solid black;width:1em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td colspan="2" style="height:1em;border-bottom:1px solid black;width:2em"></td><td style="border-right:1px solid black;border-bottom:1px solid black;height:1em;width:1em"></td><td style="height:1em;border-bottom:1px solid black;width:1em"></td><td colspan="2" style="height:1em;border-bottom:1px so