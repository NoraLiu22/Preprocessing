<meta class="swiftype" name="title" data-type="string" content="United Arab Emirates Moves Needle Forward Against Film Censorship With New Rating Category"/>
<meta class="swiftype" name="author" data-type="string" content="Nick Vivarelli"/>
<meta class="swiftype" name="published_at" data-type="date" content="2021-12-20 10:24:04"/>
<meta class="swiftype" name="image" data-type="enum" content="https://variety.com/wp-content/uploads/2021/11/SLH-25812_RC.jpg?w=1024"/>
<meta class="swiftype" name="comment_count" data-type="integer" content="0"/>
<meta class="swiftype" name="tags" data-type="string" content="Censorship"/>
