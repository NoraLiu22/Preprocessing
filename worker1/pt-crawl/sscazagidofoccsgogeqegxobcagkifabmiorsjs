    <hr class="hrSection">

    <div class="row uk-grid-divider pt-2">
        <div class="col-8">
    <div class="row uk-grid-divider">
        <div class="col-12">
            <div class="article pb-4">
                <div>
                    <!-- <a class="muted-link" href="#" title=""> -->
                        <span class="red fs-20 noto-bold">
