<!-- standard BlockArticleVariantStandard -->
            <time datetime="2020-09-01 05:00:00" data-format="article-display" data-show-date="always" data-show-time="today-only" data-timestamp="1598932800" itemprop="datePublished" class="article-timestamp formatTimeStampEs6" full-date="31.08.2020">&nbsp;
            </time>


            <h1 class="headline ">Wessex FM becomes Greatest Hits Radio</h1>


    
                <address class="author-details">
        
                    <div class="author-image">
                        <img src="/web/20210315160225im_/https://www.dorsetecho.co.uk/resources/images/3623124?type=thumb">
                    </div>
        

                    <div class="author-name-container author-no-job ">
                        <span class="author-name">
        
                            By
        

        
                            <a href="/web/20210315160225/https://www.dorsetecho.co.uk/author/profile/1818.Martin_Lea/">Martin Lea</a>
        

        
                            <a href="https://web.archive.org/web/20210315160225/http://www.twitter.com/@DorsetEchoMart" target="_blank" class="twitter-button">
                                <span class="fa fa-twitter"></span> @DorsetEchoMart
                            </a>
        
                        </span>
        
                    </div>
                </address>
    




            <div class="article-row-container">
                <div class="article-row-container-left">
                    <div class="v2">

                        <!-- standard BlockArticleContent --><div id="BlockArticleContent">



    

        <div class="article-hero light-background">
            
            <div class="article-image-containter">
                <img src="/web/20210315160225im_/https://www.dorsetecho.co.uk/resources/images/11757865.jpg?display=1&amp;htype=0&amp;type=responsive-gallery" style="min-height: 333" alt="Wessex FM station HQ in Dorchester (Google Maps)"/>

                
                    
                    <div class="nq-article-card-label-standalone nq-article-inline-card-label updated-label"><h4>Updated</h4></div>
                
            </div>
            <p class="hero-caption"><span>Wessex FM station HQ in Dorchester (Google Maps)</span></p>
            
            
        </div>
    
    <div class="article-credits clearfix">

    </div>
    <!---->

    


    
