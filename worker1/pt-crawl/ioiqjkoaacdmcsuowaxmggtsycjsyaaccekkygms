                            <textarea name="comment" class="comment-entry-box" placeholder="Your reply"></textarea>
                            <button class="btn btn-sm btn-primary button-post-comment">Post reply</button>
                        </form>

                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>

            <div id="editTemplate">
                <div data-id="" class="edit-comment-form" style="display: none;">
                    <div class="alert alert-bad alert-danger edit-comment-form-warning" style="display: none;">
                        <span class="icon icon-alert"></span> <span class="message"></span>
                    </div>
                    <textarea class="edit-comment-box"></textarea>
                    <button class="btn btn-sm btn-success edit-save">Save comment</button>
                    <button class="btn btn-sm btn-danger edit-cancel">Cancel</button>
                </div>
            </div>

            <div id="deleteTemplate">
                <div data-id="" class="delete-comment-form" style="display: none;">
                    <div class="alert alert-bad alert-danger delete-comment-form-warning" style="display: none;">
                        <span class="icon icon-alert"></span> <span class="message"></span>
                    </div>
                    <p>Are you sure you want to delete this comment?</p>
                    <button class="btn btn-sm btn-danger delete-confirm" data-id="">Yes, delete the comment</button>
                    <button class="btn btn-sm btn-success delete-cancel" data-id="">No, keep the comment</button>
                </div>
            </div>

            <div id="reportTemplate" class="report-form hidden">
                <div id="commentsform" class="commentPost" style="background-color: lightgrey; padding: 10px; border: solid darkgrey 1px;">
                    <h3>Report This Comment</h3>
                    <div class="validateError hidden alert alert-danger"></div>

                    <form method="post" class="genericForm clearfix">
                        <input type="hidden" name="id" id="comment-id" value=""/>
