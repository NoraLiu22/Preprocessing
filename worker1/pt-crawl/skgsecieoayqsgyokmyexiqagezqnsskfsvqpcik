                <mo accent="false">&#x00AF;<!-- ¯ --></mo>
              </mover>
            </mrow>
            <mo>,</mo>
            <mrow class="MJX-TeXAtom-ORD">
              <mover>
                <mi>&#x03B6;<!-- ζ --></mi>
                <mo accent="false">&#x00AF;<!-- ¯ --></mo>
              </mover>
            </mrow>
          </mrow>
          <mo>)</mo>
        </mrow>
        <mo>=</mo>
        <mo stretchy="false">(</mo>
        <mn>0</mn>
        <mo>,</mo>
        <mn>0</mn>
        <mo stretchy="false">)</mo>
      </mstyle>
