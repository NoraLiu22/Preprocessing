	<!-- ux/ctshared/templates/hearst/article/headline.tpl -->
			<h1 class="headline entry-title">Trending: Why one city is booming</h1>
	<!-- e ux/ctshared/templates/hearst/article/headline.tpl -->
<!-- e ux/ctpost/templates/hearst/article/headline.tpl -->
            <!-- src/business/templates/hearst/article/types/byline.tpl -->

<h5 class="byline"><span class="name"></span></h5>
<!-- e src/business/templates/hearst/article/types/byline.tpl -->
                                                                <h5 class="timestamp updated" title="2013-05-23T01:00:45Z">
                Published 1:00&nbsp;am, Thursday, May 23, 2013
            </h5>
        </div>

<!-- e src/business/templates/hearst/article/types/standard_title.tpl -->

