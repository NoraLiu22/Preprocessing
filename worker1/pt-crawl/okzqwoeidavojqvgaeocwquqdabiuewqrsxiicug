			<h1 class="headline entry-title">Disney moves filming of 'Millionaire' to Stamford</h1>
	<!-- e ux/ctshared/templates/hearst/article/headline.tpl -->
<!-- e ux/ctpost/templates/hearst/article/headline.tpl -->
									                <!-- hearst/common/author_name.tpl -->
<p class="byline">Olivia Just</p>
<!-- e hearst/common/author_name.tpl -->
