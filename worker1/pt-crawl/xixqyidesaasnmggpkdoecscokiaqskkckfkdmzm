        </msub>
        <mo>&#x2248;<!-- ≈ --></mo>
        <mn>4.8</mn>
        <mo>+</mo>
        <mn>1.5</mn>
        <msub>
          <mi>M</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>S</mi>
          </mrow>
        </msub>
        <mo>,</mo>
      </mstyle>
    </mrow>
