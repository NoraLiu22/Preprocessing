                            <div class="medium__player-wrapper">
                                            <div class="js-player-wrapper player-wrapper player-wrapper--audio"
                            id="player-wrapper__urn-srf-audio-0a04bbb9-dc60-4144-a915-1b86f9ee4558_3"></div>
