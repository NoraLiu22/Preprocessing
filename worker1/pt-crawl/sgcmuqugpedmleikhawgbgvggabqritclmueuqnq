    <div class="hide-rss-link hdnce-e hdnce-collection-101518-premium_headlinelist_illus"><!-- hearst/collections/premium_headlinelist_illus.tpl -->
<!-- collection.premium_headlinelist_illus.101518 -->
	<!-- templates/design/collection/premium_headlinelist_illus/single_zone.tpl  -->


            <div class="headlineList single" data-component="collection" data-collection-type="list" data-collection-id="101518">
            <div class="moduleHeader">
                <h2><!-- widgets/hearst/moduleHeader/widget.tpl -->
                                            <h2><span class="first-word">Business</span> for Insiders</h2>
                        <!-- e widgets/hearst/moduleHeader/widget.tpl --></h2>
            </div>
            
<!-- ferd/hearst/templates/collections/premium_headlinelist_illus_body.tpl -->
            
                <ul class="headlineList__list">
