        <mo stretchy="false">[</mo>
        <mn>0</mn>
        <mi mathvariant="normal">&#x0025;<!-- % --></mi>
        <mo>,</mo>
        <mn>100</mn>
        <mi mathvariant="normal">&#x0025;<!-- % --></mi>
        <mo stretchy="false">]</mo>
      </mstyle>
    </mrow>
