    <h1 class="headline entry-title">ITV America Stamford production hub set for 2020 opening</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl --><!-- hearst/common/author_name.tpl -->
<p class="byline">By <a href="/web/20200710182817/https://www.stamfordadvocate.com/author/paul-schott/">Paul Schott</a></p>
<!-- e hearst/common/author_name.tpl -->    <span class="timestamp">Updated
        <time itemprop="datePublished" datetime="2019-06-22T17:40:12Z">
            1:40&nbsp;pm EDT, Saturday, June 22, 2019
        </time>
    </span>
</div>
<!-- e hearst/article/types/standard_title.tpl -->
