<!-- src/business/templates/hearst/article/headline.tpl -->
    <h1 class="headline entry-title">Stamford’s building boom shows no sign of slowing</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl --><!-- hearst/common/author_name.tpl -->
<p class="byline">By Barry Lytton</p>
<!-- e hearst/common/author_name.tpl -->    <span class="timestamp">Updated
        <time itemprop="datePublished" datetime="2019-04-05T17:27:42Z">
            1:27&nbsp;pm EDT, Friday, April 5, 2019
        </time>
    </span>
</div>
<!-- e hearst/article/types/standard_title.tpl -->
    <div class="article-share">
