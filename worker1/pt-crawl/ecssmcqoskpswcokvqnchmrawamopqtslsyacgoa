<!-- src/business/templates/hearst/article/headline.tpl -->
    <h1 class="headline entry-title">Royal Bank of Scotland to sell Stamford building</h1>
    <!-- e src/business/templates/hearst/article/headline.tpl --><!-- hearst/common/author_name.tpl -->
<p class="byline">By <a href="/web/20190828001216/https://www.stamfordadvocate.com/author/paul-schott/">Paul Schott</a></p>
<!-- e hearst/common/author_name.tpl -->    <span class="timestamp">Updated
        <time itemprop="datePublished" datetime="2018-06-04T18:40:20Z">
            2:40&nbsp;pm EDT, Monday, June 4, 2018
        </time>
    </span>
</div>
<!-- e hearst/article/types/standard_title.tpl -->
    <div class="article-share">
                                    <!-- hearst/article/mobile_share.tpl -->

<div class="social-links " data-social-url="https%3A%2F%2Fwww.stamfordadvocate.com%2Fbusiness%2Farticle%2FRoyal-Bank-of-Scotland-to-sell-Stamford-building-12965306.php" data-social-hash-id="" data-social-blurb="The%20embattled%20Royal%20Bank%20of%20Scotland%20is%20putting%20its%20downtown%20Stamford%20office%20building%20up%20for%20sale%2C%20but%20it%20plans%20to%20keep%20operating%20there%20for%20many%20more%20years%2C%20Hearst%20Connecticut%20Media%20has%20learned.%20%0A%0ARBS%E2%80%99%20decision%20fits%20into%20a%20cost-cutting%20plan%20for%20a%20firm%20whose%20chronic%20financial%20and%20regulatory%20troubles%20have%20led%20to%20hundreds%20of%20layoffs%20in%20recent%20years%20at%20its%20regional%20headquarters%20at%20600%20Washington%20Blvd.%20The%20sale%20is%20not%20expected%20to%20directly%20affect%20the%20approximately%20500%20employees%20who%20remain%20in%20Stamford%20and%20focus%20on%20RBS%E2%80%99%20NatWest%20Markets%20corporate%20and%20institutional%20banking%20services.%20%0A%0A%E2%80%9CAs%20we%20have%20reduced%20the%20size%20of%20our%20global%20footprint%2C%20it%20now%20makes%20economic%20sense%20for%20RBS%20to%20sell%20the%20building%20at%20600%20Washington%20Blvd." data-media-url="https%3A%2F%2Fs.hdnux.com%2Fphotos%2F73%2F60%2F45%2F15665486%2F5%2FrawImage.jpg" data-social-title="Royal%20Bank%20of%20Scotland%20to%20sell%20Stamford%20building">
