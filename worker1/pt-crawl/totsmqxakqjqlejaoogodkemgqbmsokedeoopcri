<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 200px 100px 0;border-color:Yellow"></div>
<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 100px 200px 0;border-color:Yellow"></div><div style="border:solid transparent;background-color:initial;position:absolute;width:100px;line-height:0;left:100px; top:100px; border-width:100px 0 0 1065.03150684px; border-left-color:darkkhaki"></div>
<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 200px 100px 0;border-color:darkkhaki"></div>
<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 100px 200px 0;border-color:darkkhaki"></div><div style="border:solid transparent;background-color:initial;position:absolute;width:100px;line-height:0;left:100px; top:100px; border-width:100px 0 0 595.02425868362px; border-left-color:Green"></div>
<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 200px 100px 0;border-color:Green"></div>
<div style="position:absolute;line-height:0;border-style:solid;left:0;top:0;border-width:0 100px 200px 0;border-color:Green"></div><div style="border:solid transparent;background-color:initial;position:absolute;width:100px;line-height:0;left:100px; top:100px; border-width:100px 0 0 319.33475053594px; border-left-color:DarkOrange"></div>
