    <table border="0" cellspacing="0" cellpadding="0" id="table18">
      <tr>
        <td class="tdnumnum"><a name="1"></a>1&nbsp;</td>
        <td class="tdnumtext">
        SUMMARY / PRESS RELEASE CONCERNING THE TREATISE</td>
      </tr>
    </table>
    </td>
  </tr>

  <tr>
    <td class="tdnumspace">
      <a href="javascript:printthis();">
      <img border="0" src="../IMAGES/print.gif" alt="- evt. praktisk at kunne l�se teksten off-line"></a></td>
  </tr>

