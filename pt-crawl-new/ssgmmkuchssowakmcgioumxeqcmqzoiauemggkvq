<script type="text/javascript">
	var twitterLoaded = false;
		monthNames = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];

	$(function() {
	/*	var YQL = "https://query.yahooapis.com/v1/public/yql?q=select%20title%2Clink%2Cdescription%2CpubDate%20from%20rss%20where%20url%3D%22https%3A%2F%2Fopenrecordspa.wordpress.com%2Ffeed%2F%22&format=json&diagnostics=true&callback=";
		$.getJSON(YQL, function(res) {}, "jsonp").error(function(){
			$("#tab-a").html('<a href="https://openrecordspennsylvania.com/" target="blank" class="alert-link"><div class="alert alert-info"><i class="fa fa-lg fa-info-circle"></i> No data found, or unable to read RSS feed. Click here to view our Open Records blog</div></a>');
		}).success(function(res){
			for(i=0;i<res.query.results.item.length;i++){
				var curI = res.query.results.item[i]
					oldD = new Date(curI.pubDate)
					newD = oldD.getDate() + ' ' + monthNames[oldD.getMonth()] + ' ' + oldD.getFullYear()
					newHTML =  '<h3 class="blog-title"><a href="' + curI.link + '" target="_blogPost">' + curI.title + '</a></h3>'
        			newHTML += '<h5 class="blog-date">' + newD + '</h5>'
        			newHTML += '<p>' + curI.description + '</p>'
        			newHTML += '<hr>'

				$("#tab-a").append(newHTML)
			}
		});*/

		$.ajax({
	        url: 'https://openrecordspa.wordpress.com/feed',
	        type: 'GET',
	        dataType: "xml"
	    }).done(function(xml) {
	        $.each($("item", xml), function(i, e) {
	        	var oldD = new Date($(e).find('pubDate'));
				var newD = oldD.getDate() + ' ' + monthNames[oldD.getMonth()] + ' ' + oldD.getFullYear();
				var newHTML =  '<h3 class="blog-title"><a href="' + $(e).find('link') + '" target="_blogPost">' +$(e).find('title') + '</a></h3>';
        			newHTML += '<h5 class="blog-date">' + newD + '</h5>';
        			newHTML += '<p>' + $(e).find('description') + '</p>';
        			newHTML += '<hr>';

				$("#tab-a").append(newHTML);
	        });
	    });


		$('#tab-d-link').click(function() {
			// For some reason, Twitter thinks that a max width of 520px is good enough, and doesn't allow anything wider.
			// You're not the boss of me, Twitter!
			if (!twitterLoaded) {
				var tFrame = document.getElementById('twitter-widget-0');

				//After twitter update, height and width have to be set using the data attributes in the tag below
				// if (tFrame) {					
				// 	$(tFrame).css('width', '100%');
				// 	$(tFrame.contentDocument).find('.timeline-Widget, .timeline-Viewport').css({'max-width': '100%', 'width': '100%'});
				// }

				twitterLoaded = true;
			}
		});
	});
</script>




