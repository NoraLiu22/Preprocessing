	</div>
	<div class="col-md-3 hidden-sm hidden-xs hidden-print">
		
				<script type="text/javascript">
					$(document).ready(function() {
						var navItems = '';

						$('#nav-content h3').each(function(i) {
							var index = i + 1;
							$(this).before('<a id="' + index + '"></a>');
							navItems += '<li><a href="#' + index + '">' + $(this).text() + '</a></li>';
						});

						$('.navbar-page nav > ul').append(navItems);
						$('body').addClass('scrollspy').scrollspy({target: '.navbar-page'});
						$('.navbar-page').affix({offset: {top: 230}});

						$('.navbar-page .panel-title i').click(function() {
							$('html, body').animate({scrollTop: 0}, 'slow');
						});
					});
				</script>

				<div class="panel panel-info navbar-page">
					<div class="panel-heading">
						<h3 class="panel-title">
							Topics
							<div class="pull-right">
								<i class="fa fa-arrow-circle-o-up" title="Back to Top"></i>
							</div>
						</h3>
					</div>
					<div class="panel-body">
						<nav class="nav-cols-3">
							<ul class="nav"></ul>
						</nav>
					</div>
