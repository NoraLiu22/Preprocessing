    <script>
        function getQueryParams() {

            var params = {};
            var query = window.location.search ? window.location.search.substr(1).split('&') : [];

            var cur = null;
            for (var i = 0; i < query.length; i++) {
                cur = query[i].split('=');
                if (cur && cur.length == 2) {
                    params[cur[0]] = cur[1];
                }// if
            }// for

            return params;
        }

        $(document).ready(function () {

            var params = getQueryParams();

            if (params && params.optout !== null && params.optout !== undefined) {
                // optout cookies and lsid once page loads
                optOut('', params.optout);
            }// if-else

        });

    </script>
