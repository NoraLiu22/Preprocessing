<script>
	var newsletter_footer = document.getElementById("newsletter-email-footer");
	newsletter_footer.addEventListener("keyup", function(event) {
		// Number 13 is the "Enter" key on the keyboard
		if (event.keyCode === 13) {
			// Cancel the default action, if needed
			event.preventDefault();
			// Trigger the button element with a click
			document.getElementById("newsletter-submit-footer").click();
		}
	});
	$(function() {
		$('#newsletter-submit-footer').click( function() {
			var newsletter_email = $("#newsletter-email-footer").val();
			window.dataLayer.push({'newsletter-email': newsletter_email});
			sessionStorage.setItem('newsletter-email',$('#newsletter-email').val());
			window.open($(this).attr('href') + '&email=' + $('#newsletter-email-footer').val());
			return false;
		});
	});
</script>
