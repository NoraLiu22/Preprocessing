    <script>
    var newsletter_homepage = document.getElementById("newsletter-email");
	newsletter_homepage.addEventListener("keyup", function(event) {
		// Number 13 is the "Enter" key on the keyboard
		if (event.keyCode === 13) {
			// Cancel the default action
			event.preventDefault();
			// Trigger the button element with a click
			document.getElementById("newsletter-submit-homepage").click();
		}
	});
    $(function() {
        $('#newsletter-submit-homepage').click( function() {
            var newsletter_email = $("#newsletter-email").val();
            window.dataLayer.push({'newsletter-email': newsletter_email});
            sessionStorage.setItem('newsletter-email',$('#newsletter-email').val());
            window.open($(this).attr('href') + '&email=' + $('#newsletter-email').val());
            return false;
        });
    });
    </script>
