function getHTML(t){var i="";return i+='<div class="rce-directory">',t.forEach(function(t){postHTML='<div class="rce-listing"><div class="listing-img"><a href="'+t.link+'" rel="bookmark" title="'+t.title+'" target="_blank"><img decoding="async" src="'+t.image+'" alt="'+t.title+'" title="'+t.title+'"></a></div><h3><a href="'+t.link+'" rel="bookmark" title="">'+t.title+"</a></h3></div>",i+=postHTML}),i+="</div>"}function populatePosts(t){var i=[];jQuery("#api-posts");t.forEach(function(t,e){var a={};a.link=t.link,a.title=t.title.rendered,a.image=t.company_avatar,i.push(a)});var e=getHTML(i);jQuery("#api-posts").html(e)}function pullPosts(t,i){i+="/wp-json/wp/v2/listings?_embed&per_page="+t,jQuery.get(i,populatePosts)}
jQuery( document ).ready(function() {
    pullPosts(4, 'https://racecar-engineering.com/directory');
