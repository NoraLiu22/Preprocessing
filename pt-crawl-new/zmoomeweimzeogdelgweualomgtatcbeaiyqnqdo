<script>
    //$( ".vc_tta-panel-heading .vc_tta-panel-title a" ).attr("data-toggle", "collapse");
	//$( ".vc_tta-panel-heading .vc_tta-panel-title a" ).attr("data-bs-toggle", "collapse");
    //$( ".vc_tta-panel-heading .vc_tta-panel-title a" ).attr("data-bs-target", this.attr("href"));
    function loadCarAccordion(element){
        var target = $(element.target), article;
        if(target.is(".doubleSliderNextButton") || target.is(".doubleSliderPrevButton")){
            var currentSlide = parseInt($(".td-gallery-slide-count .td-gallery-slide-item-focus")[0].textContent);
            var totalSlides = parseInt($(".td-gallery-slide-count")[0].textContent.split(' of ')[1]);
            if(target.is(".doubleSliderNextButton") && currentSlide < totalSlides){
                $(".td-doubleSlider-1 .td-slide-item").css("display", "none");
                currentSlide++;
                var showSlideClass = ".td-doubleSlider-1 .td-item" + currentSlide;
                $(showSlideClass).css("display", "block");
                $(".td-gallery-slide-count .td-gallery-slide-item-focus").text(currentSlide);
            }else if(target.is(".doubleSliderPrevButton") && currentSlide > 1){
                $(".td-doubleSlider-1 .td-slide-item").css("display", "none");
                currentSlide--;
                var showSlideClass = ".td-doubleSlider-1 .td-item" + currentSlide;
                $(showSlideClass).css("display", "block");
                $(".td-gallery-slide-count .td-gallery-slide-item-focus").text(currentSlide);
            }
        }
        if(target.is(".vc_tta-title-text") || target.is(".vc_tta-tab a") || element == "pageload"){
            $(".vc_tta-tab").removeClass("vc_active");
            $(".vc_tta-panel-body .vc_grid.vc_row .vc_grid-item").css("display","none");
            setTimeout(() => {
                if(window.location.hash){
                    var hash = window.location.hash;
                }else if(document.getElementsByClassName("vc_tta-tab")[0].childNodes[0]){
                    var hash = document.getElementsByClassName("vc_tta-tab")[0].childNodes[0].getAttribute("href");
                }
                if(hash){
                    var elem = hash + "go .vc_tta-panel-body .vc_grid.vc_row .vc_grid-item";
                    $(elem).css("display","block");
                    var activetab = 'a[href="'+ hash + '"]';
                    $(activetab).parent().addClass("vc_active");
                }
            }, 10);
        }
    };
    function editVCTabId(){
        var VCTabs = document.querySelectorAll(".vc_tta-panel");
        for(var i=0; i<VCTabs.length; i++){
            var newId = $(VCTabs[i]).attr("id") + "go";
            VCTabs[i].id = newId;
        }
    }
    document.ready = setTimeout(() => {
        loadCarAccordion("pageload"), editVCTabId()}, 400);
    $(document).click(loadCarAccordion);
</script>
