			var pmc_ga_dimensions = {"dimension1":"single-pmc-gallery-vertical","dimension2":"single-pmc-gallery-vertical_item","dimension3":"1235903071","dimension4":"robert-lang","dimension5":"culture|news","dimension6":"met-gala|photo-gallery","dimension8":"culture","dimension10":"2024","dimension11":"05","dimension12":"06","dimension13":"18","dimension14":"18","dimension25":"2024-05-07T01:18:38+00:00","dimension26":"2024-05-06 18:18:38","dimension27":"Monday","dimension29":"anonymous","dimension35":""};
