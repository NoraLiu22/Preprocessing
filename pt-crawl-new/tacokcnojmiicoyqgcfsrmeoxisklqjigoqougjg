window.onload=function(){
   	let btn1 = document.getElementById("button1");
	let btn2 = document.getElementById("button2");
	let btn3 = document.getElementById("button3");
	let btn4 = document.getElementById("button4");
	let btn5 = document.getElementById("button5");
	let btn6 = document.getElementById("button6");
	let btn7 = document.getElementById("button7");
   btn1.addEventListener("click", triggerExample);
   btn2.addEventListener("click", triggerExample2);
	btn3.addEventListener("click", triggerExample3);
	btn4.addEventListener("click", triggerExample4);
	btn5.addEventListener("click", triggerExample5);
	btn6.addEventListener("click", triggerExample6);
	btn7.addEventListener("click", triggerExample7);
}	

function triggerExample() {
  const element = document.querySelector('#example');
  element.select();
  element.setSelectionRange(0, 99999);
  document.execCommand('copy');
}
	function triggerExample2() {
  const element = document.querySelector('#example2');
  element.select();
