<script>
	if (typeof cards === 'undefined') {
	const cards = document.querySelectorAll(".featured-partners-card");
	cards.forEach(card => {
		card.addEventListener("click", function(event) {
			const dataValue = card.getAttribute("data-value");
			if (dataValue && dataValue.trim() !== "") {
				window.open(dataValue, "_blank");
			} else {
				console.log("No CTA Link found");
			}
		});
	});
}
</script>

	
