        <script type="text/javascript">

            function getKeyValue(keyValuePairStr, key, keyValuePairDelimeter, valueAssignmentCharacter) {
                var keyName = key + valueAssignmentCharacter;
                var keyValuePairs = keyValuePairStr.split(keyValuePairDelimeter);
                for (var i = 0; i < keyValuePairs.length; i++) {
                    var keyValuePair = keyValuePairs[i];
                    while (keyValuePair.charAt(0) == ' ')
                        keyValuePair = keyValuePair.substring(1);
                    if (keyValuePair.indexOf(keyName) == 0)
                        return keyValuePair.substring(keyName.length, keyValuePair.length);
                }
                return "";
            }

            function allowPerformanceCookies() {
                // Retrieves the OneTrust cookie and determines whether or not the user has opted out of performance cookies.  If no performance cookie blocking setting is found, we return true.
                try {
                    var OptanonConsentCookie = getKeyValue(decodeURIComponent(document.cookie), 'OptanonConsent', ';', '=');
                    var OptanonConsentCookieGroups = getKeyValue(OptanonConsentCookie, 'groups', '&', '=');
                    var performanceCookiesSetting = getKeyValue(OptanonConsentCookieGroups, '2', ',', ':');
                    if (performanceCookiesSetting == "") {
                        console.log("OneTrust performance cookies setting not found.  Allowing analytics tag managers to load.");
                        return true;
                    }
                    else {
                        return performanceCookiesSetting == '1';
                    }
                } catch (err) {
                    console.error("Error occured determing OneTrust performance cookies setting.  Message: " + err.message);
                }
                return true;
            }

            // Conditionally loading the analytics tag manager scripts.
            if (allowPerformanceCookies()) {
                
                // Adobe Launch Script
                var launchScriptSrc = "//assets.adobedtm.com/fcbff6e82c08/ba94369e179d/launch-180ff4990fd5.min.js";
                var launchScript = document.createElement('script');
                launchScript.src = launchScriptSrc;
                document.getElementsByTagName('head')[0].appendChild(launchScript);
                

                
                // Google Tag Manager (GTM) Script
                var gtmID = 'GTM-PMTSM7V';
                var gtmScript = document.createElement('script');
                gtmScript.innerHTML = "(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer','" + gtmID + "');";
                document.getElementsByTagName('head')[0].appendChild(gtmScript);
                
            }
            else {
                console.log('User preferences blocked performance cookies, analytics tag manager scripts will not be loaded.');
            }
        </script>
        <!-- End Conditionally Loading Analytics Tag Managers -->
    





