<script>
    $(document).ready(function () {

        var a = $(".PagerUl").width();

        //$(".PagerUl").attr("style", "width:" + a + "px;");

        $(".newsfeedPager").css("width", a);

        $(".PagerUl > li").click(function () {

            var b = $(this).attr("linkcount");

            $(".PagerUl > li").removeClass("secili");
            $(this).addClass("secili");

            $(".newsfeedTab").hide();
            setTimeout(function () {
                $(".newsfeed > [tabcount='" + b + "']").show();
            }, 0);
            

        });

        if ('' != "" && '' != undefined) {
            $("#SagKisimKategori").text("diğer " + '' + " haberleri");
        }
        else if ($("#SayfaKategori").val() != "" && $("#SayfaKategori").val() != undefined) {
            $("#SagKisimKategori").text("diğer " + $("#SayfaKategori").val() + " haberleri");
        }
        else {
            $("#SagKisimKategori").text("diğer haberler");
        }

    });
</script>

