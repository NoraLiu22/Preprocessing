<script>
    $(document).ready(function () {

        $(".bx-prev").hide();
        $(".bx-next").hide();

        $(".bx-viewport").hover(function () {
            $(".bx-prev").show();
            $(".bx-next").show();
        }, function () {
            $(".bx-prev").hide();
            $(".bx-next").hide();
        });

        $(".bx-prev").hover(function () {
            $(this).show();
            $(".bx-next").show();
            return false;
        }, function () {
            $(".bx-prev").hide();
            $(".bx-next").hide();
        });

        $(".bx-next").hover(function () {
            $(this).show();
            $(".bx-prev").show();
        }, function () {
            $(".bx-prev").hide();
            $(".bx-next").hide();
        });

    });
</script>

