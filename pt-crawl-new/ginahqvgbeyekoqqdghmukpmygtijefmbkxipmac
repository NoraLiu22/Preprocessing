<script>
    var tarih = new Array();
    var i = 0;
    $(document).ready(function () {

        var initialLocaleCode = 'tr';
        var datetimeNow = getFormattedDate(new Date())


        $.ajaxSetup({
            async: false
        });
        var datetimeNow = getFormattedDate(new Date());
        $('#calendar').fullCalendar({
            header: {
                left: 'prev,next today',
                center: '',
                right: 'title'
            },
            defaultDate: datetimeNow,
            locale: initialLocaleCode,
            eventLimit: false,
            height: "auto",

            viewDisplay: function (element) {
             
            },
            events: 'https://www.aa.com.tr/tr/sirkethaberleri/ActivityCalendar/ActivityCalendar',
            eventClick: function (calEvent, jsEvent, view) {

                debugger;
                var d = calEvent._start._d;
                var ay = parseInt(d.getMonth()) + 1;
                var tarih = d.getDate() + "." + ay + "." + d.getFullYear();
                debugger
                location.href = 'https://www.aa.com.tr/tr/sirkethaberleri/activitycalendar/?date=' + tarih;

               

            }
        });

        $.each($.fullCalendar.locales, function (localeCode) {
            $('#locale-selector').append(
				$('<option/>')
					.attr('value', localeCode)
					.prop('selected', localeCode == initialLocaleCode)
					.text(localeCode)
			);
        });

        $('#locale-selector').on('change', function () {
            if (this.value) {
                $('#calendar').fullCalendar('option', 'locale', this.value);
            }
        });
        //textEdit();


        $(".fc-icon").click(function () {
            setTimeout(function () {
                //textEdit();
            }, 0);
        })

        $(".fc-button").click(function () {
            setTimeout(function () {
                //textEdit();
            }, 0);
        })

        $(".fc-more").click(function () {
            $(".fc-popover").hide();
        });
    });

    

    function getFormattedDate(date) {
        var day = date.getDate();
        var month = date.getMonth() + 1;
        var year = date.getFullYear().toString().slice(0);
        return year + '-' + month + '-' + day;
    }



</script>


