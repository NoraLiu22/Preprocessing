<script>
$(function() {
	//shorthand onLoad call	

	$('.chocolat-parent').Chocolat( {
		imageSize     : 'contain',
		loop           : true,
	});

	$('.submenu-button').on('click touchend', function(e) {
		e.preventDefault();

		currentMenu = $(this).next()

		$('.submenu').not(currentMenu).slideUp();
		currentMenu.slideToggle();
	});

});
</script>


