<style>
	.alert {
		font-size: 16px;
	}

	#publicRecord {
		font-weight: bold;
		text-transform: uppercase;
	}

	#type-org {
		display: none;
	}

	.fieldError {
		display: none;
		color: #f00;
	}

	.fieldError i.fa {
		margin-right: 5px;
	}

	#emailError > p.form-control-static {
		padding: 0;
	}
</style>

<script type="text/javascript">


	function isFieldValid(id) {
		var el = document.getElementById(id);

		// IE doesn't support the HTML5 form validation reportValidity method, so use the ponyfill function instead
		return (el.reportValidity) ? el.reportValidity() : reportValidity(el);
	}


	function setFieldValid(id, errorID) {
		if (!id || !errorID) {
			return false;
		}

		var el = document.getElementById(id);

		if (el) {
			el.setCustomValidity('');
			$('#' + errorID).hide();
		}
	}


	function setFieldInvalid(id, errorID, errorMsg, validMsg, alertMsg) {
		if (!id || !errorID || !errorMsg) {
			return false;
		}

		var el = document.getElementById(id);

		if (el) {
			el.setCustomValidity(validMsg);
			$('#' + errorID + 'Msg').html(errorMsg);
			$('#' + errorID).show();

			if (alertMsg) {
				_alert(alertMsg);
			}
		}
	}


	function checkDocket() {
		var el = document.querySelector('#docket');
		var docket = $(el).val();
		var errorMsg = 'The specified Docket number does not exist.';

		if (docket) {
			if ($(el).valid()) {
				$.ajax({
					url: '/Resources/cfc/Docket.cfc?method=docketExists&returnFormat=json',
					method: 'post',
					dataType: 'json',
					data: {
						docket: docket
					},
					success: function(exists) {
						if (exists === false) {
							setFieldInvalid(el.id, 'docketError', errorMsg, errorMsg, errorMsg);
						}
						else {
							setFieldValid(el.id, 'docketError');
							checkEmail();
						}
					},
					error: ajaxError
				});
			}
		}
		else {
			setFieldValid(el.id, 'docketError');
		}
	}

/* ------------------------------------------------------------------------------------------------
		This function has been commented out where it has been used to allow the same email to be used
		            (this may need to be updated in the future)
	--------------------------------------------------------------------------------------------------*/
	function checkEmail() {
		var el = document.querySelector('#email');
		var email = $(el).val();
		var docket = $('#docket').val();

		if (email && docket && isFieldValid('docket')) {
			if ($(el).valid()) {
				$.ajax({
					url: '/Resources/cfc/Docket.cfc?method=checkDocketAccess&returnFormat=json',
					method: 'post',
					dataType: 'json',
					data: {
						docket: docket,
						email: email
					},
					success: function(exists) {
						if (exists === false) {
							setFieldValid(el.id, 'emailError');
						}
						else {
							var errorMsg = 'This Email address already has access to the specified Docket number.';

							if (exists == "pending") {
								errorMsg = 'This Email address has already requested access to the specified Docket number.';
							}

							var errorMsgFull = errorMsg + '  Please <a href="/Portal/login.cfm" target="_portal">log in</a> to the appeal portal to view.';

							setFieldInvalid(el.id, 'emailError', errorMsgFull, errorMsg, errorMsgFull);
						}
					},
					error: ajaxError
				});
			}
		}
		else {
			setFieldValid(el.id, 'emailError');
		}
	}


	$(function() {
		$('#state').select2({placeholder: 'State'});

		$('#type').on('change', function() {
			if ($(this).val() == 'organization') {
				$('#organization').val('');
				$('#type-org').show();
			}
			else {
				$('#type-org').hide();
				$('#organization').val('Personal');
			}
		});


		$('#docket').on('change', checkDocket);
		// $('#email').on('change', checkEmail);

		$('#phone').on('change', function() {
			var val = $(this).val().trim().replace(/[^\d]/g, '').replace(/^(\d{3})(\d{3})(\d{4})/, '$1-$2-$3');
			$(this).val(val);
		});


		$('#btn-reset').click(function() {
			setFieldValid('docket', 'docketError');
			setFieldValid('email', 'emailError');
		});

		//alert user if too many links are in the Requested Records field
		document.getElementById('comments').addEventListener('blur', function(){
			const linkRegex = /[(http(s)?):\/\/(www\.)?a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}\b([-a-zA-Z0-9@:%_\+.~#?&\/=]*)/g;
			const linksAllowed = 3;
			const linkArr = [...this.value.matchAll(linkRegex)];

			if(linkArr.length > Number(linksAllowed)){
				_alert(`Comments contains too many links. This field may only contain up to ${linksAllowed} links.`, 'Form Error', BootstrapDialog.TYPE_DANGER)
			}
		});

		const textRegex = /^[\w\s,\.!&?:\/\-|()':]+$/;

		$("#dipForm").validate({
			// debug: true,
			rules: {
				docket: {
					required: true,
					pattern: /^\d{4}[-]?\d{4}$/
				},
				firstName: {
					required: true,
					pattern: textRegex
				},
				lastName: {
					required: true,
					pattern: textRegex
				},
				address1: {
					pattern: textRegex
				},
				address2: {
					pattern: textRegex
				},
				city: {
					pattern: textRegex
				},
				comments: {
					pattern: textRegex
				},
				organization: {
					required: function(el) {
						return ($('#type').val() == 'organization');
					}
				},
				zip: {
					pattern: /^\d{5}(-\d{4})?$/
				},
				email: {
					email: true
				},
				phone: {
					required: true,
					pattern: /^(\d{3}-){2}\d{4}$/
				}
			},
			messages: {
				docket: {
					required: 'The Docket # field is required.',
					pattern: 'Invalid Docket Number.  Must match the pattern 2019-1234.'
				},
				firstName: {
					required: 'First Name is required.',
					pattern: 'First Name cannot contain special characters.'
				},
				lastName: {
					required: 'Last Name is required.',
					pattern: 'Last Name cannot contain special characters.'
				},
				address1: {
					pattern: 'Address1 cannot contain special characters.'
				},
				address2: {
					pattern: 'Address2 cannot contain special characters.'
				},
				city: {
					pattern: 'City cannot contain special characters.'
				},
				comments: {
					pattern: 'Comments cannot contain special characters.'
				},
				zip: {
					pattern: 'Invalid Zip Code.  Must match the pattern 12345 or 12345-6789.'
				},
				email: {
					email: 'Invalid email address.'
				},
				phone: {
					required: 'The Phone Number field is required.',
					pattern: 'Invalid Phone number.  Must match the pattern 123-456-7890.'
				}
			},
			submitHandler: function(form) {
				removeEmptyFiles();

				if (!isFieldValid('docket') || !isFieldValid('email')) {
					return false;
				}

				if ($('#type').val() == 'personal') {
					$('#organization').val('Personal');
				}

				var formVals = getFormVals('dipForm');

				$.post({
					url: '/Resources/cfc/OOR.cfc?method=formHasHTMLAjax&returnFormat=json',
					dataType: 'json',
					data: {
						FORM: JSON.stringify(formVals)
					},
					success: function(hasHTML) {
						if (hasHTML === true) {
							_alert('Invalid input detected. Please check your input and try again.');
						}
						else {
							submitForm();
						}
					},
					error: ajaxError
				});
			}
		});


		checkDocket();
	});


	function submitForm() {
		var form = $('#dipForm').get(0);

		if (!$('#email').val().trim()) {
			_confirm('Without specifying an email address, you will not be given access to the appeal portal.<br><br>Do you wish to continue without an email address?', function(ok) {
				if (ok) {
					$('#btn-submit').prop('disabled', true);
					form.submit();
				}
			}, 'Confirmation Message', BootstrapDialog.TYPE_DANGER, 'fa fa-exclamation-triangle');
		}
		else {
			$('#btn-submit').prop('disabled', true);
			form.submit();
		}
	}
</script>







<h1>Request Direct Interest Participant Status</h1>



<div class="alert alert-info">
	<i>Direct interest participant</i> - A person or entity, other than the requester or agency, who has been granted the ability to participate before the OOR under &sect; 77.75 (relating to request for direct interest participant status).
</div>

<div class="alert alert-info">
	Please accept this as a Request to Participate in a currently pending appeal before the Office of Open Records.  The statements made herein and in any attachments are true and correct to the best of my knowledge, information and belief.  I understand this statement is made subject to the penalties of 18 Pa.C.S. &sect; 4904, relating to unsworn falsifications to authorities.
</div>

<div class="row">
	<div class="col-xs-12">
		<form class="form form-horizontal" id="dipForm" name="dipForm" method="post" enctype="multipart/form-data">
			<div class="form-group">
				<label class="col-sm-2 control-label">Docket #:</label>
				<div class="col-sm-2">
					<input type="text" class="form-control" id="docket" name="docket" value="" maxlength="9">
				</div>
				<div class="col-sm-8 fieldError" id="docketError">
					<p class="form-control-static"><i class="fa fa-lg fa-exclamation-triangle"></i> <span id="docketErrorMsg"></span></p>
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-2 control-label">Name:</label>
				<div class="col-sm-4">
					<input type="text" class="form-control" id="firstName" name="firstName" placeholder="First Name" maxlength="100">
				</div>
				<div class="col-sm-4">
					<input type="text" class="form-control" id="lastName" name="lastName" placeholder="Last Name" maxlength="100">
				</div>
			</div>
			<div class="form-group">
				<label class="col-sm-2 control-label">Type:</label>
