	<div id="ctl00_cphBrowserNavigationWarning_pnlBrowserNavigationWarning">
	
	<script type='text/javascript'>
                        var warnBackHash = 'PosseWizard';
                        var hashSetupDone = false;

                        function addHashEvent(el, eventType, action) {
                            if (el.addEventListener) {
                                el.addEventListener(eventType, action, false);
                            }
                            else if (el.attachEvent) { // IE <=9
                                el.attachEvent('on' + eventType, action);
                            }
                        }

                        function warnBackNavigation(e) {
                            if (hashSetupDone && window.location.hash == '#' + warnBackHash + '1') {
                                alert('WARNING: Browser back button is not supported inside permit wizards. Please use the navigation buttons/links on the page.');
                            }
                        }

                        function addWarnHashes(e) {
                            window.location.hash = warnBackHash + '1';
                            // browsers do not add an extra history item unless there is a delay of time between changes
                            setTimeout(function () {
                                window.location.hash = warnBackHash + '2';
                                hashSetupDone = true;
                            }, 200);
                        }

                        addHashEvent(window, 'load', addWarnHashes);
                        addHashEvent(window, 'hashchange', warnBackNavigation);
                    </script>
</div>

