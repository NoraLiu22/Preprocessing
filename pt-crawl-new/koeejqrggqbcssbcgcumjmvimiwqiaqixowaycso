<script>
const wrapper = document.querySelector('#solutions-mobile')
const tabLinks = document.querySelectorAll('.tab-link--mobile')

tabLinks.forEach((e,i) => {
    e.addEventListener('click', () => changeTab(e,i))
})

changeTab(tabLinks[0], 0)

function changeTab(target, i){
    const tabs = wrapper.querySelectorAll('.tab-content')
    tabLinks.forEach( e => {
        e.classList.remove('active')
    })

    tabs.forEach( e => {
        e.style.display = 'none'
    })
    
    target.classList.add('active')
    tabs[i].style.display = 'flex'
}
