function pricesLow (){
	var tempL=confirm("Why are your prices are so low?\nAardvark has been in business for 33-1/3 years.  All the equipment is paid off, and we have no loans against anything. Since our operation doesn't have to facilitate customers off the street, we operate out of the owner's home.  No rent is paid.  Low fixed costs, savings to the customers.\n\nClick OK to view the full detailed price breakdown.");
	if(tempL){
		location="pricedetails.htm";
	}
}
function oneStep (){
	alert("One Step Plating:\nWe offer, but discourage, doing your project with One Step plating. With One Step plating, no mother plates are made. There is no way to make additional stampers without remastering the project.  The precautions are as follows:\n\n1. There is no guarantee concerning the life of the plates. This includes breakage at the plant.\n\n2. All problems concerning skipping and noise on a one step project must be reported to Aardvark within 45 days of mastering. The lacquers are discarded after 45 days, and therefore no tracing of errors are possible. Full price for re-mastering and plating will apply after 45 days of mastering.\n\n3. No stampers can be re-ordered on a one step process.");
}
function showSlides() {
  var i;
  var slides = document.getElementsByClassName("mySlides");
  var dots = document.getElementsByClassName("dot");
  for (i = 0; i < slides.length; i++) {
    slides[i].style.display = "none";  
  }
  slideIndex++;
  if (slideIndex > slides.length) {slideIndex = 1}    
  //for (i = 0; i < dots.length; i++) {
  //  dots[i].className = dots[i].className.replace(" active", "");
  //}
  slides[slideIndex-1].style.display = "block";  
  //dots[slideIndex-1].className += " active";
  setTimeout(showSlides, 4000); // Change image every 4 seconds
}
</script>
