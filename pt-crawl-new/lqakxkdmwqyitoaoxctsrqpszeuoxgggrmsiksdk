        // Right-sidebar "Related Content" accordion functiality

        $('.toggle').on('click', function (e) {
            e.preventDefault();

            var $this = $(this);

            if ($this.next().hasClass('inner-show')) {
                $this.next().removeClass('inner-show');

                $this.next().slideUp(350);
            } else {
                $this.parent().parent().find('div .inner').removeClass('inner-show');
                $this.parent().parent().parent().find('div.related-list-group-item-heading').removeClass("inner-closed");
                $this.parent().parent().find('div .inner').slideUp(350);
                $this.next().toggleClass('inner-show');
                $this.next().slideToggle(350);
            }
        });

