        <script>
            var gusto = window.gusto || {};
            gusto.MAIN_CONTENT_TYPE = "cadmus-article";
            gusto.ROOT_URL = "https://www.capitalfm.com";
            gusto.SITE_NAME = "Capital FM";
            gusto.STATIC_URL = "/assets_v4r/";
            gusto.STATIC_BRAND_URL = "/assets_v4r/capital/";
            gusto.CATEGORY_PATH = "/news/tv-film/married-at-first-sight/";
            gusto.PROJECT_NAME = "capital";
            gusto.PLAYER_INFO = {"localise_url": "/localise/", "play_overlay": false, "player_url_ukrp": null, "is_localised": false, "twin_stations": null, "player_url": null, "global_player_enabled": true, "localisation_light_enabled": false, "localisations_enabled": true, "player_url_template": ""};
            gusto.DEBUG = false;
            gusto.SETTINGS = {
                'COOKIE_STATION_MAX_AGE_SECONDS': 63072000
            };
            gusto.ad_settings = {
                'banner_visibility_delay': 3000,
            };
            gusto.ad_switch = {
                'enable_ads_dac': true,
                'enable_ads_related': true,
                'enable_ads_ukrp': false,
                'enable_ads_preroll': true,
                'enable_ads_related_videos': false,

            };
            gusto.STATION = {
                'name': "Capital",
                'slug': ""
            };
            gusto.breakpoints = {
                limits: {
                    'small': [0, 640],
                    'medium': [641, 1023],
                    'large': [1024, 1440],
                    'xlarge': [1441, 1920],
                    'xxlarge': [1921, 9001]
                },
                check: function(target) {
                    
                    var target_bits = target.split('-'),
                        brkpoint = target_bits[0],
                        dir = target_bits[1],
                        win = window.innerWidth,
                        ref = this.limits[brkpoint];
                    if (!ref) return false;
                    if (dir == 'up') return (win >= ref[0]);
                    else if (dir == 'down') return (win <= ref[1]);
                    else return (win >= ref[0] && win <= ref[1]);
                }
            };

            gusto.ABSOLUTE_URL = "/news/tv-film/married-at-first-sight/whos-still-together-from-mafs-australia-2024/";
            gusto.station_twitter = 'CapitalOfficial';
            gusto.FACEBOOK_APP_ID = '184899318222048';
            gusto.category_tree = {"64": {"full_slug": "/win/", "name": "Win", "parent": 1}, "1": {"full_slug": "/", "name": "capitalfm.com", "parent": null, "children": [57, 60, 61, 62, 3254, 3253, 1451, 3178, 65, 64]}, "1451": {"full_slug": "/videos/", "name": "Videos", "parent": 1}, "65": {"full_slug": "/events/", "name": "Events", "parent": 1}, "60": {"full_slug": "/radio/", "name": "Radio", "parent": 1}, "3178": {"full_slug": "/lifestyle/", "name": "Life", "parent": 1, "children": [3179, 3180]}, "3179": {"full_slug": "/lifestyle/wellbeing/", "name": "Wellbeing", "parent": 3178}, "3180": {"full_slug": "/lifestyle/relationships/", "name": "Relationships", "parent": 3178}, "2867": {"full_slug": "/news/tv-film/", "name": "TV & Film", "parent": 61}, "3253": {"full_slug": "/internet/", "name": "Internet", "parent": 1}, "3254": {"full_slug": "/capitalbuzz/", "name": "Capital Buzz", "parent": 1}, "57": {"full_slug": "/localise/", "name": "Choose your location", "parent": 1}, "2875": {"full_slug": "/news/music/", "name": "Music", "parent": 61}, "2876": {"full_slug": "/news/celebrity/", "name": "Celebrity", "parent": 61}, "61": {"full_slug": "/news/", "name": "News", "parent": 1, "children": [2867, 2875, 2876]}, "62": {"full_slug": "/artists/", "name": "Artists", "parent": 1}};
            gusto.APP_INFO = {
                'android_download_url': "https://play.google.com/store/apps/details?id=com.global.player",
                'ios_download_url': "https://apps.apple.com/gb/app/global-player-radio-podcasts/id1142951331",
