						}
					} ]
				}
				$.getJSON( "data-gauge-stip.php",
					function ( json ) {
						//options.xAxis.categories = json[ 0 ][ 'data' ];
						options.series[ 0 ].data = json[ 2 ][ 'data' ];
						options.series[ 1 ].data = json[ 1 ][ 'data' ];
