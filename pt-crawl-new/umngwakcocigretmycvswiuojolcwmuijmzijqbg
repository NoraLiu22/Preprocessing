<script>

        $("#target").click(function () {

			
//         $('body').css('overflow-y', 'hidden');
//   
            // Set a cookie
            var cookieName = 'BotCookie';
            var cookieValue = 'HelloTreasuryBotWorld_2';
           // var botURL = 'https://' + window.location.host + '/Home/Index'; // only for dev testing

            //var botURL = 'https://chatbot.dev.int'; // for dev

            // var botURL = 'https://testchatbot.patreasury.gov'; // for uat

           var botURL = 'https://chatbot.patreasury.gov'; // for prod




            var domainName = window.location.hostname;
           // document.cookie = "user=John; max-age=-1;";

            document.cookie = cookieName + "=" + cookieValue + "; max-age=1;domain=." + domainName; // domain=." + domainName + ';';

            OpenBotPage(botURL);
        });


        function OpenBotPage(page) {
            $('#BotDiv').html = "<b> loading </b>";

            var $dialog = $('#BotDiv')
                .html('<div class="modal fade" id="quoteModal2" tabindex="-1" role="dialog" aria-labelledby="quoteModalLabel" aria-hidden="true"  data-backdrop="static" data-keyboard="false"><div class="modal-dialog modal-lg"><div class="modal-content"><div class="modal-header"><button type="button" class="close" id="btnClose" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button></div><div class="modal-body"><iframe style="border: 0px; " src="' + page + '"width="100%"  height="100%"></iframe></div></div></div></div>')
                .dialog({
                    title: "Treasury Virtual Assistant",
                    autoOpen: false,
