<script>
(function($) {
  $.fn.shuffle = function() {
    var m = this.length, t, i;
    while (m) {
      i = Math.floor(Math.random() * m--);
      t = this[m];
      this[m] = this[i];
      this[i] = t;
    }
    return this;
  };

  $('#lee-m-map a.market').shuffle().each(function(index) {
    $(this).css('z-index', index);
    $(this).delay(20 * index).fadeIn(200);
  });
  
}(jQuery));
</script></div><div id="tncms-block-1817801" class="tncms-block"><style>
