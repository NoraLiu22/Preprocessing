<style type="text/css">
	.email-info {
		margin: 10px 0 10px 20px;
	}
</style>

<script type="text/javascript">
	$(document).ready(function() {
		$('#btn-unsub').click(function(e) {
			e.preventDefault();		

			_confirm('Are you sure you wish to unsubscribe from all email lists?', function(ok) {
				if (ok) {
					$(".sub-group").prop('checked', false)
					$('#emailSubForm').submit();
				}
			}, 'Confirm Unsubscribe');
		});


		$('#btn-sub').click(function(e) {
			var actionVal = $(this).data("action");
			$('#action').val(actionVal);

			
				if ($('input[type=checkbox]:checked').length === 0) {
					e.preventDefault();
					_alert('At least one subscription must be checked.');
				}
			
		});

		

		

		$('#emailSubForm').validate({
			// debug: true,
			rules: {
				nameFirst: 'required',
				nameLast: 'required',
				email: {
					required: true,
					email: true
				}
			},
			messages: {
				nameFirst: 'The First Name field is required.',
				nameLast: 'The Last Name field is required.',
				email: {
					required: 'The Email field is required.',
					email: 'Invalid Email Address.'
				}
			}
		});
	});
</script>







<h1>Email Subscriptions</h1>



<form class="form form-horizontal" id="emailSubForm" name="emailSubForm" method="post">
	<div class="form-group">
		<label class="col-sm-2 control-label">Name:</label>
		<div class="col-md-4 col-sm-5">
			<input type="text" class="form-control" id="nameFirst" name="nameFirst" placeholder="First Name" title="First Name" maxlength="30">
		</div>
		<div class="col-md-4 col-sm-5">
			<input type="text" class="form-control" id="nameLast" name="nameLast" placeholder="Last Name" title="Last Name" maxlength="50">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label">Email:</label>
		<div class="col-md-8 col-sm-10">
			<input type="email" class="form-control" id="email" name="email" maxlength="200">
		</div>
	</div>
	<div class="form-group">
		<label class="col-sm-2 control-label">Subscriptions:</label>
		<div class="col-sm-10">
			<div>
				<label class="checkbox-inline" for="subFD">
					<input type="checkbox" class="sub-group" name="subFD" id="subFD" value="1"> Daily Final Determination Updates
				</label>
				<div class="alert alert-info email-info">
					Each day, you will receive an email containing a list of all Final Determinations posted on the previous day.
				</div>
			</div>
			<div>
				<label class="checkbox-inline" for="subCourts">
					<input type="checkbox" class="sub-group" name="subCourts" id="subCourts" value="1"> Court Filings Update
				</label>
				<div class="alert alert-info email-info">
					You will receive regular emails containing recent court filings related to cases previously decided by the OOR. 
				</div>
			</div>
			<div>
				<label class="checkbox-inline" for="subNews">
					<input type="checkbox" class="sub-group" name="subNews" id="subNews" value="1"> Newsletter
				</label>
				<div class="alert alert-info email-info">
					You will receive an occasional newsletter from the Office of Open Records about issues related to the Right-to-Know Law, the Sunshine Act, and government transparency.
				</div>
			</div>
		</div>
	</div>

	<div class="form-group">
