            };
            gusto.ad_switch = {
                'enable_ads_dac': true,
                'enable_ads_related': true,
                'enable_ads_ukrp': false,
                'enable_ads_preroll': true,
                'enable_ads_related_videos': false,

            };
            gusto.STATION = {
                'name': "Classic FM",
