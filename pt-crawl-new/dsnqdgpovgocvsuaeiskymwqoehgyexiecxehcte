              <a href="https://instagram.com/phillymayor" class="phs" data-analytics="social">
              <i class="fab fa-instagram fa-2x" title="Instagram" aria-hidden="true"></i>
                <span class="show-for-sr">Instagram</span>
              </a>
            </div>
                                      </div>
        </td>
      </tr>
      </table>
  </div>

                    </section>
