<script>
window.tp = window.tp || [];

(function (tp) {
	var customVariables = JSON.parse( decodeURIComponent( '%7B%22pmc-page_type%22%3A%22single-pmc_list%22%2C%22pmc-primary-category%22%3A%22Music%22%2C%22pmc-category%22%3A%5B%22Music%22%2C%22Music%20Lists%22%5D%2C%22pmc-primary-vertical%22%3A%22%22%2C%22pmc-vertical%22%3A%22%22%2C%22pmc-wp-logged-in%22%3A%22no%22%2C%22pmc-harmony-enabled%22%3A%22no%22%2C%22pmc-page-subtype%22%3A%22%22%2C%22pmc-post_type%22%3A%22pmc_list%22%2C%22pmc-is_free%22%3A%22no%22%2C%22pmc-always-paywall%22%3A%22no%22%2C%22pmc-branded-content%22%3A%22no%22%2C%22pmc-evergreen%22%3A%22no%22%2C%22pmc-show-regwall%22%3A%22no%22%7D' ) );

	var contentAuthor = decodeURIComponent( '' );

	var cxense_site_ID = decodeURIComponent( '' );

	var tags = JSON.parse( decodeURIComponent( '%5B%22Angus%20Young%22%2C%22BB%20King%22%2C%22Bonnie%20Raiit%22%2C%22Brian%20May%22%2C%22Carlos%20Santana%22%2C%22Carrie%20Brownstein%22%2C%22Chuck%20Berry%22%2C%22David%20Gilmour%22%2C%22Duane%20Allman%22%2C%22Ed%20O%27Brien%22%2C%22Eddie%20Van%20Halen%22%2C%22George%20Harrison%22%2C%22H.E.R.%22%2C%22Jack%20White%22%2C%22James%20Hetfield%22%2C%22Jeff%20Beck%22%2C%22Jerry%20Garcia%22%2C%22Jimi%20Hendrix%22%2C%22Jimmy%20Page%22%2C%22John%20Lennon%22%2C%22Joni%20Mitchell%22%2C%22Jonny%20Greenwood%22%2C%22Keith%20Richards%22%2C%22Kirk%20Hammett%22%2C%22Liz%20Phair%22%2C%22Lucy%20Dacus%22%2C%22Malcolm%20Young%22%2C%22Neil%20Young%22%2C%22Nile%20Rodgers%22%2C%22Paul%20McCartney%22%2C%22Prince%22%2C%22Sister%20Rosetta%20Tharpe%22%2C%22St.%20Vincent%22%2C%22Tom%20Morello%22%2C%22Trey%20Anastasio%22%2C%22Vernon%20Reid%22%5D' ) );

	if (contentAuthor) {
		console.log('PMC Piano tp.push: [setContentAuthor=' + contentAuthor + ']');
		tp.push(['setContentAuthor', contentAuthor]);
	}


	if (tags.length > 0) {
		console.log('PMC Piano tp.push: [setTags=]', tags);
		tp.push(['setTags', tags]);
	}

	Object.keys(customVariables).forEach(function (key) {
		if (!Object.prototype.hasOwnProperty.call(customVariables, key)) {
			return;
		}

		var val = Array.isArray(customVariables[key])
			? customVariables[key].join(',')
			: customVariables[key];

		console.log('PMC Piano tp.push: [setCustomVariable, ' + key + '=' + val + ']');

		tp.push(['setCustomVariable', key, val]);
	});

	// Set pmc-utm_campaign as custom variable
	try {
		let queryString = (window.location.hash || window.location.search).substring(1);

		// Get query string from "window.pmc.tracking.get_properties_string" if possible
		// Note that "window.pmc.tracking" removes and stores utm parameters from URL
		if(window.pmc &&  window.pmc.tracking && window.pmc.tracking.get_properties_string) {
			queryString = window.pmc.tracking.get_properties_string();
		}
		
		const utmParams = queryString.match(/utm_([^=]+)\=([^&#]+)/g); 

		if(utmParams && 0 < utmParams.length) {
			utmParams.forEach(function(keyValue) {
				const [key, value] = keyValue.split("=");
				tp.push(['setCustomVariable', 'pmc-' + key, value]);
				console.log('PMC Piano tp.push: [setCustomVariable, ' + 'pmc-' + key + '=' + value + ']');
			})
		}
	} catch(e) {
		console.log("Error: " + e.toString())
	}

	// Set CxenseSiteId for the Content Module execution
	if (cxense_site_ID) {
		console.log('PMC Piano tp.push: [setCxenseSiteID=' + cxense_site_ID + ']');
		tp.push(['setCxenseSiteId', cxense_site_ID]);
	}

})(window.tp);

</script>

