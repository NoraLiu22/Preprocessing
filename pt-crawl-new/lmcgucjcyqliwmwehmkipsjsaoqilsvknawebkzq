    console.warn('CMP error', `message: ${message_type}`);
    document.getElementById(window.__SP_SCRIPT_TAG_ID__).error = 1;
    window.dispatchEvent(new CustomEvent(window.__SP_EVENT_ERROR__));
  },
  onConsentReady(message_type, uuid, string, info) {
    const privacySettingsElm = document.getElementById('privacy-settings');

    if (message_type === 'gdpr' && info.applies) {
      privacySettingsElm.innerHTML = 'Privacy settings';
      privacySettingsElm.onclick = () => {
        _sp_.gdpr.loadPrivacyManagerModal(559246);
      };
    }

    if (message_type === 'ccpa' && info.applies) {
      privacySettingsElm.innerHTML = 'Do Not Sell or Share My Information';
      privacySettingsElm.onclick = () => {
        const ccpaId =
          window.location.host.indexOf('.co.uk') > 0 ? 882792 : 883186;

        _sp_.ccpa.loadPrivacyManagerModal(ccpaId);
      };
    }
  },
}; </script><script id="sp-script" src="https://cmpv2.independent.co.uk/unified/wrapperMessagingWithoutDetection.js" async onerror="this.error=1;window.dispatchEvent(new CustomEvent('indy:spError'));"></script><script> let _comscore = window._comscore || [];
