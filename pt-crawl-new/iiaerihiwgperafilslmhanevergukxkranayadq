    $(function(){
            /* automatic form expand */
            $('#eedition-featured-search-1076083-term').focus(function(){
                $('#eedition-featured-search-1076083-collapse').collapse('show');
                $('#eedition-featured-search-1076083-collapse-toggle .advanced-text').addClass('hide');
                $('#eedition-featured-search-1076083-collapse-toggle .close-text').removeClass('hide');
            });
            $('#eedition-featured-search-1076083-collapse').on('hide.bs.collapse', function () {
                $('#eedition-featured-search-1076083-collapse-toggle .advanced-text').removeClass('hide');
                $('#eedition-featured-search-1076083-collapse-toggle .close-text').addClass('hide');
            })
            $('#eedition-featured-search-1076083-collapse').on('show.bs.collapse', function () {
                $('#eedition-featured-search-1076083-collapse-toggle .advanced-text').addClass('hide');
                $('#eedition-featured-search-1076083-collapse-toggle .close-text').removeClass('hide');
            })
        var uA = window.navigator.userAgent;
        var vendor = window.navigator.vendor;

        function initFlatpickrJS() {
            var fromDatepicker = document.getElementById('input-d1');
            var toDatepicker = document.getElementById('input-d2');
            
            fromDatepicker.setAttribute('type', 'text');
            toDatepicker.setAttribute('type', 'text');
            
            flatpickr(fromDatepicker, {});
            flatpickr(toDatepicker, {});
            
            fromDatepicker.readOnly = false;
            toDatepicker.readOnly = false;
        }

        if (/Safari/i.test(uA) && /Apple Computer/.test(vendor) && !/Mobi|Android/i.test(uA)) {
            //Desktop Safari
            initFlatpickrJS();
        }

        if (uA.indexOf('Trident/') > -1) {
            initFlatpickrJS();
        }

        /* form submission */
        $('#eedition-featured-search-1076083 .search-submit').on(__tnt.client.clickEvent, function(){
            $('#eedition-featured-search-1076083 .search-submit').find('[data-fa-i2svg]').replaceWith('<i class="fas tnt-spinner tnt-spin"></i>');
            $('#eedition-featured-search-1076083').submit();
            return false;
        });
    });
        $("#site-search-1076083 .dropdown-menu li a").click(function(){
            var selText = $(this).text();
            var selValue = $(this).attr("data-value");
            $(this).parents('.input-group-btn')
                .find('.dropdown-toggle')
                .html(selText+' <span class="caret"></span>')
                .dropdown( 'toggle' );
            $("#site-search-1076083 input[name=d1]").val(selValue);
            return false;
        });
    $('#site-search-1076083-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1076083').submit();
        return false;
    });

    $('#site-search-1076083').on('submit', function(){
        if( $('#site-search-1076083-term').val() ){
            $('#site-search-1076083-btn').find('[data-fa-i2svg]').replaceWith('<i class="fas tnt-spinner tnt-spin"></i>');
            return true;
        }else{
            return false;
        }
    });
</script></div></div><div class="row">
