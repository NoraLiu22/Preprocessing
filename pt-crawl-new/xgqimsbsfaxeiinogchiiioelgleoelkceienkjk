hwc"><gu-island name="SubNav" priority="enhancement" deferUntil="visible" props="{&quot;subNavSections&quot;:{&quot;links&quot;:[{&quot;title&quot;:&quot;Film&quot;,&quot;longTitle&quot;:&quot;Film&quot;,&quot;url&quot;:&quot;/film&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Books&quot;,&quot;longTitle&quot;:&quot;Books&quot;,&quot;url&quot;:&quot;/books&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Music&quot;,&quot;longTitle&quot;:&quot;Music&quot;,&quot;url&quot;:&quot;/music&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Art &amp; design&quot;,&quot;longTitle&quot;:&quot;Art &amp; design&quot;,&quot;url&quot;:&quot;/artanddesign&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;TV &amp; radio&quot;,&quot;longTitle&quot;:&quot;TV &amp; radio&quot;,&quot;url&quot;:&quot;/tv-and-radio&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Stage&quot;,&quot;longTitle&quot;:&quot;Stage&quot;,&quot;url&quot;:&quot;/stage&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Classical&quot;,&quot;longTitle&quot;:&quot;Classical&quot;,&quot;url&quot;:&quot;/music/classicalmusicandopera&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false},{&quot;title&quot;:&quot;Games&quot;,&quot;longTitle&quot;:&quot;Games&quot;,&quot;url&quot;:&quot;/games&quot;,&quot;children&quot;:[],&quot;mobileOnly&quot;:false}]},&quot;currentNavLink&quot;:&quot;Books&quot;,&quot;position&quot;:&quot;footer&quot;}" config="{&quot;renderingTarget&quot;:&quot;Web&quot;,&quot;darkModeAvailable&quot;:false,&quot;assetOrigin&quot;:&quot;https://assets.guim.co.uk/&quot;}"><div data-print-layout="hide" data-testid="sub-nav" data-component="sub-nav" style="--sub-nav-link:var(--sub-nav-link-footer);" class="dcr-v6jxf0"><ul role="list" class="dcr-1gt8egs"><li><a data-src-focus-disabled="tru