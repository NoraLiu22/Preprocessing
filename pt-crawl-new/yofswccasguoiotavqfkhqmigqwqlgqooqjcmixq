    embedded_svc.settings.enabledFeatures = ['LiveAgent'];
    embedded_svc.settings.entryFeature = 'LiveAgent';

    embedded_svc.init(
      'https://uww.my.salesforce.com',
      'https://uww.my.site.com/grantees',
      gslbBaseURL,
      '00D00000000hi9m',
      'MFT_Chat_Agents',
      {
        baseLiveAgentContentURL: 'https://c.la1-c1-ph2.salesforceliveagent.com/content',
        deploymentId: '5721G000000L3Eu',
        buttonId: '5731G000000L3P7',
