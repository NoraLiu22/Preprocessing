function balalaika()
{
	document.s.string4.selectedIndex=document.s.string2.selectedIndex=document.s.string3.selectedIndex=4;
	document.s.string1.selectedIndex=9;
	tune(0);
}
function sc()
{
	var a=document.s.chord.options[document.s.chord.selectedIndex].text,b=document.s.majmin.options[document.s.majmin.selectedIndex].text;
	var d=new Object(),e=new Object(),i,j,k,l,akkord,start,f=new Object(),d1=new Object,d2=new Object,d3=new Object,d4=new Object,m=new Object();
	b=b.replace(/-/,"_");

	for(i=0;i<12;i++)
	{
		if(Noten[i]==a)
		{
			k=0;
			for(j=i;j<12;j++){e[k]=Noten[j];k++;}
			for(j=0;j<i;j++){e[k]=Noten[j];k++;}
		}
	}
	d=c[b].split(';');
	akkord='';for(i=0;i<d.length-1;i++) akkord=akkord+e[d[i]]+";"
	k=Noten.join(";");
	l=k+";"+k+";"+k+";";
	e=akkord.split(';');

	s1=l.substring(l.indexOf(document.s.string1.options[document.s.string1.selectedIndex].text+";"),256);
	s2=l.substring(l.indexOf(document.s.string2.options[document.s.string2.selectedIndex].text+";"),256);
	s3=l.substring(l.indexOf(document.s.string3.options[document.s.string3.selectedIndex].text+";"),256);
	s4=l.substring(l.indexOf(document.s.string4.options[document.s.string4.selectedIndex].text+";"),256);

	i=document.s.fret.selectedIndex;
	document.getElementById('incomplete').style.visibility='hidden';
	document.getElementById('chord').style.visibility='hidden';

//alg1
	if(document.s.alg[0].checked)
	{
		j=document.s.zerofret.checked;
		akkord=testen(s1,i,e,j)+akkord;
		akkord=testen(s2,i,e,j)+akkord;
		akkord=testen(s3,i,e,j)+akkord;
		akkord=testen(s4,i,e,j)+akkord;

		akk2=akkord.split(';');
		sss1=s1.split(';');
		sss1=sss1[akk2[3]]
		sss2=s2.split(';');
		sss2=sss2[akk2[2]]
		sss3=s3.split(';');
		sss3=sss3[akk2[1]]
		sss4=s4.split(';');
		sss4=sss4[akk2[0]];
		sss=sss1+";"+sss2+";"+sss3+";"+sss4+";";
		//alert(sss+": "+sss.search("C"+";"));
		inc=sss.search(akk2[5]+";");
		if (inc!=-1) inc=sss.search(akk2[6]+";");
		if (inc!=-1) inc=sss.search(akk2[7]+";");
		if (inc!=-1) inc=sss.search(akk2[8]+";");

		if (inc==-1)
		{
			document.getElementById('incomplete').style.visibility='visible';
			document.getElementById('chord').style.visibility='visible';
		}

	}
//alg2
	else
	{
		if(e[3]==e[0]) e[3]=e[document.s.rep.selectedIndex];

		d1=s1.split(';');k=d1[0];for(j=0;j<i;j++) d1[j]="z";if(document.s.zerofret.checked) d1[0]=k;
		d2=s2.split(';');k=d2[0];for(j=0;j<i;j++) d2[j]="z";if(document.s.zerofret.checked) d2[0]=k;
		d3=s3.split(';');k=d3[0];for(j=0;j<i;j++) d3[j]="z";if(document.s.zerofret.checked) d3[0]=k;
		d4=s4.split(';');k=d4[0];for(j=0;j<i;j++) d4[j]="z";if(document.s.zerofret.checked) d4[0]=k;

		d1=a2_getmax(d1,e);d2=a2_getmax(d2,e);d3=a2_getmax(d3,e);d4=a2_getmax(d4,e);

		for(i=0;i<4;i++) e[i]=-1;
		for(i=0;i<e.length-1;i++) {m[i]=0;}
//alert(d1[0]+" "+d1[1]+" "+d1[2]+" "+d1[3]+String.fromCharCode(13)+d2[0]+" "+d2[1]+" "+d2[2]+" "+d2[3]+String.fromCharCode(13)+d3[0]+" "+d3[1]+" "+d3[2]+" "+d3[3]+String.fromCharCode(13)+d4[0]+" "+d4[1]+" "+d4[2]+" "+d4[3]);
		for(j=0;j<4;j++)
		{
			for(i=0;i<e.length-1;i++) if(m[i]>=0)
			{
				m[i]=0;
				if(!isNaN(d1[i])) m[i]=m[i]+d1[i];
				if(!isNaN(d2[i])) m[i]=m[i]+d2[i];
				if(!isNaN(d3[i])) m[i]=m[i]+d3[i];
				if(!isNaN(d4[i])) m[i]=m[i]+d4[i];
			}
			k=(maximum(m[0],m[1],m[2],m[3]));
			d=k.split(";");
			m[d[0]]=-1;
			k=minimum(d1[d[0]],d2[d[0]],d3[d[0]],d4[d[0]]);
			d=k.split(";");
			e[3-d[0]]=d[1];
			if(d[1]!=99)
			{
				if(d[0]==0) d1[0]=d1[1]=d1[2]=d1[3]=Number.NaN;
				if(d[0]==1) d2[0]=d2[1]=d2[2]=d2[3]=Number.NaN;
				if(d[0]==2) d3[0]=d3[1]=d3[2]=d3[3]=Number.NaN;
				if(d[0]==3) d4[0]=d4[1]=d4[2]=d4[3]=Number.NaN;
			}
		}
		akkord=e.join(";");
	}
	zeige(akkord);
}

function maximum()
{
	var i,j,k,a=maximum.arguments;
	k=-999;
	for(i=0;i<a.length;i++)	if(k<a[i]){k=a[i];j=i;}
	return j+";"+k;
}
function minimum()
{
	var i,j,k,a=minimum.arguments;
	k=999;
	for(i=0;i<a.length;i++)	if(k>a[i]){k=a[i];j=i;}
	return j+";"+k;
}

function a2_getmax(saite,akkord)
{
	var i,j,k,d=new Object();
	for(i=0;i<akkord.length-1;i++)
	{
		for(j=0;j<15;j++)
			if(akkord[i]==saite[j]){d[i]=j;break;}
		if(d[i]!=j) d[i]=99; // Note nicht gefunden, hohe Priorität damit andere Note gespielt werden kann
	}
	return d;
}
function reverse()
{
	var s,k,l,e,i,t1,t2,t3,t4;
	var e=new Object();
	i=document.s.p4.value;if(i>15||i<0||isNaN(i)||i=='')document.s.p4.value="X";
	i=document.s.p3.value;if(i>15||i<0||isNaN(i)||i=='')document.s.p3.value="X";
	i=document.s.p2.value;if(i>15||i<0||isNaN(i)||i=='')document.s.p2.value="X";
	i=document.s.p1.value;if(i>15||i<0||isNaN(i)||i=='')document.s.p1.value="X";
	s='';
	k=Noten.join(";");
	l=k+";"+k+";"+k+";";

//	e=akkord.split(';');
	if(document.s.p1.value!='X')s1=l.substring(l.indexOf(document.s.string4.options[(parseInt(document.s.string4.selectedIndex)+parseInt(document.s.p1.value))%12].text+";"),256); else s1=';';
	if(document.s.p2.value!='X')s2=l.substring(l.indexOf(document.s.string3.options[(parseInt(document.s.string3.selectedIndex)+parseInt(document.s.p2.value))%12].text+";"),256); else s2=';';
	if(document.s.p3.value!='X')s3=l.substring(l.indexOf(document.s.string2.options[(parseInt(document.s.string2.selectedIndex)+parseInt(document.s.p3.value))%12].text+";"),256); else s3=';';
	if(document.s.p4.value!='X')s4=l.substring(l.indexOf(document.s.string1.options[(parseInt(document.s.string1.selectedIndex)+parseInt(document.s.p4.value))%12].text+";"),256); else s4=';';
	e=s1.split(';');s1=e[0];e=s2.split(';');s2=e[0];e=s3.split(';');s3=e[0];e=s4.split(';');s4=e[0];

	s="\n";
	s=s+"  "+document.s.string1.options[document.s.string1.selectedIndex].text+"-String: "+s4+"\n";
	s=s+"  "+document.s.string2.options[document.s.string2.selectedIndex].text+"-String: "+s3+"\n";
	s=s+"  "+document.s.string3.options[document.s.string3.selectedIndex].text+"-String: "+s2+"\n";
	s=s+"  "+document.s.string4.options[document.s.string4.selectedIndex].text+"-String: "+s1+"\n";

	t1=l.substring(l.indexOf(s1+";"),256);
	t2=l.substring(l.indexOf(s2+";"),256);
	t3=l.substring(l.indexOf(s3+";"),256);
	t4=l.substring(l.indexOf(s4+";"),256);

	if(s1==s2)s2='';if(s1==s3)s3='';if(s1==s4)s4='';if(s2==s3)s3='';if(s2==s4)s4='';if(s3==s4)s4='';

	e=t1.split(";");l='';if (s1!='') for(i=0;i<12;i++){if(e[i]==s1)l=l+i+";";if(e[i]==s2)l=l+i+";";if(e[i]==s3)l=l+i+";";if(e[i]==s4)l=l+i+";";}t1=l;
	e=t2.split(";");l='';if (s2!='') for(i=0;i<12;i++){if(e[i]==s1)l=l+i+";";if(e[i]==s2)l=l+i+";";if(e[i]==s3)l=l+i+";";if(e[i]==s4)l=l+i+";";}t2=l;
	e=t3.split(";");l='';if (s3!='') for(i=0;i<12;i++){if(e[i]==s1)l=l+i+";";if(e[i]==s2)l=l+i+";";if(e[i]==s3)l=l+i+";";if(e[i]==s4)l=l+i+";";}t3=l;
	e=t4.split(";");l='';if (s4!='') for(i=0;i<12;i++){if(e[i]==s1)l=l+i+";";if(e[i]==s2)l=l+i+";";if(e[i]==s3)l=l+i+";";if(e[i]==s4)l=l+i+";";}t4=l;

	e=t1.split(";");if (e.length==4)t1=t1+"0;"
	e=t2.split(";");if (e.length==4)t2=t2+"0;"
	e=t3.split(";");if (e.length==4)t3=t3+"0;"
	e=t4.split(";");if (e.length==4)t4=t4+"0;"

	l='';
	for (i=0;i<document.s.majmin.options.length;i++)
	{
		t=document.s.majmin.options[i].text;
		t=t.replace(/-/,"_");
		if (c[t]==t1)l=l+"  "+s1.substring(0,2)+umw(t)+"\n";
		if (c[t]==t2)l=l+"  "+s2.substring(0,2)+umw(t)+"\n";
		if (c[t]==t3)l=l+"  "+s3.substring(0,2)+umw(t)+"\n";
		if (c[t]==t4)l=l+"  "+s4.substring(0,2)+umw(t)+"\n";
	}

	if(l=='')l="No chords found!\n";else l="Chord(s) found:\n"+l;
	s="=== CHORD INFO ===\n\nChord notes: "+s+"\n"+l+"\n"
	s=s+"Q: Why doesn't the reverse chord finder       \nfind all chords that are generated by\nthe 'simple chords' algorithm?\n";
	s=s+"A: Because some of these chords are\nincomplete. It can just find chords that\nuse *ALL* the notes of a chord.\n";
	s=s+"\n";
	s=s+"Another thing, if the chord uses more\nthan 4 notes the reverse chord find will\nnot find chords that include the quint!";
	s=s+"\n\n© 2003 by WS64 \& ChordFind.com";
	alert(s);
}
function umw(t)
{
	t=t.replace("_","-");
	if(t=="Major")t='';
	if(t=="Minor")t='m';
	return t;
}

function testen(k,start,e,zero)
{
	var i,j,raus,d=new Object();
	d=k.split(';');
	raus=false;
	if(zero) for(i=0;i<e.length;i++) if(e[i]==d[0]) {raus=true;return "0;";break;}
	if(!raus)
	for(j=start;j<16;j++) for(i=0;i<e.length;i++) if(e[i]==d[j]) {raus=true;return j+";";break;}
	if(!raus) return "-1;";
}
function zeige(akkord)
{
	var d=new Object();
	d=akkord.split(";");
//	alert(akkord);
	resetfret();
	if(d[3]!=0) {if(d[3]>=0&&d[3]<=16) {swap('S1-'+d[3],'full.gif');document.s.p4.value=d[3];} else {swap('S1-0','noplay.gif');document.s.p4.value="X";}} else document.s.p4.value=0;
	if(d[2]!=0) {if(d[2]>=0&&d[2]<=16) {swap('S2-'+d[2],'full.gif');document.s.p3.value=d[2];} else {swap('S2-0','noplay.gif');document.s.p3.value="X";}} else document.s.p3.value=0;
	if(d[1]!=0) {if(d[1]>=0&&d[1]<=16) {swap('S3-'+d[1],'full.gif');document.s.p2.value=d[1];} else {swap('S3-0','noplay.gif');document.s.p2.value="X";}} else document.s.p2.value=0;
	if(d[0]!=0) {if(d[0]>=0&&d[0]<=16) {swap('S4-'+d[0],'full.gif');document.s.p1.value=d[0];} else {swap('S4-0','noplay.gif');document.s.p1.value="X";}} else document.s.p1.value=0;
}
function resetfret()
{
	var i;
	for(i=0;i<16;i++) swap('S1-'+i,'empty.gif');
	for(i=0;i<16;i++) swap('S2-'+i,'empty.gif');
	for(i=0;i<16;i++) swap('S3-'+i,'empty.gif');
	for(i=0;i<16;i++) swap('S4-'+i,'empty.gif');
}

function MM_findObj(n,d)
{
	var p,i,x;
	if(!d) d=document;
	if((p=n.indexOf("?"))>0&&parent.frames.length)
	{
		d=parent.frames[n.substring(p+1)].document;
		n=n.substring(0,p);
	}
	if(!(x=d[n])&&d.all) x=d.all[n];
	for(i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  	for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
	if(!x&&document.getElementById) x=document.getElementById(n);
	return x;
}
function MM_swapImage() { //v3.0
  	var i,j=0,x,a=MM_swapImage.arguments;
	document.MM_sr=new Array;
	for(i=0;i<(a.length-2);i+=3)
   		if((x=MM_findObj(a[i]))!=null)
		{
			document.MM_sr[j++]=x;
			if(!x.oSrc) x.oSrc=x.src;
			x.src=a[i+2];
		}
}
function swap(a,b){MM_swapImage(a,'','x/'+b);}

function tune(i)
{
	var k,s,j;
	for(k=1;k<11;k++) if(inst[k][4])swap(inst[k][4],"sw-"+inst[k][5]);
	if(i>0)
	{
		j=i;
		if(i==5)
		{
			j=document.s.uk.selectedIndex;
			if(j==0)
				{j=5;inst[5][5]="ukulele.gif";}
			else if (j==1)
				{j=6;inst[5][5]="ukulele.gif";}
			else if (j==2)
				{j=7;inst[5][5]="bukulele.gif";}
			else if (j==3)
				{j=13;inst[5][5]="chevaquinho.gif";}
		}
		if(i==1)
		{
			j=document.s.ma.selectedIndex;
			if(j==0)
				{j=1;inst[1][5]="mandolin.gif";}
			else if(j==1)
				{j=3;inst[1][5]="mandocello.gif";}				
		}
		if(j==2)
		{
			if(document.s.bo.selectedIndex==1) j=8;
			if(j==2)inst[2][5]="bouzouki.gif";
			if(j==8)inst[2][5]="irishbouzouki.gif";
		}
		if(j==3)
		{
			if(document.s.ba.selectedIndex==1)j=9;
			if(document.s.ba.selectedIndex==2)j=1;
			if(document.s.ba.selectedIndex==3)j=12;
			if(document.s.ba.selectedIndex==4)j=13;
			if(document.s.ba.selectedIndex==5)j=14;
		}
		document.s.string1.selectedIndex=inst[j][0];
		document.s.string2.selectedIndex=inst[j][1];
		document.s.string3.selectedIndex=inst[j][2];
		document.s.string4.selectedIndex=inst[j][3];
		swap(inst[i][4],"t-"+inst[i][5]);
	}
	document.getElementById("erl").innerHTML=inst[i][6];
	sc();
}
function change_leri()
{
	var i;
	if (document.s.leri[1].checked)
	{
		for(i=1;i<16;i++) {document.getElementById("a"+i).innerHTML=i;document.getElementById("a"+i).align="";}
		document.getElementById("spieg").style.filter="";
	}
	else
	{
		for(i=1;i<16;i++) {document.getElementById("a"+i).innerHTML=16-i;document.getElementById("a"+i).align="left";}
		document.getElementById("spieg").style.filter="FlipH()";
	}
}
function zmerk()
{
	if(document.s.fret.selectedIndex==0)
	{
		zmerker=document.s.zerofret.checked;
		document.s.zerofret.checked=true;
		document.s.zerofret.disabled=true;
	}
	else
	{
		document.s.zerofret.checked=zmerker;
		document.s.zerofret.disabled=false;
	}
	sc();
}
</script>
</HEAD>

