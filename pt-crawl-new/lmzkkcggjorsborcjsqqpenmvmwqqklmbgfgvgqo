                    setFontLoadedClassOnBody();
                  }
                }, 15);
                document.fonts && document.fonts.ready.then(() => {
                  setFontLoadedClassOnBody();
                  clearInterval(fontLoadTimer);
                });
              } else {
                window.addEventListener('load', function() {
                  setFontLoadedClassOnBody();
                })
              }
