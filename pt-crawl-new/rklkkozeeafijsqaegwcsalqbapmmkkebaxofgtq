// URL Parameter Tracking
  
var siteurl = location.href;
var urlQuery = location.search.substring(1);

// URL Query Strings
function storeUrlQuery() {
  // Get values/key pairs from URL query string
  function parseUrlQuery(queryString) {
    var params = {},
      queries,
      temp,
      i,
      l;
    // Split into key/value pairs
    queries = queryString.split("&");
    // Convert the array of strings into an object
    for (i = 0, l = queries.length; i < l; i++) {
      temp = queries[i].split("=");
      params[temp[0].toLowerCase()] = temp[1];
    }
    return params;
  }

  var urlQueryAll = urlQuery;
  var parsedQuery = parseUrlQuery(urlQueryAll);
  var utmId = parsedQuery["utm_id"];
  var utmMedium = parsedQuery["utm_medium"];
  var utmSource = parsedQuery["utm_source"];
  var utmCampaign = parsedQuery["utm_campaign"];
  var cid = parsedQuery["cid"];

  // Check if query string exists
  if (urlQuery.length) {
    if (urlQueryAll != null) {
      if (sessionStorage.getItem("urlQueryAll") == null) {
        sessionStorage.setItem("urlQueryAll", urlQueryAll);
      }
    }

    if (utmId != null) {
      if (sessionStorage.getItem("utmId") == null) {
        sessionStorage.setItem("utmId", utmId);
      }
    }

    if (utmMedium != null) {
      if (sessionStorage.getItem("utmMedium") == null) {
        sessionStorage.setItem("utmMedium", utmMedium);
      }
    }

    if (utmSource != null) {
      if (sessionStorage.getItem("utmSource") == null) {
        sessionStorage.setItem("utmSource", utmSource);
      }
    }

    if (utmCampaign != null) {
      if (sessionStorage.getItem("utmCampaign") == null) {
        sessionStorage.setItem("utmCampaign", utmCampaign);
      }
    }

    if (cid != null) {
      if (sessionStorage.getItem("cid") == null) {
        sessionStorage.setItem("cid", cid);
      }
    }
  } else {
    if (sessionStorage.getItem("urlQueryAll") != null) var urlQueryAll = sessionStorage.getItem("urlQueryAll");
    if (sessionStorage.getItem("utmId") != null) var utmId = sessionStorage.getItem("utmId");
    if (sessionStorage.getItem("utmMedium") != null) var utmMedium = sessionStorage.getItem("utmMedium");
    if (sessionStorage.getItem("utmSource") != null) var utmSource = sessionStorage.getItem("utmSource");
    if (sessionStorage.getItem("utmCampaign") != null) var utmCampaign = sessionStorage.getItem("utmCampaign");
    if (sessionStorage.getItem("cid") != null) var cid = sessionStorage.getItem("cid");
  }
}

// URL Query Strings
function useUrlQuery() {
  var urlQueryAll = sessionStorage.getItem("urlQueryAll");
  var utmMedium = sessionStorage.getItem("utmMedium");
  var utmId = sessionStorage.getItem("utmId");
  var utmSource = sessionStorage.getItem("utmSource");
  var utmCampaign = sessionStorage.getItem("utmCampaign");
  var cid = sessionStorage.getItem("cid");

  // Target links and add query string to href with target class
  function addUrlQueryToLink(queryVal) {
    var urlQueryVal = queryVal.replace("utm_id", "mscklid").replace("cid", "zanpid");
    $(".url-query-target").attr("href", function () {
      return this.href + "?" + urlQueryVal;
    });
  }

  if (urlQueryAll != null) addUrlQueryToLink(urlQueryAll);
}

if (window.sessionStorage) {
  storeUrlQuery();
  useUrlQuery();
}


// Number of clicks on the trigger element initially set on 0
let numOfClicks = 0;
// Get trigger element 
const trigger = document.getElementById("menu-trigger");
// Get body element
const bodyEl = document.getElementsByTagName("body")[0];

// Set onclick function to trigger element
trigger.onclick = () => {
    numOfClicks += 1;
    // Check if number of clicks is an even value: 
    // odd value - first click, even value - second click
    const isNumOfClicksEven = numOfClicks % 2 === 0;
    // On first click set body's overflow property to "auto", 
    // On second click set body's overflow property to "hidden"
    isNumOfClicksEven ? bodyEl.style.overflow = "auto" : bodyEl.style.overflow = "hidden";
};
  
