    oneLinkURL: oneLinkURL,
    afParameters: {
        mediaSource: mediaSource,
        afCustom: [
            custom_ss_ui
        ]
    }
});

// Populate link and qr code
if (result) {
    downloadLink.setAttribute('href', result.clickURL);
    if (__tnt.client.platform.ios || __tnt.client.platform.android) {
        downloadSection.classList.remove('hide');
    } else {
        window.AF_SMART_SCRIPT.displayQrCode("qr_code_1");
        window.AF_SMART_SCRIPT.displayQrCode("qr_code_2");
        scanQr1.classList.remove('hide');
        scanQr2.classList.remove('hide');
        badges1.classList.remove('hide');
        badges2.classList.remove('hide');
    }
}

