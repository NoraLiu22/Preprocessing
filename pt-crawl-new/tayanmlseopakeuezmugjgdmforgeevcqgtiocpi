<script>
	$(function() {
		$('#form-login').keypress(function(e) {
			if (e.which == 13) {
				$('#btn-login').click();
			}
		});


		$('#btn-reset').click(function() {
			var email = $('#email').val();

			if (email) {
				_confirm("This will send a confirmation to the email address you've entered.  Please follow the instructions in the confirmation email to complete the password reset.\n\nAre you sure you wish to continue?", function(ok) {
					if (ok) {
						$.post({
							url: '/Resources/cfc/Docket.cfc?method=requestPasswordReset&returnFormat=json',
							dataType: 'json',
							data: {
								email: email
							},
							success: function(status) {
								if (status.msg) {
									_alert(status.msg);
								}
							},
							error: ajaxError
						});
					}
				});
			}
			else {
				_alert("Email field required.");
			}
		});
	});
</script>





