<script>
  require(['childrens/showMore', 'lib/minpubsub', 'jquery-1', 'domReady!'], function (ShowMore, pubsub, $) {
    var showMore = ShowMore.create({
      containerId: 'pocket-content-card-container',
      portal: '.content-collection-section__portal',
      panel: '.content-collection-section__panel',
      items: '.content-list__item',
      mask: '.content-collection-section__mask',
      controls: '.content-collection-section__controls',
      toggleButton: '.content-collection-section__toggle-button .button__link',
      toggleButtonContainer: '.content-collection-section__toggle-button',
      toggleButtonLabel: '.content-collection-section__toggle-button .button__text',
      itemType: 'ways to Join In',
      collapseLabelPrefix: 'less',
      animationEnabled: false,
      maxInitialItemsShown: 25
    });

    pubsub.subscribe('/showMore/loadedContent/pocket-content-card-container', function (addedPage) {
        var content = $(addedPage);
         angular.element(document).injector().invoke(function($compile) {
             var scope = angular.element(content).scope();
             $compile(content)(scope);
         });
    });

});

</script>

