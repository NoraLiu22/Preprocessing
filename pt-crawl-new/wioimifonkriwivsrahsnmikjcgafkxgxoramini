		<!-- Google Charts -->
		<script type="text/javascript" src="google-charts.js"></script>
		<script>		
			
			//Get the button:
			mybutton = document.getElementById("topBtn");
			// When the user scrolls down 20px from the top of the document, show the button
			window.onscroll = function() {
				scrollFunction()
			};

			function scrollFunction() {
				if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
					mybutton.style.display = "block";
				} else {
					mybutton.style.display = "none";
				}
			}
			// When the user clicks on the button, scroll to the top of the document
			function topFunction() {
				document.body.scrollTop = 0; // For Safari
				document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
			}
			// Contact Dropdown //
			function ContactDrop() {
				document.getElementById("ContactDropdown").classList.toggle("show");
			}

			function ContactFilter() {
				var input, filter, ul, li, a, i;
				input = document.getElementById("ContactInput");
				filter = input.value.toUpperCase();
				div = document.getElementById("ContactDropdown");
				a = div.getElementsByTagName("a");
				for (i = 0; i < a.length; i++) {
					txtValue = a[i].textContent || a[i].innerText;
					if (txtValue.toUpperCase().indexOf(filter) > -1) {
						a[i].style.display = "";
					} else {
						a[i].style.display = "none";
					}
				}
			}
			// Project Dropdown //
			function ProjectDrop() {
				document.getElementById("ProjectDropdown").classList.toggle("show");
			}

			function ProjectFilter() {
				var input, filter, ul, li, a, i;
				input = document.getElementById("ProjectInput");
				filter = input.value.toUpperCase();
				div = document.getElementById("ProjectDropdown");
				a = div.getElementsByTagName("a");
				for (i = 0; i < a.length; i++) {
					txtValue = a[i].textContent || a[i].innerText;
					if (txtValue.toUpperCase().indexOf(filter) > -1) {
						a[i].style.display = "";
					} else {
						a[i].style.display = "none";
					}
				}
			}
		</script>
