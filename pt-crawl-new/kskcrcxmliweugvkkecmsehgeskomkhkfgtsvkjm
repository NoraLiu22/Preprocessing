        <div itemprop="image" itemscope itemtype="https://schema.org/ImageObject">
            <meta itemprop="width" content="1450">
            <meta itemprop="height" content="850">
            <meta itemprop="contentUrl" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/f/e3/fe319248-c497-52b9-a8ff-d841b7cd6bfd/660162acd49be.image.png?resize=1200%2C703">
            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/f/e3/fe319248-c497-52b9-a8ff-d841b7cd6bfd/660162acd49be.image.png?resize=1200%2C703">
