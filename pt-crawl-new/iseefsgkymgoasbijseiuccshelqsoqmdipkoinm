		<script>
			(function($) {
				// Setup the Tabs.
				const tabs = $('#widget-79051-1 [role="tab"]')
				const tablist = $('#widget-79051-1 [role="tablist"]')
				const panels = $('#widget-79051-1 [role="tabpanel"]')

				tabs.click(function () {
					tabs.attr('aria-selected', false)
					tabs.removeAttr('tabindex')
					$(this).attr('aria-selected', true)
					$(this).attr('tabindex', -1)
					panels.hide()
					panels.filter(`#${$(this).attr('aria-controls')}`).show().addClass('active')
				})

				// Setup wheel vertical movement to scroll the Tablist horizontally.
				tablist.on('mousewheel', function(e) {
					e = e.originalEvent
					if (!e.deltaY) return

					e.currentTarget.scrollLeft += e.deltaY + e.deltaX
					e.preventDefault()
				})
			}(jQuery));
		</script>
	<div class="url_swap_running" style="display: none;"></div></body>
</html>
 contentType 9 text/html url 76 https://www.usatoday.com:443/money/homefront/lawn/weed-man-lawn-care-review/ responseCode 3 200 