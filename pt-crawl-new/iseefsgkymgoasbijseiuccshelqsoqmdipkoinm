ktop-menu .desktop-button{display:flex;align-items:center;height:100%;padding:0 15px;background:var(--color-secondary);font-size:12px}.c-header .sub-navigation-menu{display:none}@media (min-width:992px){.c-header .sub-navigation-menu{display:block}.c-header .sub-navigation-menu-wrapper{max-width:1140px;margin-right:auto;margin-left:auto;padding-right:var(--lateral-padding);padding-left:var(--lateral-padding);max-width:1440px;height:100%}.c-header .sub-navigation-menu .sub-menu{display:none}.c-header .sub-navigation-menu-nav .menu{display:flex;padding-left:18%;gap:0 28px;position:relative}.c-header .sub-navigation-menu-nav .menu-item{display:flex;align-items:center}.c-header .sub-navigation-menu-nav .menu-item.subnav-main-link a{text-transform:uppercase;color:#2d6749}}@media (min-width:992px) and (max-width:1024px){.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>a{font-size:13px}}@media (min-width:992px){.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu{position:absolute;top:100%;border-top:4px solid var(--color-primary);background:#303030;padding:20px;display:none;gap:28px 20px;grid-template-columns:repeat(2,1fr);min-width:298.5px;max-width:577px;max-height:calc(100vh - 150px);overflow:hidden}.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu li{align-items:flex-start;min-width:258.5px}}@media (min-width:992px) and (min-width:992px) and (max-width:1024px){.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu li{min-width:initial}}@media (min-width:992px){.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu li .no-link,.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu li a{color:var(--color-white)}.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu>li>a{border-bottom:2px solid var(--color-primary);padding:0}.c-header .sub-navigation-menu-nav .menu-item.menu-item-depth-0>.sub-menu>li>.no-link{border-bottom:2px solid var(