<script>
	(function() {
      // Check if the body has the class .logged-in
      var isUserLoggedIn = document.body.classList.contains('logged-in');

      // If the user is logged in, do not perform the redirect
      if (isUserLoggedIn) {
          return;
      }

      // If the user is not logged in, make the AJAX request, and redirect user to unsupported eu USAT page
      var xhr = new XMLHttpRequest();
      xhr.open('GET', 'https://ipapi.co/json/?key=ae8a2b23d976565564426e1638592413ae61fa79', true);
      xhr.onreadystatechange = function() {
          if (xhr.readyState === 4 && xhr.status === 200) {
              var response = JSON.parse(xhr.responseText);
              if (response.country_code !== 'US') {
                  window.location.replace('https://www.usatoday.com/unsupported-eu/');
              }
          }
      };
      xhr.send();
  	})();
    
</script>
