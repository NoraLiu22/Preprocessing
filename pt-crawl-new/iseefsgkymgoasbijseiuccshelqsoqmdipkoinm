	--_quote-size: 35px;
	position: absolute;
	content: '';
	width: var(--_quote-size);
	aspect-ratio: 1/1; 
	background-image: url('https://d2hal1rtfsjwuv.cloudfront.net/wp-content/uploads/2024/03/21171510/svgviewer-output-1.svg');
	background-size: cover;
	top: calc(var(--_quote-size)/-2);
	left: 50%;
	transform: translatex(-50%);
}

