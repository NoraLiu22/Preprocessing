    <script>
            $(document).ready(function () {
                $('.mobile_search').on('click touch', function () {
                    $('#arama-form2').submit();
                    return false;
                });
                if (getCookie("v") == null || getCookie("v") == ''){
                    $('.uiVersion').addClass('oldVersion').removeClass('newVersion').text('Regresar a la versi&#243;n antigua');
                }else if(getCookie("v")=='1'){
                    $('.uiVersion').addClass('oldVersion').removeClass('newVersion').text('Regresar a la versi&#243;n antigua');
                }else{
                    $('.uiVersion').addClass('newVersion').removeClass('oldVersion').text('Ir a la versi&#243;n nueva');
                }



                $('.newVersion').on('click', function () {
                    setCookie("v", 1, 999);
                    $(this).addClass('oldVersion').removeClass('newVersion');
                    window.location.href = 'https://www.aa.com.tr/es';
                    return false;
                });

                $('.oldVersion').on('click', function () {
                    setCookie("v", 0, 999);
                    $(this).addClass('newVersion').removeClass('oldVersion');
                    window.location.href = 'https://www.aa.com.tr/es';
                    return false;
                });
            });

    </script>

