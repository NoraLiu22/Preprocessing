<script defer>
  let cat = '';
  let bodyClasses = document.querySelector('body').classList;
  let isArticle = bodyClasses.contains('single-post');
  let gciTitle = window.document.title;
  let gciCanon = document.querySelector("link[rel='canonical']").href;
  let gciRefer = window.document.referrer;
  let gciPath = window.location.pathname;
  if(isArticle) {
      let articleData = document.getElementById('article-schema');
      let article = JSON.parse(articleData.innerText);
      cat = gciPath.split('/').filter(e => e).at(-2);
      gciAnalytics.view({
          "content-byline": article['author'][0]['name'],
          "content-contributor-names": article['author'][0]['name'],
          "content-cst": "money:homefront",
          "content-date-published": article['datePublished'],
          "content-headline": gciTitle,
          "content-id": "17133445009",
          "content-name": gciTitle,
          "content-protection-state": "free",
          "content-source": "Homefront",
          "content-ssts-section": "money",
          "content-ssts-subsection": "money:homefront",
          "content-ssts-subtopic": "money:homefront:" + cat,
          "content-ssts-topic": "money:homefront:" + cat,
          "content-type": "story pages",
          "content-title": gciTitle,
          "page-full-url": window.location.href,
          "event-type": "pageview",
          "page-canonical-url": gciCanon,
          "page-referrer": gciRefer,
      });
  } else {     
      let isHome = bodyClasses.contains('home');
 	  let catHome = isHome ? 'front' : 'hub';
      cat = isHome ? '' : ":" + gciPath.split('/').filter(e => e).slice(-1);
      gciAnalytics.view({
          "content-cst": "money:homefront:main",
          "content-name": gciPath,
          "content-ssts-section": "money",
          "content-ssts-subsection": "money:homefront",
          "content-ssts-subtopic": "money:homefront" + cat,
          "content-ssts-topic": "money:homefront" + cat,
          "content-type": "sub-section " + catHome,
          "content-title": gciTitle,
          "page-full-url": window.location.href,
          "event-type": "pageview",
          "page-canonical-url": gciCanon,
          "page-referrer": gciRefer,
      });
  }
</script>

<script>
	(function() {
      // Check if the body has the class .logged-in
      var isUserLoggedIn = document.body.classList.contains('logged-in');

      // If the user is logged in, do not perform the redirect
      if (isUserLoggedIn) {
          return;
      }

      // If the user is not logged in, make the AJAX request, and redirect user to unsupported eu USAT page
      var xhr = new XMLHttpRequest();
      xhr.open('GET', 'https://ipapi.co/json/?key=ae8a2b23d976565564426e1638592413ae61fa79', true);
      xhr.onreadystatechange = function() {
          if (xhr.readyState === 4 && xhr.status === 200) {
              var response = JSON.parse(xhr.responseText);
              if (response.country_code !== 'US') {
                  window.location.replace('https://www.usatoday.com/unsupported-eu/');
              }
          }
      };
      xhr.send();
  	})();
    
</script>
