         class=" footable_parent ninja_table_wrapper loading_ninja_table wp_table_data_press_parent semantic_ui colored_table">
                <table data-ninja_table_instance="ninja_table_instance_2" data-footable_id="199955" data-filter-delay="1000" aria-label="Our expert opinion - Mondays sales CRM"            id="footable_199955"
