    <script defer src="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/live/libraries/flex/components/lee_ds_v2/resources/scripts/sticky-kit.min.js?_dc=1713852024"></script>
    <script>
        jQuery(document).ready(function($){
            
            $('#search-form').on('submit', function(){
                $('.search-page-container').addClass('submit');
                return true;
            });

            $('.update-filter').on('change', function(){
                var id = $(this).data('update');
                var val = $(this).val();
                if(id === 's-type'){
                    var val = $.map($('input[data-update=s-type]:checked'), function(n, i){
                        return n.value;
                    }).join(',');
                }
                $('#'+id).val(val);
                $('#input-type-all').prop('checked', false);
            });

            $('#input-type-all').on('change', function(){
                if(this.checked){
                    $('input[data-update=s-type]').prop('checked', false);
                    $('#s-type').val('');
                }
            });

            $('#update-results-btn').click(function(){
                $('#search-form').submit();
            });

            $('.advanced-toggle a').click(function(){
                $('.search-page-container').toggleClass('filters-open');
            });
        });
        
        (function($){
            if(window.innerWidth >= 992 ){
                $(window).load(function(){
                    $('#sticky-bottom-big-ad').stick_in_parent({
                        'parent': $('.main-sidebar'),
                        'offset_top': 80,
                        'spacer': false
                    });
                });
            }
        })(jQuery);
    </script><div class="row">
