<script>
const truncates = document.querySelectorAll('[truncate]');
const truncate = () => {
	console.log('hi')
  let maxW = 0;
  truncates.forEach(e => {
    if (e.classList.contains('truncate')) { e.classList.remove('truncate')}
  	e.style.width='auto';
  })
  maxW = truncates[1].parentElement.offsetWidth;
  truncates.forEach(e => {
  	//e.style.width='auto';
  	//console.log(e.parentElement.offsetWidth);
    //if (e.classList.contains('truncate')) { e.classList.remove('truncate')}
    //setTimeout(() => {e.style.width=e.parentElement.offsetWidth+'px'}, 100);
    e.style.width = maxW + 'px';
    e.classList.add('truncate');
  })
}
truncate();

function throttle(func, delay) {
  let timerId;
  return function () {
    if (!timerId) {
      func.apply(this, arguments);
      timerId = setTimeout(() => {
        timerId = null;
      }, delay);
    }
  };
}

function debounce(func, delay) {
  let timerId;
  return function () {
    clearTimeout(timerId);
    timerId = setTimeout(() => {
      func.apply(this, arguments);
    }, delay);
  };
}

// Throttle the resize event with a small delay
const throttledTruncate = throttle(truncate, 100); // Adjust the delay (in milliseconds) as needed

// Debounce the resize event with a longer delay
const debouncedTruncate = debounce(() => {
  throttledTruncate(); // Call the throttled function when debounce delay is reached
}, 200); // Adjust the delay (in milliseconds) as needed

// Attach the debounced function to the resize event
window.addEventListener('resize', debouncedTruncate);
