			<script type="text/javascript">

				var map_ratio = 1;

				function initialize_google_map() {

					var latLng = new google.maps.LatLng(40.7432513, -73.9893393);



					var mapOptions = {

						center: latLng,

						zoom: 16,

						mapTypeId: google.maps.MapTypeId.ROADMAP,

						zoomControlOptions: {

						  style: google.maps.ZoomControlStyle.LARGE

						},

						scrollwheel: false,

						mapTypeControl: true

					};



					var map = new google.maps.Map(document.getElementById("google-map-0"), mapOptions);



					var contentString = "Awesome Content";



					var infowindow = new google.maps.InfoWindow({

						content: "<p><strong><a href=\"https:\/\/lumina.nyc\/\" target=\"_blank\"><img class=\"alignnone wp-image-1101 \" src=\"https:\/\/lumina.nyc\/wp-content\/uploads\/2017\/01\/lumina-logo-small.png\" alt=\"\" width=\"100\" height=\"63\" \/><\/a><\/strong><\/p>"

					});



					var marker = new google.maps.Marker({

						position: latLng,

						map: map,

						title:"Lumina"

					});



					// google.maps.event.addListener(marker, "click", function() {

					// 	infowindow.open(map,marker);

					// });


					infowindow.open(map,marker);



					map_ratio = jQuery("#google-map-0").width() / 386;



				}



		

				jQuery(window).load(initialize_google_map);



			</script>

		</div><div class="large-1 columns"></div></div><div class="row"><div class="large-2 large-offset-1 columns">

