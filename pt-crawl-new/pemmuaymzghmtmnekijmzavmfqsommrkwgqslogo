        <div class="scripts">
                <script>
        document.getElementById('fbShareBtn').onclick = function() {
            var fbpopup = window.open("https://www.facebook.com/sharer/sharer.php?u=https://www.independent.co.uk/betting/best-betting-sites-uk", "pop", "width=600, height=400, scrollbars=no");
            return false;
        }
        document.getElementById('twitterShareBtn').onclick = function() {
            var fbpopup = window.open("http://twitter.com/share?text=Check this out...&url=https://www.independent.co.uk/betting/best-betting-sites-uk", "pop", "width=800, height=600, scrollbars=no");
            return false;
        }
    </script>
    <script>
        let contributorLink2124 = document.getElementById('contributorLink2124');
        let contributorLinkCopied2124 = document.getElementById('contributorLinkCopied2124');
        contributorLink2124.addEventListener('click', function (e) {
            e.preventDefault();

            let link = contributorLink2124.getAttribute('data-href');
            navigator.clipboard.writeText(link);
            contributorLinkCopied2124.classList.remove('hidden');
            setTimeout(() => {
                contributorLinkCopied2124.classList.add('hidden');
            }, 2000);
        });
    </script>
    <script>
        let contributorLink2411 = document.getElementById('contributorLink2411');
        let contributorLinkCopied2411 = document.getElementById('contributorLinkCopied2411');
        contributorLink2411.addEventListener('click', function (e) {
            e.preventDefault();

            let link = contributorLink2411.getAttribute('data-href');
            navigator.clipboard.writeText(link);
            contributorLinkCopied2411.classList.remove('hidden');
            setTimeout(() => {
                contributorLinkCopied2411.classList.add('hidden');
            }, 2000);
        });
    </script>
    <script>
        let contributorInfoIcon2124 = document.getElementById('contributor_info_icon_2124');
        let contributorInfoIconSvg2124 = document.getElementById('contributor_info_icon_svg_2124');
        let contributorBio2124 = document.getElementById('contributor_bio_2124');
        let contributorClose2124 = document.getElementById('contributor_close_2124');
        let contributorCardVisible2124 = false;

        contributorInfoIcon2124.addEventListener('click', function (event) {
            event.stopPropagation();
            
            // Hide any other open author bio cards
            document.querySelectorAll('.contributor-bio-card').forEach((bioCard) => {
                if (bioCard !== contributorBio2124 && !bioCard.classList.contains('hidden')) {
                    bioCard.classList.add('hidden');
                }
            });

            // Toggle Associated Author Bio Card Visibility 
            contributorBio2124.classList.toggle('hidden');
            contributorCardVisible2124 = !contributorCardVisible2124;
        });

        contributorClose2124.addEventListener('click', function (event) {
            event.stopPropagation();
            contributorBio2124.classList.toggle('hidden');
            contributorCardVisible2124 = !contributorCardVisible2124;
        });

        contributorBio2124.addEventListener('click', function (event) {
            event.stopPropagation();
        });

        window.addEventListener('click', function (event) {
            if (contributorCardVisible2124 && event.target !== contributorInfoIconSvg2124 && event.target !== contributorBio2124) {
                contributorBio2124.classList.add('hidden');
                contributorCardVisible2124 = false;
            }
        });
    </script>
        <script>
        let contributorInfoIcon2411 = document.getElementById('contributor_info_icon_2411');
        let contributorInfoIconSvg2411 = document.getElementById('contributor_info_icon_svg_2411');
        let contributorBio2411 = document.getElementById('contributor_bio_2411');
        let contributorClose2411 = document.getElementById('contributor_close_2411');
        let contributorCardVisible2411 = false;

        contributorInfoIcon2411.addEventListener('click', function (event) {
            event.stopPropagation();
            
            // Hide any other open author bio cards
            document.querySelectorAll('.contributor-bio-card').forEach((bioCard) => {
                if (bioCard !== contributorBio2411 && !bioCard.classList.contains('hidden')) {
