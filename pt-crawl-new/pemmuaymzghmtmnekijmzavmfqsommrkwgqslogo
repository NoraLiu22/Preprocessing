<style>
    /*
        Tailwind default style makes the style for [hidden] display: none;
        which breaks the hidden="until-found" functionality for some browsers
        as the element will never be found by the browser's Find feature.
        To enable that feature, we override the Tailwind style here to display: block !important
        and use h-0 + overflow-hidden to hide the content instead.
    */
    .automation-read-more-wrapper [hidden] {
        display: block !important;
    }
</style>

<script>
    window.addEventListener('DOMContentLoaded', function() {
        const container = document.getElementById('collapsible-section-2121');
        const content = container.querySelector('.read-more-content');
        const expandButton = container.querySelector('.expand-button');
        const collapseButton = container.querySelector('.collapse-button');

        function updateButton(state = 'expand') {
            if (state === 'expand') {
                expandButton.classList.remove('hidden');
                collapseButton.classList.add('hidden');
            } else {
                expandButton.classList.add('hidden');
                collapseButton.classList.remove('hidden');
            }
        }

        // Only allow this behavior on supported browsers
        // https://caniuse.com/?search=beforematch
        if (('onbeforematch' in document.body)) {
            // This event fires when the browser finds a match with the Find feature
            // Expand the content and update button to expand state
            content.addEventListener("beforematch", () => {
                content.classList.remove('h-0');
                updateButton('collapse');
            });
        }

        expandButton.onclick = () => {
            content.removeAttribute('hidden');
            // Show container
            content.classList.remove('h-0');
            // Update button to collapse state
            updateButton('collapse');
        };

        collapseButton.onclick = () => {
            // Re-add hidden attribute for browser Find feature
            content.hidden = "until-found";
            // Hide container
            content.classList.add('h-0');
            // Update button to expand state
            updateButton();

            // Scroll to the top of the container - offset (navs, sticky banners etc)
            setTimeout(() => {
                let bodyRect = document.body.getBoundingClientRect().top;
                let elementRect = container.getBoundingClientRect().top;
                let elementPosition = elementRect - bodyRect;
                let scrollPosition = elementPosition - 100;
                window.scrollTo({
                    top: scrollPosition,
                    behavior: 'smooth'
                })
            }, 100);
        };
    });
</script>
            
