<script language="JavaScript">

function validateForm(){
	//length for the fields autore title and publisher should be greater than 2
	if (document.ricerca.autore.value!="" && document.ricerca.autore.value.length<=2){
		alert('length of parameter \"Contributor\" should be greater than 2');
		document.ricerca.autore.focus();
		return false;	
	}	
	if (document.ricerca.title.value!="" && document.ricerca.title.value.length<=2){
		alert('length of parameter \"Title\" should be greater than 2');
		document.ricerca.title.focus();
		return false;	
	}
	
	//at least one parameter
	if (document.ricerca.autore.value=="" && document.ricerca.title.value=="" ){
		alert('at least one parameter should be inserted');
		document.ricerca.autore.focus();
		return false;	
	}	
	return true;
}
</script>
