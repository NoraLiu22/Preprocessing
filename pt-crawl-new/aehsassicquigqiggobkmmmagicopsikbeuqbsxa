<script>
$('.nav-data').nav({
    styleMobile: 'panel',
    includeRootElement:false,
    includeChildrenDesktop: false,
    includeSearchDesktop: false,
    dirMobile: 'left',
    backButtonLabel: '{{{parent.text}}}',
    onComplete: function() {
        $('#navMobileSearchInput').attr('placeholder', 'Search');
         $(".nav--desktop--list--topMenu").attr("role", "menu");
 
    $(".nav--desktop--list--topMenu--menuItem a").attr({
      "role": "menuitem",
      "tabindex": "0"
    });
    }
});
    
