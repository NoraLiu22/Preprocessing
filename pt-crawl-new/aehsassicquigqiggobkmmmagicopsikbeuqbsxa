$('[cm-toggle]').insertAfter('.module-links--footer li:last-child');

// Cookie delete function
function removeCookies(cookieList) {
    let domain = window.location.hostname.split('.').slice(-2).join('.');
    let activeCookies = document.cookie.split(';').map((cookie) => cookie.split('=')[0].trim());

    cookieList.forEach((name) => {
        if (name.includes('*')) {
            let regex = new RegExp(name.replace('*', '.*'));
            activeCookies.forEach((activeCookie) => {
                if (regex.test(activeCookie)) {
                    document.cookie = `${activeCookie}=; domain=${domain}; path=/; expires=Thu, 01 Jan 1970 00:00:00 UTC;`;
                }
            });
            return;
        }
        activeCookies.includes(name) && (document.cookie = `${name}=; domain=${domain}; path=/; expires=Thu, 01 Jan 1970 00:00:00 UTC;`);
    });
}

