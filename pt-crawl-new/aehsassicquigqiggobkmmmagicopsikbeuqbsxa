    <script>
        Q4Login.IframeRedirectURI = "https://login.q4inc.com/authorize?client_id=gKrDY1E9BUSWfU7DGuW2CO4rjdskiNg7&scope=openid+profile+email&response_type=code&response_mode=query&nonce=byokT3WKf9fe8KXiDURLdtVfQLk&state=eyJhbGciOiJodHRwOi8vd3d3LnczLm9yZy8yMDAxLzA0L3htbGRzaWctbW9yZSNobWFjLXNoYTI1NiIsInR5cCI6IkpXVCJ9.eyJyZXR1cm5VcmwiOiJodHRwczovL3d3dy5tYWN5c2luYy5jb20vcTRsb2dpbmNhbGxiYWNrLmFzcHgiLCJleHAiOjE3MjAyOTg4MTYuMCwiaWF0IjoxNzE1MDM5MjE2LjAsImlzcyI6IlE0LldlYi5QdWJsaWMuU2l0ZSIsImF1ZCI6InVwc3RyZWFtLmNkbi53ZWIucHJkLnE0aW5jLmNvbSJ9.iGQE32kYA8aMsTDUvKUMtHAUWVrD544_e-8oCJc5dGA&redirect_uri=https%3a%2f%2fauth.platform.q4inc.com%2fauth%2fpublicAuthRedirect&prompt=none";
        (function () {
            if (window.location.hash.includes("login=true")) {
                // q4pid is set in the modal so it should already be there
                const pendoId = window.localStorage.getItem("q4pid");

                // email is also from modal
                const email = window.localStorage.getItem("q4l_email");

                let redirectUrl = $('#hdnRedirectToLoginUrl').val();
                redirectUrl = redirectUrl.replace("_q4pidval_", pendoId);
                redirectUrl = redirectUrl.replace("_q4lemail_", email);

                window.location.replace(redirectUrl);
            }
        })();
    </script>
