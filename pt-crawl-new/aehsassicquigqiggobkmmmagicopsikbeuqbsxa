     <div class="search-container">
        <div id="navDesktopSearch" class="nav--desktop--search_container closed" aria-describedby="search-tip">
           <label id="navDesktopSearchText" class="nav--desktop--search_container--text module-search_text sr-only" for="navDesktopSearchInput">
           Search query
           </label>
           <button id="navDesktopSearchBtn" class="nav--desktop--search_container--btn" form="navDesktopSearchForm" type="button" aria-label="Click to open search" aria-expanded="false" aria-haspopup="true" aria-controls="navDesktopSearchInput">
           <span class="button_text q4-icon_search" aria-hidden="true"></span>
           </button>
            <fieldset>
                <legend class="sr-only">search</legend>
           <input id="navDesktopSearchInput" class="nav--desktop--search_container--input closed" name="SearchTerm" form="navDesktopSearchForm" type="text" maxlength="256" title="Search query" placeholder="Search" />
                </fieldset>
                </div>
        <span id="search-tip" class="tooltip" role="tooltip" aria-hidden="true" aria-label="Search" data-position="left">Search</span>
     </div>
  </div>
</div>
<script>
  $(document).ready(function () {
     const $menuItems = $(
       ".pane--static-header .nav--desktop--list--innerMenu--menuItem"
     );
     const $menuButtons = $(
       '.pane--static-header .nav--desktop--list--innerMenu--menuItem a[aria-haspopup="true"]'
     );
      
      let $currentMegaMenu = null;

   $menuButtons.on("mouseenter focusin", function () {
       const $this = $(this);
       const $parent = $this.parent();
       const $megaMenu = $parent.children(".nav--desktop--list--submenu-item");

       // Show mega menu
       $this.attr("aria-hidden", "false");
       $megaMenu.attr("aria-hidden", "false");
       $megaMenu.removeClass("closed");
       $this.attr("aria-expanded", "true");
       $('body').addClass('megaMenuActive');

       $megaMenu.stop(true, true).fadeIn(0);

       if ($currentMegaMenu && $currentMegaMenu[0] !== $megaMenu[0]) {
           // Hide previously opened mega menu
           $currentMegaMenu.css("display", "none");
           $currentMegaMenu.attr("aria-hidden", "true");
           $currentMegaMenu.find("a").attr("tabindex", "-1");
           $currentMegaMenu.addClass("closed");
       }

       $currentMegaMenu = $megaMenu;
   });

     $(".pane--static-header").on("keydown", function (e) {
       if (e.which === 27) {
         e.preventDefault();
         const $allMegaMenu = $(".nav--desktop--list--submenu-item");
         // Hide mega menu and reset aria-expanded attribute
         $menuButtons.attr("aria-hidden", "true");
         $allMegaMenu.attr("aria-hidden", "true");
         $allMegaMenu.css("display", "none");
         $menuButtons.attr("aria-expanded", "false");
         $allMegaMenu.fadeOut(300);
       }
     });

  
     $menuButtons.on("mouseleave focusout click", function () {
       const $this = $(this);
       const $parent = $this.parent();
       const $megaMenu = $parent.children(".nav--desktop--list--submenu-item");
       $('body').removeClass('megaMenuActive');
  
       if (!$megaMenu.hasClass("closed")) {
         return;
       }
  
       // Hide mega menu
       $this.attr("aria-hidden", "true");
       $megaMenu.attr("aria-hidden", "true");
       $megaMenu.css("display", "none");
       $this.attr("aria-expanded", "false");
       $megaMenu.fadeOut(300);
     });
  
     // Add event handlers to mega menu items
     $(".pane--static-header .nav--desktop--list--submenu-item").on("mouseleave", function (e) {
         const $megaMenu = $(this);
         const menuOffset = $megaMenu.offset();
         const menuWidth = $megaMenu.outerWidth();
         const menuHeight = $megaMenu.outerHeight();
  
         // Check if mouse leaves from left, right, or bottom
         const isFromLeft = e.pageX <= menuOffset.left;
         const isFromRight = e.pageX >= menuOffset.left + menuWidth;
         const isFromBottom = e.pageY >= menuOffset.top + menuHeight;
  
         if (isFromLeft || isFromRight || isFromBottom) {
             // Hide mega menu
             $megaMenu.attr("aria-hidden", "true");
             // setTimeout(function () {
                 $megaMenu.addClass("closed");
                 $megaMenu.css("display", "none");
                 $('body').removeClass('megaMenuActive');
             // }, 300);
             $menuItems.children("button").attr("aria-expanded", "false");
         }
     });
  
  
     // Add event handlers to parent buttons
     $('.pane--static-header a[aria-haspopup="true"]').on("keydown", function (e) {
       if (e.which === 13 || e.which ===40) {
         // Enter key
         e.preventDefault();
         const $this = $(this);
         const $megaMenu = $this.siblings(".nav--desktop--list--submenu-item");
  
         if ($this.attr("aria-hidden") === "false") {
           // Show mega menu
           $megaMenu.find("a").attr("tabindex", "0");
           $megaMenu.find("a").first().focus();
         }
       }
     });
  
     // Add tab index to mega menu items
     $(".pane--static-header .nav--desktop--list--submenu-item a").attr(
       "tabindex",
       "-1"
     );
  
     // Search button functionality
     const $searchBtn = $("#navDesktopSearchBtn");
     const $searchInput = $("#navDesktopSearchInput");
     const $searchContainer = $("#navDesktopSearch");
     const $formId = "navDesktopSearchForm";
     const $searchTip = $("#search-tip");
  
     // Add event handlers to search button
     $searchBtn.on("mouseenter focusin", function () {
       // Show search tip
       $searchTip.attr("aria-hidden", "false");
       $searchTip.attr("aria-controls", "navDesktopSearchBtn");
     });
  
     $searchBtn.on("mouseleave focusout", function () {
       // Hide search tip
       $searchTip.attr("aria-hidden", "true");
       $searchTip.attr("aria-controls", "");
     });
  
     $searchBtn.on("click keydown", function (event) {
       if (event.type === "click" || event.key === "Enter") {
         // toggle Expand search button
         $searchBtn.attr(
           "aria-expanded",
           $searchBtn.attr("aria-expanded") === "true" ? "false" : "true"
         );
  
         // Show search input
         $searchInput.toggleClass("closed");
         $searchContainer.toggleClass("closed");
         // Toggle aria-hidden attribute
         $searchInput.attr(
           "aria-hidden",
           $searchInput.attr("aria-hidden") === "true" ? "false" : "true"
         );
         // Toggle aria-controls attribute
         $searchInput.attr(
           "aria-controls",
           $searchInput.attr("aria-controls") === "" ? "navDesktopSearchBtn" : ""
         );
         $searchInput.focus();
       }
     });
  
     $(document).on("click", function (e) {
       // Collapse search button and hide search input if click is outside the search area
       if (
         !$searchBtn.is(e.target) &&
         !$searchInput.is(e.target) &&
         $searchInput.has(e.target).length === 0
       ) {
         // Hide search input
         $searchInput.addClass("closed");
         $searchContainer.addClass("closed");
         $searchInput.attr("aria-hidden", true);
         $searchInput.attr("aria-controls", "");
         $searchInput.blur();
  
         // Collapse search button
         $searchBtn.attr("aria-expanded", "false");
       }
     });
  
     const $searchForm = $(
       '<form id="' + $formId + '" action="/search-results" method="post"></form>'
     );
  
     function updateSearchAction(ev) {
       const searchValue = $searchInput.val();
  
       $searchForm.attr("action", "/search-results?SearchTerm=" + searchValue);
     }
  
     $("body").append($searchForm);
     $("#" + $formId).on("submit", updateSearchAction);
  
    // Selected State and Scroll to functionality       
     function setSelectedLink(url) {
         $(".nav--desktop--list--submenu-item a").removeClass("selected");
         $('button[aria-haspopup="true"]').removeClass("selected");
         $(`.nav--desktop--list--submenu-item a[href="${url}"]`).addClass("selected");
         $(`.nav--desktop--list--submenu-item a[href="${url}"]`)
           .closest(".nav--desktop--list--submenu-item")
           .siblings('a[aria-haspopup="true"]')
           .addClass("selected");
       }

       function selectButtonsLinks() {
         const selectedUrl = localStorage.getItem("selectedUrl");
         const currentUrl = window.location.href.replace(/^(?:\/\/|[^\/]+)*\//, "");
         if (selectedUrl && selectedUrl.includes(currentUrl)) {
           setSelectedLink(selectedUrl);
         }
       }

       function onSubmenuItemClick(event) {
         event.preventDefault();
         const url = $(event.currentTarget).attr("href");
         const isBlank = $(event.currentTarget).attr("target") === "_blank";

         if (url.includes("#")) {
           const hash = url.substring(url.indexOf("#") + 1).trim();
           const container = $(`.module-${hash}`);
           if (container.length) {
             $("html, body").animate(
               {
                 scrollTop: container.offset().top - 120,
               },
               1000,
               () => {
                 $("body").addClass("no-outline");
                 setTimeout(() => {
                   $("body").removeClass("no-outline");
                 }, 500);
               }
             );
             setSelectedLink(url);
             localStorage.setItem("selectedUrl", url);
             return;
           }
         }

         setSelectedLink(url);
         localStorage.setItem("selectedUrl", url);
         if (isBlank) {
           window.open(url, "_blank");
         } else {
           window.location.href = url;
         }
       }

       // Initialize
       selectButtonsLinks();
       $(".nav--desktop--list--submenu-item a").click(onSubmenuItemClick);

  
   // Sticky Nav Functionality
   (function () {
     const $header = $(".pane--header");
     let lastScrollPosition = $(window).scrollTop();
     const scrollThreshold = 20;
     let debounceTimeout;
     const isIOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;

     function debounce(func, wait) {
       return function () {
         clearTimeout(debounceTimeout);
         debounceTimeout = setTimeout(() => func.apply(this, arguments), wait);
       };
     }

     function handleScroll() {
       const currentScrollPosition = $(window).scrollTop();

       if (
         Math.abs(currentScrollPosition - lastScrollPosition) <= scrollThreshold
       ) {
         return; // Ignore small scroll changes
       }

       if (currentScrollPosition < lastScrollPosition || currentScrollPosition === 0) {
         $header.removeClass("hidden-nav"); // Scrolling up or reached the top of the page
       } else {
         $header.addClass("hidden-nav"); // Scrolling down
       }

       lastScrollPosition = currentScrollPosition;
     }

     const debouncedHandleScroll = isIOS ? debounce(handleScroll, 100) : handleScroll;
     $(window).on("scroll", debouncedHandleScroll);
   })();

      setTimeout(function(){
          // Set ARIA roles and attributes for the menu items
$menuItems.attr("role", "menuitem");
$menuButtons.attr("role", "button");
$menuButtons.attr("aria-haspopup", "true");
          
           $(".nav--desktop--list--topMenu").attr("role", "menubar");

  // Add ARIA role "menu" to each submenu ul element
  $(".nav--desktop--list--submenu").attr("role", "menu");

  // Add ARIA role "menuitem" to each menu item (anchor) within a submenu
  $(".nav--desktop--list--submenu a").attr("role", "menuitem");
      }, 1000);
      $menuButtons.on("keydown", function (e) {
  if (e.which === 13) {
    // Enter key should trigger the menu
    e.preventDefault();
    // Handle menu expansion here
  }
});
  });
  
</script>
        </div>
    </div>
</div></span></span></div>
            </div>
