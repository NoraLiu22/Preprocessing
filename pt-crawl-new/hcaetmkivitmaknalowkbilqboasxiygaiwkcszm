    <script>
        var OneSignal = window.OneSignal || [];

        OneSignal.push(function() {
            OneSignal.SERVICE_WORKER_PARAM = { scope: '/push/onesignal/' };
            OneSignal.SERVICE_WORKER_PATH = 'OneSignalSDKWorker.js'
            OneSignal.SERVICE_WORKER_UPDATER_PATH = 'OneSignalSDKUpdaterWorker.js'
            OneSignal.init({
                requiresUserPrivacyConsent: !!(window.__tcfapi),
                appId: 'a5bd75e4-7c99-47d4-8592-36e29450531b',
                promptOptions: {
                    slidedown: {
                        prompts: [
                            {
                                type: 'push',
                                autoPrompt: true,
                                text: {
                                    actionMessage: 'Don’t miss a thing! Get notifications for important news & events right in your browser',
                                    acceptButton: 'Allow',
                                    cancelButton: 'Cancel',
                                },
                                delay: {
                                    pageViews: 3,
                                    timeDelay: 120,
                                }
                            }
                        ]
                    },
                    customlink: {
                        enabled: true,
                        style: 'button',
                        size: 'medium',
                        color: {
                            button: '#FFFFFF',
                            text: '#333333',
                        },
                        text: {
                            subscribe: 'ACTIVATE NOTIFICATIONS',
                            unsubscribe: 'DEACTIVATE ',
                            explanation: 'Don’t miss a thing! Get notifications for important news & events right in your browser',
                        },
                        unsubscribeEnabled: true,
                    },
                },
                notifyButton: {
                    enable: true,
                },
            });

            
                OneSignal.on('subscriptionChange', function(isSubscribed) {
                    if (isSubscribed === true) {
                        OneSignal.sendTags({
                            sub_cat: 'food-travel',
                        })
                    }
                });
            
        });

        window.dataLayer = window.dataLayer || [];

        OneSignal.push(() => {
            OneSignal.on('notificationPermissionChange', permissionChange => {
                let push_action = 'close';
                if (permissionChange.to === 'granted') {
                    push_action = 'allow'
                } else if (permissionChange.to === 'block') {
                    push_action = 'block'
                }
                window.dataLayer.push({
                    event: 'manage_push',
                    method: 'native prompt',
                    push_action
                })
            });

            OneSignal.on('popoverAllowClick', () => {
                window.dataLayer.push({
                    event: 'manage_push',
                    method: 'slide prompt',
                    push_action: 'allow',
                })
            });

            OneSignal.on('popoverCancelClick', () => {
                window.dataLayer.push({
                    event: 'manage_push',
                    method: 'slide prompt',
                    push_action: 'block',
                })
            });

            OneSignal.on('popoverClosed', () => {
                window.dataLayer.push({
                    event: 'manage_push',
                    method: 'slide prompt',
                    push_action: 'close',
                })
            });

            const pushBox = document.querySelector('#pushBox');
            if (pushBox) {
                pushBox.addEventListener('click', async (e) => {
                    if (e.target.nodeName === 'BUTTON') {
                        window.dataLayer.push({
                            event: 'manage_push',
                            method: 'push subscription form', 
                            push_action: !await OneSignal.getSubscription() ? 'activate' : 'deactivate',
                        })
                    }
                })
            }

            document.querySelector('body').addEventListener('click', async (e) => {
                if (['subscribe-button', 'unsubscribe-button'].includes(e.target.id)) {
                    window.dataLayer.push({
                        event: 'manage_push',
                        method: 'bell prompt',
                        push_action: !await OneSignal.getSubscription() ? 'activate' : 'deactivate',
                    })
                }
            })
        });

        // Extra configuration for articles & category pages
        
            let osTagsForPage = {"lifestyle":0,"food-travel":0};

            function osUpdateTags(osTags) {
                OneSignal.push(function() {
                    OneSignal.isPushNotificationsEnabled(function(isEnabled) {
                        if (isEnabled) {
                            OneSignal.sendTags(osTags).then(function(tagsSent) {});
                        }
                    });
                });
            }

            if (typeof localStorage !== "undefined") {
                let osTags = {};
                let localTags = getOSTags();
                let tagsUpdated = false;

                for (key in osTagsForPage) {
                    if (key in localTags) {
                        if (!isNaN(localTags[key])) {
                            localTags[key] = (localTags[key] + 1 || 1);
                            osTags[key] = localTags[key];
                            tagsUpdated = true;
                        }
                    } else {
                        osTags[key] = 1;
                        tagsUpdated = true;
                    }
                }

                localStorage.setItem('osTags', JSON.stringify(osTags));

                if (tagsUpdated) {
                    osUpdateTags(osTags);
                }

                function getOSTags() {
                    return localStorage.getItem('osTags') ? JSON.parse(localStorage.getItem('osTags')) : [];
                }
            }
        
    </script>


    

    

    
