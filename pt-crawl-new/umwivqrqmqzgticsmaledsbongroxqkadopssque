                class="mw-w-full @sm:mw-w-30 mw-px-2 mw-py-2 mw-text-xs mw-font-semibold mw-rounded-xl mw-border mw-border-solid mw-border-gray-300 mw-cursor-pointer mw-odds-format-btn-fractional"
