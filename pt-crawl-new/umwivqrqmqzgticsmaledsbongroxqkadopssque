        let contributorLink1869 = document.getElementById('contributorLink1869');
        let contributorLinkCopied1869 = document.getElementById('contributorLinkCopied1869');
        contributorLink1869.addEventListener('click', function (e) {
            e.preventDefault();

            let link = contributorLink1869.getAttribute('data-href');
            navigator.clipboard.writeText(link);
            contributorLinkCopied1869.classList.remove('hidden');
            setTimeout(() => {
                contributorLinkCopied1869.classList.add('hidden');
