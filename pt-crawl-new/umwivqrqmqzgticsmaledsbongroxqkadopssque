<style>
    /*
        Tailwind default style makes the style for [hidden] display: none;
        which breaks the hidden="until-found" functionality for some browsers
        as the element will never be found by the browser's Find feature.
        To enable that feature, we override the Tailwind style here to display: block !important
        and use h-0 + overflow-hidden to hide the content instead.
    */
    .faq-content [hidden] {
        display: block !important;
    }
</style>

<script>
    window.addEventListener('DOMContentLoaded', function() {
        const container = document.getElementById('faq-4785');

        function hideAllAnswers(exlcude = null) {
            container.querySelectorAll('.faq-answer').forEach(element => {
                if (element !== exlcude) {
                    element.hidden = "until-found";
                    element.classList.add('h-0');
                    element.classList.remove('p-4');
                    element.parentNode.querySelector('.faq-question-svg').classList.remove(
                    '-rotate-90');
                }
            });
        }

        function showAnswer(answerElement, svgElement) {
            answerElement.classList.remove('h-0');
            answerElement.classList.add('p-4');
            svgElement.classList.add('-rotate-90');
        }

        // Only allow this behavior on supported browsers
        // https://caniuse.com/?search=beforematch
        if (('onbeforematch' in document.body)) {
            // This event fires when the browser finds a match with the Find feature
            // Expand the content and update button to expand state
            container.querySelectorAll('.faq-content').forEach(element => {
                let answer = element.querySelector('.faq-answer');
                answer.addEventListener("beforematch", () => {
                    // Show the answer clicked and hide all other answers
                    showAnswer(answer, element.querySelector('.faq-question-svg'));
                    hideAllAnswers(answer);
                });
            });
        }

        container.querySelectorAll('.faq-content').forEach(element => {
            element.addEventListener("click", () => {
                let answer = element.querySelector('.faq-answer');
                if (answer.classList.contains('h-0')) {
                    // Show the answer clicked and hide all other answers
                    answer.removeAttribute('hidden');
                    showAnswer(answer, element.querySelector('.faq-question-svg'));
                    hideAllAnswers(answer);
                } else {
                    // Hide the answer clicked and all other answers
                    hideAllAnswers();
                }
            });
        });
    });
</script>



