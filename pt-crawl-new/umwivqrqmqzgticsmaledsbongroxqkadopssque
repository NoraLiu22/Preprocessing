    <script>
        let contributorInfoIcon1869 = document.getElementById('contributor_info_icon_1869');
        let contributorInfoIconSvg1869 = document.getElementById('contributor_info_icon_svg_1869');
        let contributorBio1869 = document.getElementById('contributor_bio_1869');
        let contributorClose1869 = document.getElementById('contributor_close_1869');
        let contributorCardVisible1869 = false;

        contributorInfoIcon1869.addEventListener('click', function (event) {
            event.stopPropagation();
            
            // Hide any other open author bio cards
            document.querySelectorAll('.contributor-bio-card').forEach((bioCard) => {
                if (bioCard !== contributorBio1869 && !bioCard.classList.contains('hidden')) {
                    bioCard.classList.add('hidden');
                }
            });

            // Toggle Associated Author Bio Card Visibility 
            contributorBio1869.classList.toggle('hidden');
            contributorCardVisible1869 = !contributorCardVisible1869;
        });

        contributorClose1869.addEventListener('click', function (event) {
            event.stopPropagation();
            contributorBio1869.classList.toggle('hidden');
            contributorCardVisible1869 = !contributorCardVisible1869;
        });

        contributorBio1869.addEventListener('click', function (event) {
            event.stopPropagation();
        });

        window.addEventListener('click', function (event) {
            if (contributorCardVisible1869 && event.target !== contributorInfoIconSvg1869 && event.target !== contributorBio1869) {
                contributorBio1869.classList.add('hidden');
                contributorCardVisible1869 = false;
            }
        });
    </script>
