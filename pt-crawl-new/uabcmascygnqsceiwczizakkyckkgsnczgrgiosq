    <script>
        function lineFocusEmail() {
            document.getElementById('line-email-form-input').style.border = '2px solid #276FBF';
            document.getElementById('line-email-submit').style.background = '#EC1A2E';
            document.getElementById('line-email-submit-icon').style.background = '#EC1A2E';
        }

        function lineFocusOutEmail() {
            document.getElementById('line-email-form-input').style.border = '1px solid #BBBBBB';
            document.getElementById('line-email-submit').style.background = '#BBBBBB';
            document.getElementById('line-email-submit-icon').style.background = '#BBBBBB';
        }

        function lineSucess() {
            document.getElementById('line-email-form-input').style.border = '2px solid #157F1F';
            document.getElementById('line-email-submit').style.background = '#EC1A2E';
            document.getElementById('line-email-submit-icon').style.background = '#EC1A2E';
            document.getElementById('line-email-error-message').classList.add('invisible');
            document.getElementById('line-email-submit').disabled = true;
        }

        function lineError() {
            document.getElementById('line-email-form-input').style.border = '2px solid #A20021';
            document.getElementById('line-email-submit').style.background = '#EC1A2E';
            document.getElementById('line-email-submit-icon').style.background = '#EC1A2E';
            document.getElementById('line-email-error-message').classList.remove('invisible');
        }

        document.getElementById('line-email-form').addEventListener('submit', function(event) {
            event.preventDefault(); // Prevent the default form submission

            // honeypot
            if (document.getElementById('email_line').value || document.getElementById('name_line').value) {
                return;
            }

            // Get the email value from the form
            const email = document.getElementById('line-email-form-input').value;
            if (!validateEmail(email)) {
                lineError();
                return;
            }

            const marketing = document.getElementById('accept_terms_checkbox').checked;

            // Call your Cloudflare API Worker
            addEmailToBraze(email, marketing);
            lineFocusOutEmail();
            lineSucess();
        
        });
    </script>
