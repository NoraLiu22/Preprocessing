        BidBarrel.setTargeting({"env": "prod"});
        BidBarrel.setTargeting({"pv": __pv});
        BidBarrel.setTargeting({"ptype": "list_page"});

        if (MC_USER_LOGGED_IN){
            BidBarrel.setTargeting({"user": "registered"});
        }else{
            BidBarrel.setTargeting({"user": "anon"});
        }

        
        
