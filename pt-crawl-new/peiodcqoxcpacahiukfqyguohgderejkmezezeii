            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/0/b6/0b69bd8e-8f06-11ee-b3f1-0bb1cd062dd7/6567ba52eb314.image.jpg?resize=795%2C500">
                <img
                    src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
                    alt="Downtown Staunton"
                    class="img-responsive owl-lazy default"
                    width="1815"
