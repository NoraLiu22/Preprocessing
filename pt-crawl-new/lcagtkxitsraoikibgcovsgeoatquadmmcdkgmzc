	$(function(){
		 setTimeout(function(){
		 	// $('#carousel-2989a .carousel-inner').height($('#carousel-2989a .carousel-inner').height()+'px');
		 	var height = $('#carousel-2989a .carousel-inner').height();

		 			 	
		 	$('#carousel-2989a .carousel-inner').height(height+'px');
		 	$('#carousel-2989a .carousel-inner .photo').css('overflow','hidden');

		 	
		 	$('#carousel-2989a .carousel-inner').css('overflow','hidden');
		}, 1000);
		 fitSliderArrow($('#carousel-2989a .item:first'));
	});
	$('#carousel-2989a').on('slide.bs.carousel', function(e){
		var slide = $(e.relatedTarget);
		var carousel = slide.closest('.carousel');
		carousel.find('.arrows').hide();
	}).on('slid.bs.carousel', function(e){
		var slide = $(e.relatedTarget);
		fitSliderArrow(slide);

	})
	</script>
