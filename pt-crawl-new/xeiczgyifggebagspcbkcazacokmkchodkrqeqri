<script type="text/javascript">

    var rotate = "5";
    var rotateVal = 0;
    var rotateList = "753,1144,1160,1161,1162";
    var detailList = "106776,103172,7159,7158,103123";
    var mediaList = "187356,180181,20503,20501,179919";
    var titleList = "Recreation+-+Swimming,Recreation+-+Portable+Swimming+Pool+at+9th+and+Brown+Streets,Snow+Cartage,''Snowing+Like+Hell''+-+Snow+Removal+-+Looking+South,Independence+Hall+-+Winter+Snow+Scenes";
    var rList = rotateList.split(",");
    var dList = detailList.split(",");
    var mList = mediaList.split(",");
    var tList = titleList.split(",");
        
    if (rotate > 1)
    {
        var prevFeature = document.createElement("img");
        prevFeature.id = 'prevFeature';
        prevFeature.className = 'PrevFeature';        
    
        document.getElementById("FeatureMat").appendChild(prevFeature);
        setTimeout(setFirstImage, 3000);
    }   

    function setFirstImage() {
        setImages();
        setInterval(setImages, 5000);
    }

    function setImages() {
        var assetImage, 
            assetLink, 
            imageArr, 
            linkArr;
        rotateVal++;
        if (rotateVal == rotate)
        {
            rotateVal = 0;
        }
        
        assetImage = Ext.get("ctl11_imgFeaturePhoto");
        assetLink = Ext.get("ctl11_lnkFeaturePhoto");
        Ext.get("prevFeature").dom.src = assetImage.dom.src;
        
        imageArr = assetImage.dom.src.split("=");
        imageArr[1] =  rList[rotateVal];
        
        linkArr = assetLink.dom.href.split("=");
        linkArr[1] = dList[rotateVal];
        
        assetImage.dom.src = imageArr.join("=");
        assetLink.dom.href= linkArr.join("=");
        assetImage.syncFx().slideIn('l', {duration:.5});
        
        if (rotateCaption) //photoDetail control
        {
            rotateCaption(rotateVal);
        }
        if (rotateTheme) //theme-specific rotation if any
        {
            rotateTheme(rotateVal);
        }        
        
        
    }

</script>

