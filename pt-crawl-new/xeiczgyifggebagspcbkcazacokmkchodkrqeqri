    <script type="text/javascript">
         Sajara.Util.ready(function () {
            Sajara.Widgets.Menu(Sajara.Util.extend(true, Sajara.Config.Menu, Sajara.Config.Client.Menu)).init();
        
            if (document.getElementById("themeMenuControl_loginStatus").innerHTML === "Login")
            {
                $("#sajara-mainmenu-admin").css({height: '1px'});
            }
        });
        
        var loginTimer;
        function showLogin() {
            clearTimeout(loginTimer);
            if (document.getElementById("themeMenuControl_loginStatus").innerHTML === "Login")
            {
                document.getElementById("themeMenuControl_LoginPanel").style.visibility = "visible";               
            }
        }
        // adapted from http://www.quirksmode.org/js/events_mouse.html#mouseover
        function checkHide(e) {        
            //get the event and target (the node the mouse left)
            clearTimeout(loginTimer);
            if (!e) 
            {   
                var e = window.event;
            }
	        var tg = (window.event) ? e.srcElement : e.target;
	        //ignore mouseout events from children
	        if (tg.id != 'themeMenuControl_LoginPanel' && tg.id != 'themeMenuControl_loginStatus') 
	        {
	            return;
	        }
	        var tg2;
	        if (tg.id == 'themeMenuControl_LoginPanel') {tg2 = document.getElementById("themeMenuControl_loginStatus");}
	        else if (tg.id == 'themeMenuControl_loginStatus') {tg2 = document.getElementById("themeMenuControl_LoginPanel");}
	        //ignore mouseout events into children and between the panel and link
	        var reltg = (e.relatedTarget) ? e.relatedTarget : e.toElement;
	        while (reltg != tg && reltg != tg2 && reltg.nodeName != 'BODY')
		    {
		        reltg= reltg.parentNode
		    }
	        if (reltg == tg || reltg == tg2) 
	        {
	            return;
	        }
            //only react to mouseout events going off the login panel
            loginTimer = setTimeout(hideLogin, 400);           
           
        }
        function clearHide() {
            clearTimeout(loginTimer);
        }
        
        function hideLogin() {
            document.getElementById("themeMenuControl_LoginPanel").style.visibility = "hidden";
            document.getElementById("themeMenuControl_loginForm_PasswordRequired").style.visibility = "hidden";
            document.getElementById("themeMenuControl_loginForm_UserNameRequired").style.visibility = "hidden";
        }
        
        function logOut() {
            if (document.getElementById("themeMenuControl_loginStatus").innerHTML === "Logout")
            {
                location = "./Logout.aspx?ReturnURL=Home.aspx";
            }
        }
    </script>
    

