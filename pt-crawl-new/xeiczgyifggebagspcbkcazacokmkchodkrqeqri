	Ext.onReady(function() {
		var featureFlag = document.createElement("img");
		featureFlag.id = 'featureFlag';
		featureFlag.className = 'FeatureFlag';
		featureFlag.src="App_Themes/PhillyHistory/Images/Home/markerLg.gif";    

