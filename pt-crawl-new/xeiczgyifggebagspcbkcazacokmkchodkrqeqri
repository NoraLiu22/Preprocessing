<script type="text/javascript">
Ext.onReady(function () {
    var list = Ext.query('div.SearchLinkImageWrap a');   
    
    for (var i = 0, len = list.length; i < len; i++) {
        var url = list[i].href.split('?');
        Ext.Ajax.request({
            url: 'FirstSearchMedia.ashx',
            params: { request:'FirstImage', start:0, limit:1, urlqs:url[1], elid:list[i].id },
            success: showImage, 
            failure: showNoImage
        });
    }
    
    function showImage(response) {
        var info = response.responseText.split('^');
        if(info.length && info.length >= 1) {
            Ext.get(info[1]).dom.firstChild.src = info[0];
        }
    }
    
    function showNoImage(response){
        
    }
    
});
</script>
