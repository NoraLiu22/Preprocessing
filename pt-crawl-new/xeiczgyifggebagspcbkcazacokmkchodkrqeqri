        Ext.onReady(function () {
        
            var link = document.getElementById(linkId);
            if (SHARE_TYPE == "ShareThis" && SHARETHIS != null && shareOn == "true")
            {
                var obj = SHARETHIS.addEntry({ 
                    title: 'PhillyHistory Image', 
                    url: window.location.href.split(page)[0] + url
                    }, 
                    {button: false, offsetLeft:-130, onclick:onShare}
                );
                obj.attachButton( link );
                SHARETHIS.onReady();    
                link.style.cursor = "pointer";
                link.title = "Share This";                
            }
            else
            {
                link.style.display = "none";
                link.parentNode.innerHTML += "<a id='shareLink' href='" + url + "&amp;tab=" + tab + "'><img src='" + link.src + "' style='border-width:0px;' /></a>"
            }
        });
                
        function onShare(shareObj) {
            if (SHARE_TYPE == "ShareThis" && shareObj)
            {
                //just log the button click for ShareThis
                var text = "Opened ShareThis dialog with share url: " + shareObj.properties.url;
                                    
                var logUrl = "";
                   
                if (window.location.href.indexOf("Members") != -1)
                {
                    logUrl = window.location.href.split("Members")[0] + "JsLog.ashx";
                }
                else
                {
                    logUrl = window.location.href.split(page)[0] + "JsLog.ashx";
                }
                                    
                Ext.Ajax.request({
                    url: logUrl,
                    params: {type:"ShareThis", responseText:text}
                });
            }
        }
        
        function rotateCaption(rotateVal) {
            
            var captionDiv = "FeatureCaptionWrap";
            var caption = CaptionList.split("*"); 
            if (caption.length > 1)
            {
                Ext.get(captionDiv).dom.getElementsByTagName("span")[0].innerHTML = caption[rotateVal];
            }
            
        }
        
    </script>

		</div></td>
	</tr>
</table>
            </div>
