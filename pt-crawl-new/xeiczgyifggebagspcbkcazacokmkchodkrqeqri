		detailLink.id = 'detailLink';
		detailLink.className = 'detailLink';
		detailLink.innerHTML = "<img class='detailBullet' src='App_Themes/PhillyHistory/Images/Home/captionBullet.gif'/> Photo Details";
	    
		document.getElementById("FeatureDetails").appendChild(detailLink);
	    
		var buyLink = document.createElement("a");
		buyLink.id = 'buyLink';
		buyLink.className = 'buyLink';
		buyLink.innerHTML = "<img class='detailBullet' src='App_Themes/PhillyHistory/Images/Home/captionBullet.gif'/> Buy This Photo";
	    
		document.getElementById("FeatureDetails").appendChild(buyLink);
	    
	    if (navigator.appVersion.indexOf("MSIE") != -1)
		{
			buyLink.attachEvent('onclick', function() {
				onBuyLink(rotateVal);
			});
		}
		else 
		{
			buyLink.setAttribute('onclick', 'javascript:onBuyLink(rotateVal)');
		}	    
	    rotateTheme(0);
	});

	function onBuyLink(val) {
		//var link = BuyLinkList.split("*")[val];
		FOTOMOTO.API.checkinImage('ctl11_imgFeaturePhoto')
		FOTOMOTO.API.showWindow(FOTOMOTO.API.PRINT, 'ctl11_imgFeaturePhoto');
            
		//window.open (link, "Pictopia", "width=920,height=630,resizable=1,scrollbars=1");
	}
	
	function rotateTheme(rotateVal) {
		var detailId = dList[rotateVal];
		var detailLink = Ext.get("detailLink");
				
		detailLink.dom.href = "./Detail.aspx?assetId=" + detailId;	
	}
