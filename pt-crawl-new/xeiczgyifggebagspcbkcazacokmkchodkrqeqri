        if (document.getElementById("featureBackground"))        
        {document.getElementById("featureBackground").style.height = (getOffsetTop2(document.getElementById("columns")) + 50) + "px";}
        function getOffsetTop2(obj) {
		    var cur = 0;
		    if(obj.offsetParent) {		
			    while(obj.offsetParent) {
				    cur+=obj.offsetTop;
				    obj = obj.offsetParent;
			    }
		    }		
     
		    return cur + document.body.offsetTop;
	    }

    
    </script>
