                   data-target="#photo-carousel-474b9000-c068-11ee-a905-9f89747b475b">
                <span class="fas tnt-expand"></span>
            </span>
            <div class="image" 
                    data-toggle="modal"
                     
                        data-photo-target=".photo-806a74ba-c07b-11ee-9498-2fa4c5a97c87"
                        data-instance="#gallery-items-474b9000-c068-11ee-a905-9f89747b475b-photo-modal"
                        data-target="#photo-carousel-474b9000-c068-11ee-a905-9f89747b475b">
                
                
                
                
        <div itemprop="image" itemscope itemtype="https://schema.org/ImageObject">
            <meta itemprop="width" content="1024">
            <meta itemprop="height" content="1536">
            <meta itemprop="contentUrl" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/8/06/806a74ba-c07b-11ee-9498-2fa4c5a97c87/65bab49894e49.image.jpg">
            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/8/06/806a74ba-c07b-11ee-9498-2fa4c5a97c87/65bab49894e49.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
