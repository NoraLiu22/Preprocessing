            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/f/f2/ff2cfb84-c07a-11ee-8305-0fc831fa5186/65bab3d5deab1.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
