    <script>
        var popupTimeout = new Array(3);

        function findPos(obj) {
            var curleft = curtop = 0;
            if (obj.offsetParent) {
                do {
                    curleft += obj.offsetLeft;
                    curtop += obj.offsetTop;
                }
                while (obj = obj.offsetParent);
                return [curleft, curtop];
            }
        }

        function setPopupPosition() {
            popupList = new Array("exactWords", "allWords", "anyWords");

            for (var i in popupList) {
                popupId = popupList[i] + "Popup";
                if ($(popupId) != null && $(popupId) != undefined) {
                    currPos = findPos($(popupList[i] + "Radio"));
                    modX = currPos[0];
                    modY = currPos[1];
                    $(popupList[i] + "Popup").style.top = modY;
                    $(popupList[i] + "Popup").style.left = modX;
                }
            }
        }

        function showPopup(popupId) {
            hideAllPopups(popupId);
            clearTimeout(popupTimeout[popupId]);
            //$(popupId).style.display = "block";
            $(popupId).appear({ duration: .25 });
        }

        function hidePopup(popupId) {
            popupTimeout[popupId] = setTimeout('$("' + popupId + '").fade(0.25)', 1000);
        }

        function hideAllPopups(notPopup) {
            popupList = new Array("exactWords", "allWords", "anyWords");

            for (var i in popupList) {

                popupId = popupList[i] + "Popup";
                if ($(popupId) != null && $(popupId) != undefined && popupId != notPopup) {
                    $(popupId).hide();
                }
            }
        }

        function showCertType() {
            if (window.certTypeTimeout !== undefined)
                clearTimeout(certTypeTimeout);
            $('certType').appear({ duration: .1 });
        }

        function hideCertType() {
            certTypeTimeout = setTimeout('$("certType").fade({duration: .1})', 500);
        }
        function certTypeRollOn(id) {
            $(id).style.backgroundColor = '#AFC9DD';
        }

        function certTypeRollOff(id) {
            $(id).style.backgroundColor = 'transparent';
        }


        function staticSearch(keywords, searchCategory, searchType) {
            //console.log("search", keywords, searchCategory, searchType)
            document.getElementById("txtSearch").value = keywords;
            document.getElementById("radio-search-product").checked = true;
            document.getElementById("search-type-all-words").checked = true;
            document.getElementById("frmSearch").submit();            
        }
    </script>

