<script language="JavaScript">
	function tryPrint () {
		var contentWindow = document.getElementById("IFrame_StatuteText").contentWindow;
		var result = contentWindow.document.execCommand('print', false, null);
		if(!result)  contentWindow.print();
	}
	
	jQuery(document).ready(function() { 
        
        jQuery( "#btn-textsize-normal" ).bind( "click", function() {        
         	textZoom('normal');    
        });
        jQuery( "#btn-textsize-medium" ).bind( "click", function() {        
         	textZoom('medium');       
        });
        jQuery( "#btn-textsize-large" ).bind( "click", function() {        
           	textZoom('large'); 
        });

	});
	
	g_lineHeightRemoved = false;
	
	
	function textZoom(size) {
		 
		 if(!g_lineHeightRemoved) {
			g_lineHeightRemoved = true;
			var nodes = jQuery('#IFrame_StatuteText').contents().find('p');
			jQuery.each(nodes, function(index,element) {
				jQuery(element).css('line-height','normal');
			});
		 }
		 
		 switch(size) {
		 
			case "normal":
				jQuery('#IFrame_StatuteText').contents().find('body').css('font-size','');
			break;
			
			case "medium":
				jQuery('#IFrame_StatuteText').contents().find('body').css('font-size', '1.1em');
			break;
			
			case "large":
				jQuery('#IFrame_StatuteText').contents().find('body').css('font-size', '1.2em');
			break;
		 }
		 
		 reAdjustIframeHeight(size);
	}
	
	function reAdjustIframeHeight(size) {
						
		// Documents brought over from mainframe and havn't been yet grinded will contain a <pre> tag. Documents that have been grinded will not.
		var src = jQuery('#IFrame_StatuteText').contents().find("pre").html();
		
		if (src != undefined && src.length > 0) {			
			//This will handle any documents that were brought over from the mainframe that have not yet been introduced to the Grinder.        
			var head = jQuery("#IFrame_StatuteText").contents().find("head");
			
			switch(size) {
			 
				case "normal":
					var css = '<style>pre { font-size: 1em; }</style>';
				break;
				
				case "medium":
					var css = '<style>pre { font-size: 1.1em; }</style>';
				break;
				
				case "large":
					var css = '<style>pre { font-size: 1.2em; }</style>';
				break;
			}		
		
  			jQuery(head).append(css);
			
		}
		
		// This will handle any documents that were precviously sent thru the Grinder.
		var contentsHeight = jQuery('#IFrame_StatuteText').contents().find("body").height() + 50 + "px";
		jQuery('#IFrame_StatuteText').css('height', contentsHeight);		
					
	}
</script>
	


