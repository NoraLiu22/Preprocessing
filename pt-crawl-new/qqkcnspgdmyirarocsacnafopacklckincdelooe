    	<script type="text/javascript">
			jQuery( document ).ready(function() {	
				jQuery("#IFrame_StatuteText").load(function() {			
					var iframeHeight =  jQuery(this).contents().find("body").height() + 50 + "px"
					//console.log("New height" + iframeHeight);
					jQuery(this).css('height', iframeHeight)
					// load in parent window when linking from within documnent so we don't display header and footer twice
					jQuery(this).contents().find("body a").attr('target','_parent');
				}).attr("src", 'https://www.legis.state.pa.us//WU01/LI/LI/US/HTM/2008/0/0003..HTM?11');	;		
						
			});	
		</script>	
	
						</div><!-- End Page -->
					</section><!-- End Container -->
				</div><!-- End Home -->
