<script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js'></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.js"></script>
<script src="https://malsup.github.io/jquery.form.js"></script>
<script >
$( document ).ready(function() {

   $('.unsubscribe-all').click(function(event) {
     unsubscribeAll();
    });

  $('.row-cards label').click(function(event) {
    $("#btn-save").addClass('jump');
  });

  $("#btn-save").click(function(event) {
    $(this).removeClass('jump');
  });

  $('.alert-modal .close').click(function(event) {
    $(".alert-modal").removeClass('modal-open');
  });

// wait for the DOM to be loaded
    $(document).ready(function() {
        // bind 'form' and provide a simple callback function
        $('#form').ajaxForm(function() {
          $(".alert-modal").addClass('modal-open');
          return false;
        });
    });

    window.onscroll = function () {
      var updateBar = document.getElementById("update-bar");
      var updateBarHeight = updateBar.offsetHeight;
      var scrollPosition = window.innerHeight + window.scrollY;
      var documentHeight = document.body.offsetHeight;

      if (scrollPosition >= (documentHeight - updateBarHeight)) {
        updateBar.classList.remove("sticky");
      } else {
        updateBar.classList.add("sticky");
      }
    };

});


function unsubscribeAll(){

  $('input:checkbox').each(function (index, value) {
    $(this).removeAttr('checked');
    $(this).prop('checked', false);
  });

}
//# sourceURL=pen.js
</script>

