            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/f/43/f43c1cfc-a59d-11ee-9f4b-2f8a2f06ac11/658da23cdb1ca.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
