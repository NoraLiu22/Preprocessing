            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/a/96/a96d3a3e-a59e-11ee-82ae-1fd96e59112d/658da3822740e.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
