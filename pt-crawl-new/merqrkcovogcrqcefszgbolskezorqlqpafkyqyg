<dd>(1.2) <i><a href="/wiki/she" title="she">She</a> <a href="/wiki/turn" title="turn">turned</a> <a href="/wiki/the" title="the">the</a> <a href="/wiki/table" title="table">table</a> <a href="/wiki/leg" title="leg">legs</a> <a href="/wiki/with" title="with">with</a> <a class="mw-selflink selflink">care</a> <a href="/wiki/and" title="and">and</a> <a href="/wiki/precision" title="precision">precision</a>.</i> → <a href="/wiki/precyzyjnie" title="precyzyjnie">Precyzyjnie</a> <a href="/wiki/z" title="z">z</a> <b><a href="/wiki/dba%C5%82o%C5%9B%C4%87" title="dbałość">dbałością</a></b> <a href="/wiki/wytoczy%C4%87" title="wytoczyć">wytoczyła</a> <a href="/wiki/noga" title="noga">nogi</a> <a href="/wiki/st%C3%B3%C5%82" title="stół">stołu</a>.</dd>
<dd>(2.1) <i><a href="/wiki/call" title="call">Call</a> <a href="/wiki/your" title="your">your</a> <a href="/wiki/mother" title="mother">mother</a> <a href="/wiki/now_and_then" title="now and then">now and then</a> <a href="/wiki/and" title="and">and</a> <a href="/wiki/let" title="let">let</a> <a href="/wiki/she" title="she">her</a> <a href="/wiki/know" title="know">know</a> <a href="/wiki/you" title="you">you</a> <a class="mw-selflink selflink">care</a>.</i> → <a href="/wiki/dzwoni%C4%87" title="dzwonić">Zadzwoń</a> <a href="/wiki/do" title="do">do</a> <a href="/wiki/matka" title="matka">matki</a> <a href="/wiki/od_czasu_do_czasu" title="od czasu do czasu">od czasu do czasu</a> <a href="/wiki/i" title="i">i</a> <a href="/wiki/pokaza%C4%87" title="pokazać">pokaż</a> <a href="/wiki/ona" title="ona">jej</a>, <a href="/wiki/%C5%BCe" title="że">że</a> <a href="/wiki/si%C4%99" title="się">się</a> <b><a href="/wiki/troszczy%C4%87" class="mw-redirect" title="troszczyć">troszczysz</a></b>.</dd>
<dd>(2.4) <i><a href="/wiki/he" title="he">He</a> <a href="/wiki/doesn%27t" title="doesn&#39;t">doesn't</a> <a class="mw-selflink selflink">care</a> <a href="/wiki/about" title="about">about</a> <a href="/wiki/his" title="his">his</a> <a href="/wiki/ex" title="ex">ex</a> <a href="/wiki/anymore" title="anymore">anymore</a>.</i> → <a href="/wiki/on" title="on">Jego</a> <a href="/wiki/nie" title="nie">nie</a> <b><a href="/wiki/obchodzi%C4%87" title="obchodzić">obchodzi</a></b> <a href="/wiki/ju%C5%BC" title="już">już</a> <a href="/wiki/jego" title="jego">jego</a> <a href="/wiki/by%C5%82a" title="była">była</a>.</dd>
