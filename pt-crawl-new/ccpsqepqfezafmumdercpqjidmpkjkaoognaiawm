  <script>
    var tptags = [];
    (function() {
      // Zone & AI
      DEBUGLOGR.log("PIANO", "Started anonymous function");
      tp.push(["setZone", "Web"]);
      tp.push(["setCloudflareWorkerUrl", "https://id.toledoblade.com"]);
      // tp.push(["setCxenseSiteId", "1129578120355680562"]);
      if (typeof PGVARS.requesttype === "string" && PGVARS.requesttype !== "") {
        tptags.push("pagetype-" + PGVARS.requesttype.toLowerCase());
      }
      // Section tags
      if (typeof PGVARS.sectionpath === "string" && PGVARS.sectionpath !== "") {
        DEBUGLOGR.log("PIANO", "Found section path");
        var spatharray = PGVARS.sectionpath.split("/");
        spatharray.forEach(function(item) {
          DEBUGLOGR.log("PIANO", "Adding item '" + item + "'");
          if (item !== "") { tptags.push(item); }  
        });
      }
      // Other data (v2)
      if (PGVARS.requesttype === "STORY") {
        DEBUGLOGR.log("PIANO", "Request is story");
        if (PGFNS.issetmulti(pgStoryZeroJSON, "articles", "0")) {
          DEBUGLOGR.log("PIANO", "Found articles[0]");
          var story = pgStoryZeroJSON.articles[0];
          if (typeof story.paid === "string") {
            if (story.paid === "0") {
              DEBUGLOGR.log("PIANO", "Adding tag 'free'");
              tptags.push("free");
            } else if (story.paid === "1") {
              if (typeof story.paidusersonly === "string" && story.paidusersonly === "1") {
                DEBUGLOGR.log("PIANO", "Adding tag 'paid-users-only'");
                tptags.push("paid-users-only");
              } else {
                DEBUGLOGR.log("PIANO", "Adding tag 'metered'");
                tptags.push("metered");
              }
            }
          }
          var pubdate = "";
          if (typeof pubdate !== "") {
            DEBUGLOGR.log("PIANO", "Running setContentCreated with pubdate='" + pubdate + "'");
            tp.push(["setContentCreated", pubdate]);
          }
        }
        if (typeof PGPAGEDATA !== "undefined" && PGFNS.issetmulti(PGPAGEDATA, "authorapi", "author", "fullName")) {
          DEBUGLOGR.log("PIANO", "Found Author API data. Running setContentAuthor with author='" + PGPAGEDATA.authorapi.author.fullName + "'");
          tp.push(["setContentAuthor", PGPAGEDATA.authorapi.author.fullName]);
        } else if (typeof pgStoryZeroJSON === "object" && PGFNS.issetmulti(pgStoryZeroJSON, "articles", "0", "author")) {
            DEBUGLOGR.log("PIANO", "Found pgStoryZeroJSON author.");
            (function() {
              var bylineobj = PGFNS.byline.getAPIArray(pgStoryZeroJSON["articles"][0].author);
              if (bylineobj) {
                DEBUGLOGR.log("PIANO", bylineobj);
                if (PGVARS.site === "blade" && bylineobj.apiorg === "theblade" && typeof bylineobj.apiauthor === "string") {
                    DEBUGLOGR.log("PIANO", "Byline apiorg is theblade. Using author " + bylineobj.apiauthor);
                    tp.push(["setContentAuthor", bylineobj.apiauthor]);
                } else if (PGVARS.site === "pg" && bylineobj.apiorg === "pittsburghpostgazette" && typeof bylineobj.apiauthor === "string") {
                    DEBUGLOGR.log("PIANO", "Byline apiorg is pittsburghpostgazette. Using author " + bylineobj.apiauthor);
                    tp.push(["setContentAuthor", bylineobj.apiauthor]);
                }
              } else {
                DEBUGLOGR.log("PIANO", "Couldn't get bylineobj");
              }
            })();
          // PGFNS.byline.getAPIArray(pgStoryZeroJSON["articles"][0].author)

        }
      } else {
        tptags.push("free");
      }
      if (tptags.length > 0) {
        DEBUGLOGR.log("PIANO", "Running setTags with this array:");
        DEBUGLOGR.log(tptags);
        tp.push(["setTags", tptags]);
      }
    })();
  </script>

  
