      settings: {
        slidesToShow: 2,
        slidesToScroll: 2
      }
    }
    // You can unslick at a given breakpoint now by adding:
    // settings: "unslick"
    // instead of a settings object
  ]
});
		
$(window).on('load', function() {
    $(".slickTrigger").each(function(){
		$(this).on('init', function(event, slick){
			$(this).removeClass("slickPending");
		});
        $(this).slick();
    })
});
</script>
