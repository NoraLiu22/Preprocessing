<script>
  var list = document.querySelectorAll('.global-footer *')
  var seen = {}

  for (let i = 0; i < list.length; i++) {
    var id = list[i].getAttribute('id')
    
    if (seen[id]) {
      list[i].setAttribute('id', `${id}_${i}`)
    }
    if (id) seen[id]=1
  }
</script></div>

