    <script>
        require([
            'jquery',
            'jquery/hover-intent'
                                ], function($) {
            $('.price-box', $("#nav17150033866638dffa3580d")).each(function(){
                $(this).removeClass('price-box').addClass('price-box1');
                $(this).attr('data-role','priceBox1');
            })
            
            jQuery(document).ready(function($) {
                var $navSections = $('.nav-sections');

                jQuery('#nav17150033866638dffa3580d p').each(function() {
                    var $this = $(this);
                    if ($this.html().replace(/\s|&nbsp;/g, '').length == 0)
                        $this.remove();
                });

                
                // Hover on Level 1 items
                $("#nav17150033866638dffa3580d .nav-item").hoverIntent({
                    sensitivity: 7,
                    interval: 100,
                    timeout: 100,
                    over: function() {
                        $(this).addClass('current');
                        var id = $(this).data("dynamic-id");
                        var $dynamicItemsAnchor = $(this).parents('.dynamic-items').find('li > .nav-anchor');
                        var $anchor = $(this).find("> .nav-anchor");
                        var anchorAriaExpanded = $anchor.attr('aria-expanded');

                        if (id) {
                            // Remove Active to all the submenu items
                            $(this).parents(".dynamic-items").find("li").removeClass("dynamic-active");

                            $dynamicItemsAnchor.each(function(i,el) {
                                var attrAriaExpanded = $(el).attr('aria-expanded');
                                if (typeof attrAriaExpanded !== 'undefined' &&  attrAriaExpanded !== false) {
                                    $(el).attr('aria-expanded','false');
                                    $(el).siblings('.keyfocus-arrow').attr('aria-expanded','false');
                                }
                            });

                            // Activate the current submenu item
                            $(this).addClass('dynamic-active');

                            // Change aria if has children
                            if (typeof anchorAriaExpanded !== 'undefined' &&  anchorAriaExpanded !== false) {
                                $anchor.attr('aria-expanded','true');
                                $anchor.siblings('.keyfocus-arrow').attr('aria-expanded','true');
                            }

                            // Activate the content
                            $("#nav17150033866638dffa3580d ."+id).parent().find(".dynamic-item")
                                .removeClass("dynamic-active");
                            $("#nav17150033866638dffa3580d ."+id).addClass("dynamic-active");
                        }

                        if ($(this).data('hovericon')) {
                            $(this).children('.nav-anchor').find('.item-icon')
                                .attr('src', $(this).data('hovericon'));
                        }
                        if ($(this).data('caret') && $(this).data('hovercaret')) {
                            $(this).children('.nav-anchor').find('.ves-caret')
                                .removeClass($(this).data('caret'))
                                .addClass($(this).data('hovercaret'));
                        }

                        var child_anchor = $(this).children('.nav-anchor');
                        $(child_anchor).css({
                            "background-color": $(child_anchor).data("hover-bgcolor"),
                            "color": $(child_anchor).data("hover-color")
                        });
                                                if ($(this).hasClass('level0')) {
                            var mParentTop = $('#top-menu-1715003386 .navigation').offset().top;
                            var mParentHeight = $(this).parent().height();
                            var mTop =  $(this).height();
                            var mHeight = $(this).height();
                            var mParent = $(this).parent();
                            if (mHeight < mParentHeight) {
                                mTop = $(this).offset().top - mParent.offset().top + mHeight;
                            }
                            $(this).children('.submenu').css({top:mTop});
                        }
                        
                        // Add nav-hover overlay if opening a menu item with submenu
                        if (!$(this).find('span').hasClass("keyfocus-arrow")) {
                            $('body').removeClass('nav-hover');
                        }
                        else {
                            $('body').addClass('nav-hover');
                        }
                    },
                    out: function() {
                        var id = $(this).attr('id');
                        if ($(this).data('iconsrc')) {
                            $(this).children('.nav-anchor').find('.item-icon')
                                .attr('src', $(this).data('iconsrc'));
                        }
                        if ($(this).data('caret')) {
                            $(this).children('.nav-anchor').find('.ves-caret')
                                .removeClass($(this).data('hovercaret'))
                                .addClass($(this).data('caret'));
                        }
                        $(this).removeClass('current');
                        var child_anchor = $(this).children('.nav-anchor');
                        var link_bgcolor = $(child_anchor).data("bgcolor");
                        var link_color = $(child_anchor).data("color");
                        if(!link_bgcolor|| typeof(link_bgcolor) == 'undefined') {
                            link_bgcolor = 'none';
                        }
                        if(!link_color || typeof(link_color) == "undefined") {
                            link_color = 'none';
                        }

                        $(child_anchor).css({
                            "background-color": link_bgcolor,
                            "color": link_color
                        });

                        // Remove nav-hover overlay
                        $('body').removeClass('nav-hover');
                    }
                });

                
                
                /** ACCORDION */
                
                /* OPENER */
                $('#nav17150033866638dffa3580d .opener').on('click', function(e) {
                    var self = this;
                    e.preventDefault();
                    $("#nav17150033866638dffa3580d .nav-item").removeClass("item-active");
                    var parent = $(this).parents(".nav-item").eq(0);
                    var subMenu = $(parent).find(".submenu").eq(0);
                    var level1parent = $(this).parents(".mega-col-level-1").eq(0);
                    var lsCarouselMenu = $(parent).find('.ls-carousel-menu');
                    var openerSiblings;
                    var subMenuSiblings;

                    $(this).toggleClass('item-active');

                    if (level1parent.length) {
                        subMenuSiblings = $(level1parent).siblings().find(".submenu");
                        openerSiblings = $(level1parent).siblings().find('.opener');
                    }
                    else {
                        subMenuSiblings = $(parent).siblings().find(".submenu");
                        openerSiblings = $(parent).siblings().find(".opener");
                    }

                    // Close previous active accordions
                    subMenuSiblings.stop().slideUp('100');
                    openerSiblings.stop().removeClass("item-active");

                    subMenu.stop().slideToggle('100', function() {

                        if (lsCarouselMenu.length) {
                            lsCarouselMenu.slick('setPosition');
                        }

                    });
                    subMenu.css({'height':''});

                    return false;
                });

                /* TOGGLE */
                $("#top-menu-1715003386 .ves-navtoggletop-menu-1715003386")
                    .click(function() {
                        $('html').removeClass('nav-before-open nav-open');
                        $('.ves-overlaytop-menu-1715003386').show();
                        $("#top-menu-1715003386").append(
                            '<div class="ves-overlay ves-overlaytop-menu-1715003386"></div>'
                        );
                        $('#nav17150033866638dffa3580d').css("left", "0px");
                        if ($('html').hasClass('ves-navopen')) {
                            $('html').removeClass('ves-navopen');
                            setTimeout(function () {
                                $('html').removeClass('ves-nav-before-open');
                            }, 300);
                        }    else {
                            $('html').addClass('ves-nav-before-open');
                            setTimeout(function () {
                                $('html').addClass('ves-navopen');
                            }, 42);
                        }
                    });

                /* OFF-CANVAS */
                $(document).on("click", ".ves-overlaytop-menu-1715003386", function(){
                    $('#nav17150033866638dffa3580d').css("left","");
                    $('html').removeClass('ves-navopen');
                    setTimeout(function () {
                        $('html').removeClass('ves-nav-before-open');
                    }, 300);
                    $(this).remove();
                    return false;
                });

                /* STICKY MENU */
                
                var mobileDrill, desktopDrill;
                /* WINDOW RESIZE */
                $(window).on("resize", function() {
                    var disable_bellow_number = $("#nav17150033866638dffa3580d").attr("data-disable-bellow");
                    if(disable_bellow_number) {
                        if ($(window).width() < parseInt(disable_bellow_number)) {
                            $("#top-menu-1715003386").hide();
                        } else {
                            $("#top-menu-1715003386").show();
                        }
                    }
                    if ($(window).width() < 768) {
                        $("#top-menu-1715003386").addClass("nav-mobile").removeClass('nav-desktop');
                        /** DRILLDOWN MOBILE MENU */
                        
                                                $('#top-menu-1715003386 .opener').removeClass('item-active');
                    } else {
                        $("#top-menu-1715003386 .submenu").css({'display':''});
                        $("#top-menu-1715003386")
                            .removeClass("nav-mobile")
                            .addClass('nav-desktop');

                        /** DRILLDOWN MOBILE MENU */
                        
                        
                    }

                    // TABLET
                    $("#nav17150033866638dffa3580d .nav-item").click(function() {
                        if ($(window).width() >= 768 && $(window).width() < 1024) {
                            var parent = $(this).parent();
                            if ($(parent).children('.submenu').length == 1) {
                                if ($(parent).hasClass('level0')) {
                                    if ($(parent).hasClass('current')) {
                                        $(parent).removeClass('current');
                                        return false;
                                    }
                                    $("#nav17150033866638dffa3580d .level0").removeClass('current');
                                    var mParent = $('#top-menu-1715003386 .navigation').offset().top;
                                    var mHeight = $(parent).offset().top;
                                    var mTop    = (mHeight - mParent) + $(parent).height();
                                    $(parent).children('.submenu').css({top:mTop});
                                }
                                $(parent).addClass('current');
                                if ($(parent).hasClass('submenu-alignleft')
                                    || $(parent).hasClass('submenu-alignright')) {
                                    if (($(parent).offset().left + $(parent).children('.submenu').width())
                                        > $(window).width()) {
                                        $(parent).children('.submenu').css('max-width','100%');
                                        $(parent).css('position','static');
                                    }
                                }
                                return false;
                            }
                        } else {
                            $("#nav17150033866638dffa3580d").find('.submenu').css('max-width','');
                            $("#nav17150033866638dffa3580d").find('.submenu-alignleft').css('position','relative');
                        }
                    });
                }).resize();
                var toggle_nav = $("#nav17150033866638dffa3580d").attr("data-toggle-mobile-nav");
                if(toggle_nav == true || toggle_nav == 'true' || toggle_nav==1){
                    if(!$('html').hasClass('megamenu-init-toggle')) {
                        $('html').addClass('megamenu-init-toggle');
                        $(document).on("click", ".action.nav-toggle", function () {
                            if ($('html').hasClass('nav-open')) {
                                $('html').removeClass('nav-open');
                                setTimeout(function () {
                                    $('html').removeClass('nav-before-open');
                                }, 300);
                            } else {
                                $('html').addClass('nav-before-open');
                                setTimeout(function () {
                                    $('html').addClass('nav-open');
                                }, 42);
                            }
                        });
                    }
                }
            });
        });
    </script>
    <script>
        require([
            'jquery',
            'scrollInDiv'
        ], function($, scrollInDiv) {
            /* ADA Accessibility */
            $(window).on('load',function() {
                var $nav = $('.main-menu-wrapper'),
                    $opener = $nav.find('.keyfocus-arrow');
                    $level0NavItem = $nav.find('.nav-item.level0');

                $nav.find('li.level0').each(function(i, el) {
                    // Activate Aria attributes for first item on submenu
                    // Cause first item of submenu always has class dynamic-active
                    if ($(el).find('.level1 .dynamic-active > .keyfocus-arrow').length
                        && $(window).width() > 992) {
                        $(el).find('.level1 .dynamic-active > .keyfocus-arrow').attr('aria-expanded', 'true');
                        $(el).find('.level1 .dynamic-active > .nav-anchor').attr('aria-expanded', 'true');
                    }

                    // Add aria-hidden attribute to submenu
                    $(el).find('> .submenu').attr('aria-hidden', 'true');
                });

                // Position top of arrow carousel to be vertical centered to the image
                function positionCarouselArrowMobile($carousel) {
                    if ($carousel.find('.fotorama__arr').length
                        && $carousel.find('.image-wrapper img').first().height()) {
                        var newArrowTop = ($carousel.find('.image-wrapper img').first().height() / 2) - 20;
                        $carousel.find('.fotorama__arr').css('top', newArrowTop);
                    }
                }

                // Submenu Open Handler
                function submenuOpen($_this) {

                    // Submenu Opener for Desktop
                    if ($(window).width() > 992) {
                        var $header = $('header'),
                            $_el;

                        // If Level 0 is Open
                        if ($_this.closest('li.level0').hasClass('keyfocus-hover')
                            || $_this.closest('li.level0').hasClass('current')) {

                            $_this.attr('aria-expanded', 'false');
                            $_this.siblings('.nav-anchor').attr('aria-expanded', 'false');
                            $_this.siblings('.submenu').attr('aria-hidden', 'true');

                            // Submenu on Level 1
                            if ($_this.closest('.level1').length) {
                                var $_level1 = $_this.closest('.level1'),
                                    $_li = $_this.closest('li.nav-item'),
                                    id = $_li.attr('data-dynamic-id');

                                // Deactivate all items on Level 1
                                $_this.closest('li.level0')
                                    .find('.level1 .keyfocus-arrow')
                                    .attr('aria-expanded', 'false');

                                $_this.closest('li.level0')
                                    .find('.level1 .keyfocus-arrow')
                                    .siblings('.nav-anchor')
                                    .attr('aria-expanded', 'false');


                                // Activate current item on level 1
                                $_this.attr('aria-expanded', 'true');
                                $_this.siblings('.nav-anchor').attr('aria-expanded', 'true');

                                // Level 1 deactivate dynamic-active toggle elements
                                $_level1.find('li.dynamic-active')
                                        .removeClass('current').removeClass('dynamic-active');
                                // Level 1 activate current dynamic-active toggle elements
                                $_li.addClass('current').addClass('dynamic-active');
                                // Level 1 deactivate dynamic-active content elements
                                $_level1.find('.dynamic-content .nav-item.level1.dynamic-active')
                                        .removeClass('dynamic-active');
                                // Level 1 activate current dynamic-active content elements
                                $('#' + id).addClass('dynamic-active');
                            }
                            else {
                                $('.keyfocus-hover').removeClass('keyfocus-hover');
                                $('header').removeClass('page-header--nav-hover');
                                $('body').removeClass('nav-hover');

                                // Remove current class of Level 0 which appears on hover
                                if ($_this.closest('li.level0').hasClass('current')) {
                                    $_this.closest('li.level0').removeClass('current');
                                }
                            }
                        }
                        // If Level 0 is closed - Open the submenu
                        else {
                            // Remove all previous activated keyfocus-hover
                            $('.keyfocus-hover > .nav-anchor').attr('aria-expanded', 'false');
                            $('.keyfocus-hover > .keyfocus-arrow').attr('aria-expanded', 'false');
                            $('.keyfocus-hover > .submenu').attr('aria-hidden', 'true');
                            $('.keyfocus-hover').removeClass('keyfocus-hover');

                            // Aria activate keyfocus-arrow and nav-anchor for Level 0 and level 1
                            $_this.attr('aria-expanded', 'true');
                            $_this.siblings('.nav-anchor').attr('aria-expanded', 'true');
                            $_this.siblings('.submenu').attr('aria-hidden', 'false');

                            // Adding keyfocus-hover only to Level 0
                            $_this.closest('li.level0').addClass('keyfocus-hover');
                            $('header').addClass('page-header--nav-hover');
                            $('body').addClass('nav-hover');
                        }
                    }
                    // Submenu opener for Mobile and Tablet
                    else {
                        $("#nav17150033866638dffa3580d .nav-item").removeClass("item-active");
                        var parent = $_this.parents(".nav-item").eq(0),
                            subMenu = $(parent).find(".submenu").eq(0),
                            level1parent = $_this.parents(".mega-col-level-1").eq(0),
                            lsCarouselMenu = $(parent).find('.ls-carousel-menu'),
                            scrollableNavDiv = $('.nav-sections-item-content'),
                            openerSiblings,
                            subMenuSiblings;

                        // Toggle the clicked item
                        $_this.toggleClass('item-active');
                        $_this.attr('aria-expanded', function(index, attr) {
                            return attr == 'true' ? 'false' : 'true';
                        });
                        $_this.siblings('.nav-anchor').attr('aria-expanded', function(index, attr) {
                            return attr == 'true' ? 'false' : 'true';
                        });
                        $_this.siblings('.submenu').attr('aria-hidden', function(index, attr) {
                            return attr == 'true' ? 'false' : 'true';
                        });

                        // Assign variables for submenu Level 1 (static block content) and submenu Level 0
                        if (level1parent.length) {
                            subMenuSiblings = $(level1parent).siblings().find('.submenu');
                            openerSiblings = $(level1parent).siblings().find('.keyfocus-arrow');
                        } else {
                            subMenuSiblings = $(parent).siblings().find('.submenu');
                            openerSiblings = $(parent).siblings().find('.keyfocus-arrow');
                        }

                        // Close all siblings
                        subMenuSiblings.hide();
                        openerSiblings.removeClass('item-active').attr('aria-expanded', 'false');
                        openerSiblings.siblings('.nav-anchor').attr('aria-expanded', 'false');
                        openerSiblings.siblings('.submenu').attr('aria-hidden', 'true');

                        subMenu.stop().toggle('100',function() {
                            // Reposition Slick Slider from submenu Level 1 static block content
                            if (lsCarouselMenu.length) {
                                lsCarouselMenu.slick('setPosition');
                                positionCarouselArrowMobile(lsCarouselMenu);
                            }

                            // Scroll to current opened item
                            setTimeout(function(){
                                scrollInDiv(scrollableNavDiv, parent, true);
                            }, 300);
                        });
                        subMenu.css({'height':''});

                        return false;
                    }
                }

                $opener.keydown(function(e) {
                    if (event.code === 'Enter' || event.code === 'Space') {
                        e.stopPropagation();
                        submenuOpen($(this));
                    }
                });

                $opener.click(function(e) {
                    e.stopPropagation();
                    submenuOpen($(this));
                });

                
                    // Handle the Submenu Level 0 opener on hover of the nav item
                    function submenuHoverHandlerIn() {
                        if ($(window).width() > 992) {
                            $(this).find('> .nav-anchor').attr('aria-expanded', 'true');
                            $(this).find('> .keyfocus-arrow').attr('aria-expanded', 'true');
                            $(this).find('> .submenu').attr('aria-hidden', 'false');
                        }
                    }

                    // Handle the Submenu Level 0 on hover out of the nav item
                    function submenuHoverHandlerOut() {
                        if ($(window).width() > 992) {
                            $(this).find('> .nav-anchor').attr('aria-expanded', 'false');
                            $(this).find('> .keyfocus-arrow').attr('aria-expanded', 'false');
                            $(this).find('> .submenu').attr('aria-hidden', 'true');

                            if ($(this).hasClass('keyfocus-hover')) {
                                $(this).removeClass('keyfocus-hover');
                            }
                        }
                    }

                    $level0NavItem.hover( submenuHoverHandlerIn, submenuHoverHandlerOut );

                
                $('body').on('keydown', function(e) {
                    if (event.code === 'Enter' || event.code === 'Space') {
                        if (!$(e.target).closest('li.level0').find('.keyfocus-arrow').length) {
                            $('.keyfocus-hover').removeClass('keyfocus-hover');
                            $('header').removeClass('page-header--nav-hover');
                        }
                    }

                    // ADA - Close megamenu on press Escape button
                    if (event.code === 'Escape') {
                        $('.nav-item').each(function(i,el) {
                            // Close if hovered or if open by keyfocus-arrow
                            if ( $(el).hasClass('current') || $(el).hasClass('keyfocus-hover')) {
                                $(el).removeClass('current').removeClass('keyfocus-hover');
                                $(el).find('> .nav-anchor').attr('aria-expanded', 'false');
                                $(el).find('> .keyfocus-arrow').attr('aria-expanded', 'false');
                                $('header').removeClass('page-header--nav-hover');
                                $('body').removeClass('nav-hover');
                                setTimeout(function() {
                                    $(el).closest('.nav-item.level0').find('> .nav-anchor').focus();
                                }, 100);
                            }
                        });
                    }

                });
            });
        });
    </script>
                </div>
                    </div>
    </div>
