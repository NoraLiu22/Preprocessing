    <script>
        /*
         * Dynamically inject header-user-link, header-cart and other user-specific markup
         */
        $(document).ready(function(){
            $('.picto-reset').click(function(){
               $('#header-searchbox-field').val('');
               return false;
            });

            $.ajax({
                type: 'GET',
                url: "\/gb-en\/ajax\/user-information?context=shopHome",
                cache: false,
                success: function(data) {
                    for (var selector in data.contents) {
                        if (data.contents.hasOwnProperty(selector)) {
                            $(selector).html(data.contents[selector]);
                        }
                    }

                    ajaxUserInformationCallback(data);
                    if (data.gtm) {
                        dataLayer.push(data.gtm);
                    }
                    if (typeof data.gtmV2 !== 'undefined' && data.gtmV2 !== '') {
                        dataLayer.push(data.gtmV2);
                    }
                    gtminit();
                    if(data.plans) {
                        $.each(data.plans, function(propKey, propValue) {
                            if(propKey == 'show_promises' && propValue == 1) {
                                $('.promises-block').show();
                                $('.arrow-up').show();
                            }

                            $.each(propValue, function(offerName, propOffer) {

                                var button_label = propOffer['button_label'];
                                // update button label
                                $('div.' + offerName + '-button a').text(button_label);
                                // update button link
                                if(propOffer['button_link']) {
                                    $('div.' + offerName + '-button a').attr('href', propOffer['button_link']);
                                    $('div.' + offerName + '-button a').removeClass("gtm_event_trigger");
                                    $('div.' + offerName + '-button a').removeAttr("gtm-data");
                                }
                                // display trial picto
                                if(propOffer['show_trial'] == 1) {
                                    $('div.trial-' + offerName).show();
                                }
                                // display prorata message
                                if(propOffer['show_prorata'] == 1) {
                                     $('div.' + offerName + '-prorata').show();
                                }
                            });
                        });
                    }
                }
            });
        });
    </script>

