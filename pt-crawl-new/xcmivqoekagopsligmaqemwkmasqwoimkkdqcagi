<script type="text/javascript">
    jQuery(document).ready(function($) {
        // initialize FlexNav
        $(".flexnav").flexNav();
        $(document).bind("touchstart", function(e)
        {
            var mainMenus = document.getElementsByClassName("flexnav with-js opacity sm-screen flexnav-show");
            if(mainMenus[0] != null){
                var menuBlock = $('#rockhurst_header_main_nav').parent();
                if (!menuBlock.is(e.target) // if the target of the click isn't the container...
                    && menuBlock.has(e.target).length === 0) // ... nor a descendant of the container
                {
                    var menus = document.getElementsByClassName('menu-button');
                    menus[0].click();
                }
            }
        });

        //fix the search form in the header
        $("#edit-keys").next().children().val("");
        document.getElementById("edit-keys").placeholder = "SEARCH";
    });
</script>
