        function waitForFreestar() {
          return new Promise((resolve) => {
            const checkFreestar = () => {
              if (typeof freestar !== 'undefined' && freestar.config) {
                resolve();
              }
            };

            const observer = new MutationObserver(checkFreestar);

            observer.observe(document.body, { childList: true, subtree: true });

            // Stop observing after 10 seconds (adjust the time as needed)
            setTimeout(() => {
              observer.disconnect();
              resolve();
            }, 10000);
          });
        }

        (async function() {
          await waitForFreestar();
