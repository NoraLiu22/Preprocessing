    function getLambdaAnalyticsUrl() {
      if (window.n2.config.env === 'stage') {
        return 'https://pi4dftunh4.execute-api.us-east-1.amazonaws.com/stage/pix-trk/?event_timestamp=';
      } else {
        return 'https://0gv2ds5jh3.execute-api.us-east-1.amazonaws.com/prod/pix-trk/?event_timestamp=';
      }
