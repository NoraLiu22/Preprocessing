googletag.display = function(adunit) {
    const adCalled = googletag.pubads().getSlots().find(slot => slot.getSlotElementId() == adunit);
    console.log("ad called: ", adCalled.getSizes());
    var sizes = adCalled.getSizes().map(function(size) {
        if (size.getWidth && size.getHeight) {
            return [size.getWidth(), size.getHeight()];
        }
        else {
            return [1, 1];
        }
    });

    iasPETSlots.push({
        adSlotId: adCalled.getSlotElementId(),
        size: sizes,
        adUnitPath: adCalled.getAdUnitPath(),
    });

    var IASPET_TIMEOUT = 2000;
    var __iasPETTimeoutRequestAds = setTimeout(requestAds(adCalled), IASPET_TIMEOUT);

    window.iasDataHandler = function(adSlotData) {
        clearTimeout(__iasPETTimeoutRequestAds);
        __iasPET.setTargetingForGPT();
//googletag.pubads().refresh([adCalled])
    };

    function requestAds(slot) {
        googletag.cmd.push(function() {
           // googletag.pubads().refresh([slot]);
        });
    }

    __iasPET.queue.push({
        adSlots: iasPETSlots,
        dataHandler: window.iasDataHandler,
    });

}
    

});

