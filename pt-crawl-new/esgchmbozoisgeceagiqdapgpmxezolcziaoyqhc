        REBELMOUSE_STDLIB.createElementChangeListener(".authentication--goto", function(articleElement){
	var forgot_link_1 = document.querySelectorAll(".authentication--goto")[0];
    var forgot_link_2 = document.querySelectorAll(".authentication--goto")[1];
    var signup_link_1 = document.querySelectorAll(".authentication--goto")[2];
    var signup_link_2 = document.querySelectorAll(".authentication--goto")[3];

    const submitButton = document.querySelectorAll('.authentication--submit');
    for (i = 0; i < submitButton.length; i++) {
          submitButton[i].setAttribute('disabled', 'true');
    }

    [forgot_link_1, forgot_link_2].forEach((element)=>{
        element.addEventListener('click', event => {
            document.querySelector(".login-headline").classList.add("hidden");
            document.querySelector(".forgot-headline").classList.remove("hidden");
            document.querySelector(".forgotten-msj-wrapper").classList.remove("hidden");
