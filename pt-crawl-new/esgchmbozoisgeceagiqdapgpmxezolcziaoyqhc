window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        var myParam = localStorage.getItem("session_email");
if (myParam != null) {
  var calledInput = false;
  window.REBELMOUSE_STDLIB.createElementChangeListener(
    ".authentication--field-email .authentication--input",
    function () {
      var allInputs = document.querySelectorAll(
        ".authentication--field-email .authentication--input"
      );

      if (allInputs.length < 1 || calledInput) {
        return;
      }

      calledInput = true;
      var input = allInputs[1];

      input.value = myParam;
      input.classList.add("valid");

      localStorage.removeItem("session_email");
    }
  );

  var calledError = false;
  window.REBELMOUSE_STDLIB.createElementChangeListener(".authentication--field-email .form-input__error", function () {
    var allErrors = document.querySelectorAll(
      ".authentication--field-email .form-input__error"
    );

    if (allErrors.length < 1 || calledError) {
      return;
    }

    calledError = true;

    var error = allErrors[1];

    error.classList.add("form-input__note");
    error.innerText =
      "This account already exists. Please enter your password to log in.";

    setTimeout(function () {
      error.innerText = "Please enter a valid email";
      error.classList.remove("form-input__note");
    }, 3000);
  });
}
    

