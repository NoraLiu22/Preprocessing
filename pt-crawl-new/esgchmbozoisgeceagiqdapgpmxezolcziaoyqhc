    <script class="rm-i-script" id="user-javascript-active" type="text">
        window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function bootRpm(){
            const INTERNAL_CLASS_PREFIXES = ["tag-", "post-section-", "rm-fl-"];

            function filterOutNoisyClassNames(className) {
                function doesClassNameStartWithPrefix(badPrefix) {
                    return className.indexOf(badPrefix) === 0;
                }
            
                return !INTERNAL_CLASS_PREFIXES.some(doesClassNameStartWithPrefix);
            }
            
            function signatureMaker(id, tagName, classList) {
                const cleanClassList = classList.filter(filterOutNoisyClassNames).sort();
                return [id, tagName, cleanClassList];
            }
            
            window.__rpmOptions = window.__rpmOptions || {
                lcpTrackerOptions: {
                    signatureMaker: signatureMaker,
                },
                clsTrackerOptions: {
                    signatureMaker: signatureMaker,
                },
            };
            
            
            window.REBELMOUSE_STDLIB.loadExternalScript("https://www.rebelmouse.com/pharos/client/v1/mjs/web.mjs", function(){})
        });

window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        REBELMOUSE_STDLIB.createElementChangeListener(
  ".authentication--input",
  function (elem) {
    document
      .querySelectorAll(
        ".authentication--field-email .authentication--input"
      )[1]
      .setAttribute("placeholder", "Email");
    document
      .querySelectorAll(
        ".authentication--field-email .authentication--input"
      )[2]
      .setAttribute("placeholder", "Email");
    document
      .querySelectorAll(
        ".authentication--field-password .authentication--input"
      )[1]
      .setAttribute("placeholder", "Password");

    document.querySelector(".authentication--select")[0].innerText = "Year of Birth";
    document.querySelector(".authentication--select")[0].disabled = true;
  }
);

var extraField = false;
let validFields = {};

const allowFormToSubmit = () => {
  const submitButton = document.querySelectorAll(".authentication--submit");
  for (i = 0; i < submitButton.length; i++) {
    submitButton[i].setAttribute("disabled", "true");
  }
  if (Object.values(validFields).every(Boolean)) {
    console.log("all" + Object.values(validFields).every(Boolean));
    return (submitButton[0].disabled = false);
  } else {
    if (validFields["email"] && validFields["password"]) {
      console.log("email and password");
      // enable button for login
      return (submitButton[1].disabled = false);
    } else {
      var urlParams = new URLSearchParams(window.location.search);
      var myParam = urlParams.get("redirect");
      if (validFields["password"] && myParam != null) {
        console.log("just password");
        // enable button for login already register
        return (submitButton[1].disabled = false);
      }
      if (validFields["email"]) {
        console.log("just email");
        // enable button for forgotten password
        return (submitButton[2].disabled = false);
      }
    }
  }
};

// Small functions for valid/invalid classes
const addInvalidClass = (element) => element.classList.add("invalid");
const removeInvalidClass = (element) => element.classList.remove("invalid");
const addValidClass = (element) => element.classList.add("valid");
const removeValidClass = (element) => element.classList.remove("valid");

// Show field label after user interaction with field
const showFieldLabel = (element) => {
  const parent = element.closest(".authentication--label");
  const fieldLabel = parent.querySelector(".authentication--label-caption");

  return fieldLabel.classList.add("show");
};

// Show error message
const showErrorMessage = (element) => {
  const parent = element.closest(".authentication--label");
  const errorContainer = parent.querySelector(".form-input__error");

  if (errorContainer) {
    return errorContainer.classList.add("show");
  }
};

// Hide error message
const hideErrorMessage = (element) => {
  const parent = element.closest(".authentication--label");
  const errorContainer = parent.querySelector(".form-input__error");

  if (errorContainer) {
    return errorContainer.classList.remove("show");
  }
};

const setValid = (input) => {
  validFields[input.name] = true;
  removeInvalidClass(input);
  hideErrorMessage(input);
  addValidClass(input);

  return allowFormToSubmit();
};

const setInvalid = (input) => {
  validFields[input.name] = false;
  removeValidClass(input);
  showErrorMessage(input);
  addInvalidClass(input);

  console.log("input - ", input);

  return allowFormToSubmit();
};

const createErrorMesage = (input) => {
  const type = input.type;
  const errorsText = {
    email: "Please enter a valid email",
    text: "Numbers and special characters aren't allowed",
    password:
      "Must be at least 8 characters, include an upper and lower case character and a number",
    "select-one": "You must be at least 18 years old to create an account",
  };

  if (errorsText[type]) {
    const createContainer = document.createElement("div");
    createContainer.classList.add("form-input__error");
    if (type == "password") {
      	if(!input.parentElement.parentElement.parentElement.classList.contains("login-form")){
        	createContainer.innerText = errorsText[type];
        }
  		input.insertAdjacentHTML("afterend","<i class='far fa-eye togglePassword'></i>");
      	const toogle = input.parentElement.querySelector(".togglePassword");
      	toogle.addEventListener("click", (event) => {
          if (input.type == "password") {
            input.setAttribute("type", "text");
            toogle.classList.add("block");
          } else {
            input.setAttribute("type", "password");
            toogle.classList.remove("block");
          }
        });
    }else{
    	createContainer.innerText = errorsText[type];
    }
    return input.insertAdjacentElement("afterend", createContainer);
  }
};

// Email validation
const validateEmail = function () {
  const input = event.target;
  const resetFormParent = input.closest('.reset-form');
  const submitButton = document.querySelectorAll(".authentication--submit");
  const parent = input.closest('.authentication--label');
  let error = parent.querySelector('.form-input__error');

  if (input.value.length > 0) {
    showFieldLabel(input);
    if (/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(input.value)) {
      if(input.value.length > 75){
        error.innerText = "Must have less than 75 characters";
      	setInvalid(input);
      } else {
        setValid(input);
      }
    } else {
      error.innerText = "Please enter a valid email";
      setInvalid(input);
    }
  } else {
    removeValidClass(input);
    removeInvalidClass(input);
    hideErrorMessage(input);
  }
};

// Password validation
const validatePassword = function () {
  const input = event.target;
  const reg_format = new RegExp("^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.{8,})");

  if (input.value.length > 0) {
    showFieldLabel(input);
    if (reg_format.test(input.value)) {
      setValid(input);
    } else {
      setInvalid(input);
    }
  }
};

// First name, last name validation
const validateName = function () {
  const input = event.target;
  const nameFormat = /^[a-zA-Z]+(\s{0,1}[a-zA-Z])*$/;
  const parent = input.closest('.authentication--label');
  let error = parent.querySelector('.form-input__error');

  if (input.value.length > 0) {
    showFieldLabel(input);
    
    if (input.value.match(nameFormat)) {
      if (input.value.length > 40) {
        error.innerText = "Must have between 1 and 40 characters";
        setInvalid(input);
      }else{
      	setValid(input);
      }
    } else {
      error.innerText = "Numbers and special characters aren't allowed";
      setInvalid(input);
    }
  } else {
    validFields[input.name] = false;

    allowFormToSubmit();
    removeValidClass(input);
    removeInvalidClass(input);
    hideErrorMessage(input);
  }
};

const validateYear = function () {
  const input = event.target;

  showFieldLabel(input);
  if (input.value.trim() === "") {
    setInvalid(input);
  } else {
    setValid(input);
  }
};

const addInputHandler = (input, validationFunction) => {
  input.addEventListener("input", validationFunction);
};

const addBlurHandler = (input, validationFunction) => {
  input.addEventListener("blur", validationFunction);
};

const addChangeHandler = (input, validationFunction) => {
  input.addEventListener("change", validationFunction);
};

window.REBELMOUSE_STDLIB.createElementChangeListener(
  ".authentication--select, .authentication--input",
  function (field) {
    validFields[field.name] = false;
    createErrorMesage(field);

    if (field.type === "email") {
      addInputHandler(field, validateEmail);
      addBlurHandler(field, validateEmail);
    }

    if (field.type === "password") {
      addInputHandler(field, validatePassword);
    }

    if (field.type === "text") {
      addInputHandler(field, validateName);
    }

    if (field.name === "BirthYear") {
      addChangeHandler(field, validateYear);
    }

    if (field.type === "checkbox") {
      setValid(field);
    }
  }
);

// Creating our own policy text
const linkUrl =
  "https://www.independent.co.uk/service/privacy-policy-a6184181.html";

/* Hide marketing opt-in box, 
CHECK https://rebelmouse.atlassian.net/browse/INDY-2614 
for details */
window.REBELMOUSE_STDLIB.createElementChangeListener(".authentication--privacy_policy-module", function (ppm) {
    ppm.remove();
  }
);

window.REBELMOUSE_STDLIB.createElementChangeListener(
  ".authentication--privacy_policy-module .authentication--label-caption",
  function (textContainer) {
    const textTemplate = `I would like to be emailed about offers, events and updates from Indy100 and our sister title The Independent. Read our <a href="${linkUrl}" target="_blank">Privacy notice</a>`;
    textContainer.innerText = "";
    textContainer.insertAdjacentHTML("beforeend", textTemplate);
    
    const container = textContainer.closest('.authentication--privacy_policy-module');

    const template = `<div class="opt-out-policy js-opt-out">Opt-out-policy</div><div class="opt-out-policy__description">You can opt-out at any time by signing in to your account to manage your preferences. Each email has a link to unsubscribe.</div>`;
    container.insertAdjacentHTML("beforeend", template);
    const element = document.querySelector(".js-opt-out");
    const description = document.querySelector(".opt-out-policy__description");
    const toggleActive = function () {
      element.classList.toggle("active");
      return description.classList.toggle("active");
    };
    element.addEventListener("click", toggleActive);
  }
);

// Year of birth logic

window.REBELMOUSE_STDLIB.createElementChangeListener(
  "#yearOfBirth",
  function (select) {
    const currentAvailableYear = Number(new Date().getFullYear() - 18);
    const optionsForSelect = 100;

    for (let i = 0; i < optionsForSelect; i += 1) {
      const option = `<option class="form-select__option" value="${
        currentAvailableYear - i
      }">${currentAvailableYear - i}</option>`;
      select.insertAdjacentHTML("beforeend", option);
    }
  }
);

function findError(errors) {
  if (
    !errors ||
    typeof errors !== "object" ||
    Object.keys(errors).length === 0
  ) {
    return null;
  }

  return Object.values(errors)
    .flatMap((e) => e)
    .join(" ");
}

window.REBELMOUSE_STDLIB.createElementChangeListener(
  ".signup-form.authentication--form",
  function (form) {
    form.addEventListener("authentication:error", function (event) {
      event.stopPropagation();
      var message =
        event.detail.message ||
        event.detail.error ||
        findError(event.detail.errors);

      if (message && message.includes("already registered")) {
        localStorage.setItem(
          "session_email",
          document.querySelector(
            ".authentication--field-email .authentication--input"
          ).value
        );
        window.location.href = "/st/Registration?redirect=true";
      }

      return false;
    });
  }
);

window.REBELMOUSE_STDLIB.createElementChangeListener(
  ".authentication--form.login-form",
  function (form) {
    form.addEventListener("authentication:error", function (event) {
      event.stopPropagation();
	  
      var error = form.querySelector(
        ".authentication--field-password .form-input__error"
      );
      var input = form.querySelector(
        ".authentication--field-password .authentication--input"
      );
      var error_before = error.innerText;
      error.innerText = "Email and password don't match";
      error.classList.add("show");
      input.classList.add("invalid");
      input.classList.remove("valid");
      form.querySelector(".authentication--submit").disabled = true;
      setTimeout(function () {
        error.innerText = error_before;
        error.classList.remove("show");
      }, 3000);
      return false;
    });
  }
);

window.REBELMOUSE_STDLIB.createElementChangeListener(
  ".authentication--form.reset-form",
  function (form) {
    form.addEventListener("authentication:error", function (event) {
      event.stopPropagation();

      var message =
        event.detail.message ||
        event.detail.error ||
        findError(event.detail.errors);

      if (message) {
        document.querySelector(
          ".forgotten-msj-wrapper .text-element"
        ).innerText = "Email not found";
      }

      return false;
    });

    form.addEventListener("authentication:success", function (event) {
      document.querySelector(".forgotten-msj-wrapper .text-element").innerHTML =
        "Reset email sent.			 <br>Please check your inbox.<p class='smaller_msj'>Please check your spam folder if it does 			not appear.<p>";
      form.querySelector(".authentication--submit").disabled = true;
    });
  }
);

const chromeAutofill = () => {
  const interval = setInterval(() => {
    const password = document.querySelector('.login-form .authentication--input[type="password"]');
    const isAutofilled = window.getComputedStyle(password, null).getPropertyValue('appearance') === 'menulist-button';

    if (isAutofilled) {
      const submit = document.querySelector('.login-form .authentication--submit');
      submit.disabled = false;
      console.log('im in');
      clearInterval(interval);
    }
  }, 1e3);
}

chromeAutofill();

const passwordChange = () => {
  const password = document.querySelector('.login-form .authentication--input[type="password"]');
  
  password.addEventListener('paste', function() {
    console.log('password input value - ', password.value);
    allowFormToSubmit();
  });
}

passwordChange();
    

});

