window.REBELMOUSE_LOWEST_TASKS_QUEUE.push(function(){

    
        const backToTop = () => {
  const button = document.querySelector('.js-scroll-top');
  
  if (!button) {
    return false;
  }
  
  const toTop = () => {
    return window.scrollTo({
      top: 0,
      left: 0,
      behavior: 'smooth'
    });
  }
  
  button.addEventListener('click', toTop);
}

backToTop();
    

});</script>
