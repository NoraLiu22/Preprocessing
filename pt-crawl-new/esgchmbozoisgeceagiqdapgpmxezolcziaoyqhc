            document.querySelectorAll(".authentication--field-email .authentication--label-caption")[2].innerText = "Email address";
        });
    });

    [signup_link_1, signup_link_2].forEach((element)=>{
        element.addEventListener('click', event => {
          	
            document.body.scrollTop = 0; // For Safari
            document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
          
            document.querySelector(".login-headline").classList.add("hidden");
            document.querySelector(".forgot-headline").classList.add("hidden");
            document.querySelector(".forgotten-msj-wrapper").classList.add("hidden");
            document.querySelector(".signup-headline").classList.remove("hidden");
            document.querySelector(".signup-image").classList.remove("hidden");
            document.querySelector(".form-footer__wrapper").classList.remove("hidden");
            document.querySelector(".form-footer__wrapper").classList.add("gray");
            document.querySelector(".register_link_logo").classList.add("hidden");
            document.querySelector(".clickToLogin span").innerText = "Already have an account?";
        });
    });
})

