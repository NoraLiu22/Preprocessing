          var iasSlotTargeting = {};
          for (const slotId in adSlotDataObj.slots) {
              const slotIdValues = adSlotDataObj.slots[slotId];
              var slotParams = "";
              for (const key in slotIdValues) {
                  var segments = slotIdValues[key];
