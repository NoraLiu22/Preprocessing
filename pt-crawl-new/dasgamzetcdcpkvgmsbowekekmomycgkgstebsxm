  <source srcset="https://cdn.vox-cdn.com/thumbor/LRoEr73kTNYDL1JSEKF-aIeIUV8=/0x0:5616x3744/320x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 320w, https://cdn.vox-cdn.com/thumbor/X9a76JKfNyTKY-555Xwk6fAVXac=/0x0:5616x3744/520x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 520w, https://cdn.vox-cdn.com/thumbor/69bslZfEYWwf4-BPl2_S0RBQUro=/0x0:5616x3744/720x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 720w, https://cdn.vox-cdn.com/thumbor/EFC54V7Y9_Yu7KYFLX94XbUG46w=/0x0:5616x3744/920x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 920w, https://cdn.vox-cdn.com/thumbor/MJsoPdGve3vZSX9rLJN6vIcEe2c=/0x0:5616x3744/1120x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 1120w, https://cdn.vox-cdn.com/thumbor/M2GEsbjMqcxg1_WjWUc8lmjdSpg=/0x0:5616x3744/1320x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 1320w, https://cdn.vox-cdn.com/thumbor/sbm1pu2xlOzfwL28q-pY0m4YZeM=/0x0:5616x3744/1520x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 1520w, https://cdn.vox-cdn.com/thumbor/-X8opUW6gzK_9VJycdUFsN3lGSw=/0x0:5616x3744/1720x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_Still_IMG_6329.jpg 1720w, https://cdn.vox-cdn.com/thumbor/9mJPzymDNaAHfu0AX6GA0KRXn6w=/0x0:5616x3744/1920x0/filters:focal(0x0:5616x3744):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/22459361/TNCFU_SIF_