  <source srcset="https://cdn.vox-cdn.com/thumbor/3jr-e4mwXjDt5sxaGvmx9oWlwfE=/0x0:1498x751/320x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 320w, https://cdn.vox-cdn.com/thumbor/JD5ZzRD80QLGiOSEJZhdACK8Mu0=/0x0:1498x751/520x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 520w, https://cdn.vox-cdn.com/thumbor/4hwsqmyxhnp9A5mhea_q2ZGA_PM=/0x0:1498x751/720x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 720w, https://cdn.vox-cdn.com/thumbor/yRVcIRMVrpshbF9bLJhLU1UE6-M=/0x0:1498x751/920x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 920w, https://cdn.vox-cdn.com/thumbor/oy3AkVYBMLWE8ba1zjao-CiVaS4=/0x0:1498x751/1120x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 1120w, https://cdn.vox-cdn.com/thumbor/HL9fQ0u5kfc3yV1q65VEjCPU40Q=/0x0:1498x751/1320x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 1320w, https://cdn.vox-cdn.com/thumbor/RmTEsEnbnuCCcdFRT4yUWj5tVu8=/0x0:1498x751/1520x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 1520w, https://cdn.vox-cdn.com/thumbor/cIOAZJLOTaYhHTHHOHjJDqBmlQ8=/0x0:1498x751/1720x0/filters:focal(0x0:1498x751):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/24238592/ghosts_of_sugar_land_netflix_review_e1571177618210.jpg 1720w, https: