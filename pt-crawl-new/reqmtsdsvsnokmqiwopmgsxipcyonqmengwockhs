ent_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Vehicle Research", "target_url": "https://driving.ca/find-compare/", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/find-compare/> Vehicle Research </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Reviews", "target_url": "https://driving.ca/category/reviews", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/reviews/> Reviews </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Advice", "target_url": "https://driving.ca/category/features", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/features/> Advice </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "News", "target_url": "https://driving.ca/category/auto-news", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/auto-news/> News </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Experts", "target_url": "https://driving.ca/category/column", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/column/> Experts </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Car Culture", "target_url": "https://driving.ca/category/car-culture", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/car-culture/> Car Culture </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "anchor_text": "anchor_text", "target_url": "target_url", "layout_section": "layout_section"}, "mp_event_type": "Navigation", "extra_keys": ["click_vertical_position_percentage", "click_vertical_position_pixels"]}}, "click_source_type": "menu_item", "anchor_text": "Vehicle Types", "target_url": "https://driving.ca/category/vehicle-types", "layout_section": "secondary-nav-list"}'class=header-list-item-link data-evt=click data-evt-typ=click href=/category/vehicle-types/> Vehicle Types </a></li><li><a data-evt-val='{"control_fields": {"mparticle": {"keys": {"click_source_type": "click_source_type", "