                </div>
              </div> <!-- .c-card -->

            </div> <!-- .swiper-slide -->
          
        </div> <!-- .swiper-wrapper -->
      </div> <!-- .swiper -->
    </div> <!-- .medium-container -->
  </div> <!-- .container -->

</section> <!-- .people-cards-slider-section -->

          </article> <!-- .type-post -->

    

       

          
          
        </section> <!-- .main-content -->
