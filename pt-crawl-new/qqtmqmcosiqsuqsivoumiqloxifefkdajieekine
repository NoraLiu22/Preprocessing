<script type="text/javascript">
    var userLang = navigator.language || navigator.userLanguage;
    var lang;
    var rawUrlLength = "3";
    $(document).ready(function () {
        if(rawUrlLength=="1"){

        if(getCookie("lang") != "" && getCookie("lang")!="undefined")
        {
            if (userLang.indexOf("-") > -1) {
                lang = userLang.split("-")[0];
            }
            else {
                lang = userLang;
            }

            if(''=='true'){
                lang = 'en';
            }

            setCookie("lang", lang, 999);
            window.location.href = 'https://www.aa.com.tr/' + lang;
        }
        else
        {
            if (userLang.indexOf("-") > -1) {
                lang = userLang.split("-")[0];
            }
            else {
                lang = userLang;
            }

            if(lang!="tr"){
                setCookie("lang", lang, 999);
                
                window.location.href = 'https://www.aa.com.tr/' + lang;
            }

        }
    }
    })



</script>



        
