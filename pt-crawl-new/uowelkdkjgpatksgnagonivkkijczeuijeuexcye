<script>
	$(function(){
		
		$("#feedback-link").click(function(e){
			
			e.preventDefault();
			
			$.get("/portal/feedback-form",
					{page: "/"}, 
				function(data){
					$("#feedback-body").html(data);
					$('#feedback-modal').modal();
			});

		});
	});
</script>
