<script>
	$(function() {
		$("#stateSubmit").hide();
	
		$("#state-select").change(function(){
			if (typeof ga !== 'undefined')
				ga('set', 'dimension2', $(this).val());
			
			setTimeout(function(){ $("#stateform").submit(); }, 500);
			
		});
		
		$("#request-absentee-ballot").click(function() {
			navigate("https://www.fvap.gov/fpca-privacy-notice");
		});
		
		$(".subscribe").click(function() {
			window.open("https://public.govdelivery.com/accounts/USFVAP/subscribers/new");
		});
 	});
</script>

<script>
	$(function() {

		$.get("/general-election.json", function(data) {
			if (data == null)
				$("#electionCountdown").hide();
			else
				$("#generalElectionHeader").text(data);
		});
		
		var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
	
		function buildUpcomingElectionItem(key, values) {
            
			var date = moment(key, "YYYY-MM-DD");
			
			//add calendar image
			var htmlString = 
				'<tr>' + 
					'<td>' +
						'<div class="holder">' +
							'<div class="month red">' +
					    			'<p>'+ monthNames[date.month()] + '</p>' +
							'</div>' +
							'<div class="day">' +
								'<p>'+ (date.date()) + '</p> ' +
							'</div>' +
						'</div>' +
					'</td>' +
					'<td>';
					
			//add list of elections to right of calendar image
			for (var i = 0; i < values.length; i++){
				if (i + 1 == 4) {
					htmlString += '<div><a href="/guide/upcoming-elections" style="font-weight:bold;">... View All</a></div>';
				} 
				else {
					htmlString += '<div>';
					
					if (values[i].electionType.id == 5 && values[i].stateRule == null) {
						htmlString += '<span class="state">General Election</span>';
					} 
					else {
						htmlString += 
							'<span class="state">' +
								'<a href="' + values[i].stateRule.state.urlEnding + '">' +
									values[i].stateRule.state.name +
								'</a> - ' +
							'</span>' + 
							'<span class="election-type">' +
								values[i].electionType.name + 
							'</span>';
					}
				}
			}
									
			htmlString += '</td></tr>';
			
			return htmlString;
		}

		$.get("/upcoming-dates.json", function(response) {
			var count = 0;
			var data = response.data;
		
			for(var key in data) {
				$("#elections").append(buildUpcomingElectionItem(key, data[key]));
				count++;
			}
		
			var htmlString = '<tr class="last"><td colspan="2">';
			
			if (count == 0)
				htmlString += '<h3>No Upcoming Election Dates</h3>';
			else
				htmlString += '<a class="upcoming-elections" href="/guide/upcoming-elections" >View All Upcoming Election Dates</a>';
				
			htmlString += '</td></tr>';
			
			$("#elections").append(htmlString);
		}); 
	});
	
	var imageCount = document.getElementById("carousel-top-id").getElementsByClassName("unique-hero-image").length;
	if (2 > imageCount) {
		$("#carousel-left-button").css('visibility', 'hidden');
		$("#carousel-right-button").css('visibility', 'hidden');
	}
</script>
