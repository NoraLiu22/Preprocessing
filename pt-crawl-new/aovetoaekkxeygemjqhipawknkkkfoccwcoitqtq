<script>
    function goToAccount() {
        if (paywall.common.clientId === 'www.forbes.pl.okonto.front.onetapi.pl') {
            location.href = 'https://konto.forbes.pl';
        } else {
            location.href = 'https://konto.onet.pl/data/personal';
        }
    }
