<script>
    window.paidModel = {
        userHash: '',
        isLoggedIn: false,
        isPaying: false,
        terms: [],
        resources: []
    };

    var params = (new URL(document.location)).searchParams;
    var fromMobileApp = params.get('is_m_app') || readCookie('is_m_app');

    function readCookie(name) {
        const s = encodeURIComponent(name) + '=';
        const array = document.cookie.split(';');
        for (var i in array) {
            var c = array[i];
            while (c.charAt(0) === ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(s) === 0) {
                return decodeURIComponent(c.substring(s.length, c.length));
            }
        }
        return undefined;
    }

    function addCustomCookie(name, value) {
        var date = new Date();
        var domain = '.' + window.location.host;

        date.setDate(date.getDate() + 1);

        document.cookie = name + "=" + value + ";expires=" + date
            + ";domain=" + domain + ";path=/";
    }

    function removeCustomCookie(name) {
        var date = new Date();
        var domain = '.' + window.location.host;

        date.setDate(date.getDate() - 1);

        document.cookie = name + "=1;expires=" + date
            + ";domain=" + domain + ";path=/";
    }

    var cookie = readCookie('user_data');
    if (cookie) {
        try {
            var userData = JSON.parse(cookie);
            if (userData) {
                paidModel.userHash = userData.userHash;
                paidModel.isLoggedIn = true;
                paidModel.isPaying = userData.isPaying;
                paidModel.terms = userData.termIds;
                paidModel.resources = userData.resourceIds;
            }
        } catch(e) { }
    }

    cookieNoAds = readCookie('prm_no_ads');
    adsNoBanner = false;
    reducedAdsNet = hasReducedAdsNet(paidModel.resources);
    onetAds = {
    "async": 1,
    "sgn_cookie": 0,
    "whitelist": 0,
    "keywords": [
        "new_ucs",
        "screening_general"
    ],
    "target": "RASP_FORBES/GLOWNA",
    "DV": "RASP_FORBES/GLOWNA",
    "kropka": {},
    "lazyPercentageDesktop": "100",
    "lazyPercentageMobile": "150",
    "keyvalues": {
        "ci": "node-forbes",
        "structure": "glowna"
    },
    "cmd": [],
    "adSlots": [],
    "mobile": 0,
    "autoslot": 1,
    "no_gemius": 1,
    "cookieInfo": 1,
    "eventsapi": true,
    "adsNoBanner": false,
    "lazy": 1,
    "lazyPercentage": "100"
};
    if (shouldHideAds()) {
        onetAds.DV = onetAds.DV.replace(/(RASP_(M)?NEWSWEEK)/, '$1' + '_PAID');
        onetAds.target = onetAds.target.replace(/(RASP_(M)?NEWSWEEK)/, '$1' + '_PAID');
        onetAds.keywords.push("no_screening");
    }

    if (typeof identifierEI !== "undefined") {
        onetAds.kropka['EI'] = identifierEI;
    }

    function hasReducedAdsNet(resources) {
        return hasReducedAdsNetResource(resources) && pageWithReducedAdsNet();
    }

    function hasReducedAdsNetResource(resources) {
        var requiredResources = "".split(',');
        return resources && resources.filter(resource => requiredResources.includes(resource)).length > 0; 
    }

    function pageWithReducedAdsNet() {
        return !!false;
    }

    function shouldHideAds() {
        return (cookieNoAds && cookieNoAds === '1') || fromMobileApp;
    }

    onetAds.keyvalues = onetAds.keyvalues || {};
    onetAds.keyvalues.isLoggedIn = paidModel.isLoggedIn;
    onetAds.keyvalues.isPaying = paidModel.isPaying;
    onetAds.keyvalues.terms = paidModel.terms;
    onetAds.keyvalues.resources = paidModel.resources;
    onetAds.keyvalues.isArticleClosed = false;
    onetAds.keyvalues.hasReducedAdsNet = shouldHideAds();
</script>

