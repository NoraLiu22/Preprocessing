ase-in-out lg:mx-0"><div><div class="duet--media--content-warning relative" style="padding-top:51.93181818181818%"><span style="box-sizing:border-box;display:block;overflow:hidden;width:initial;height:initial;background:none;opacity:1;border:0;margin:0;padding:0;position:absolute;top:0;left:0;bottom:0;right:0"><img alt="" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" decoding="async" data-nimg="fill" style="position:absolute;top:0;left:0;bottom:0;right:0;box-sizing:border-box;padding:0;border:none;margin:auto;display:block;width:0;height:0;min-width:100%;max-width:100%;min-height:100%;max-height:100%;object-fit:cover"/><noscript><img alt="" loading="lazy" decoding="async" data-nimg="fill" style="position:absolute;top:0;left:0;bottom:0;right:0;box-sizing:border-box;padding:0;border:none;margin:auto;display:block;width:0;height:0;min-width:100%;max-width:100%;min-height:100%;max-height:100%;object-fit:cover" sizes="(max-width: 1023px) 100vw, 744px" srcSet="https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/376x195/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 376w, https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/384x199/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 384w, https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/415x216/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 415w, https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/480x249/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 480w, https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/540x280/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 540w, https://duet-cdn.vox-cdn.com/thumbor/0x0:880x457/640x332/filters:focal(440x229:441x230):format(webp)/cdn.vox-cdn.com/uploads/chorus_asset/file/24729613/2.jpg 640w, https://