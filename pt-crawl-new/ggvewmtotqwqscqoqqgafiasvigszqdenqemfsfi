    <script src="/js/global/micromodal.js"></script>
<script>
    $(document).ready(function(){
        MetaCM.RatingWidget.init();
        MicroModal.init();

        // faux checkbox handling
        $('.review_wrapper .flat_checkbox').on("click", function(){
            $(this).toggleClass('checked');
        });

        // track label text display
        var lbl = $("label[for='review_body']");
        var char_count_obj = $('.char_count');

        $('#review_body').on('keyup', function(){
            var val = $.trim($('#review_body').val());
            if ( val.length == 0 ) {
                lbl.show();
                if (document.getElementById('viewers_review_id') == null){
                    $('#user_rating_submit').show();
                }
            } else {
                lbl.hide();
                $('#user_rating_submit').hide();
            }

            // track character counts
            var obj = $(this);
            var _val = obj.val();
            // jquery will return newlines as a single character.  Some browsers will have two characters in the actual textarea.
            // make sure everything is the same.
            _val = _val.replace(/\r(?!\n)|\n(?!\r)/g, '\r\n');
            var newCount = _val.length;
            char_count_obj.text(newCount);
            if ( newCount < 75 || newCount > 5000 ) {
                char_count_obj.addClass('error');
                $('.user_review_form .review_submit_btns').css("background", "#AEAEAE");
            }
            else {
                char_count_obj.removeClass('error');
                $('.user_review_form .review_submit_btns').css("background", "rgba(67,67,67,1)");
            }
        });
        $('#review_body').keyup();
    })
</script>


                
