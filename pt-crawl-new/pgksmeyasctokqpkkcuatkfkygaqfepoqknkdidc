            <!-- NOTE: removed a div and applied the classes to the ul inside it -->
            <ul class="taxonomy-child-list taxonomy-lvl-0">
                        <li class="taxonomy-child-list-item">
            <div class="taxonomy-label-wrap taxonomy-child-label  taxonomy-parent-label " data-id="GroupId/111" data-parentid="GroupType/30">
                
                <div class="taxonomy-parent-arrow-wrap">
                    
                        <i class="taxonomy-parent-arrow icon-general-arrow-filled-right"></i>
