function scrollPageDown(delay) {
  // Delay the scroll operation by 1 second (1000 milliseconds)
  setTimeout(function() {
    // Scroll down by 1 pixel
    window.scrollBy(0, 1);
  }, delay);
}

// Call the function to trigger the scroll
scrollPageDown(500);
scrollPageDown(1000);
        
