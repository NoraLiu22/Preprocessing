            window.csrftoken = "ISei0sHOH3P2fUt7UoRCOL9sW1xcaTotuamXT6PhnULFHVFEDp5al6O2BxC77ADz";
        </script>
        


        
        
        
        <script>
            // Load all scripts non-async
            footerScripts.forEach(function(src) {
                var scriptEl = document.createElement('script');
                scriptEl.src = src;
                scriptEl.async = false;
                document.head.appendChild(scriptEl);
            });
        </script>
    


    <script>
        
        window.lazySizesConfig.preloadAfterLoad = false;
    </script>

    <script>
        
        const d = new Date();
        const m = d.getMonth();

        d.setMonth(d.getMonth() - 10);
        if (d.getMonth() == m) d.setDate(0);
        d.setHours(0,0,0);
        const lastMonth = (d/1000|0);

        document.querySelectorAll(".action-card--message").forEach(function(element) {
            const createdOn = element.dataset.created;

            if (createdOn >= lastMonth) {
                element.classList.remove('hidden');
            }
        });
    </script>

    <script type="module">
        import { bindListFiltering } from '/media/assets/public/js/modules/list_helpers.js?v=20220805';

        (function defer() {
            if ('List' in window) {
                const listOptions = {
                    valueNames: ['collection-name', 'collection-date'],
                    page: 1000
                };

                const collectionsList = new List('explore-collections-content', listOptions);

                

                bindListFiltering(collectionsList, {
                    noResultsSelector: '#no-results',
                    paginatorSelector: '#titlesPag', 
                    matchingCountSelector: '.matching-count--collections',
                    listCountSelector: '.explore__list-count',
                    listSearchSelector: '#list-search',
                    addAlternatingClasses: true,
                });
            } else {
                setTimeout(function() { defer(); }, 50);
            }
        }());
    </script>



