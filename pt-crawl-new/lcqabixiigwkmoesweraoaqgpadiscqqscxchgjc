        // Priority scripts loaded in head
        const priorityScripts = [
            "https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js",
            "/media/assets/public/js/priority.js?v=20220805",
        ];

        // Footer scripts loaded later
