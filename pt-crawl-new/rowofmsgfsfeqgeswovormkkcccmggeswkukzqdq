h dcr-13rbi4"></div></div></div></aside><section class="dcr-17td87q"><div class="dcr-1p6z1rt"><gu-island name="Carousel" priority="feature" deferUntil="visible" props="{&quot;heading&quot;:&quot;More on this story&quot;,&quot;trails&quot;:[{&quot;url&quot;:&quot;https://www.theguardian.com/lifeandstyle/wordofmouth/2013/jun/04/smell-flavour-palate-nose&quot;,&quot;linkText&quot;:&quot;Wake up and smell the flavour&quot;,&quot;showByline&quot;:false,&quot;byline&quot;:&quot;Amy Fleming&quot;,&quot;image&quot;:{&quot;src&quot;:&quot;https://i.guim.co.uk/img/static/sys-images/Guardian/Pix/pictures/2013/6/3/1370269631328/Woman-drinking-coffee-010.jpg?width=300&amp;quality=85&amp;auto=format&amp;fit=max&amp;s=27575b78b315264c25ef341e072cd096&quot;,&quot;altText&quot;:&quot;&quot;},&quot;carouselImages&quot;:{&quot;300&quot;:&quot;https://i.guim.co.uk/img/static/sys-images/Guardian/Pix/pictures/2013/6/3/1370269631328/Woman-drinking-coffee-010.jpg?width=300&amp;quality=85&amp;auto=format&amp;fit=max&amp;s=27575b78b315264c25ef341e072cd096&quot;,&quot;460&quot;:&quot;https://i.guim.co.uk/img/static/sys-images/Guardian/Pix/pictures/2013/6/3/1370269631328/Woman-drinking-coffee-010.jpg?width=460&amp;quality=85&amp;auto=format&amp;fit=max&amp;s=38713dd69da2725df65634464c20683f&quot;},&quot;isLiveBlog&quot;:false,&quot;pillar&quot;:&quot;lifestyle&quot;,&quot;designType&quot;:&quot;Feature&quot;,&quot;format&quot;:{&quot;display&quot;:0,&quot;theme&quot;:4,&quot;design&quot;:10},&quot;webPublicationDate&quot;:&quot;2013-06-04T15:49:00.000Z&quot;,&quot;headline&quot;:&quot;Wake up and smell the flavour&quot;,&quot;shortUrl&quot;:&quot;https://www.theguardian.com/p/3ga64&quot;,&quot;discussion&quot;:{&quot;isCommentable&quot;:true,&quot;isClosedForComments&quot;:true,&quot;discussionId&quot;:&quot;/p/3ga64&quot;},&quot;dataLinkName&quot;:&quot;feature | group-0 | card-@1&quot;},{&quot;url&quot;:&quot;https://www.theguardian.com/lifeandstyle/2013/may/29/chocolate-bags-filled-with-cal