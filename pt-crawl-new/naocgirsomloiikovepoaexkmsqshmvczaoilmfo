    xmlHttp.open('GET', '7digital_addbasket.asp?release='+release+'&trackid='+trackid, true);
    xmlHttp.onreadystatechange = function () {
        if (xmlHttp.readyState == 4) {
						resultText = xmlHttp.responseText;
