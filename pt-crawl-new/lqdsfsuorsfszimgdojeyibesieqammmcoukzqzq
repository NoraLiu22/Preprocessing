</div></span><span class='HeaderPaneDiv6'><div id="_ctrl0_ctl18_divModuleContainer" class="module module-embed">
    <div class="module_container module_container--outer">
        
        <div class="module_container module_container--inner">
            <button id="navMobileSearchBtnCustom" class="nav--mobile--search_container--btn" form="navMobileSearchForm" type="button" aria-label="Click to open search" aria-expanded="true" aria-haspopup="true" aria-controls="navMobileSearchInput" aria-hidden="false">
<span class="button_text q4-icon_search" aria-hidden="true"></span>
</button>

<script>
function initializeMobileSearch() {
  const $body = $('body');
  const $searchInputContainer = $('.nav--mobile--search_container').detach();
  const $navMobileMenu = $('#nav--mobile-menu');
  const $navMobileSearchBtnCustom = $('#navMobileSearchBtnCustom');
  const $navDesktopContainer = $('.nav--desktop--container');
  const $navMobileToggle = $navDesktopContainer.find('.nav--mobile--toggle');
  const $layout = $('.layout');

  $searchInputContainer.insertBefore($navMobileMenu);

  let triggeredByObserver = false;

  $navMobileSearchBtnCustom.click(function() {
    if (!triggeredByObserver && $navMobileToggle.attr('aria-expanded') === 'true') {
      $navMobileToggle.click();
    }
    $searchInputContainer.fadeToggle();
    $(this).toggleClass('active');
    $layout.toggleClass('searchActive');

    // Reset the flag
    triggeredByObserver = false;
  });

  const observer = new MutationObserver(function(mutations) {
    mutations.forEach(function(mutation) {
      if ($body.hasClass('nav--mobile-active') && $layout.hasClass('searchActive')) {
        triggeredByObserver = true;
        $navMobileSearchBtnCustom.click();
      }
    });
  });

  observer.observe($body[0], {attributes: true});
}

// Call the function to initialize mobile search
initializeMobileSearch();


</script>
        </div>
    </div>
