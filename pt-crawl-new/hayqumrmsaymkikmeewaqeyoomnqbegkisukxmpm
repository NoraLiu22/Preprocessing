<script>
    var $menuD = $("#submenu-list");
    $menuOF = $("#menu");
    $menuB = $("#menu-btn");
    $userB = $("#user-btn");
    $cartB = $("#cart-btn");
    $searchB = $("#search-btn");
    $window = $(window);

    $menuDisplay = function() {
        setTimeout(function() {
            $menuD.toggleClass('transform-submenu')
        }, 150);
        $('html, body').toggleClass('stop-scroll');
        if ($menuB.html() === 'X') {
            setTimeout(function() {
                $menuB.html('MENU');
                $menuOF.removeClass('overlay');
            }, 300);
        } else {
            $menuB.html('X');
            $menuOF.addClass('transform').addClass('overlay');
        }
        $userB.toggleClass('transform-btn');
        $cartB.toggleClass('transform-btn');
        $searchB.toggleClass('transform-btn');
    }

    $scrollDisplay = function() {
        var y = window.pageYOffset;
        var t = $menuB.html();
        if (y >= 150 && t !== 'X') {　
            $menuOF.toggleClass('show');
            $menuB.addClass('transform-btn')
        } else if (t == 'X') {
            $menuB.addClass('transform-btn transform-active')
        } else {
            $menuB.removeClass('transform-btn transform-active')
        }
    }

    window.addEventListener("scroll", $scrollDisplay);
    
    $menuBActive=function() {
        var t = $menuB.text();
        y = $window.scrollTop();
        $menuB.toggleClass('transform-active');
        if (y <= 150 && t == 'X') {
            $menuB.toggleClass('transform-btn');
        }
    }
   
    $menuOF.click(function(){
        $menuDisplay()
    }).click(
            function(){$menuBActive()});
     
    $menuB.click(function(e) {
        e.stopPropagation(
            $menuDisplay()
        )
    }).click(function(){$menuBActive()}); 


    $(function() {
        $('.submenu > h3').click(function(e) {
            e.stopPropagation(
            $(this).next('.submenu-content').toggleClass('show')
        )});
    });
   
   $('.btns').click(function(e) {
            e.stopPropagation()
      });
   $('.btns, .submenu, .submenu-list').click(function(e) {
            e.stopPropagation()
      });

</script>

