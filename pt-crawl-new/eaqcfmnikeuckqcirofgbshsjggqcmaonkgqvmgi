<div id="menuComments" class="l-panel l-panel--comments l-longform-panel-comments l-panel--noSidebar">
	<div class="l-panel__inner">
		<h2 class="l-panel__header c-heading c-heading--primary">
			<span>Comments</span>
			<button data-close-menu data-trackable="true" data-trackaction="Comments | open false" class="l-panel__button">
				<span class="sr-only">Close comments menu</span>
					<svg class="c-icon  l-panel__icon" focusable="false">
		<use xlink:href="https://globalnews.ca/wp-content/themes/shaw-globalnews/assets/dist/icons/out/symbol/svg/sprite.symbol.svg?v=beigjoens#close"></use>
	</svg>
				</button>
		</h2>
		<div class="o-container">
				<div id="fb-root"></div>
	<script defer src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&appId=318812448281278&version=v3.3"></script>

	<script>
		window.fbAsyncInit = function() {
			var currentCommentID = 0;
			// Track when user posts a comment
			FB.Event.subscribe( 'comment.create', function(response) {
				if ( currentCommentID !== response.commentID ) {
					currentCommentID = response.commentID;
					if ( typeof( gn_analytics ) !== 'undefined' && typeof( gn_analytics.Analytics ) !== 'undefined' ) {
						gn_analytics.Analytics.track( 'adobe', { action: 'facebook comment' } );
					}
				}
			});
		};

		// When comments widget is inserted, remove loading placeholder
		document.addEventListener( 'DOMContentLoaded', function() {
						var embed = document.querySelector( '.c-fbComments__embed' );
						var placeholder = document.querySelector( '.c-fbComments__placeholder' );
						var observer = new MutationObserver( function( mutationList, observer ) {
							if ( embed.getAttribute( 'fb-xfbml-state' ) === 'parsed'
							|| embed.getAttribute( 'fb-xfbml-state' ) === 'rendered' ) {
								observer.disconnect();
								placeholder.classList.add( 'is-hidden' );
							}
						});
						observer.observe( embed, { attributes: true, childList: false, subtree: false } );
		});
	</script>

