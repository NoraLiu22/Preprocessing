<script>
    $('#site-search-1075948-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1075948').submit();
        return false;
    });

    $('#site-search-1075948').on('submit', function(){
        // Filter query
        var elem = document.querySelector("#site-search-1075948 input[name=q]"),
        sQueryFiltered = elem.value.replace(/\?/g, '');
    elem.value = sQueryFiltered;

        if( $('#site-search-1075948-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
