				<script>
				if ('serviceWorker' in navigator) {
					window.addEventListener('load', () => {
						navigator.serviceWorker.register("/service-worker.js?v=n6j4loyb5").then(registration => {
							console.log('Service Worker registered! Scope: ' + registration.scope);
						}).catch(err => {
							console.log('Service Worker registration failed: ' + err);
						});
					});
				}
			</script>
		
				<script>
				document.addEventListener("DOMContentLoaded", function() {
					var installPromptEvent;

				window.addEventListener('beforeinstallprompt', (e) => {
						e.preventDefault();
						installPromptEvent = e;
						
						var currentSetting = Cookies.get('a2hs-dismissed');
						if ( !currentSetting || 'dismissed' !== currentSetting ) {
						document.getElementById("a2hs-popover").classList.add("show");
						}
					});

					document.getElementById("a2hs-button").addEventListener('click', function() {
						document.getElementById("a2hs-popover").classList.remove("show");
						
						// install prompt needs to be triggered by a user gesture...thus the two step process
						installPromptEvent.prompt();
						installPromptEvent.userChoice.then((choiceResult) => {
							if (choiceResult.outcome === 'accepted') {
								console.log('User accepted the A2HS prompt');
							} else {
								console.log('User dismissed the A2HS prompt');
							}
							installPromptEvent = null;
						});

						Cookies.remove('a2hs-dismissed');
					});

					document.getElementById("a2hs-close").addEventListener('click', function() {
						document.getElementById("a2hs-popover").classList.remove("show");

						// store result and don't show prompt for 1 week or 3 months if this is a repeat dismissal
						var currentSetting = Cookies.get('a2hs-dismissed');
						var duration = (currentSetting && 'dismissed' === currentSetting) ? 90 : 7;
						Cookies.set('a2hs-dismissed', 'dismissed', { expires: duration });
						});
					});
			</script>
		</head>

