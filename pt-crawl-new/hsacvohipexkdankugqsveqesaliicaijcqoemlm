        <script>
            var gusto = window.gusto || {};
            gusto.MAIN_CONTENT_TYPE = "";
            gusto.ROOT_URL = "https://www.bigtop40.com";
            gusto.SITE_NAME = "Big Top 40";
            gusto.STATIC_URL = "/assets_v4r/";
            gusto.STATIC_BRAND_URL = "/assets_v4r/bigtop40/";
            gusto.CATEGORY_PATH = "/";
            gusto.PROJECT_NAME = "bigtop40";
            gusto.PLAYER_INFO = {"localise_url": "/localise/", "play_overlay": false, "player_url_ukrp": "https://radioplayer.musicradio.com", "is_localised": true, "twin_stations": null, "player_url": null, "global_player_enabled": false, "localisation_light_enabled": false, "localisations_enabled": false, "player_url_template": ""};
            gusto.DEBUG = false;
            gusto.SETTINGS = {
                'COOKIE_STATION_MAX_AGE_SECONDS': 63072000
            };
            gusto.ad_settings = {
                'banner_visibility_delay': 3000,
            };
            gusto.ad_switch = {
                'enable_ads_dac': true,
                'enable_ads_related': false,
                'enable_ads_ukrp': false,
                'enable_ads_preroll': true,
                'enable_ads_related_videos': false,

            };
            gusto.STATION = {
                'name': "BigTop40",
                'slug': ""
            };
            gusto.breakpoints = {
                limits: {
                    'small': [0, 640],
                    'medium': [641, 1023],
                    'large': [1024, 1440],
                    'xlarge': [1441, 1920],
                    'xxlarge': [1921, 9001]
                },
                check: function(target) {
                    
                    var target_bits = target.split('-'),
                        brkpoint = target_bits[0],
                        dir = target_bits[1],
                        win = window.innerWidth,
                        ref = this.limits[brkpoint];
                    if (!ref) return false;
                    if (dir == 'up') return (win >= ref[0]);
                    else if (dir == 'down') return (win <= ref[1]);
                    else return (win >= ref[0] && win <= ref[1]);
                }
            };

            gusto.ABSOLUTE_URL = "/";
            gusto.station_twitter = 'bigtop40';
            gusto.FACEBOOK_APP_ID = '605966399498613';
            gusto.category_tree = {"288": {"full_slug": "/", "name": "Music", "parent": null, "is_page": true}, "1": {"full_slug": "/", "is_page": true, "name": "bigtop40.com", "parent": null, "children": [405, 79, 72, 73, 418, 419]}, "418": {"full_slug": "/listen-capital/", "name": "Listen to Capital", "parent": 1}, "419": {"full_slug": "/listen-heart/", "name": "Listen to Heart", "parent": 1}, "72": {"full_slug": "/features/", "name": "Features", "parent": 1}, "73": {"full_slug": "/win/", "name": "Competitions", "parent": 1}, "79": {"full_slug": "/news/", "name": "News", "parent": 1}, "309": {"full_slug": "/", "name": "J", "parent": null, "is_page": true}, "405": {"full_slug": "/charts/", "name": "Charts", "parent": 1}};
