}
if (data.foursquareURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.foursquareURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#foursquare"></use></svg>Foursquare</a></li>';
hasSocialMedia = 1;
}
if (data.pinterestURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.pinterestURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#pinterest"></use></svg>Pinterest</a></li>';
hasSocialMedia = 1;
}
outputSocialMedia += '</ul></div></div>';
if(hasSocialMedia == 1) {
jQuery("#sml_position").replaceWith(outputSocialMedia);
}
});
});
</script>
