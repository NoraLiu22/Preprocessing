<script language="javascript" type="text/javascript">

    $(document).ready(function () {

        var prm = Sys.WebForms.PageRequestManager.getInstance();

        prm.add_endRequest(EndRequest);

        function EndRequest(sender, args) {
            $('div').jqTransform();

        }
    });
    $('input[type="text"]').live('keydown', function (e) { // checking if enter key press hit search button event 
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) {

            var objExtender = $find("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_CollapsiblePanelExtender1"); // getting id for advanced panel
            var lblshow = document.getElementById("lblZipJavaScript"); // zip code id for validation

            if (objExtender.get_Collapsed()) { // choose search click event base on panel extender 
                var clickButton1 = document.getElementById("btnQSearch");
                clickButton1.click();
            }
            else {
                //first check zip code validation then search 
                var zip = document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_txtZipCode").value + document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_txtPlus4").value;
                var isValid = /^\d{5}$|^\d{5}\d{4}$/.test(zip);
                if (isValid) {
                    lblshow.style.display = 'none';
                    var clickButton2 = document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_btnASearch");
                    clickButton2.click();
                }
                else {
                    lblshow.style.display = 'block';
                }
            }
            return false;
        }
    });
      
</script>
<script type="text/javascript" language="javascript">
    /// hide unhide search button for quick search
    var objExtender;
    var trshow;
    var thide;
    function pageLoad(sender, args) {
        objExtender = $find("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_CollapsiblePanelExtender1");
        objExtender.add_expandComplete(getCollapsibleState);
        objExtender.add_collapseComplete(getCollapsibleState);
    }
    function getCollapsibleState() {
        if (objExtender.get_Collapsed()) {
            trshow = document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_trQuickSearchbuttons");
            trshow.style.display = 'block';
            document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_hdtext").value = "collapse";
        }
        else {
            trhide = document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_trQuickSearchbuttons");
            trhide.style.display = 'none';
            document.getElementById("MainContent_tcSearch_tpQuickSearch_ucQuickSearch_hdtext").value = "expand";
        }
    }

    function DisplayAffirmationFields() {
        //   var selectedvalue = $("#" + cbl.id + " input:checked").val();
        var searchtype = document.getElementById("rblSearchType").id;
        var ifaffonly = $("#" + searchtype + " input:radio:checked").val();
        //var selectedItems = "";
        //        $("[id*=" + cbl.id + "] input:checked").each(function () {

        //            selectedItems += $(this).val() + "\r\n";
        //        });

        if (ifaffonly == "Affirmations") {

            var chkAffWithKnwldg = document.getElementById("chkAffWithKnwldg");
            // alert(chkAffWithKnwldg);
            if (chkAffWithKnwldg.checked)
                $("#trAffirmationsWithKnwldg").show();
            else
                $("#trAffirmationsWithKnwldg").hide();

            var chkAffRefusal = document.getElementById("chkAffRefusal");
            if (chkAffRefusal.checked)
                $("#trAffirmationsRefusal").show();
            else
                $("#trAffirmationsRefusal").hide();
        }

        else {
            $("#trAffirmationMoreOptions").hide();
            $("#trAffirmationsWithKnwldg").hide();
            $("#trAffirmationsRefusal").hide();
        }

        return false;
    }

    function DisplayPACText() {

        var searchtype = document.getElementById("rblSearchType").id;
        var ifregonly = $("#" + searchtype + " input:radio:checked").val();
        //        var checkBoxList = cbl.getElementsByTagName("input");
        var show = 'false';
        var chkRegPAC = document.getElementById("chkRegPAC");
        var chkRegCPC = document.getElementById("chkRegCPC");

        if (ifregonly == "Registration") {

            if (chkRegPAC.checked || chkRegCPC.checked)
                show = 'true';
            else
                show = 'false';

            if (show == 'true') {
                $("#trRegistrationPACOptions").show();
                $("#trPACTextFields").show();
            }
            if (show == 'false') {
                $("#trRegistrationPACOptions").show();
                $("#trPACTextFields").hide();
            }
        }
        else {
            $("#trRegistrationPACOptions").hide();
            $("#trPACTextFields").hide();
        }

        return false;
    }
</script>
