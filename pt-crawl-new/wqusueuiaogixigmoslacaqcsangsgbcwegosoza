        function DisplayPanel(rbl, clr) {
            var selectedvalue = $("#" + rbl.id + " input:radio:checked").val();
            var searchtype = document.getElementById("rblSearchType").id;
            var ifregonly = $("#" + searchtype + " input:radio:checked").val();

            if (clr == 1) { clearall() } //clearing first textboxes on button change 

            if (ifregonly == "Registration") {
                if (selectedvalue == "P") {
                    $("#trPrinciple").show();
                    $("#trLobbyingFirm").hide();
                    $("#trLobbyist").hide();
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'block';
                    $("#trFirmSpcfCrit").hide();
                    $("#trLobbyistSpcfCrit").hide();
                }
                else if (selectedvalue == "F") {
                    $("#trPrinciple").hide();
                    $("#trLobbyingFirm").show();
                    $("#trLobbyist").hide();
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                    $("#trFirmSpcfCrit").show();
                    $("#trLobbyistSpcfCrit").hide();
                }
                else if (selectedvalue == "L") {
                    $("#trPrinciple").hide();
                    $("#trLobbyingFirm").hide();
                    $("#trLobbyist").show();
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                    $("#trFirmSpcfCrit").hide();
                    $("#trLobbyistSpcfCrit").show();
                }
                else if (selectedvalue == "All") {
                    $("#trPrinciple").show();
                    $("#trLobbyingFirm").show();
                    $("#trLobbyist").show();
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'block';
                    $("#trFirmSpcfCrit").show();
                    $("#trLobbyistSpcfCrit").show();
                }
            }
            else {

                document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                $("#trFirmSpcfCrit").hide();
                $("#trLobbyistSpcfCrit").hide();

                if (selectedvalue == "P") {
                    $("#trPrinciple").show();
                    $("#trLobbyingFirm").hide();
                    $("#trLobbyist").hide();
                }
                else if (selectedvalue == "F") {
                    $("#trPrinciple").hide();
                    $("#trLobbyingFirm").show();
                    $("#trLobbyist").hide();
                }
                else if (selectedvalue == "L") {
                    $("#trPrinciple").hide();
                    $("#trLobbyingFirm").hide();
                    $("#trLobbyist").show();
                }
                else if (selectedvalue == "All") {
                    $("#trPrinciple").show();
                    $("#trLobbyingFirm").show();
                    $("#trLobbyist").show();
                }
            }


            return false;
        }

        function DisplayMoreOptions(rbl, clr) {
            var selectedvalue = $("#" + rbl.id + " input:radio:checked").val();
            var regtype = document.getElementById("rblRegistrationType").id;
            var selectedtype = $("#" + regtype + " input:radio:checked").val();

            if (clr == 1) { clearall() } //clearing first textboxes on button change 

            if (selectedvalue == "Registration") {

                $("#trRegistrationMoreOptions1").show();
                $("#trRegistrationMoreOptions2").show();
                $("#trExpesesMoreOptions").hide();
                $("#trAffirmationMoreOptions").hide();
                $("#trAffirmationsWithKnwldg").hide();
                $("#trAffirmationsRefusal").hide();
                $("#trExpenseYear").hide();
                $("#trRegPeriod").show();
                $("#lblLobbyingDates").show();
                $("#txtLobbyingDates").show();
                $("#trAddress").show();
                $("#lblAddress").show();
                $("#trExpLobbyingSubject").hide();
                $("#trRegistrationPACOptions").show();
                $("#trPACTextFields").hide();
                // DisplayPACText(cblRegistrationPACOptions)
                DisplayPACText();

                if (selectedtype == "P") {
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'block';
                    $("#trFirmSpcfCrit").hide();
                    $("#trLobbyistSpcfCrit").hide();
                }
                else if (selectedtype == "F") {
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                    $("#trFirmSpcfCrit").show();
                    $("#trLobbyistSpcfCrit").hide();
                }
                else if (selectedtype == "L") {
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                    $("#trFirmSpcfCrit").hide();
                    $("#trLobbyistSpcfCrit").show();
                }
                else if (selectedtype == "All") {
                    document.getElementById("trPrincipalSpcfCrit").style.display = 'block';
                    $("#trFirmSpcfCrit").show();
                    $("#trLobbyistSpcfCrit").show();
                }



            }
            else if (selectedvalue == "Expenses") {
                $("#trRegistrationMoreOptions1").hide();
                $("#trRegistrationMoreOptions2").hide();
                $("#trExpesesMoreOptions").show();
                $("#trAffirmationMoreOptions").hide();
                $("#trAffirmationsWithKnwldg").hide();
                $("#trAffirmationsRefusal").hide();
                $("#trExpenseYear").show();
                $("#trRegPeriod").hide();
                $("#lblLobbyingDates").hide();
                $("#txtLobbyingDates").hide();
                document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                $("#trFirmSpcfCrit").hide();
                $("#trLobbyistSpcfCrit").hide();
                $("#trAddress").hide();
                $("#lblAddress").hide();
                $("#trExpLobbyingSubject").show();
                $("#trRegistrationPACOptions").hide();
                $("#trPACTextFields").hide();
            }
            else if (selectedvalue == "Affirmations") {
                $("#trRegistrationMoreOptions1").hide();
                $("#trRegistrationMoreOptions2").hide();
                $("#trExpesesMoreOptions").hide();
                $("#trAffirmationMoreOptions").show();
                $("#trAffirmationsWithKnwldg").hide();
                $("#trAffirmationsRefusal").hide();
                $("#trExpenseYear").show();
                $("#trRegPeriod").hide();
                $("#lblLobbyingDates").hide();
                $("#txtLobbyingDates").hide();
                document.getElementById("trPrincipalSpcfCrit").style.display = 'none';
                $("#trFirmSpcfCrit").hide();
                $("#trLobbyistSpcfCrit").hide();
                $("#trAddress").hide();
                $("#lblAddress").hide();
                $("#trExpLobbyingSubject").hide();
                $("#trRegistrationPACOptions").hide();
                $("#trPACTextFields").hide();
                // DisplayAffirmationFields(cblAffirmationsMoreOptions)
                DisplayAffirmationFields();

            }
            return false;
        }


        //clearing first textboxes on button change 
        function clearall() {
            document.getElementById('txtRegNumber').value = "";
            document.getElementById('txtPrincipalName').value = "";
            document.getElementById('txtFirmName').value = "";
            document.getElementById('txtLFirstName').value = "";
            document.getElementById('txtLLastName').value = "";
            return false;
        }
    </script>
