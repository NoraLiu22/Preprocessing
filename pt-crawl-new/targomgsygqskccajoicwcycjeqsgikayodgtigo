<script type="text/javascript" src="https://code.jquery.com/jquery-1.11.0.min.js"></script>
<script type="text/javascript" src="https://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/cloud9carousel@2.2.0/jquery.cloud9carousel.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>

<script>
  document.querySelector('#PHistory').classList.remove('render-lightbox');

	var $jQuery = jQuery.noConflict();
	$jQuery(document).ready(function() {
		$jQuery('.link-see-recipes').on('click', function(evt) {
      evt.preventDefault();
      var target = 	$jQuery(this).attr('href');
      var headerHeight = 	$jQuery('#header').outerHeight();
			var wrapperOffset = 0;

			if($jQuery('body .wrapper')) {
				wrapperOffset = $jQuery('body .wrapper').offset().top;
			}

			$jQuery('html, body').animate(
				{
					scrollTop: 	$jQuery(target).offset().top - headerHeight - wrapperOffset
				},
				1000
			);
    });

		$jQuery(document).on('click', '.recipes__navToggler', function() {
      let navContainer = 	$jQuery(this).closest('.recipes__navContainer');
      navContainer.toggleClass('active');
    });

		var bottleSlider = false;

    initBottleSlider();

		if ($jQuery(window).outerWidth() <= 768) {
			$jQuery('.hero__slider--products').slick({
				fade: true,
				dots: false,
				arrows: false,
				asNavFor: '#hero__bottles__slides'
			});
		} else {
			$jQuery('.hero__slider--products').slick({
				fade: true,
				dots: false,
				arrows: false
			});
		}

    function initBottleSlider() {
      $('#hero_bottles_slidesWrapper').html($('#hero__bottles__slides_template').html());
      $('#hero_bottles_slidesWrapper').find('.bottleSlides').attr('id', 'hero__bottles__slides');

      if ($jQuery(window).outerWidth() <= 768) {
        $jQuery('#hero__bottles__slides').slick({
          dots: false,
          arrows: true,
          centerMode: true,
          centerPadding: '100px',
          prevArrow: 	$jQuery(".hero__slider--nav .prev"),
          nextArrow: 	$jQuery(".hero__slider--nav .next"),
          asNavFor: '.hero__slider--products'
        });
      } else {
        $jQuery('#hero__bottles__slides').Cloud9Carousel({
					yOrigin: 42,
					yRadius: 40,
					buttonLeft: 	$jQuery(".hero__slider--nav .prev"),
					buttonRight: 	$jQuery(".hero__slider--nav .next"),
					itemClass: 'bottleSlide',
					autoplay: 1,
					bringToFront: true,
					frontItemClass: 'active',
					onLoaded: function() {
						$jQuery('#hero__bottles__slides').css('visibility', 'visible');
						$jQuery('#hero__bottles__slides').css('display', 'none');
						$jQuery('#hero__bottles__slides').fadeIn(1500);
					},
					onRendered: function(carousel) {
						var currentIndex = carousel.nearestIndex();
						$jQuery('.hero__slider--products').slick('slickGoTo', currentIndex);
					}
				});
      }
    }

    $(window).on('resize', function() {
      initBottleSlider();
    })
	});
</script>
