<script>
jQuery(function($) {
	var HasTooltip = $('[data-toggle="tooltip"]');
	  HasTooltip.on('click', function(e) {
	    e.preventDefault();
	    var isShowing = $(this).data('isShowing');
	    HasTooltip.removeData('isShowing');
	    if (isShowing !== 'true')
	    {
	      HasTooltip.not(this).tooltip('hide');
	      $(this).data('isShowing', "true");
	      $(this).tooltip('show');
	    }
	    else
	    {
	      $(this).tooltip('hide');
	    }
	
	  }).tooltip({
    	animated: 'fade',
    	placement: 'bottom',
    	html: true,
	    trigger: 'manual'
	  });
});   
</script>
