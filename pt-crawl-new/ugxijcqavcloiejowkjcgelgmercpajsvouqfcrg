<div class="taxonomy-tree-wrap collapsible-facet">

        <header class="sf-group-header">
            <h4 class="title">GeoRef Subject</h4>
            <i class="sf-group-header-icon icon-general_arrow-right"></i>
        </header>
        <div class="taxonomy-node" data-id="GroupType/30">
            <!-- NODE PARENT -->
            
            <!-- NODE CHILDREN -->
            <!-- NOTE: removed a div and applied the classes to the ul inside it -->
            <ul class="taxonomy-child-list taxonomy-lvl-0">
                        <li class="taxonomy-child-list-item">
            <div class="taxonomy-label-wrap taxonomy-child-label  taxonomy-parent-label " data-id="GroupId/111" data-parentid="GroupType/30">
                
                <div class="taxonomy-parent-arrow-wrap">
                    
                        <i class="taxonomy-parent-arrow icon-general-arrow-filled-right"></i>
                </div>
                <div class="taxonomy-label">
                    all geography including DSDP/ODP Sites and Legs
                </div>
            </div>

            
                <ul class="taxonomy-child-list taxonomy-lvl-1">
                            <li class="taxonomy-child-list-item">
