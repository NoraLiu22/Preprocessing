<script>
  // form fields styling
  var sendBtn = document.getElementById("send-btn");
  var mailformat=/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;

  if (sendBtn) {
    sendBtn.onclick = function() {
      document.getElementById("first-name-required-comment").style.display = "none";
      document.getElementById("last-name-required-comment").style.display = "none";
      document.getElementById("email-required-comment").style.display = "none";
      //document.getElementById("phone-required-comment").style.display = "none";
      document.getElementById("company-name-required-comment").style.display = "none";

      document.getElementById("First-Name").style.border = "1px solid #D0D0D0";
      document.getElementById("Last-Name").style.border = "1px solid #D0D0D0";
      document.getElementById("Email").style.border = "1px solid #D0D0D0";
      document.getElementById("Company-Name").style.border = "1px solid #D0D0D0";
      document.getElementById("Phone").style.border = "1px solid #D0D0D0";

      // validate email address
      var emailAddress = document.getElementById("Email").value;
      if(!emailAddress)
      {
        document.getElementById("Email").focus();
        document.getElementById("invalid-email-comment").style.display = "none";
      }


      if(document.getElementById("First-Name").value == "") {
        document.getElementById("First-Name").style.border = "1px solid #B00020";
        document.getElementById("first-name-required-comment").style.display = "flex";
      }
      if(document.getElementById("Last-Name").value == "") {
        document.getElementById("Last-Name").style.border = "1px solid #B00020";
        document.getElementById("last-name-required-comment").style.display = "flex";
      }
      if(emailAddress == "")
      {
        document.getElementById("Email").style.border = "1px solid #B00020";
        document.getElementById("email-required-comment").style.display = "block";
      }
      else
      {
        if(emailAddress.match(mailformat))
        {
          document.getElementById("Email").focus();
          document.getElementById("invalid-email-comment-1").style.display = "none";
          return true;
        }
        else
        {
          document.getElementById("Email").focus();
          document.getElementById("invalid-email-comment-1").style.display = "block";
          return false;
        }
      }
      if(document.getElementById("Company-Name").value == "")
      {
        document.getElementById("Company-Name").style.border = "1px solid #B00020";
        document.getElementById("company-name-required-comment").style.display = "flex";
      }
    };
  };

</script>

<script>
  // when input is entered, remove "required" comment
  $("#First-Name").keydown(function () {
    document.getElementById("first-name-required-comment").style.display = "none";
    document.getElementById("First-Name").style.border = "1px solid #5e63e5";
  });
  $("#Last-Name").keydown(function () {
    document.getElementById("last-name-required-comment").style.display = "none";
    document.getElementById("Last-Name").style.border = "1px solid #5e63e5";
  });
  $("#Email").keydown(function () {
    document.getElementById("email-required-comment").style.display = "none";
    document.getElementById("Email").style.border = "1px solid #5e63e5";
  });
  $("#Company-Name").keydown(function () {
    document.getElementById("company-name-required-comment").style.display = "none";
    document.getElementById("Company-Name").style.border = "1px solid #5e63e5";  
  });

</script>


<script>
  //check if all input fields are filled and turn on the send button
  $("#First-Name").keyup(function () {
    if(document.getElementById("First-Name").value && document.getElementById("Last-Name").value && document.getElementById("Email").value && document.getElementById("Company-Name").value)
    {
      document.getElementById("send-btn").style.backgroundColor="#5e63e5";
    }
    else {
      document.getElementById("send-btn").style.backgroundColor="#d8d8d8";
    }
  });

  $("#Last-Name").keyup(function () {
    if(document.getElementById("First-Name").value && document.getElementById("Last-Name").value && document.getElementById("Email").value && document.getElementById("Company-Name").value)
    {
      document.getElementById("send-btn").style.backgroundColor="#5e63e5";
    }
    else {
      document.getElementById("send-btn").style.backgroundColor="#d8d8d8";
    }
  });

  $("#Email").keyup(function () {
    if(document.getElementById("Email").value.match(mailformat))
    {
      document.getElementById("invalid-email-comment-1").style.display = "none";
      if(document.getElementById("First-Name").value && document.getElementById("Last-Name").value && document.getElementById("Email").value && document.getElementById("Company-Name").value)
      {
        document.getElementById("send-btn").style.backgroundColor="#5e63e5";
      }
      else {
        document.getElementById("send-btn").style.backgroundColor="#d8d8d8";
      }
    }
    else
    {
      document.getElementById("Email").focus();
      document.getElementById("invalid-email-comment-1").style.display = "block";
    }
  });


  $("#Phone").keyup(function () {
    if(document.getElementById("First-Name").value && document.getElementById("Last-Name").value && document.getElementById("Email").value && document.getElementById("Company-Name").value)
    {
      document.getElementById("send-btn").style.backgroundColor="#5e63e5";
    }
    else {
      document.getElementById("send-btn").style.backgroundColor="#d8d8d8";
    }
  });

  $("#Company-Name").keyup(function ()
                           {
    if(document.getElementById("First-Name").value && document.getElementById("Last-Name").value && document.getElementById("Email").value && document.getElementById("Company-Name").value)
    {
      document.getElementById("send-btn").style.backgroundColor="#5e63e5";
    }
    else {
      document.getElementById("send-btn").style.backgroundColor="#d8d8d8";
    }
  });
</script>


<!-- go to a specific tab -->
<script>
  var Webflow = Webflow || [];
  Webflow.push(function () {
    var tabName = getParam('tab');
    if (!tabName) return;

    $('.' + tabName).triggerHandler('click');

    function getParam(name) {
      name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
      var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
          results = regex.exec(location.search);
      return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }
  });
</script>
<!-- end go to a specific tab --><!-- START Twitter sahre -->
<script type="text/javascript">
  function tweet() { 
    var u = location.href;
    var t="Tweet"; 
    window.open('http://twitter.com/share?url='+encodeURIComponent(u)+'&text=','twitsharer','toolbar=0,status=0,width=626,height=436'); 
    return false;    
  } 
</script>

<script>
  var Webflow = Webflow || [];
  Webflow.push(function () { 
    var next_href = $('#posts_list .w--current').parent().next().find('a').attr('href');
    var previous_href = $('#posts_list .w--current').parent().prev().find('a').attr('href');

    //if last post in list
    if(next_href == undefined) {
      next_href = $('#posts_list').children().children().first().find('a').attr('href');
      //$('#next_button').fadeOut(); //optional - remove if you want to loop to beginning
    }

    //if first post in list
    if(previous_href == undefined) {
      previous_href = $('#posts_list').children().children().last().find('a').attr('href');
      //$('#previous_button').fadeOut();  //optional - remove if you want to loop to end
    }

    //apply hrefs to next / previous buttons
    $('#next_button').attr('href', next_href);
    $('#previous_button').attr('href', previous_href);

    // Open text links in RTX on a new tab
    const txt_links = document.querySelectorAll('.post-body p a');
    txt_links.forEach((link)=>{
      link.setAttribute('target','_blank');
    })

  });
</script>
