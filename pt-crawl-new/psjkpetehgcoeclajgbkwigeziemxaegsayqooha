  const obsBody = document.body;
  const obsMenu = document.querySelector('.obs_menu');
  const obsOverlay = document.querySelector('.obs_overlay');
  const obsMenuBtn = document.querySelector('.obs_menu_btn');
  const obsMenuCloseBtn = document.querySelector('.obs_menu_close_btn');

  function openMenu() {
    obsMenu.classList.add('obs_menu--open');
    obsOverlay.classList.add('obs_overlay--active');
    obsBody.classList.add('overflow-y-hidden');
  }

  function closeMenu() {
    obsMenu.classList.remove('obs_menu--open');
    obsOverlay.classList.remove('obs_overlay--active');
    obsBody.classList.remove('overflow-y-hidden');
  }


  obsMenuBtn.addEventListener('click', () => {
    if (obsMenu.classList.contains('obs_menu--open')) {
      closeMenu();
    } else {
      openMenu();
    }
  });

  obsOverlay.addEventListener('click', () => {
    closeMenu();
  });

  obsMenuCloseBtn.addEventListener('click', () => {
    closeMenu();
  });

  document.addEventListener("DOMContentLoaded", () => {
    const el_html = document.documentElement,
    el_body = document.querySelector('body'),
    header = document.querySelector("#header"),
    navIsSticky = function() {
      const wScrollTop	 = window.pageYOffset || el_body.scrollTop,
        treshold       = '100',
        classSticky	   = 'scrolling',
        classFound	   = el_html.classList.contains(classSticky),
        navHeight      = '100',
        bodyRect	     = el_body.getBoundingClientRect(),
        scrollValue	   = 0;

      if ( wScrollTop > (scrollValue + treshold) && !classFound ) {
        el_html.classList.add(classSticky);
      }

      if ( wScrollTop < 2 && classFound) {
        el_html.classList.remove(classSticky);
      }
    },

    onScrolling = function() {
      navIsSticky();
    };

    window.addEventListener('scroll', function(){
      window.requestAnimationFrame( onScrolling );
    });
  });

</script>          <article> <header class="highlights pb-25 container wrapper">   <div class="mb-15">   <a href="/culture/" class="text-decoration-none latino-50 " data-piano="Page_Article,Fil_Ariane,,1,navigation"> Culture  </a>  </div>    <h1 class="highlights-title container d-flex flex-column ms-lg-0 ">  <span class="antonia-600b antonia-lg-800">Billie Eilish, par André Manoukian : « Sa voix m’a tué »</span> </h1> </header> <div class="container"> <div class="wrapper"> <div class="article-page">  <section class="article-page__body"> <div class="content">     <figure class="hook-media--standard "> <picture class="hook-media_picture pb-10">            <source media="(min-width: 769px)" srcset="https://focus.nouvelobs.com/2019/12/16/1520/0/3648/2432/510/340/75/0/6357d54_x7pShw68QnPLVhoMcgBzI3-a.jpg 1x,https://focus.nouvelobs.com/2019/12/16/1520/0/3648/2432/1020/680/50/0/6357d54_x7pShw68QnPLVhoMcgBzI3-a.jpg 2x" width="510" height="340"> <source media="(max-width: 768px)" srcset="https://focus.nouvelobs.com/2019/12/16/1520/0/3648/2432/375/250/75/0/6357d54_x7pShw68QnPLVhoMcgBzI3-a.jpg 1x,https://focus.nouvelobs.com/2019/12/16/1520/0/3648/2432/750/500/50/0/6357d54_x7pShw68QnPLVhoMcgBzI3-a.jpg 2x" width="375" height="250"> <img class="mx-auto" src="https://focus.nouvelobs.com/2019/12/16/1520/0/3648/2432/510/340/75/0/6357d54_x7pShw68QnPLVhoMcgBzI3-a.jpg" width="510" height="340" alt="Billie Eilish">   </picture>  <figcaption class="hook-media_figcaption pb-30"> <p class="latino-300 text-neutral_800">  Billie Eilish <span class="text-neutral_600 ms-5">KENNETH CAPELLO</span>  </p> </figcaption>  </figure>       <p class="article__description latino-800 pb-20 pb-md-30">  <span class="latino-800b"></span>  ILS VONT FAIRE 2020 - Elue meilleure nouvelle artiste de l’année aux American Music Awards, cette autrice-compositrice-interprète de 18&nbsp;ans est devenue une sensation musicale internationale. Une révélation et un choc émotionnel pour André Manoukian. </p>   <div class="article__author pb-20 pb-md-30"> <p class="latino-400b text-neutral_800">     Par André Manoukian </p> <p class="latino-400 text-neutral_600 mt-10">   Publié le <time datetime="2019-12-20 06:00:54"> 20 décembre 2019 à 6h00</time> </p>  </div>      <div class="article__share d-flex align-items-center pb-20 pb-md-30 gap-20  "> <p class="latino-50 text-neutral_600" style="margin-top:0.2rem;">Abonné</p> <div style="border-left:1px solid #909090;height:15px" class=""></div> <div class="d-flex gap-15">      <button data-article-key="20191220.OBS22547" aria-label="lire plus tard" data-section="Culture" data-piano="Page_Article,Sous_date_publi,A_lire_plus_tard,1er_clic,action" class="popover-trigger btn btn--small btn--transparent btn--icon-only read-later">  <span class="latino-100 ">Lire&nbsp;plus&nbsp;tard</span>  <i class="icon  icon-read-after" aria-hidden="true"></i> </button>      <button id="comment" aria-label="Commenter" class="popover-trigger btn btn--small btn--transparent btn--icon-only" data-piano="Page_Article,Sous_date_publi,Commenter,1er_clic,action"> <span class="latino-100">Commenter</span>   <svg class="icon icon--comment-alt " style="--fill:#171717">  <use href="/bucket/d99d4ddc964606991915ad102f5c979bb2aff096/v3/img/sprite.svg#comment-alt"></use> </svg>  </button> <div id="popover-comment" data-popover-for="comment" class="popover p-30" role="tooltip" style="width: 375px"> <div class="floating-close"> <button class="popover-close btn btn--small btn--icon-only btn--secondary"> <div class="sr-only">Masquer ce message</div>   <svg class="icon icon--navigation_croix " style="--fill:#171717">  <use href="/bucket/d99d4ddc964606991915ad102f5c979bb2aff096/v3/img/sprite.svg#navigation_croix"></use> </svg>  </button> </div> <p class="popover-header antonia-300eb text-neutral_800 pb-10 pe-30">  Vous voulez participer au débat&nbsp;?  </p> <p class="popover-body pb-30 latino-400 text-neutral_800"> S’abonner permet de commenter les articles. Et pas que&nbsp;: vou