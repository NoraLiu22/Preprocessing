window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        function getElId(element) {
    var body = element.closest(".body");
    if (body) {
        return body.dataset.elid;
    }
    var article = element.closest("[elid]");
    if (article) {
        return article.getAttribute("elid");
    }
    return null;
}

