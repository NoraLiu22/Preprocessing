   var particle_to_insert_after=document.querySelectorAll('.body .rebellt-item.col1');

   var recirculation_module=document.querySelector('.recirculation-module-wrap')
   var recirculation_module1=recirculation_module.cloneNode(true);
   recirculation_module1.classList.add('first_recirculation');
    if( particle_to_insert_after[0] && recirculation_module && recirculation_module.querySelector('.recirculation-widget')){
       particle_to_insert_after[0].insertAdjacentElement('afterend', recirculation_module1)
   }

  var recirculation_module2=recirculation_module.cloneNode(true);
   recirculation_module2.classList.add('second_recirculation');

