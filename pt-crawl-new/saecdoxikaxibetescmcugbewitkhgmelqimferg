    const cookiesEnabled = navigator.cookieEnabled;
    function setCookie(cname, cvalue, exms, path) {
      if (cookiesEnabled) {
        let d = new Date();
        d.setTime(d.getTime() + exms);
        let expires = 'expires=' + d.toUTCString();
        document.cookie = cname + '=' + cvalue + ';' + expires + ';path=' + path;
      } else {
        console.warn('cookies not enabled: session id cannot be saved');
      }
    }
    function getCookie(cname) {
      if (cookiesEnabled) {
        let name = cname + '=';
        let ca = document.cookie.split(';');
        for (let i = 0; i < ca.length; i++) {
          let c = ca[i];
          while (c.charAt(0) === ' ') {
            c = c.substring(1);
          }
          if (c.indexOf(name) === 0) {
            return c.substring(name.length, c.length);
          }
        }
      } else {
        console.warn('cookies not enabled: session id cannot be retrieved');
      }
      return null;
    }
    const generateSessionID = function () {
      if (getCookie('n2_session_id')) {
        return getCookie('n2_session_id');
      } else {
        let sid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
          let r = Math.random() * 16 | 0, v = c === 'x' ? r : r & 0x3 | 0x8;
          return v.toString(16);
        });
        try {
          setCookie('n2_session_id', sid, 1800000, '/');
        } catch (err) {
          console.warn('Not able to set session id on cookies', err);
        }
      }
    };
    const GenerateSession = function () {
      try {
        if (!window.n2.session_id) {
          window.n2.session_id = generateSessionID();
        }
        if (!window.n2.device_category) {
          window.n2.device_category = 481 <= window.innerWidth && 1024 > window.innerWidth ? 'tablet' : 480 > window.innerWidth ? 'phone' : 'desktop';
        }
      } catch (err) {
        console.error('Storied: Failed to generate session');
      }
    };
    const generateProjectSession = function () {
      var url = window.location.href;
      var preview = false;
      var storied = ['cloud.storied.co', 'n2ps.storied.co', 'stage.storied.co', 'fedev.storied.co', 'dev.storied.co'];
      for (let index = 0; index < storied.length; index++) {
        if ((url.indexOf(storied[index]) === -1) && /projects/g.test(url)) {
        }
      }
      if (!preview && (url.indexOf('localhost') === -1)) {
        var project_id = window.n2.config.app_id;
        if (!getCookie('n2_app_session_' + project_id)) {
          window.n2.analytics.new_session = true;
        }
        try {
          setCookie('n2_app_session_' + project_id, project_id, 1800000, '/');
        } catch (err) {
          console.warn('Not able to set session app on cookies', err);
        }
      }
    }
