        <div id="ad-mpu-2" class="dac__mpu-2">
            <script>
                (function(){
                    var ua_string = window.navigator.userAgent;
                    var disable_ads = ua_string.indexOf('NOGUSTOADS') > -1;
                    var log_ads = ua_string.indexOf('LOGGUSTOADS') > -1;
                    var ad_mapping =  window.GLOBALADVERTISING.mappings['mpu-2'];

                    if(ad_mapping) {
                        if (disable_ads) {
                            document.querySelectorAll('.dac__mpu-card, .dac__stream-mpu-card').forEach(e => e.parentNode.removeChild(e));
                        }
                        else {
                            if(log_ads) {
                                console.log('Ad position defined', 'mpu-2');
                            }
                            
                                gusto.lazy_ads.push({
                                    'id': 'ad-mpu-2',
                                    'breakpoint': null
                                });
                            
                        }
                    }
                    else {
                        console.error('No mapping defined for mpu-2');
                    }
                })();
            </script>
        </div>
    

</div>

        
            
