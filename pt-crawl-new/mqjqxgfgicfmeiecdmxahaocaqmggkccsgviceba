<script type="text/javascript">
	$(function() {
		$('.btn-group-link').click(function() {
			var linkID = $(this).data('link');

			$('html, body').animate({
				'scrollTop':   $('#' + linkID).offset().top
			}, 500);
		});


		$('#nameSal').select2({placeholder: 'Salutation', minimumResultsForSearch: -1});
		$('#state').select2({placeholder: 'State'});
		$('#county').select2({placeholder: 'County'});
		$('#agency').select2();

		$('#agency').on('select2:select', function(e) {
			var data = e.params.data;

			$('#agency').val(3808).change();

			if (data.id != '3808') {
				BootstrapDialog.show({
					title: '<i class="fa fa-info-circle"></i> Important Message',
					message: "You have selected an agency other than the Office of Open Records. To obtain  " + data.text.replace(/ \(.*\)/g, '') + " records, you must contact the agency directly. <a href='/RTKL/AOROInfo.cfm?id=" + data.id + "'>Click here to obtain the Agency Open Records Officer contact information for " + data.text.replace(/ \(.*\)/g, '') + "</a>." +
							"<br><br>" +
							"If you wish to request records from the Office of Open Records, please close this alert and select the Office of Open Records from the agency drop down box to continue.  Please note that the OOR decides appeals of denied requests for government records and is <strong>NOT</strong> a warehouse of records for state and local agencies. Each agency is responsible for maintaining its own records and should be requested from the relevant agency.",
					buttons: [{
						label: "Continue to request<br>records from the OOR",
						cssClass: 'btn-primary pull-left',
						action: function(dialogRef) { dialogRef.close() }
					},{
						label: "Find AORO contact information<br> for another agency",
						cssClass: 'btn-default',
						action: function() { window.location.href = '/RTKL/AOROSearch.cfm' }
					}]
				});
			}
		});


		$('#btn-submit').click(function(e) {
			e.preventDefault();
			removeEmptyFiles();

			if ($('#rtkRequestForm').valid()) {
				var formVals = getFormVals('rtkRequestForm');

				$.post({
					url: '/Resources/cfc/OOR.cfc?method=formHasHTMLAjax&returnFormat=json',
					dataType: 'json',
					data: {
						FORM: JSON.stringify(formVals)
					},
					success: function(hasHTML) {
						if (hasHTML === true) {
							_alert('Invalid input detected. Please check your input and try again.');
						}
						else {
							$('#rtkRequestForm')[0].submit();
						}
					},
					error: ajaxError
				});
			}
		});

		//alert user if too many links are in the Requested Records field
		document.getElementById('records').addEventListener('blur', function(){
			const linkRegex = /[(http(s)?):\/\/(www\.)?a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}\b([-a-zA-Z0-9@:%_\+.~#?&\/=]*)/g;
			const linksAllowed = 3;
			const linkArr = [...this.value.matchAll(linkRegex)];

			if(linkArr.length > Number(linksAllowed)){
				_alert(`Records Requested contains too many links. This field may only contain up to ${linksAllowed} links.`, 'Form Error', BootstrapDialog.TYPE_DANGER);
			}
		});

		const textRegex = /^[\w\s,\.!&?:\/\-|\(\)':#;%]+$/;

		$("#rtkRequestForm").validate({
			// debug: true,
			// ignore: '.file-attachment',
			rules: {
				nameFirst: {
					required: true,
					pattern: textRegex
				},
				nameLast:  {
					required: true,
					pattern: textRegex
				},
				org:  {
					pattern: textRegex
				},
				address1:  {
					required: true,
					pattern: textRegex
				},
				address2:  {
					pattern: textRegex
				},
				city:  {
					required: true,
					pattern: textRegex
				},
				state: 'required',
				zip: {
					required: true,
					pattern: /^\d{5}(-\d{4})?$/
				},
				phone: {
					pattern: /^(\d{3}-){2}\d{4}$/
				},
				email: {
					required: true,
					email: true
				},
				agency: 'required',
				records:  {
					required: true,
					pattern: textRegex
				},
			},
			messages: {
				nameFirst: {
					required: 'First Name is required.',
					pattern: 'First Name cannot contain special characters.'
				},
				nameLast: {
					required: 'Last Name is required.',
					pattern: 'Last Name cannot contain special characters.'
				},
				org: {
					pattern: 'Organization cannot contain special characters.'
				},
				address1: {
					required: 'Address 1 is required.',
					pattern: 'Address 1 cannot contain special characters.'
				},
				address2: {
					pattern: 'Address 2 cannot contain special characters.'
				},
				city: {
					required: 'City is required.',
					pattern: 'City cannot contain special characters.'
				},
				state: 'The State field is required.',
				zip: {
					required: 'The Zip Code field is required.',
					pattern: 'Invalid Zip Code.  Must match the pattern 12345 or 12345-6789.'
				},
				phone: {
					pattern: 'Invalid Phone number.  Must match the pattern 123-456-7890.'
				},
				email: {
					required: 'The Email Address field is required.',
					email: 'Invalid email address.'
				},
				agency: 'The Agency field is required.',
				records: {
					required: 'The Records Requested field is required.',
					pattern: 'The Records Requested field cannot contain special characters.'
				}
			}
		});
	});
</script>







<h1>Request OOR Records</h1>



<div class="row">
	<div class="col-xs-12">
		<p>Are you requesting records from a local government, school district, or state agency but are unsure where or whom to direct your request to?  Please <a href="AOROSearch.cfm">click here</a> for a database of Agency Open Records Officers.</p>
		<p>Are you requesting records such as birth or death certificates, divorce records, marriage licenses, criminal proceedings, background check, or police report?  Please <a href="CommonlyMisdirected.cfm">click here</a> for a chart of records most commonly requested.</p>
		<p>Are you requesting records from the Office of Open Records, meaning records that were created by Office of Open Records employees such as open records appeal files?  The OOR decides appeals of denied requests, maintaining records only related to its operations, and is not a general repository or warehouse for government records.</p>
		<p>
			The OOR will not docket appeals or respond to requests where the OOR has reason to believe that a requester's identity is anonymous or has been misrepresented to the OOR.
		</p>
	</div>
</div>

<div class="row margin-bottom-20">
	<div class="col-xs-12">
		<h4>Please click one of the following buttons:</h4>

		<div class="btn-group btn-group-justified" role="group">
			<div class="btn-group" role="group">
				<button type="button" class="btn btn-default btn-group-link" data-link="link-form">
					<h4>Via Online Form</h4>
					<i class="fa fa-5x fa-check-square"></i>
				</button>
			</div>
			<div class="btn-group" role="group">
				<button type="button" class="btn btn-default btn-group-link" data-link="link-email">
					<h4>Via Email</h4>
					<i class="fa fa-5x fa-paper-plane"></i>
				</button>
			</div>
			<div class="btn-group" role="group">
				<button type="button" class="btn btn-default btn-group-link" data-link="link-mail">
					<h4>Via Mail</h4>
					<i class="fa fa-5x fa-envelope"></i>
				</button>
			</div>
			<div class="btn-group" role="group">
