        <div id="content-area">
            <div class="region region-content">
    <div data-drupal-messages-fallback class="hidden"></div>
<div id="block-pace-theme-content" data-block-plugin-id="system_main_block" class="block block-system block-system-main-block">
  
    
      

<article  class="entity--type-node node--type-gtfs-route">

  <div class="title-image-row clearfix">
    <h1><span>272 - Milwaukee Avenue North</span>
</h1>

    
  </div>

  <div class="views-element-container"><div class="hide view view-passenger-notices view-id-passenger_notices view-display-id-route_listing_block js-view-dom-id-eb36b5bbb27f6bf4050392de04bbabe0937b970cc6e9bb56add807bb894c1341">
  
    
      
  
          </div>
</div>


  <div class="bar-wrapper">
    <div class="bar">
      <h2>
            <div class="field field--name-field-route-type field--type-list-string field--label-hidden field__item">Fixed Route</div>
      </h2>
      <h4>
            <div class="field field--name-field-description field--type-string-long field--label-hidden field__item">Provides weekday and Saturday service between Golf Mill Shopping Center in Niles and Hawthorn Mall in Vernon Hills via Wheeling along Milwaukee Ave. </div>
      </h4>
    </div>
  </div>

  
            <div class="field field--name-field-arcgis field--type-string field--label-hidden field__item"><div class="embed-container"><iframe width="100%"
            height="421"
            frameborder="0"
            scrolling="no"
            marginheight="0"
            marginwidth="0"
            title="Route map"
