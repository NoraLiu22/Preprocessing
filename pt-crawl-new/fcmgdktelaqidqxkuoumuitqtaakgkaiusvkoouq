	data-ad-path='/21783347309/andscape.com/culture/article'
	data-ad-targeting='{"sp":"andscape","objid":"320801","lang":"en","devType":"desktop","tag":["nba","nba-playoffs","new-york-knicks","jalen-brunson"]}'
	data-post-tags='["NBA","NBA Playoffs","New York Knicks","Jalen Brunson"]'
