    $(function () {
        $("#start-subscription").on("click", function (e) {
            e.preventDefault();
            e.stopPropagation();
            $('html, body').animate({
                scrollTop: $("#FirstName").offset().top - 200
            }, 1000);
            $('#FirstName').focus();
        });

