function buyTrack(isrc,htrackid) {
  document.getElementById('t'+htrackid).innerHTML="<img src='/images/wait2.gif'>";
	var xmlHttp = null;
try {
    // Mozilla, Opera, Safari sowie Internet Explorer (ab v7)
    xmlHttp = new XMLHttpRequest ();
} catch(e) {
    try {
        // MS Internet Explorer (ab v6)
        xmlHttp  = new ActiveXObject("Microsoft.XMLHTTP");
    } catch(e) {
        try {
            // MS Internet Explorer (ab v5)
            xmlHttp  = new ActiveXObject("Msxml2.XMLHTTP");
        } catch(e) {
            xmlHttp  = null;
        }
    }
}
if (xmlHttp) {
    xmlHttp.open('GET', '7digital_buy.asp?isrc='+isrc, true);
    xmlHttp.onreadystatechange = function () {
        if (xmlHttp.readyState == 4) {
						resultText = xmlHttp.responseText+"//";
						results = resultText.split("/");
						if (results[2]=="") {
							resultText="nicht verfügbar";
						} else {
							resultText = "<a href=\"#\" onclick=\"addBasket(" + results[0] + ", " + results[1] +", "+htrackid+");return false;\">CHF " + parseFloat(results[2]).toFixed(2) + ": kaufen</a>";
						}
            document.getElementById('t'+htrackid).innerHTML=resultText;
        }
    };
    xmlHttp.send(null);
}
}




function addBasket(release,trackid,htrackid) {
document.getElementById('t'+htrackid).innerHTML="<img src='/images/wait2.gif'>";
	var xmlHttp = null;
try {
    // Mozilla, Opera, Safari sowie Internet Explorer (ab v7)
    xmlHttp = new XMLHttpRequest ();
} catch(e) {
    try {
        // MS Internet Explorer (ab v6)
        xmlHttp  = new ActiveXObject("Microsoft.XMLHTTP");
    } catch(e) {
        try {
            // MS Internet Explorer (ab v5)
            xmlHttp  = new ActiveXObject("Msxml2.XMLHTTP");
        } catch(e) {
            xmlHttp  = null;
        }
    }
}
if (xmlHttp) {
    xmlHttp.open('GET', '7digital_addbasket.asp?release='+release+'&trackid='+trackid, true);
    xmlHttp.onreadystatechange = function () {
        if (xmlHttp.readyState == 4) {
						resultText = xmlHttp.responseText;
						results = resultText.split("/");
						resultText = "In <a href=\"basket.asp\">Warenkorb</a> hinzugefügt";
            document.getElementById('t'+htrackid).innerHTML=resultText;
        }
    };
    xmlHttp.send(null);
}
}


