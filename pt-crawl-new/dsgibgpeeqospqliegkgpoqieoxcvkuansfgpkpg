            <meta itemprop="contentUrl" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/6/a8/6a8d4a34-bbab-11ee-9464-0f49c5764a57/65b2a29a0d59d.image.jpg?resize=1200%2C800">
            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/6/a8/6a8d4a34-bbab-11ee-9464-0f49c5764a57/65b2a29a0d59d.image.jpg?resize=1200%2C800">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
