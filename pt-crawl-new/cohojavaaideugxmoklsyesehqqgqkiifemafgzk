<script type="text/plain" class="optanon-category-C0004">
function sportico_linkedin_conversion_tracking() {
	var img_tag = document.createElement('img');
	img_tag.height = '1';
	img_tag.width = '1';
	img_tag.style.display = 'none';
	img_tag.alt = '';
	img_tag.src = 'https://px.ads.linkedin.com/collect/?pid=2475676&conversionId=2678300&fmt=gif';
	document.body.appendChild(img_tag);
}

function sportico_conversion_tracking() {
	gtag_report_conversion();
	sportico_linkedin_conversion_tracking();
}
