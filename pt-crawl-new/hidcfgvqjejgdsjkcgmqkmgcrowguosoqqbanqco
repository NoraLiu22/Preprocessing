    <script type="text/javascript">
        $('body').click(function () {
            $('ul.switches').hide();
        });

        $('.browse-button').click(function (event) {
            event.preventDefault();
            event.stopPropagation();
            $('ul.switches').show();
        });

        $('#dynamic_select').change(function () {
            var url = $(this).val();
            window.location = url;
        });


