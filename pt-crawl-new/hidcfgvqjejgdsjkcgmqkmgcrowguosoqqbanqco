function slideImages(){
      var $active = $('#portfolio_slider .flavorslideractive');
      var $next = ($('#portfolio_slider .flavorslideractive').next().length > 0) ? $('#portfolio_slider .flavorslideractive').next() : $('#portfolio_slider :first');
      $next.css('z-index',2);//set the z-index so the image will slide across the top of  current one
      $active.css('z-index',1);//and set the current active image so it's below> 
      $next.animate({left:0},"slow",function(){//slide in the next image
              $active.removeClass('flavorslideractive').css('left',"100%");//when the slide's finished, reset the previous active image
              $next.addClass('flavorslideractive');//and set the new image to active
        });
    }

			
