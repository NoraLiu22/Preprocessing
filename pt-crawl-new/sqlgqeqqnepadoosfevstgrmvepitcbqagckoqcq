                    if(event.data && typeof event.data === 'string'){   
                        if(event.data === 'scrollToTop'){
                            const container = window.document.getElementById('main-container');
                            const containerYPos = frameWrapper
                                ? window.scrollY + frameWrapper.getBoundingClientRect().top
                                : 0;
                            const GlobalNavHeight = window.document.getElementsByClassName('GlobalNav-wrapper')[0].getBoundingClientRect().height;
                            const pageNavHeight = container
                                ? container.querySelector('nav')?.getBoundingClientRect().height
                                : 0;
                            const scrollOffset = GlobalNavHeight + (pageNavHeight || 0);

                            window.scroll(0, containerYPos - scrollOffset);    
                        }

                        if(event.data.includes('iframeHeight')){
                            const newFrameHeight = JSON.parse(event.data).iframeHeight + "px";

                            frameWrapper.style.height = newFrameHeight;
                            frameWrapper.querySelector('div').style.height = newFrameHeight;
                            frameWrapper.querySelector('iframe').height = newFrameHeight;
