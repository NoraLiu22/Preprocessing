    <script>

        document.addEventListener('DOMContentLoaded', function() {

            var w = window.innerWidth;
            var bMobile = false;
            var sClass = 'w-100';

            if (w>999) {
                // desktop
                var itemsPerSlide = 4;
                var totalItems = $('#slider2216-hidden .carousel-item-inn').length;
                // var it = totalItems;
                var it = totalItems / itemsPerSlide;
                var sInClass = 'col-3 col-md-3 col-lg-3';
            } else {
                // mobile
                var itemsPerSlide = 1;
                var totalItems = $('#slider2216-hidden .carousel-item-inn').length;
                var it = totalItems / itemsPerSlide;
                var sInClass = 'col-12 col-md-12 col-lg-12';
                bMobile = true;
            }

            if (totalItems <= itemsPerSlide) {
                $('#slider2216-multi .carousel-control-prev').hide();
                $('#slider2216-multi .carousel-control-next').hide();
            }

            if (Math.ceil(it) > 0) {
                var ix = Math.ceil(it);
                for (var i = 0; i < ix; i++) {
                    // clone containers "carousel-item"
                    var newEl = $('#slider2216-hidden .carousel-item').clone().appendTo('#slider2216 .carousel-inner');
                    newEl.addClass('elm-'+i);
                    newEl.attr('data-elm', ix-i-1);

                    // clone indicators
                    var indicator = $('#slider2216-hidden .ind-point').clone().appendTo('#slider2216-multi .carousel-indicators');
                    indicator.attr('data-slide-to', i);
                    indicator.addClass('el-'+i);
                }
            }
            $('#slider2216 .carousel-item').each(function(e) {
                $(this).addClass(sClass);
            });

            $('#slider2216 .carousel-item').first().addClass('active');
            $('#slider2216-multi .carousel-indicators .ind-point').first().addClass('active');

            $('#slider2216-hidden .carousel-item-inn').each(function(e) {
                var iCol = Math.ceil($(this).data('elm') / itemsPerSlide);
                $(this).addClass(sInClass);
                // move element to container
                $(this).appendTo(('#slider2216 .elm-'+(iCol-1)+''));
            });
            // indicator listener
            $('#slider2216-multi .carousel-indicators .ind-point').on('click', function(e) {
                $('#slider2216-multi .carousel-indicators .ind-point').removeClass('active');
                $(this).addClass('active');
            });

            // new elements on swipe
            $('#slider2216').on('slide.bs.carousel', function (e) {
                var $e = $(e.relatedTarget);
                var idx = $e.index();

                // change indicator
                $('#slider2216-multi .carousel-indicators .ind-point').removeClass('active');
                $('#slider2216-multi .carousel-indicators .el-'+idx).addClass('active');

            });
        });

    </script>


                    

