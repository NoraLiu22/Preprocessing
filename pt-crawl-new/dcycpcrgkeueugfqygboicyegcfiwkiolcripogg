    <script>
        $(function () {
            SCM.AdvancedSearch.configSettings = $.extend(SCM.AdvancedSearch.configSettings,
            {
                preSelectSingleBookFacet: "true"
            });

            if (SCM.QueryBuilder != undefined && SCM.QueryBuilder.defaults != undefined) {
                $(".querybuilder-submit").attr('value', 'Search');
                SCM.QueryBuilder.defaults.runSearch = searchRedirect;
            }
        });


        //-------------------QUERY BUILDER------------------------
        // Moved common functions to shared file in SharedWidgets
        // scm.sharedcontrols.queryBuilder.js - BENC - 7/27/2016

        function searchRedirect(redirectUrl) {
            window.location = '/searchresults/?' + redirectUrl;
        }
        //-------------------QUERY BUILDER------------------------

        //Form Input Values Cookies Set
        var today = new Date();
        var expiry = new Date(today.getUTCDate + 30);

        //Strictly Necessary Cookie
        function setCookie(name, value) {
            document.cookie = name + "=" + escape(value) + "; path=/; expires=" + expiry.toUTCString();
        }

        function getCookie(name) {
            var re = new RegExp(name + "=([^;]+)");
            var value = re.exec(document.cookie);
            return (value != null) ? unescape(value[1]) : null;
        }

        //Form Input Values Load from Cookies
        //if (input1 = getCookie("advAdvancedSearchQueryTerm")) $("#advancedSearchQueryTerm").val(input1);
        //if (input2 = getCookie("advAdvancedAuthorSearchQueryTerm")) $("#advancedAuthorSearchQueryTerm").val(input2);
        if (input3 = getCookie("advArticleTitle")) $("#articleTitle").val(input3);
        if (input4 = getCookie("advCitationJournal")) $("#citationJournal").val(input4);
        if (input5 = getCookie("advCitationYear")) $("#citationYear").val(input5);
        if (input6 = getCookie("advCitationVolume")) $("#citationVolume").val(input6);
        if (input7 = getCookie("advCitationIssueNo")) $("#citationIssueNo").val(input7);
        if (input8 = getCookie("advCitationPage")) $("#citationPage").val(input8);
        if (input9 = getCookie("advDoi")) $("#Doi").val(input9);

        $(".button").click(function () {
            //setCookie("advAdvancedSearchQueryTerm", $("#advancedSearchQueryTerm").val());
            //setCookie("advAdvancedAuthorSearchQueryTerm", $("#advancedAuthorSearchQueryTerm").val());
            setCookie("advArticleTitle", $("#articleTitle").val());
            setCookie("advCitationJournal", $("#citationJournal").val());
            setCookie("advCitationYear", $("#citationYear").val());
            setCookie("advCitationVolume", $("#citationVolume").val());
            setCookie("advCitationIssueNo", $("#citationIssueNo").val());
            setCookie("advCitationPage", $("#citationPage").val());
            setCookie("advDoi", $("#Doi").val());
        });
    </script>
</article>









        
        
    


    
 
