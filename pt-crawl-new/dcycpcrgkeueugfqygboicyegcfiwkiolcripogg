                        <script type="text/javascript">
                            function OnSuccessDoiSearch(data) {
                                if (data == "false") {
                                    alert('DOI did not match with any Article. Please try again.');
                                    return false;
                                }
                                url = data.replace("window.location = '", "").replace("';");
                                window.location.href = url;
                            }

                            function OnFailureDoiSearch(data) {
                                alert('DOI did not match with any Article. Please try again.');
                                return false;
                            }

                            $('#btnDoiSearch').click(function (evt) {
                                $('#errors').hide();
                                $.ajax({
                                    type: 'POST',
                                    url: '/Solr/DoiSearch',
                                    data: {
                                        doi: $('#Doi').val().trim()
                                    },
                                    dataType: "text",
                                    success: function (data) {
                                        OnSuccessDoiSearch(data);
                                    },
                                    error: function (data) {
                                        OnFailureDoiSearch();
                                    }
                                });
                                return false;
                            });

                        </script>
                        <div id="advancedSearchModal" class="advanced-search__modal reveal-modal" data-reveal aria-labelledby="modalTitle" aria-hidden="true" role="dialog">
                            <button class="button" class="close-reveal-modal btn btn-as-icon icon-general-close" aria-label="Close">
                                <span class="screenreader-text">Close comment form modal</span>
                            </button>
                            <div class="advanced-search__modal-text">
                                DOI did not match with any article. Please try again.
                            </div>
                        </div><!-- /#advancedSearchModal .advanced-search__modal -->

