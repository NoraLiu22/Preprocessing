<form id="newsletter-signup-app-1270102" class="es-signup bg-white -extend newsletter-signup" v-on:submit.prevent="onSubmit">
    <input class="hide" type="text" id="phone" v-model="phone" name="phone">
    <div class="es-hero relative zi3" :class="[
            loggedin && !listId ? '-loggedin' : '', 
            loading ? '-loading' : '', 
            listId ? '-unsub' : 'pb5 lg:pb10', 
            !justUnsubscribedIds.length && listId ? '-noUnsubs' : '',
            noUser ? '-noUser' : '',
            hideImage ? '-hideImage' : ''
        ]">

        <!-- intro -->
        <div class="es-cell -intro relative zi1 pb2">
            <h1><strong>Email Newsletters</strong></h1>
            
            <h4 :class="listId ? 'pb3' : 'pt1'">Convenient updates delivered to your inbox.</h4>
            

            <div v-if="loading && listId" class="es-loader pb10 pt10 es-cell -text" v-cloak>
                <p>Loading&hellip;</p>
                <div class="es-loader__circle text-primary">
                    <div></div>
                    <div></div>
                    <div></div>
                    <div></div>
                </div>
            </div>
        </div>

        <!-- status alert (if logged in) -->
        <div v-if="(loggedin || noUser) && !loading" class="es-cell -email" v-cloak>

            <!-- error -->
            <div v-if="errors.length > 0 || messages.failMsg" class="bg-highlight text-secondary_dark -extend pt3 pb3 md:pt5 md:pb5" v-cloak>
                <p v-for="error in errors">{{ error }}</p>
            </div>
            
            <div v-if="messages.status" v-html="messages.status" class="confirmation pt3 pb3 -extend bg-primary_light md:pt5 md:pb5" v-cloak></div>

            <!-- simple logged in display -->
            <div class="bg-secondary text-white -extend">
                <div v-if="!noUser && (!isApp)" class="pt3 pb3 md:pt5 md:pb5">
                    You are logged in as <span>{{ email }}</span>. <a class="text-primary_light" data-toggle="collapse" href="#es-login" aria-expanded="false" aria-controls="es-login">Manage another email</a>.
                </div>
                <div v-else-if="isApp && userNewsletterIds.length == 0" class="pt3 pb3 md:pt5 md:pb5">
                    Viewing newsletters for <span>{{ email }}</span>.
                </div>

                <div class="-extend bg-highlight zi4" :class="listId ? 'border-bottom' : ''">
                    <div id="es-login" class="collapse text-secondary" :class="noUser ? 'in' : ''">
