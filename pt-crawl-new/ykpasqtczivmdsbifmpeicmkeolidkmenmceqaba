					<script type="text/javascript">
    jQuery(document).ready(function(){
        if(jQuery('.prev-month').length > 0){
            jQuery('.prev-month').click(function(){
                jQuery.post(
                    ajaxurl,
                    {
                        'action': 'ajax_handler',
                        'what': 'calendar',
                        'month': jQuery('.calendario').find('.prev-month-value').html(),
                        'year': jQuery('.calendario').find('.prev-year-value').html()
                    },
                    function(response){
                        //Example of jquery repsonse handler
                        jQuery('.calendario').html(response);
                    }
                );
            });
        }

        if(jQuery('.next-month').length > 0){
            jQuery('.next-month').click(function(){
                jQuery.post(
                    ajaxurl,
                    {
                        'action': 'ajax_handler',
                        'what': 'calendar',
                        'month': jQuery('.calendario').find('.next-month-value').html(),
                        'year': jQuery('.calendario').find('.next-year-value').html()
                    },
                    function(response){
                        //Example of jquery repsonse handler
                        jQuery('.calendario').html(response);
                    }
                );
            });
        }
    });
</script>
