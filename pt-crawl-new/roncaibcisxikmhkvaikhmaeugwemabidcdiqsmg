  <label for="search-bar-input-below-menu" class="search-bar__label vh">Start your search here...</label>
  <input type="hidden" name="scope" value="cbeebies">
  <div class="search-bar__input-area">
    <input id="search-bar-input-below-menu" class="search-bar__input" type="text" name="q" placeholder="Start your search here...">
    <button class="search-bar__clear" aria-label="Clear Search" type="button" data-stats-interaction="search_bar" data-stats-interaction-variant="clear" data-stats-interaction-action="button">
      <i class="search-bar__clear-icon gelicon gelicon--no" aria-hidden="true"></i>
    </button>
