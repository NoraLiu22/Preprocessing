                  <source srcset="/files/styles/thunbnail_3/public/finalbannerFoodCode_SM_1200x675px_1043.png?itok=TfgsZt50 380w" media="all and (max-width: 320px)" type="image/png" sizes="100vw" width="380" height="200"/>
