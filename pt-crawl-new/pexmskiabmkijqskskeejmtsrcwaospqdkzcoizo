    var redirects = ['/news/'];

    var url = new URL(window.location);
    if (!url) return;

    function isRedirect(redirect) {
      return url.pathname === redirect;
    }

    var isRedirect = redirects.find(isRedirect);

    if (isRedirect) {
      window.location.href = window.location.origin;
    }
