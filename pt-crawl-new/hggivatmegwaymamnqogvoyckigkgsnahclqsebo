jQuery(document).ready(function($) {

  var userInput = $(".project-address input");
  var result = $(".project-address label");
  var resultDefault = result.html();
  var districtField = $(".council-district input");

  //Valides address with AIS
  var ajaxRequest = debounce(function() {
    $.ajax({
        type: "GET",
        url: 'https://api.phila.gov/ais/v1/addresses/' + encodeURIComponent(userInput.val()),
        data: { 
            gatekeeperKey : '8723ea1314061632af9bdaeb1aa944e0',
        } ,
        dataType: 'json',
        success: function(data) {
            result.val("");
            if (typeof data.features !== 'undefined' && data.features.length > 0) {
                var p = data;
                result.html(resultDefault + ' <small style="color:#3a833c">address matched </small>');
                userInput.val(p.normalized);
                districtField.val(p.features[0].properties.council_district_2016);
            }
        },
        error: function(jqXHR, textStatus, errorThrown) {
          result.html(resultDefault + ' <small style="color:red">invalid address</small>');
        }
    });
  }, 1000);

  userInput.on('keyup',function(){
    result.html(resultDefault + ' <small style="color:#f3c613">validating address...</small>');
    ajaxRequest();
  });

  // Copies Applicant first/last name to Contact Person
  $('.copy-name input[type="checkbox"]').on('change', function(){
    if ($(this).prop('checked')) {
      var first = $('.copy-name-src .name_first input').val();
      var last = $('.copy-name-src .name_last input').val();
      $('.copy-name-target .name_first input').val(first);
      $('.copy-name-target .name_last input').val(last);
    }else {
      $('.copy-name-target .name_first input').val('');
      $('.copy-name-target .name_last input').val('');
    }
  });

  //Form confirmation page
  var settings = getDatatableDefauls();

  $('.datatable').DataTable(settings);

});
   
</script>          </div>
        </main>
