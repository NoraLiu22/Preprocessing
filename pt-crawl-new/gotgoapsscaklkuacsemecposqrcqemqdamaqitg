    var loadingGraphicHTML = "<img src=\"img/loading.gif\" />&nbsp;&nbsp;&nbsp;Loading...";
    function btnContinueClick() {
      if (Page_ClientValidate() == true) {
        var LoadingPanel = document.getElementById("ctl00_BodyContentHolder_LoadingPanel");
        var ButtonPanel = document.getElementById("ctl00_BodyContentHolder_btnLookUp");
        var ErrorLabel = document.getElementById("ctl00_ErrorContentHolder_lblError");
        ButtonPanel.style.display = "none";
        ErrorLabel.innerHTML = "";
        LoadingPanel.style.display = "block";
        LoadingPanel.innerHTML = loadingGraphicHTML;
      }
    }
  </script>

