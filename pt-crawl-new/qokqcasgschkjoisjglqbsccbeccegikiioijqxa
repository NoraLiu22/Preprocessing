    <div id="block-views-article-page-redesign-block-1" class="block block-views article-page first last odd">
  
  	  
      	
  <div class="view view-article-page-redesign view-id-article_page_redesign view-display-id-block_1 article-page-redesign view-dom-id-5446762dd890d6232e9bfe45fce07423">
        
  
  
