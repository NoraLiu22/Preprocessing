<script>
 
	var map = L.map("map").setView([38.192778, -97.187778], 4);
	map.removeControl(map.zoomControl);
	map.touchZoom.disable();
    map.doubleClickZoom.disable();
    map.scrollWheelZoom.disable();
    map.boxZoom.disable();
    map.keyboard.disable();
	
    var arrayOfLatLngs = [];

   // map.options.maxZoom = 11;
   // map.options.minZoom = 2;

	L.tileLayer('https://server.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer/tile/{z}/{y}/{x}').addTo(map);

    // custom icon
    var nwrIcon = L.Icon.extend({
        options: {
            iconSize: [12, 15],
            iconAnchor: [0, 0],
            popupAnchor: [10, 0]
        }
    });

    // Create icons for NWR transmitter status
    var greenIcon = new nwrIcon({
		iconURL: '/images/nwr/mm_20_green.png'
    }); // Normal
    var redIcon = new nwrIcon({
        iconUrl: '/images/nwr/mm_20_red.png'
    }); // Out of Service
    var yellowIcon = new nwrIcon({
        iconUrl: '/images/nwr/mm_20_yellow.png'
    }); // Degraded

    function markers(lat, lon, icon, message, site) {
        L.marker([lat, lon], {
                icon: icon,
				title: site + "\nClick for information"
            })
            .addTo(map)
        arrayOfLatLngs.push([lat, lon]);
    }

	function onMapClick(e) {
	 	window.location.href = "https://www.weather.gov/nwr/outages";
    }

  map.on('click', onMapClick);

  for(x=0;x<=STATUS.length-1;x++){
     var callsign = "";
     if(STATUS[x] != "NORMAL"){
        callsign = CALLSIGN[x];
        var sitename = SITENAME[x];
        var state    = SITESTATE[x];
        var status   = STATUS[x];
        var freq     = FREQ[x];
        var lat      = LAT[x];
        var lon      = LON[x];
        var tmpIcon = redIcon;
        var nwrContent = "<table>";
        nwrContent += "<tr><td colspan='2'><h2>" + callsign + "</h2></td></tr>";
        nwrContent += "<tr><th>Transmitter</th><td>" + sitename + "</td></tr>";
        nwrContent += "<tr><th>State</th><td>" + state + "</td></tr>";
        nwrContent += "<tr><th>Frequency</th><td>" + freq + "</td></tr>";
        nwrContent += "<tr><th>Callsign</th><td>" + callsign + "</td></tr>";
        nwrContent += "</table>";
        nwrContent += "<br>";
        nwrContent += "<a href=/nwr/sites?site=" + callsign + ">View " + callsign + " radio propagation.</a>";
        nwrContent += "<br />";

        if(status == 'DEGRADED'){
           var tmpIcon = yellowIcon;
        }
       markers(lat, lon, tmpIcon, nwrContent,  sitename + ' ' + callsign);
    }
 }
	var bounds = new L.LatLngBounds(arrayOfLatLngs);

    map.fitBounds(bounds, {
        padding: [5, 5]
    });

	</script></div>
