        function RunSearch() {
            var queryString = document.getElementById("q").value;
            var baseUrl = "http://www.google.com/cse?oe=utf8&ie=utf8&source=uds&start=0&cx=012281970149422056935%3Ae3col_wgefa&hl=en&";
            document.location = baseUrl + "q=" + queryString;
            return 0;
        }
