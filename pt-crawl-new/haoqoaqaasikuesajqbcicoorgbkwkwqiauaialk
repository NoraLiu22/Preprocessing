                (function(){
                    var ua_string = window.navigator.userAgent;
                    var disable_ads = ua_string.indexOf('NOGUSTOADS') > -1;
                    var log_ads = ua_string.indexOf('LOGGUSTOADS') > -1;
                    var ad_mapping =  window.GLOBALADVERTISING.mappings['mpu-0'];

                    if(ad_mapping) {
                        if (disable_ads) {
                            document.querySelectorAll('.dac__mpu-card, .dac__stream-mpu-card').forEach(e => e.parentNode.removeChild(e));
                        }
                        else {
                            if(log_ads) {
                                console.log('Ad position defined', 'mpu-0');
                            }
                            
                                gusto.lazy_ads.push({
                                    'id': 'ad-mpu-0',
                                    'breakpoint': null
                                });
                            
                        }
                    }
                    else {
                        console.error('No mapping defined for mpu-0');
                    }
                })();
            </script>
