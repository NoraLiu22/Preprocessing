                    <div class="app-share-bar">
                        
                            
<script>

    function buildTrackerLink(pageTabId) {
        return $.ajax({
            url: urlWithCookie("/crm/ajax/build_tracker_link"),
            data: {page_tab_id: pageTabId, share_url: "https://tb.cmaworld.com/tab_share/493160"}
        }).then(function (res) {
            return res.share_url;
        }).catch(function (res) {
            showError(res);
        });
    }

    $(document).on('click', '#copy-share-link-btn', async function () {
        const inviteURL = await buildTrackerLink(PAGE_TAB_ID);
        if (window.AndroidShareHandler && window.AndroidShareHandler.share) {
            // yinzcam app
            await window.AndroidShareHandler.share(inviteURL);
        } else if (navigator.share) {
            await navigator.share({
                title: 'Share Contest',
                url: inviteURL
            });
        } else {
            const userAgent = window.navigator.userAgent.toLowerCase();
            if (userAgent.includes('wv')) {
                // android webview doesn't support navigator.share
                $('#copy_link_container').show();
                $('#android_webview_copy_link').text(inviteURL);
            } else {
                await navigator.clipboard.writeText(inviteURL);
                showSuccess("Copied!");
            }
        }
        trackShareClick(PAGE_TAB_ID, 'link');
    })

</script>


                        
                    </div>
                </div>
            
        

        
            
