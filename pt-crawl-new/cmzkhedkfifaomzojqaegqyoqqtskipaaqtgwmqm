    <script>
        let ACCESS_TYPE = 'standalone';
        const PAGE_TAB_ID = '493160';
        window.scrollParent = null;
        const IS_EMBEDDED = (function () {
            try {
                return window.self !== window.top;
            } catch (e) {
                return true;
            }
        })();

        if (IS_EMBEDDED) {
            if (ACCESS_TYPE === 'standalone') {
                ACCESS_TYPE = 'embedded'
            }
            window.addEventListener('message', (event) => {
                let parent = (event.data + '').split(',')[0];
                if (parent === `onscroll-${PAGE_TAB_ID}`) {
                    window.scrollParent = parent;
                }
            });
        }

        $(document).on("tbits.formLoad", function () {
            populateFieldValues('493160');
        });

        

        pingServer(PAGE_TAB_ID, ACCESS_TYPE);
        $(document).ready(function () {
            if (IS_EMBEDDED) {
                setInterval(() => {
                    const height = $('#container').height() + 100;
                    window.parent.postMessage({
                        id: PAGE_TAB_ID,
                        type: 'height',
                        data: {height},
                    }, '*');
                }, 100);
            }

            

            

            

            
            

            if (!IS_EMBEDDED) {
                
                    initAppTracking();
                
            }

            setTimeout(() => $('#preloader').hide(), 100);
        });

        function conversionTrack(activityId) {
            ThirdPartyTracking.sendRegistration({content_name: 'CMA World Sign Up Form'});
            if (window.trackEvent) {
                trackEvent('page_tab', null, null, PAGE_TAB_ID, activityId, null);
            }
        }

        function fanLogout() {
            $.ajax({
                url: "/application/fan_logout",
                success: function () {
                    window.location.reload(true);
                }
            });
        }

    </script>

