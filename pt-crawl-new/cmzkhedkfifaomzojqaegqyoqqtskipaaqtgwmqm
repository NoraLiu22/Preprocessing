    <script type="text/javascript">
        let activityStartTime;


        const buttons = `<div class="text-center">
                            <a class="campaign-btn campaign-btn-primary campaign-btn-narrow swiper-back-btn">Back</a>
                            <a class="campaign-btn campaign-btn-primary campaign-btn-narrow swiper-next-btn">Next</a>
                            <a class="campaign-btn campaign-btn-primary swiper-submit-btn"
                                onclick="return saveRequest();" href="javascript:void(-1);">
                                Submit</a>
                        </div>`

        async function saveRequest() {
            showThrobber();
            try {
                await submitEntryForm({activity_duration_ms: Date.now() - activityStartTime});
            } catch (e) {
                return;
            }
            location.href = urlWithCookie(`/thank_you/${PAGE_TAB_ID}`)
        }

        function initScroll(direction) {
            $(".swiper-wrapper > div:visible").wrap("<div class='swiper-slide'></div>");
            $(".swiper-slide").append(buttons);
            const swiper = new Swiper(".swiper-container", {
                direction,
                slidesPerView: 1,
                spaceBetween: 0,
                centeredSlides: true,
                allowTouchMove: false,
                centeredSlidesBounds: true,
                keyboard: {
                    enabled: true
                },
            });
            swiper.on("slideChange", function () {
                $(".swiper-next-btn").toggle(!swiper.isEnd);
                $(".swiper-submit-btn").toggle(swiper.isEnd);
            });

            $(".swiper-next-btn").on('click', function () {
                swiper.slideNext();
            });

            $(".swiper-back-btn").on('click', function () {
                if (swiper.isEnd) {
                    $(".swiper-next-btn").show();
                    $(".swiper-submit-btn").hide();
                }
                swiper.slidePrev();
            });
        }

        $(document).ready(function () {
            activityStartTime = Date.now();

            
        });
    </script>

