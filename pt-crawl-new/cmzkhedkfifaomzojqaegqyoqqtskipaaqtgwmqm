<script src="/static/third-party-tracking.js"></script>
<script>
    function initAppTracking() {
        const trackingTags = [];
        const loadArgs = [trackingTags, {page_tab_id: '493160'}];

        const closeCookiePopup = () => $(".cookie-popup").hide();
        $('.close-cookie-popup-btn').click(() => closeCookiePopup());
        $('.accept-cookies-btn').click(() => {
            createCookie('tbits_campaign_privacy', 'accept', 21);
            ThirdPartyTracking.load(...loadArgs);
            closeCookiePopup();
        });
        $('.decline-cookies-btn').click(() => {
            createCookie('tbits_campaign_privacy', 'decline', 21);
            closeCookiePopup();
        });

        const privacy_cookie_val = readCookie('tbits_campaign_privacy')
        if (!privacy_cookie_val) {
            $('.cookie-popup').show();
            createCookie('tbits_campaign_privacy', 'seen', 21);
            
        } else if (privacy_cookie_val === 'seen') {
            
                $('.cookie-popup').show();
            
        } else if (privacy_cookie_val === 'accept') {
            ThirdPartyTracking.load(...loadArgs);
        }
    }
</script>
