                if(!__tnt.user.services && window.falconOffer){
                    try {
                        var sSubUrl = 'https://subscriberservicesdsi.lee.net/subscriberservices/Content/paymentpagesingle.aspx?Domain=richmond.com&SubscriberLevel={{level}}&Return=https%3A%2F%2Frichmond.com%2Fannouncements%2F'.replace('{{level}}', window.falconOffer.level);
