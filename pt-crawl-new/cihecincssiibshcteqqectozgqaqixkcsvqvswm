<style>
	#read-faq {
		font-size: 16px;
	}

	#gMap {
		width: 100%;
		height: 500px;
	}
</style>

<script type="text/javascript">
	function initMap() {
		
		$("#gMap").attr("style","background-image:url('/Resources/images/maps.jpg');background-size:cover");
		$("#gMap").wrap("<a href='https://maps.google.com/?z=15&q=333 Market Street, Harrisburg, PA, 17101' target='_blank'></a>")
	}


	$(function() {
		initMap()

		$('#state').select2({placeholder: 'State'});
		$('#country').select2({placeholder: 'Country'});

		$('#btn-submit').click(function(e) {
			e.preventDefault();

			if (!$('#email').val() && !$('#phone').val() && !$('#phoneMobile').val()) {
				_alert('Either your email address or a phone number is required.', 'Form Error', BootstrapDialog.TYPE_DANGER);
			}
			else {
				if ($('#contactForm').valid()) {
					var formVals = getFormVals('contactForm');

					$.post({
						url: '/Resources/cfc/OOR.cfc?method=formHasHTMLAjax&returnFormat=json',
						dataType: 'json',
						data: {
							FORM: JSON.stringify(formVals)
						},
						success: function(hasHTML) {
							if (hasHTML === true) {
								_alert('Invalid input detected. Please check your input and try again.');
							}
							else {
								$('#contactForm')[0].submit();
							}
						},
						error: ajaxError
					});
				}
			}
		});


		$("#contactForm").validate({
			// debug: true,
			rules: {
				nameFirst: 'required',
				nameLast: 'required',
				address1: 'required',
				city: 'required',
				state: 'required',
				zip: {
					required: true,
					pattern: /^\d{5}(-\d{4})?$/
				},
				email: {
					// required: true,
					email: true
				},
				emailConfirm: {
					// required: true,
					email: true,
					equalTo: '#email'
				},
				phone: {
					pattern: /^(\d{3}-){2}\d{4}$/
				},
				phoneMobile: {
					pattern: /^(\d{3}-){2}\d{4}$/
				},
				comments: 'required'
			},
			messages: {
				nameFirst: 'The First Name field is required.',
				nameLast: 'The Last Name field is required.',
				address1: 'The Address 1 field is required.',
				city: 'The City field is required.',
				state: 'The State field is required.',
				zip: {
					required: 'The Zip Code field is required.',
					pattern: 'Invalid Zip Code.  Must match the pattern 12345 or 12345-6789.'
				},
				email: {
					// required: 'The Email field is required.',
					email: 'Invalid email address.'
				},
				emailConfirm: {
					required: 'The Confirm Email field is required.',
					email: 'Invalid email (confirm) address.',
					equalTo: 'The Email and Confirm Email fields must match.'
				},
				phone: {
					pattern: 'Invalid Phone number.  Must match the pattern 123-456-7890.'
				},
				phoneMobile: {
					pattern: 'Invalid Mobile Phone number.  Must match the pattern 123-456-7890.'
				},
				comments: 'The Comments field is required.'
			}
		});
	});
</script>







<h1>Contact OOR</h1>



<div class="alert alert-info" id="read-faq">
	<strong>This form should be used for general inquiries only</strong>. Please do NOT use this form to submit any Right-to-Know requests or appeals
	(<a href="/RTKL/RequestForm.cfm">RTK requests for OOR records can be submitted here</a>, and <a href="/Appeals/AppealForm.cfm">appeals of denied RTK requests can be submitted here</a>).
	Also, before contacting the OOR, please <a href="/RTKL/About.cfm#faq">review our FAQ</a> to be sure your question has not already been answered.
</div>

