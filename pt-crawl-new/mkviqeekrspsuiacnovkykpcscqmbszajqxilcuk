<script type="text/javascript">

  function wwd_subs_get_querystring(name, url) {
    name = name.replace(/[\[]/, '\\\[').replace(/[\]]/, '\\\]');
    // Find the query you are looking for either in a query string or hash.
    // ref: https://regex101.com/r/7HYblz/1
    var regX = new RegExp('[\\?&]' + name + '=([^&#]*)'),
        results = regX.exec(url);
    if (results) {
      return decodeURIComponent(results[1].replace(/\+/g, ' '));
    }
    return false;
  }

  function wwd_subs_page_tracking(ad_url) {
    if (ad_url.indexOf('?') >= 0) {
      var promo = wwd_subs_get_querystring('utm_campaign', ad_url),
          campaign = wwd_subs_get_querystring('utm_content', ad_url),
          position = wwd_subs_get_querystring('utm_term', ad_url);
      if ('function' === typeof ga && promo && campaign && position) {
        ga('ec:addPromo', {
          'id': promo,
          'name': campaign,
          'creative': promo,
          'position': position,
        });
        ga('ec:setAction', 'promo_click');
        ga('send', 'event', 'Internal Promotions', 'click', campaign);
      }
    }
  }

