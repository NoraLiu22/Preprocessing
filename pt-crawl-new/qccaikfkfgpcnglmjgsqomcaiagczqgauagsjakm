      node.parentNode.insertBefore(gads, node);
    })();
    var chelsea_googletag = chelsea_googletag || {};
    chelsea_googletag.preCmd = chelsea_googletag.preCmd || [];
    chelsea_googletag.cmd = chelsea_googletag.cmd || [];
    chelsea_googletag.defineSlot = function(slot, size, id, fixWidth) {
      var multisize = false;
      if (typeof size === 'string' && typeof id === 'undefined') {
        id = size;
        size = false;
      } else {
        if (typeof size[0] === 'undefined') return;
        multisize = (typeof size[0][0] !== 'undefined');
        if (multisize) {
          if (typeof size[0][0][0] === 'undefined' || typeof size[0][1][0][0] === 'undefined') return;
          fixWidth = false;
          multisize = size;
          size = size[0][1][0];
        }
      }
      if (false === size) {
        chelsea_googletag.preCmd.push(function() {
          (function($) {
            $('body').append($('<div/>').attr('id', id));
          })(jQuery);
        });
        googletag.cmd.push(function() {
          googletag.defineOutOfPageSlot(slot, id).addService(googletag.pubads());
        });
      } else {
        var style = fixWidth ? ' style="width:' + size[0] + 'px;height:' + size[1] + 'px;"' : '';
        document.write('<div id="' + id + '"' + style + '></div>');
        googletag.cmd.push(function() {
          var adslot = googletag.defineSlot(slot, size, id).addService(googletag.pubads());
          if (multisize)
            adslot.defineSizeMapping(multisize);
        });
      }
      chelsea_googletag.cmd.push(function() {
        googletag.display(id);
      });
    }
    chelsea_googletag.showSkin = function(image, url, target) {
      if (chelsea_googletag.skinShown) {
        if (window.console)
          window.console.log('Warning: Chelsea Skin already shown. Skipping multiple calls');
      } else {
        chelsea_googletag.skinShown = true;
        (function($) {
          $(document).ready(function() {
            $(document).scroll(function() {
              var y = $(document).scrollTop();
              if (y >= 202)
                $('body').addClass('tlc-scrolled-down');
              else
                $('body').removeClass('tlc-scrolled-down');
            });

            $('body').addClass('tlc-background-skin');

            $('head').append($('<style type="text/css">body.tlc-background-skin #tlc-background-ad-unit .tlc-ad{background:url("' + image + '")local center top no-repeat;}body.tlc-background-skin.tlc-scrolled-down #tlc-background-ad-unit .tlc-ad{background:url("' + image + '")fixed center 52px no-repeat;}</style>'));

            var a = $('<a/>').attr('href', url);
            if (target)
              a.attr('target', '_' + target);
            $('#tlc-background-ad-unit').append($('<div/>').addClass('tlc-ad').append(a));
          });
        })(jQuery);
      }
    }
