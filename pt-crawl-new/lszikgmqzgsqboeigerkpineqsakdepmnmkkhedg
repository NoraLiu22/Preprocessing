<script src="https://unpkg.com/@tryghost/content-api@1.3.7/umd/content-api.min.js"></script>
<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/locale/es.min.js"></script>

<script type="text/javascript" src="https://blog.duolingo.com/assets/built/ghost-search.min.js?v=320138c439"></script>
<script>
    fetch(`https://duolingo.ghost.io/ghost/api/v3/content/posts/?key=09f4c837d9944ae7f4e70f59a8&filter=tag:${(document.documentElement.lang || "en") === "en" ? "-[hash-lang-es,hash-lang-de,hash-lang-pt,hash-lang-ja]" : "hash-lang-" + (document.documentElement.lang || "en")}&fields=title,slug,html,plaintext,primary_tag,primary_author,meta_title,meta_description&limit=all&include=authors,tags`)
        .then(response => response.json())
        .then(data => {
            const blogPosts = data.posts;
            function search(query) {
                const searchRegex = new RegExp(query, 'gi');
                const fieldsToSearch = ['primary_author', 'primary_tag', 'html', 'plaintext', 'title', 'slug', 'meta_title', 'meta_description'];
                return blogPosts.filter(post => fieldsToSearch.some(field => {
                    if (field === 'primary_author' && post.primary_author)
                        return post.primary_author.name.match(searchRegex);
                    else if (field === 'primary_tag' && post.primary_tag)
                        return post.primary_tag.name.match(searchRegex);
                    else
                        return post[field] && post[field].match(searchRegex);
                }));
            }
            function displayResults(results) {
                const resultsElement = document.getElementById('ghost-search-results');
                resultsElement.innerHTML = '';
                if (results.length === 0) {
                    let lang = document.documentElement.lang || "en";
                    let message;
                    if (lang === "es") {
                        message = '<p>No se encontraron resultados</p>';
                    } else if (lang === "de") {
                        message = '<p>Keine Ergebnisse</p>';
                    } else if (lang === "pt") {
                        message = '<p>Nenhum resultado encontrado</p>';
                    } else if (lang === "ja") {
                        message = '<p>検索結果なし</p>';
                    } else {
                        message = '<p>No results</p>';
                    }
                    resultsElement.innerHTML = message;
                } else {
                    results.forEach(result => {
                        let displayString = '<a href="/' + result.slug + '/">' + result.title + '</a>';
                        resultsElement.innerHTML += displayString;
                    });
                }
            }
            document.getElementById('ghost-search-field').addEventListener('input', function (event) {
                const query = event.target.value.trim();
                const resultsElement = document.getElementById('ghost-search-results');
                if (query === '') {
                    resultsElement.innerHTML = '';
                } else {
                    const searchResults = search(query);
                    displayResults(searchResults);
                }
            });
        })
        .catch(error => {
            console.error('Error fetching blog posts:', error);
        });
</script>

<script>
$(document).ready(function () {
    // Mobile Menu Trigger
    $('.hamburger').click(function () {
        $('body').toggleClass('mobile-menu-open');
        $(this).toggleClass('active');
        //$('.mobile-menu').fadeToggle();
    });
    // Search Panel Triggers
    $('.search-button').click(function (e) {
        e.preventDefault();
        $('body').addClass('search-panel-open');
        $('.search-panel').addClass('active');
        $('.search-overlay').fadeIn();
    });
    $('.search-panel-close').click(function (e) {
        e.preventDefault();
        $('body').removeClass('search-panel-open');
        $('.search-panel').removeClass('active');
        $('.search-overlay').fadeOut();
    });
    $('.search-overlay').click(function () {
        $('body').removeClass('search-panel-open');
        $('.search-panel').removeClass('active');
        $(this).fadeOut();
    });
});
</script>

