<script>

document.addEventListener('DOMContentLoaded', function() {
    const input = document.getElementById('autocomplete-input');
    const resultsContainer = document.getElementById('autocomplete-results');

    function debounce(func, wait) {
        let timeout;
        return function(...args) {
            const context = this;
            const later = function() {
                timeout = null;
                func.apply(context, args);
            };
            clearTimeout(timeout);
            timeout = setTimeout(later, wait);
        };
    }

    // Function to log the search
    function logSearch(query, match) {
        fetch(`/searches/log/`, {
            method: 'POST',
            headers: {
                'X-CSRFToken': getCookie('csrftoken'),
                'Content-Type': 'application/json',
                'X-Requested-With': 'XMLHttpRequest',
            },
            body: JSON.stringify({ query: query, match: match })
        });
    }

    // Function to get the CSRF token
    function getCookie(name) {
        let cookieValue = null;
        if (document.cookie && document.cookie !== '') {
            const cookies = document.cookie.split(';');
            for (let i = 0; i < cookies.length; i++) {
                const cookie = cookies[i].trim();
                if (cookie.substring(0, name.length + 1) === (name + '=')) {
                    cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                    break;
                }
            }
        }
        return cookieValue;
    }

    // input.addEventListener('input', function() {
    const debouncedInputHandler = debounce(function(event) {
        let inputVal = this.value;

        // Clear previous results
        resultsContainer.innerHTML = '';

        // Hide results if input is less than 1 character
        if (inputVal.length < 1) {
            resultsContainer.classList.remove('show');
            document.getElementById('search-button').style.borderRadius = '20px 0 0 20px'; 
            document.getElementById('autocomplete-input').style.borderRadius = '0 20px 20px 0'; 
            return;
        }

        // Fetch and display autocomplete suggestions
        fetch(`/search/autocomplete/?term=${inputVal}`)
            .then(response => response.json())
            .then(data => {
                data.forEach(item => {
                    // Create a container div for the result item
                    let resultDiv = document.createElement('div');
                    resultDiv.classList.add('dropdown-item', 'd-flex', 'justify-content-between', 'align-items-center', 'cursor-pointer');

                    // Create a text element for the label
                    let textElement = document.createElement('span');
                    textElement.textContent = item.label;
                    textElement.classList.add('me-2', 'text-dark');

                    // Create the category pill
                    let categoryPill = document.createElement('span');
                    categoryPill.textContent = item.category;
                    categoryPill.classList.add('badge', 'border', 'px-3', 'bg-light', 'text-dark');

                    // Append elements to the container div
                    resultDiv.appendChild(textElement);
                    resultDiv.appendChild(categoryPill);

                    // Add click event listener to resultDiv
                    resultDiv.addEventListener('click', function() {
                        logSearch(item.label, true);
                        window.location.href = item.url; // Redirect to the item's URL
                    });

                    // Append the container div to the results container
                    resultsContainer.appendChild(resultDiv);
                });

                if (data.length) {
                    resultsContainer.classList.add('show');
                    document.getElementById('search-button').style.borderRadius = '20px 0 0 0'; 
                    document.getElementById('autocomplete-input').style.borderRadius = '0 20px 0 0';  
                } else {
                    resultsContainer.classList.remove('show');
                    document.getElementById('search-button').style.borderRadius = '20px 0 0 20px'; 
                    document.getElementById('autocomplete-input').style.borderRadius = '0 20px 20px 0'; 
                }
            });
    }, 100);

    input.addEventListener('input', debouncedInputHandler);

    // Hide results when clicked outside
    document.addEventListener('click', function(event) {
        if (!input.contains(event.target)) {
            resultsContainer.classList.remove('show');
            document.getElementById('search-button').style.borderRadius = '20px 0 0 20px'; 
            document.getElementById('autocomplete-input').style.borderRadius = '0 20px 20px 0'; 
        }
    });

    // Log the search when the search form is submitted
    const searchForm = document.querySelector('.searchbar');
    if (searchForm) {
        searchForm.addEventListener('submit', function(e) {
            logSearch(input.value, false);
        });
    }

});

</script>



