<span class="linenos" data-line="20"></span><span class="w">                    </span><span class="nt">"type"</span><span class="p">:</span><span class="w"> </span><span class="s2">"function call"</span><span class="p">,</span>
<span class="linenos" data-line="21"></span><span class="w">                    </span><span class="nt">"function"</span><span class="p">:</span><span class="w"> </span><span class="s2">"typed pair"</span><span class="p">,</span>
