<!-- カートイン件数表示 -->
<script type="text/javascript" >
    $w(document).ready(function ()
    {
        UpdateCartInCount();
    });
    function UpdateCartInCount()
    {
        var selector = "#cartArea .countIco";
        $w(selector).hide();
        if (navigator.cookieEnabled)
        {
            var cookienm = "glue_cartin_count";
            if (document.cookie.indexOf(cookienm + "=", 0) > -1) 
            {
                var tmp = document.cookie.split('; ');
                var arr = new Array();
                for(var i=0;i<tmp.length;i++){
                    var data = tmp[i].split('=');
                    arr[data[0]] = decodeURIComponent(data[1]);
                }
                if(0 < arr[cookienm] && arr[cookienm] <= 99)
                {
                    $w(selector).text(arr[cookienm]);
                    $w(selector).show();
                }
                else if( 99 < arr[cookienm] )
                {
                    $w(selector).text('99+');
                    $w(selector).show();
                }
            }
        }
    }
</script>
<script>
    var apiUrl = "";
    var uidCookieName = "";
    var loginTokenName = "";

    jQuery(document).ready(function () {
        ShowNotice();

        jQuery.ajax({
            url: Trance4Ssl("ec/Customer/GetDispCustomerNoticeCount"),
            type: 'GET',
        }).done(function (data, textStatus, jqXHR) {
            var count = Number(data);
            if (count > 9) {
                data = "9+";
            }
            jQuery("#NotReadCount").html(data);
        });

        var off = jQuery('#NoticeBtn').offset();
        jQuery("#MenuPopup").css("left", off.left - 50);

        var bHeight = window.innerHeight;
        jQuery("#MenuPopup").css("max-height", bHeight - 250);
    });

    jQuery(window).resize(function () {
        var off = jQuery('#NoticeBtn').offset();
        jQuery("#MenuPopup").css("left", off.left - 50);

        var bHeight = window.innerHeight;
        jQuery("#MenuPopup").css("max-height", bHeight - 250);
    });

    function CloseNotice() {
        jQuery("#MenuPopup").hide();
        jQuery("#MenuPopupBack").hide();
        jQuery("body").css("overflow", "");
        jQuery("body").css("padding-right", "0px");
        jQuery(".popupMainArea").find('.fa-circle').remove();
    };

    function OpenNotice() {
        jQuery("#MenuPopup").show();
        jQuery("#MenuPopupBack").show();
        jQuery("body").css("overflow", "hidden");

        var cHeight = document.getElementById('bodyControl').scrollHeight;
        var bHeight = window.innerHeight;

        console.log("cHeight = " + cHeight + ", bHeight = " + bHeight);

        if (cHeight > bHeight) {
            jQuery("body").css("padding-right", "17px");
        }
    };

    var limit = 8;
    var index = 0;

    function ShowNotice() {
        jQuery("#NextShowBtnArea_" + (index - 1)).hide();

        jQuery.ajax({
            url: Trance4Ssl("ec/Customer/GetDispCustomerNoticeList?offset=") + limit * index,
            type: 'GET',
        }).done(function (data, textStatus, jqXHR) {

            jQuery("#ShowNoticeArea_" + index).html(data);

            index++;
        });
    };

    var procesingFlg = false;

    function NoticeLing(url, id) {
        console.log("url:" + url);
        console.log("customer_notice_id = " + id);
        if (!procesingFlg) {
            procesingFlg = true;
            jQuery("#NotReadCircle_" + id).remove();

            jQuery.ajax({
                url: Trance4Ssl("ec/Customer/PostCustomerNoticeAlreadyRead"),
                type: 'POST',
                data: {
                    customerNoticeId: id
                },
            }).done(function (data, textStatus, jqXHR) {
                console.log("Update Success!!");
                location.href = url;
            }).always(function (data) {
                procesingFlg = false;
            });
        }
    };

    var procesingAllFlg = false;

    function NoticeAllAlreadyDisplayData() {
        if (!procesingAllFlg) {
            procesingAllFlg = true;
            jQuery("#NoticeAllAlready").remove();
            const maxDiv = document.getElementById('maxCustomerNoticeId');
            var maxCustomerNoticeId = maxDiv.dataset.id;
            var token = jQuery('input[name="__RequestVerificationToken"]').val();

            jQuery.ajax({
                url: Trance4Ssl("ec/Customer/PostCustomerNoticeAlreadyRead"),
                type: 'POST',
                data: {
                    __RequestVerificationToken: token,
                    maxCustomerNoticeId: maxCustomerNoticeId
                },
            }).done(function (data, textStatus, jqXHR) {
                console.log("Update Success!!");
                CloseNotice();
                jQuery("#NotReadCount").html(0);
            }).always(function (data) {
                procesingAllFlg = false;
            });
        }
    };
</script>

		

