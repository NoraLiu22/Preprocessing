        sort: {
            target: '#js-sort',
            option: [
                { label: 'Relevance', val: 'rank' },
                { label: 'Top Sellers', val: 'popular' },
                { label: 'New Arrivals', val: 'new' },
                { label: 'Latest First', val: 'rel' },
                { label: 'Oldest First', val: '-rel' },
                { label: 'Lowest Price First', val: '-price' },
