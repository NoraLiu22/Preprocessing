        },
        rel: {
            target: '#js-rel',
            option: [
                { label: 'All', query: 'opt.on_preorder=0&opt.on_released=0&range.rel=&term.release_month=' },
                { label: 'Pre-order', query: 'opt.on_preorder=1&opt.on_released=0&range.rel=&term.release_month='},
                { label: 'Released items', query: 'opt.on_preorder=0&opt.on_released=1&range.rel=&term.release_month=' }
                ,{ label: 'April', query: 'opt.on_preorder=0&opt.on_released=0&range.rel=&term.release_month=4&term.release_year=2024' }
                ,{ label: 'May', query: 'opt.on_preorder=0&opt.on_released=0&range.rel=&term.release_month=5&term.release_year=2024' }
                ,{ label: 'June', query: 'opt.on_preorder=0&opt.on_released=0&range.rel=&term.release_month=6&term.release_year=2024' }
                ,{ label: 'July', query: 'opt.on_preorder=0&opt.on_released=0&range.rel=&term.release_month=7&term.release_year=2024' }
