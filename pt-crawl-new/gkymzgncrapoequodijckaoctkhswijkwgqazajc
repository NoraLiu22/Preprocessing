                })
                .then(function(openWeb) {
                    if (openWeb && openWeb.messages_count) {
                        document.getElementById("openWeb-count-914930ee-f034-11ee-a03c-777ac9788471-top").innerHTML = openWeb.messages_count['914930ee-f034-11ee-a03c-777ac9788471'];
                    }
                });
            }
