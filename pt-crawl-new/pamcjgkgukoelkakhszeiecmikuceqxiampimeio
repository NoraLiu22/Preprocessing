    <script>
        jQuery(document).ready(function($){
            window.lee_fetched_services = false;

            try {
                if(typeof window.falconOffer != 'object') throw new error('Offer not defined');
                
                var sHtml = $('#lee-service-template').html();
                
                $.each(window.falconOffer, function(k, v) {
                    if(k === 'html'){
                        v = v.replace(new RegExp('{{domain}}', 'gi'), 'richmond.com')
                            .replace(new RegExp('{{site_name}}', 'gi'), 'Richmond Times-Dispatch')
                            .replace(new RegExp('{{business_name}}', 'gi'), 'Richmond Times-Dispatch')
                            .replace(new RegExp('{{site_phone}}', 'gi'), '800-468-3382');
                    }
                    if(v){
                        sHtml = sHtml.replace(new RegExp('{{'+k+'}}', 'gi'), v);
                    } else {
                        sHtml = sHtml.replace(new RegExp('{{'+k+'}}', 'gi'), '');
                    }
                });
                 
                $('#lee-services-list .packages').html(sHtml).promise().then(function(){
                    $('#lee-services-list .loading').hide();
                    $('#lee-services-list .packages').css('opacity', 1);
                });
            } catch (e) {
                if(window.console) console.error(e);
                $('#lee-services-list .loading').hide();
                $('#lee-services-list .packages').css('opacity', 1);
            }

            window.lee_fetched_services = true;
        });
    </script></div>
                </div>
            </div>
        </div>
    </div><script>
        window.lee_meter_loaded = false;
        window.lee_regwall_loaded = false;
        window.TNCMS = window.TNCMS || {};
        window.TNCMS.Access = TNCMS.Access || [];
        
        function lee_loadRegWallModal(){
            $('#lee-registration-wall-modal').modal({
                show: true,
                backdrop: 'static',
                keyboard: false
            });
        }
        
        window.TNCMS.Access.push(function(){
            TNCMS.Access.checkAccess(function(oResp){
                if(!__tnt.user.services && window.sessionStorage.getItem('lee_gv_f9d6005c-ac7f-5051-8fcc-481ac5e92d35')) return false;
                var sCase = 'paywall';
                if(oResp.required || window.show_dimissable_registration){
                    __tnt.log('--- Aud+ ---');
                    __tnt.log(oResp);
                    if(window.show_dimissable_registration) { 
                        sCase = 'regwall';
                    } else {
                        var sName = oResp.access_rule.name.toLowerCase();
                        if(sName.match('registration')){ 
                            sCase = 'regwall';
                            if(oResp.access_rule.wall_behavior === 'nag'){
                                window.show_dimissable_registration = true;
                            }
                        }                  
                    }

                    switch(sCase){
                        case 'regwall':
                            var bHasExclusion = false;
                            var bFrameLogin = false;
                            __tnt.log('LEE: load regwall');
                            window.lee_regwall_loaded = true;
                            
                            
                            
                            $('#reg-wall-close-soft').show();
                            
                            if(__tnt.user.loggedIn){
                                if(!Cookies.get('lee_regwall_signup')){
                                    $('#lee-newsletter-title').show();
                                    $('#regwall-newsletter').show();
                                } else {
                                    bHasExclusion = true;
                                }
                            } else {
                                if(oResp.access_rule.message && oResp.access_rule.message.title){
                                    $('#lee-reg-wall-title').text(oResp.access_rule.message.title);
                                }
                                bFrameLogin = true;
                                $('#lee-registration-wall').addClass('login');
                                $('#lee-regwall-login-frame').attr('src', $('#lee-regwall-login-frame').attr('src-set'));
                                $('#lee-reg-wall-title').show();
                                $('#regwall-login-frame').show();
                            }

                            if(!bHasExclusion){
                                if(bFrameLogin){
                                    $('#lee-regwall-login-frame').on('load', function(){
                                        lee_loadRegWallModal();
                                    });
                                } else {
                                    lee_loadRegWallModal();
                                }
                                __tnt.trackEvent({
                                    'category': 'registration',
                                    'action': 'wall',
                                    'label':'registration wall',
                                    'value': 1
                                });
                            }
                        break;
                        
                        default:
                            __tnt.log('LEE: load paywall');
                            window.lee_meter_loaded = true;
                            
                            if(__tnt.user.loggedIn && Cookies.get('tncms-user')){
                                $('#lee-services-modal').find('.sub-login').hide();
                            }
                            
                            if(oResp.access_rule.message && oResp.access_rule.message.title){
                                $('#lee-subscription-wall-title').text(oResp.access_rule.message.title);
                            }
                            if(oResp.access_rule.message && oResp.access_rule.message.description){
                                $('#lee-subscription-wall-text').html('<p>'+oResp.access_rule.message.description+'</p>');
                            }
                            
                            if(oResp.offer_config && oResp.offer_config.name){
                                if(typeof window.falconOffer[oResp.offer_config.name+'_title'] != 'undefined' && typeof window.falconOffer[oResp.offer_config.name+'_price'] != 'undefined' && typeof window.falconOffer[oResp.offer_config.name+'_duration'] != 'undefined'){
                                    jQuery(document).ready(function($){
                                        $('#lee-services-list').find('.promotion').html(window.falconOffer[oResp.offer_config.name+'_title']);
                                        
                                        $('#lee-services-list').find('a').each(function(){
                                            var a = $(this).attr('href').replace('?','?Price='+window.falconOffer[oResp.offer_config.name+'_price']+'&duration='+window.falconOffer[oResp.offer_config.name+'_duration']+'&');
                                            $(this).attr('href', a);
                                        });
                                            
                                        if(typeof window.falconOffer[oResp.offer_config.name+'_tagline'] != 'undefined'){
                                            $('#lee-services-list').find('.tagline').html(window.falconOffer[oResp.offer_config.name+'_tagline']);
                                        }
                                    });
                                }
                            }
                            
                            $('#lee-subscription-wall-modal').modal({
                                show: true,
                                backdrop: 'static',
                                keyboard: false
                            });
                            
                            dataLayer.push({
                                    'ecommerce':{
                                        'impressions': [{
                                            'id': window.falconOffer.level,
                                            'name': window.falconOffer.title,
                                            'price': window.falconOffer.promotional_price || window.falconOffer.start_price,
                                            'brand': "richmond.com",
                                            'category': 'subscription',
                                            'list': 'Block',
                                            'position': 1
                                        }]
                                    },
                                    'event':'tncms.event.trigger',
                                    'tncms': {
                                        'event': {
                                            'trigger': {
                                                'action': 'productImpressionsFinished',
                                                'category': 'subscription',
                                                'label':'services displayed to user'
                                            }
                                        }
                                    }
                                });
                            __tnt.trackEvent({
                                'category': 'subscription',
                                'action': 'wall',
                                'label': 'subscription wall',
                                'value': 1
                            });
                        break;
                    }
                } else {
                    __tnt.log('LEE: free view');
                    const leeNoWall = new Event('lee-free-view');
                    window.dispatchEvent(leeNoWall);
                    
                    if(!__tnt.user.services){
                        __tnt.trackEvent({
                            'category': 'subscription',
                            'action': 'free view',
                            'label': 'under subscription limit',
                            'value': 1
                        });
                        __tnt.trackEvent({
                            'category': 'registration',
                            'action': 'free view',
                            'label': 'under registration limit',
                            'value': 1
                        });
                    }
                }
            });
        });
    </script>
