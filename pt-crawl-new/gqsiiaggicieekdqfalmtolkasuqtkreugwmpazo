                        <div class="wu_tips_wrap_item wu_tips_wrap_item_100">
                            <input class="wu_submit_tips_page" type="submit"  name="wu_submit_tips_page" id="wu_submit_tips_page" value="SUBMIT YOUR TIPS"/>
                        </div>

                    </div></form>
        </div><script src="https://www.google.com/recaptcha/api.js?render=6LczN3UnAAAAAGSXKnBLbvGO4a1mBxCfvSv3J1So"></script>
                    <!-- Loader -->
                    <div class="wu_loader">
                        <div class="wu_loader_content"></div>
                    </div>

                    <!-- Success Modal -->
                    <div class="wu_modal">
                        <div class="wu_modal_content">
                            <span class="wu_close">&times;</span>
                            <p class="wu_show_message"></p>
                        </div>
                    </div>
                    
        <script>
        jQuery(document).ready(function($){
            const wu_f_name = "";
            const wu_select_sex = "";
            const wu_select_race = "";

            // Cache frequently used elements
            const $wu_f_name = $("#wu_f_name");
            const $wu_full_name = $("#wu_full_name");
            const $wu_last_seen_date = $("#wu_last_seen_date");
            const $wu_f_sex = $("#wu_f_sex");
            const $wu_f_race = $("#wu_f_race");
            const $wu_f_dob = $("#wu_f_dob");
            const $wu_f_address = $("#wu_f_address");
            const $wu_f_city = $("#wu_f_city");
            const $wu_f_weapon = $("input[name=\"wu_f_weapon\"]");
            const $warrant_unit_state = $("#wu_f_state");
            const $warrant_unit_zip = $("#wu_f_zip");

            const $wu_f_height = $("#wu_f_height");
            const $wu_f_weight = $("#wu_f_weight");
            const $loader = $(".wu_loader");
            const $modal = $(".wu_modal");
            const $modalContent = $modal.find(".wu_modal_content");
            const $showMessage = $modal.find(".wu_show_message");

            const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;

            if (wu_f_name) {
                $wu_f_name.find(`option[value="${wu_f_name}"]`).prop("selected", true);
            }

            if (wu_select_sex) {
                $wu_f_sex.find(`option[value="${wu_select_sex}"]`).prop("selected", true);
            }

            if (wu_select_race) {
                $wu_f_race.find(`option[value="${wu_select_race}"]`).prop("selected", true);
            }

            $("#wu_tips_form").submit(function(event) {
                event.preventDefault();

                $loader.show();
                $modal.hide();

                function showError(selector, error) {
                    if (selector.val().trim()) {
                        selector.removeClass("wu_error_notice");
                    } else {
                        wu_error_validation += error + " <br>";
                        selector.addClass("wu_error_notice");
                    }
                }

                function showErrorRadio(selector, error) {
                    // let $selector = $(selector);
                    let isRadioChecked = selector.is(":checked");
                
                    if (isRadioChecked) {
                        $(".is_weapon").removeClass("wu_error_notice_radio");
                    } else {
                        wu_error_validation += error + " <br>";
                        $(".is_weapon").addClass("wu_error_notice_radio");
                    }
                }

                function showErrorEmail(selector, error) {
                    if (selector.val()) {
                        if (!selector.val().match(emailRegex)) {
                            selector.addClass("wu_error_notice");  
                            wu_error_validation += error + " <br>";
                        } else {
                            selector.removeClass("wu_error_notice"); 
                        }
                    } else {
                        selector.removeClass("wu_error_notice"); 
                    }
                }

                function fileError($fileInput, maxSizeMB, maxTypeError, allowedTypes, typeError) {                
                    if ($fileInput[0].files && $fileInput[0].files[0]) {
                        var file = $fileInput[0].files[0];
                        var fileSizeMB = file.size / 1024 / 1024;
                        var fileType = file.type;

                        $fileInput.removeClass("wu_error_notice");

                        if (fileSizeMB > maxSizeMB) {
                            $fileInput.addClass("wu_error_notice");
                            wu_error_validation += maxTypeError;
                        }
                
                        if (allowedTypes.indexOf(fileType) === -1) {
                            $fileInput.addClass("wu_error_notice");
                            wu_error_validation += (wu_error_validation ? "<br>" : "") + typeError;
                        }
                    } else {
                        $fileInput.removeClass("wu_error_notice");
                    }
                }



                let wu_error_validation = "";

                showError($wu_full_name, "⚠️ Fugitive Name Is Required");

                fileError($("#wu_upload_image"), 7, "⚠️ Uploaded Image Size Is Bigger. It needs to be less than 7MB<br>", ["image/jpeg", "image/png", "image/gif"], "⚠️ Invalid Image file type. Only JPEG, PNG, GIF allowed<br>");
                fileError($("#wu_upload_video"), 40, "⚠️ Uploaded Video Size Is Bigger. It needs to be less than 40MB<br>", ["video/mp4", "video/webm", "video/ogg"], "⚠️ Invalid Video file type. Only MP4, WebM, OGG allowed<br>");


                showError($wu_last_seen_date, "⚠️ Last Seen Date  Is Required");
                showError($("#wu_f_address"), "⚠️ Address Last Seen Is Required");
                showError($wu_f_sex, "⚠️ Identifies As Is Required");
                showError($wu_f_race, "⚠️ Fugitive Race Is Required");
                showErrorRadio($wu_f_weapon, "⚠️ Does Fugitive Possess Weapons Is Required ");

                //showError($("#wu_f_who_else"), "⚠️ Who else resides at this address? Is Required");
 
                //showError($("#wu_f_place_work"), "⚠️ Fugitive Place of Work Is Required");
                showErrorEmail($("#wu_f_email"), "⚠️ Please Enter a valid Fugitive Email");
                showErrorEmail($("#wu_f_tipster_email"), "⚠️ Please Enter a valid Tipster Email");
                //showError($("#wu_f_cap_two"), "⚠️ Please Fill Out The Varification Code.");

                if (wu_error_validation) {
                    $loader.hide();
                    $modal.show();
                    $modalContent.addClass("wu_error_message");
                    $showMessage.html(wu_error_validation);
                    wu_error_validation = "";
                    return;
                }

                const siteKey = "6LczN3UnAAAAAGSXKnBLbvGO4a1mBxCfvSv3J1So"; // Replace with your actual reCAPTCHA site key
                const ajaxUrl = "https://phillysheriff.com/wp-admin/admin-ajax.php"; // Replace with your actual AJAX URL

                if (siteKey) {
                    setTimeout(function() {
                        grecaptcha.ready(function() {
                            grecaptcha.execute(siteKey, { action: "submit" }).then(function(token) {
                                submitFormWithRecaptcha(token);
                            });
                        });
                    }, 2000); // 2 seconds delay
                } else {
                    setTimeout(function() {
                        submitFormWithRecaptcha(""); // Submit the form without reCAPTCHA token
                    }, 2000); // 2 seconds delay
                }

                function submitFormWithRecaptcha(token) {
                    // Create FormData from the form
                    const formData = new FormData(document.getElementById("wu_tips_form"));
                
                    // Append additional data
                    formData.append("action", "wu_tips_form_submission");
                    formData.append("recaptcha_token", token);
                
                    // Submit the form via Ajax
                    $.ajax({
                        type: "POST",
                        url: ajaxUrl,
                        data: formData,
                        contentType: false, // Necessary for FormData
                        processData: false, // Necessary for FormData
                        success: function(response) {
                            $loader.hide();
                            const { success, data } = response;
                            if (success) {
                                $modalContent.removeClass("wu_error_message");
                            } else {
                                $modalContent.addClass("wu_error_message");
                            }
                            $modal.show();
                            $showMessage.html(data);
                        },
                        error: function(xhr, status, error) {
                            $modal.show();
                            $modalContent.addClass("wu_error_message");
                            $showMessage.html(xhr.responseText);
                        }
                    });
                }
                
            });

            // name change handling
            $wu_f_name.on("change", function() {
                const postId = $(this).val();
                if (postId) {
                    $loader.show();
                    $.ajax({
                        url: "https://phillysheriff.com/wp-admin/admin-ajax.php",
                        type: "POST",
                        data: {
                            action: "wu_tips_name_change_callback",
                            post_id: postId
                        },
                        success: function(response) {
                            $loader.hide();
                            const { wu_full_name, warrant_unit_dob, warrant_unit_address, warrant_unit_district, warrant_unit_state, warrant_unit_zip, warrant_unit_sex, warrant_unit_race, warrant_unit_height, warrant_unit_weight } = JSON.parse(response);
                                if (warrant_unit_dob) { $wu_f_dob.prop("value", warrant_unit_dob); }else{ $wu_f_dob.prop("value", ""); }
                                if (wu_full_name) { $wu_full_name.prop("value", wu_full_name); }else{ $wu_full_name.prop("value", ""); }
                                if (warrant_unit_address) { $wu_f_address.val(warrant_unit_address); }else{ $wu_f_address.val(""); }
                                if (warrant_unit_district) { $wu_f_city.val(warrant_unit_district); }else{ $wu_f_city.val(""); }

                                if (warrant_unit_state) { $warrant_unit_state.val(warrant_unit_state); }else{ $warrant_unit_state.val(""); }
                                if (warrant_unit_zip) { $warrant_unit_zip.val(warrant_unit_zip); }else{ $warrant_unit_zip.val(""); }

                                if (warrant_unit_sex) { $wu_f_sex.find(`option[value="${warrant_unit_sex}"]`).prop("selected", true); }
                                else { $wu_f_sex.find(`option[value=""]`).prop("selected", true); }
                                if (warrant_unit_race) { $wu_f_race.find(`option[value="${warrant_unit_race}"]`).prop("selected", true); }
                                else { $wu_f_race.find(`option[value=""]`).prop("selected", true); }
                                if (warrant_unit_height) { $wu_f_height.val(warrant_unit_height); }else{ $wu_f_height.val(""); }
                                if (warrant_unit_weight) { $wu_f_weight.val(warrant_unit_weight); }else{ $wu_f_weight.val(""); }
                        }
                    });
                }
            });

            // Close the success modal when the "X" button is clicked
            $(".wu_close").on("click", function () {
                $modal.hide();
                $modalContent.removeClass("wu_error_message");
                $showMessage.html("");
            });

        });
        </script>
        

				</section> <!-- </section> class="post_content" itemprop="articleBody"> -->		<div class="post_info post_info_bottom post_info_share post_info_share_horizontal"><div class="sc_socials sc_socials_size_small sc_socials_share sc_socials_dir_horizontal"><span class="share_caption">Share:</span><div class="sc_socials_item social_item_popup"><a href="https://twitter.com/intent/tweet?text=title&#038;url=link" class="social_icons social_twitter" data-link="https://twitter.com/intent/tweet?text=Submit+Tips&#038;url=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F" href="https://twitter.com/intent/tweet?text=Submit+Tips&#038;url=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F" data-link="https://twitter.com/intent/tweet?text=title&#038;url=link"><span class="icon-twitter"></span></a></div><div class="sc_socials_item social_item_popup"><a href="http://www.facebook.com/sharer.php?u=link" class="social_icons social_facebook" data-link="http://www.facebook.com/sharer.php?u=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F" href="http://www.facebook.com/sharer.php?u=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F" data-link="http://www.facebook.com/sharer.php?u=link"><span class="icon-facebook"></span></a></div><div class="sc_socials_item social_item_popup"><a href="http://pinterest.com/pin/create/link/?url=link%20" class="social_icons social_pinterest-circled" data-link="http://pinterest.com/pin/create/link/?url=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F%20" href="http://pinterest.com/pin/create/link/?url=https%3A%2F%2Fphillysheriff.com%2Fsubmit-tips%2F%20" data-link="http://pinterest.com/pin/create/link/?url=link%20"><span class="icon-pinterest-circled"></span></a></div><div class="sc_socials_item social_item_popup"><a href="http://youtube.com" class="social_icons social_youtube" data-link="http://youtube.com" href="http://youtube.com" data-link="http://youtube.com"><span class="icon-youtube"></span></a></div></div></div>
		
