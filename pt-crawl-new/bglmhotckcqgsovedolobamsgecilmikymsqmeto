    <div class="card-container">
        <div class="card-image">
            <a href="https://leeaws.com/ndc/site/richmond.com"><img src="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/live/libraries/flex/components/lee_ds_v3/resources/images/ndc-hero.png?_dc=1714629622" class="img-responsive" alt="" /></a>
        </div>
        <div class="card-body">
            <div class="card-headline">
                <h2 class="tnt-headline">Flip page-by-page through Times-Dispatch history</h2>
            </div>
            <div class="card-lead margin-bottom" style="display:block!important">
                <p>The Richmond Times-Dispatch is available on Newspapers.com, an online home to millions of historical newspaper pages from around the United States.</p>
                <div id="archive-login" style="position:relative;width:100%;height:1px;top:-8em"></div>
                <div id="archive-access">
                   <p>
                        Current digital subscribers have
                        <b>free access</b>
                        to 2 years of the Richmond Times-Dispatch archives.
                    </p>
                    <p><span id="archive-access-link-1396739" data-login-url="https://richmond.com/users/login/?referer_url=https%3A%2F%2Frichmond.com%2Fnews%2Farchives%2F" data-signup-url="https://richmond.com/users/signup/?referer_url=https%253A%252F%252Frichmond.com%252Fnews%252Farchives%252F">
                    <span class="btn btn-block btn-primary"><i class="fas tnt-spinner tnt-pulse"></i></span>
                    </span></p>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
(function(g,i,z,m,o,e,s) {
    var u_cookie, u_services, isBn=!!window.BNTech,login_url,signup_url;
    function set_button(text,url=null,type='primary') {
        var btn=g.createElement(type=='primary'?'a':'span');
        btn.className="btn btn-block btn-"+type;
        if(btn.tagName=='A') btn.href=url;
        btn.innerText=text;
        if(btn.tagName=='A') btn.setAttribute('rel','nofollow');
        z.innerHTML='';
        z.appendChild(btn);
    }
    function gc(c){if (!c){return null}return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*"+encodeURIComponent(c).replace(/[\-\.\+\*]/g,"\\$&")+"\\s*\\=\\s*([^;]*).*$)|^.*$"),"$1"))||null}
    function load_access(u,s) {
        fetch('https://leeaws.com/ndc/',{'method':'GET','headers':{'x-tncms-user':u,'x-tncms-services':s}}).then(r => r.json())
        .then(function(d) {
            console.log(d);
            switch(d.STS) {
                case '200':
                    set_button(d.text,d.url);
                    break;
                case '404':
                    set_button(d.text,d.url,'danger');
                    break;
                default:
                    console.error(d.STS,':',d.MSG);
                    set_button('Archive offline, try again later.',null);
                    break;
            }
        });
    }
    z = g.getElementById("archive-access-link-1396739");
    login_url = z.getAttribute('data-login-url');
    signup_url = z.getAttribute('data-signup-url');

    if(isBn) {
        BNTech.Queue("sso", function() {
            let bnToken = BNTech.Cookie.Get(BNTech.Cookie.Name());
            let bnServices = '';
            let bnUserData = BNTech.SSO.User.Data();
            let bnSubscription = (!!bnToken && !!bnUserData && !!bnUserData.Subscription) ? bnUserData.Subscription : [];
            bnSubscription.forEach(function(e) {
                if (bnServices!='') bnServices+=',';
                if (e!='FREG') bnServices+=e;
            });

            if (!bnToken) {
                set_button("Log In to access the archives",login_url);
            }
            else if (!bnServices) {
                set_button("Subscribe to access our archives",signup_url);
            }
            else {
                u_cookie=bnToken;
                u_services=bnServices;
                load_access(u_cookie,u_services);
            }
        });
    }
    else {
        if (!__tnt.user.loggedIn) {
            set_button("Log In to access the archives",login_url);
        }
        else if (!__tnt.user.services) {
            set_button("Subscribe to access our archives",signup_url);
        }
        else {
            u_cookie=gc('tncms-user');
            u_services=gc('tncms-services');
            load_access(u_cookie,u_services);
        }
    }
})(document,window);
</script></div></div><div class="container-breakout ad-col break-out-top"><div class="visible-lg visible-md"><div class="tnt-ads-container text-center hidden-print hidden-xs"><div style="min-height:250px; min-width:970px;" class="tnt-ads dfp-ad" data-dfp-adunit="/8438/richmond.com/news/archives" data-dfp-custom-pos="breakout_top, btf" id="breakout_top" data-dfp-size="[[970,250],[970,90],[728,90]]" data-lazy="true"></div></div>
