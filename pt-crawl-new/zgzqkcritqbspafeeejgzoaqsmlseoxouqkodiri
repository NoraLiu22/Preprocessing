  !function(){
    const PRD_HOST = 'https://als-svc.nytimes.com/als';
    const DEV_HOST = 'https://als-svc.dev.nytimes.com/als';
    const BT_KEY = 'bt';
    const MKTG_KEY = 'mktg';
    const SUB_KEY = 'sub';
    const TEST_CLIENTSIDE_KEY = 'als_test_clientside';
    const { origin, pathname, host, search } = window?.location || {};
    const alice_rules_test = new URLSearchParams(search).get('alice_rules_test');
    const isMobile = (window.innerWidth < 740);
    const params = {
      alice_rules_test,
      uri: origin + pathname,
      prop: 'wc',
      plat: isMobile ? 'mweb' : 'web',
      typ:'rev'
    }
    function setKVPValues (data) {
      return new Promise(function (resolve, reject) {
        window.googletag = window.googletag || {};
        function setTargeting (data) {
          Object.keys(data).forEach(function(key) {
            const value = data[key];
            window.googletag.pubads().setTargeting(key, value);
          });
        }
        if (window?.googletag?.apiReady) {
          setTargeting(data);
        } else {
          window.googletag.cmd = window.googletag.cmd || [];
          function onGPT() {
            setTargeting(data);
          };
          window.googletag.cmd.push(onGPT);
        }
        resolve(data);
      });
    }
    function getLocalStorageValues() {
      return {
        [TEST_CLIENTSIDE_KEY]: window.localStorage.getItem(TEST_CLIENTSIDE_KEY) || null,
        [MKTG_KEY]: window.localStorage.getItem(MKTG_KEY) || null,
        [BT_KEY]: window.localStorage.getItem(BT_KEY) || null,
        [SUB_KEY]: window.localStorage.getItem(SUB_KEY) || null,
      }
    }
    // set starting values from previous LS values
    setKVPValues(getLocalStorageValues()).catch(function(e) {
      console.warn('SOMETHING TRAGIC HAPPENED', e);
    });
    function mapServiceDataToKVPs(data = { User: {}, Asset: {} }) {
      const userFilterKeys = [BT_KEY, MKTG_KEY, SUB_KEY];
      const { User = {}, Asset = {}, als_test_clientside } = data;
      const filterUserKeys = ([key]) => userFilterKeys.includes(key);

      const validUserEntries = Object.entries(User).filter(filterUserKeys);

      const filteredUserData = Object.fromEntries(validUserEntries);

      return {
        ...Asset,
        ...filteredUserData,
        ...(als_test_clientside ? { als_test_clientside } : {})
      };
    }
    function getService (url) {
      return window.fetch(url, { credentials: 'include' })
      .then(function (response) {
        return response.json()
      })
      .then(mapServiceDataToKVPs)
      .catch(function(e) {
        console.warn('FETCH ISSUES!',e);
      });
    }
    function setLocalStorageValues (data) {
      const validKeys = [BT_KEY, MKTG_KEY, SUB_KEY, TEST_CLIENTSIDE_KEY];
      function setEntry(entry) {
        const [key, val] = entry;
        if (validKeys.includes(key)) {
          try {
            const ls_val = key === TEST_CLIENTSIDE_KEY ? 'ls-'+val : val;
            localStorage.setItem(key, String(ls_val));
          } catch (e) {
            return console.warn(e);
          }
        }
      }
      Object.entries(data).forEach(setEntry);
    }
    function getServiceUrl (host, params) {
      function filterParams ([,value]) {
        return value !== null && value !== undefined;
      }
      const baseUrl = host === 'www.nytimes.com' ? PRD_HOST : DEV_HOST;
      const url = new URL(baseUrl);
      const filteredParams = Object.entries(params || {}).filter(filterParams);
      url.search = new URLSearchParams(filteredParams).toString();
      return url.href;
    }
    getService(getServiceUrl(host, params)).then(setKVPValues).then(setLocalStorageValues).catch(console.warn);
