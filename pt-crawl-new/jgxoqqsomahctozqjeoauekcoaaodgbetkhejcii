      }

            
      
   }
    @media screen and (max-width: 979px){
      div.as-fban.fundraising-block-flex.as-fban--2 .as-fban__content--left > div:first-child,
      div.as-fban.fundraising-block-flex.as-fban--7 .as-fban__content--right > div:first-child {
         display:flex;
      }
      div.as-fban.fundraising-block-flex.as-fban--7 .as-fban__logo {
         
         margin: 0;
         align-self: center;
         margin-bottom: 20px;
      }

      div.as-fban.fundraising-block-flex.as-fban--3 .as-fban__content--right {
         padding: 0 20px;
      }
      .as-fban--7 .as-fban__invest {
          margin-right: 0px;
      }
   }
   @media screen and (max-width: 470px){
      div.as-fban.fundraising-block-flex.as-fban--7 .as-fban__content--right {
         padding: 15px;
      }
      div.as-fban .as-fban__invest {
         font-size: 20px;
         margin-bottom: 20px;
      }
      .as-fban--7 .block-width-30,
      .as-fban--7 .block-width-70 {
        width:100%;
      }
   }


/*--><!]]>*/
</style>

