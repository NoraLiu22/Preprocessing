            <meta itemprop="contentUrl" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/1/08/108a6eef-8242-517e-b23b-5b18fe0034db/65e5186f4ea7c.image.jpg?resize=1200%2C633">
            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/1/08/108a6eef-8242-517e-b23b-5b18fe0034db/65e5186f4ea7c.image.jpg?resize=1200%2C633">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
