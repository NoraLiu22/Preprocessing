	function get_si_section() {
		const nlp_categories = Object.keys(
			NXSTdata.content?.nlpCategories || {}
		)
		if (
			!nlp_categories.length
			|| nlp_categories[0].toLowerCase().includes('sensitive subjects')
		) {
			return '';
		}
