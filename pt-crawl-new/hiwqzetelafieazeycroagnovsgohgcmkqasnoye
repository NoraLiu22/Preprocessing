	<div class="civic-science-container" id="civsci-id-2122639739" data-civicscience-widget="82d19ca8-4f30-60f4-2562-28d5394c0c93"></div>
			<script type="text/javascript">
			var myCounter, counter, variant, card, variantValue, origin, nxsurl;

			var cards      = [{"layout":"minimalist","autoturn":"1","timer":"5","datasource":"chartbeat","title":"Read Next","linktext":"Next story"}];
			var posts      = {"0":{"id":1588391,"title":"Barrage of gunfire as officers stop shooter at Joel Osteen&#8217;s megachurch, newly released video shows","url":"https:\/\/www.mychamplainvalley.com\/news\/national\/ap-barrage-of-gunfire-as-officers-confront-houston-megachurch-shooter-released-body-cam-footage-shows\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/02\/65de073387de15.51130385.jpeg"},"1":{"id":1648087,"title":"Former Norwich University President accused of sex discrimination in lawsuit","url":"https:\/\/www.mychamplainvalley.com\/news\/local-news\/former-norwich-university-president-accused-of-sex-discrimination-in-lawsuit\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/04\/anarumo.jpg"},"2":{"id":1658174,"title":"Pope Francis appoints Burlington&#8217;s next bishop","url":"https:\/\/www.mychamplainvalley.com\/news\/local-news\/pope-francis-appoints-burlingtons-next-bishop\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/05\/MCDERMOTT.png"},"3":{"id":1633991,"title":"St. Albans man arrested in connection to fire that destroyed camper van","url":"https:\/\/www.mychamplainvalley.com\/news\/vermont\/arrest-warrant-requested-in-connection-to-st-albans-fire-that-destroyed-camper-van\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/04\/lapine.png"},"5":{"id":1658145,"title":"Saranac Lake PD equips patrol cars with &#8220;Carter Kits&#8221;","url":"https:\/\/www.mychamplainvalley.com\/news\/local-news\/saranac-lake-pd-equips-patrol-cars-with-carter-kits\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/05\/Screenshot-2024-05-06-162417.png"},"6":{"id":1658066,"title":"Middlebury voters to decide on library bond issue","url":"https:\/\/www.mychamplainvalley.com\/news\/local-news\/middlebury-voters-to-decide-on-library-bond-issue\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/05\/Screenshot-2024-05-06-155205.png"},"7":{"id":1657747,"title":"Sanders announces he will seek fourth term in U.S. Senate","url":"https:\/\/www.mychamplainvalley.com\/news\/vermont\/sanders-announces-he-will-seek-fourth-term-in-u-s-senate\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/02\/AP24025654166494-e1707421660124.jpg"},"8":{"id":1657465,"title":"Gradual clearing today before a brighter, drier Tuesday","url":"https:\/\/www.mychamplainvalley.com\/weather\/weather-skytracker-forecast\/gradual-clearing-today-before-a-brighter-drier-tuesday\/","image":"https:\/\/www.mychamplainvalley.com\/wp-content\/uploads\/sites\/61\/2024\/05\/TODAY-FUTURETRACKER-3.jpg"}};
			var current_id = 1655371;
			var nxsparam   = 0;

			document.addEventListener("DOMContentLoaded", function() {
				setTimeout(firstLoad, 300);
			});

			function firstLoad() {
				if(getCookie('nxst-story-id')){
					origin = getCookie('nxst-story-id');
				}

				if ( nxsparam >= posts.length ) {
					return;
				}
				while ( ( posts[nxsparam]["id"] == current_id ) || ( posts[nxsparam]["id"] == origin ) ) {
					nxsparam++;
					if ( nxsparam >= posts.length ) {
						return;
					}
				}

				Array.from(document.getElementsByClassName("nxslink")).forEach(function(elem) {
					nxsurl = posts[nxsparam]["url"] + "?nxsparam=" + (nxsparam+1);
					elem.setAttribute("href", nxsurl);
				});
				document.querySelectorAll('#btn-next').forEach(e => e.setAttribute('href', nxsurl))
				Array.from(document.getElementsByClassName("nxstitle")).forEach(function(elem) {
					elem.innerHTML = posts[nxsparam]["title"];
				});
				if(posts[nxsparam]["image"]){
					Array.from(document.getElementsByClassName("nxsimage")).forEach(function(elem) {
						elem.setAttribute("src", posts[nxsparam]["image"]);
					});
				} else {
					Array.from(document.getElementsByClassName("col-image")).forEach(function(elem) {
						elem.style.display = "none";
					});
				}

				if(getCookie('nxstoryVariation') == 'nxstory-auto-minimalist'){
					variantValue = getCookie('nxstoryVariation');
				} else{
					variantValue = 'nxstory-auto-minimalist';
				}

				card = cards.find(elem => elem['layout'] == 'minimalist' && elem['autoturn'] == 1);
				variant = 4;

				if(card['datasource'] == ''){
					return;
				}

				setCookie('nxstoryVariation', variantValue, 30 * 24);

				counter = card['timer'];
				Array.from(document.getElementsByClassName("btn-counter")).forEach(function(elem) {
					elem.innerHTML = counter;
				});

				Array.from(document.getElementsByClassName('btn-read-next')).forEach(function(elem) {
					elem.innerHTML = card['title'];
				});
				Array.from(document.getElementsByClassName('btn-next-text')).forEach(function(elem) {
					elem.innerHTML = card['linktext'];
				});
			}

			function initCounter(){
				if(variant == 0 || card == undefined){
					return;
				}

				let auxvariant = variant-1;
				if(document.getElementsByClassName('nxstory')[auxvariant]){
					const nxstoryViewEvent = new CustomEvent(
						'nxstoryView',
						{
							detail: {
								target: document.getElementsByClassName('nxstory')[auxvariant],
							}
						},
					);

					document.getElementsByClassName('nxstory')[auxvariant].style.bottom = '100px';
					dispatchEvent(nxstoryViewEvent);
				}

				if(auxvariant == 2 || auxvariant == 3){
					Array.from(document.getElementsByClassName("btn-progress")).forEach(function(elem) {
						elem.style.setProperty('-webkit-animation', 'progressBar '+counter+'s linear');
					});
					Array.from(document.getElementsByClassName("btn-progress")).forEach(function(elem) {
						elem.style.setProperty('-moz-animation', 'progressBar '+counter+'s linear');
					});
					Array.from(document.getElementsByClassName("btn-progress")).forEach(function(elem) {
						elem.style.setProperty('-webkit-animation-fill-mode', 'both');
					});
					Array.from(document.getElementsByClassName("btn-progress")).forEach(function(elem) {
						elem.style.setProperty('-moz-animation-fill-mode', 'both');
					});

					myCounter = setInterval(function () {
						if(counter > 0){
							counter--;
							Array.from(document.getElementsByClassName("btn-counter")).forEach(function(elem) {
								elem.innerHTML = counter;
							});
							if(counter < 1){
								redirect(nxsurl);
								clearInterval(myCounter);
							}
						}
					}, 1000);
				}
			}

			function dismissNxsStory(){
				clearInterval(myCounter);
				Array.from(document.getElementsByClassName('nxstory')).forEach(function(elem) {
					elem.style.bottom='-100%';
					elem.style.display='none';
				});
				return false;
			}

			function redirect(url) {
				clicked = false;
				clicktype = '';
				function addParam(url, name) {
					newurl = new URL(url);
					newurl.searchParams.append(name, 1);
					return newurl.toString()
				}
				if(event){
					event.preventDefault();
				}
				if (getCookie('nxst-story-id') === "") {
					setCookie('nxst-story-id', current_id);
				}
				if(!url){
					if(event){
						clicked = true;
						nxstorycontainer = event.target.closest('[class*=nxstory-]');
						variant = nxstorycontainer.dataset.variant;
						if (variant.includes('auto')) {
							clicktype = 'auto'
						}
						url = event.target.closest("a").getAttribute('href');
					}
				}
				if (clicked) {
					if (clicktype === 'auto'){
						url = addParam(url, 'nxstory_autoturn_click')
					} else {
						url = addParam(url, 'nxstory_click')
					}
				} else {
					url = addParam(url, 'nxstory_autoturn')
				}
				window.location.href = url;
				return false;
			}
		</script>

		<div class="nxstory nxstory-1" data-variant="nxstory-click-detail">
			<button class="close-button" onclick="dismissNxsStory()">✕</button>
			<div class="read-more">
				<a class="nxslink" href="" onclick="redirect()">
					<span class="btn-read-next">Read next</span> >
				</a>
			</div>
			<div class="main-col">
				<div class="col-title">
					<a class="nxslink" href="" onclick="redirect()">
						<span class="nxstitle"></span>
					</a>
				</div>
				<div class="col-image">
					<a class="nxslink" href="" onclick="redirect()">
						<img class="nxsimage" src="">
					</a>
				</div>
			</div>
		</div>

		<div class="nxstory nxstory-2" data-variant="nxstory-click-minimalist">
			<div class="nxs-line">
				<div class="read-more">
					<a class="nxslink" href="" onclick="redirect()">
						<span class="btn-read-next">Read next</span> >
					</a>
				</div>
				<div class="btn-next">
					<a class="nxslink" href="" onclick="redirect()">
						<span class="btn-next-text">Next</span>
					</a>
				</div>
				<button class="close-button" onclick="dismissNxsStory()">✕</button>
			</div>
		</div>

		<div class="nxstory nxstory-3" data-variant="nxstory-auto-detail">
			<button class="close-button" onclick="dismissNxsStory()">✕</button>
			<div class="read-more">
				<a class="nxslink" href="" onclick="redirect()">
					<span class="btn-read-next">Read next</span> >
				</a>
			</div>
			<div class="main-col">
				<div class="col-title">
					<a class="nxslink" href="" onclick="redirect()">
						<span class="nxstitle"></span>
					</a>
				</div>
				<div class="col-image">
					<a class="nxslink" href="" onclick="redirect()">
						<img class="nxsimage" src="">
					</a>
				</div>
			</div>
			<div class="btns-col">
				<a id="btn-next" href="" onclick="redirect()">
					<span style="width:100%;">
						<span class="btn-progress"></span>
					</span>
					<span id="btn-text">
						<span class="btn-next-text">Next story</span> in <span class="btn-counter"></span>
					</span>
				</a>
				<button id="btn-cancel" onclick="dismissNxsStory()">
					Cancel
				</button>
			</div>
		</div>

		<div class="nxstory nxstory-4" data-variant="nxstory-auto-minimalist">
			<div class="read-more">
				<a class="nxslink" href="" onclick="redirect()">
					<span class="btn-read-next">Read next</span> >
				</a>
			</div>
			<div class="btns-col">
				<a id="btn-next" href="" onclick="redirect()">
					<span style="width:100%;">
						<span class="btn-progress"></span>
					</span>
					<span id="btn-text">
						<span class="btn-next-text">Next story</span> in <span class="btn-counter"></span>
