		<script>
    function resetTourTip(redirectURL){
        var tourtipList = $("[data-tourtipkey]");
        tourtipList.map(function(index){
            $.telligent.evolution.tourTips.mark($(tourtipList[index]).attr("data-tourtipkey"), false);
        })
        $.telligent.evolution.tourTips.suppress(100);
        location = redirectURL;
    }

    function setBtnHandler(btnId, redirectURL){
        $("#"+btnId).on("click", function(e){
            resetTourTip(redirectURL);
        })
    }

    function init(){
        setBtnHandler("get-started-btn", "/");
    }

    init();
</script><div style="clear:both;"></div>
	</div>
