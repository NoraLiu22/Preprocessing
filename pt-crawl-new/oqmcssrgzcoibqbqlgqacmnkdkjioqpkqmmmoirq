function spacerHeight() {
	if ($('#lbspacer').length==1) {
		$($('#lbspacer')[0]).height($($('#lbextern')[0]).height()-92)
	}
	if ($('#skydiv').length==1) {
		$($('#skydiv')[0]).css('top',($($('#lbextern')[0]).height()+44)+'px');
	}
}
</script>

