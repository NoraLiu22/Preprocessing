<script type="text/javascript">

    $(function ()
    {
        //==============================================================
        // Workflow tools
        //==============================================================
        $.ajax({
            url: "/umbraco/surface/WorkflowToolsSurface/GetWorkflowTools/",
            type: 'POST',
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            success: function (data)
            {
                // replace contents of workflow tools div
                $("#wfTools").empty().append(data);

                // instantiate the horizontal scroll on the workflow tools
                $('#wfTools').slick({
                    dots: true,
                    infinite: true,
                    speed: 300,
                    slidesToShow: 4,
                    slidesToScroll: 1,
                    responsive: [
                        { breakpoint: 1200, settings: { slidesToShow: 3, slidesToScroll: 1 } },
                        { breakpoint: 992, settings: { slidesToShow: 2, slidesToScroll: 1 } },
                        { breakpoint: 768, settings: { slidesToShow: 1, slidesToScroll: 1 } }
                    ]
                });
            }
        });

        //==============================================================
        // Homepage Buttons
        //==============================================================
        $.ajax({
            url: "/umbraco/surface/HomeButtonsSurface/GetHomePageButtons/",
            type: 'POST',
            dataType: 'json',
            contentType: 'application/json; charset=utf-8',
            success: function (data)
            {
                // replace contents of workflow tools div
                $("#divBannerButtons").empty().append(data);
            }
        });

        //==============================================================
        // Platforms
        //==============================================================
        

    });

