    function lee_segment_audience(e){
        const sAud = window.sessionStorage.getItem('lee.segment.aud');
        if(!sAud && typeof e === 'object'){
            window.sessionStorage.setItem('lee.segment.aud', e.join(','));
        }
    } /*]]>*/</script>
