            jQuery('#lee-card-grid-1269736 article').hide();

            jQuery(document).ready(function($){
                try {
                    var oRecentItems = [];
                    var sLsRecent = localStorage.getItem('mparticle.recent');
                    if( sLsRecent ){
                        oRecentItems = JSON.parse(sLsRecent);
                        var iShow = 0;
                        $('#lee-card-grid-1269736').prepend('<div id="deduped-1269736" class="hide"></div>');
                        $('#lee-card-grid-1269736 article').each(function(i,e){
                            var id = $(e).attr('id').split('card-summary-1269736-')[1];
                            if($.inArray(id, oRecentItems) === -1){
                                $(e).detach().insertBefore("#deduped-1269736").show();
                                iShow++;
                            }
                            if(iShow === 7) return false;
                        });
                        
                        if(iShow < 7){
                            $('#lee-card-grid-1269736 article:hidden').each(function(i,e){
                                $(e).detach().insertBefore("#deduped-1269736").show();
                                iShow++;
                                if(iShow === 7) return false;
                            });
                        }
                    } else {
                        $('#lee-card-grid-1269736 article').slice(0, 7).show();
                    }

                } catch(e){
                    $('#lee-card-grid-1269736 article').slice(0, 7).show();
                    __tnt.log(e);
                }
            });
