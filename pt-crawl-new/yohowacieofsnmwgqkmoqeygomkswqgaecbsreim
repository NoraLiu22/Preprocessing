        <img src="https://bloximages.chicago2.vip.townnews.com/central.leetemplates.com/content/tncms/live/global/resources/images/blocks/deal-widget/syd-logo.png?_dc=24.05.06.11" alt="" class="img-responsive center-block" style="max-width:125px; margin-bottom:20px;">
        <div id="lee-deal-container" style="max-width: 300px; margin: 0 auto;"></div>
        <script>
            jQuery(document).ready(function($){
                function throttleIlad(func, wait){
                    var timeout;
                    return function(){
                        var context = this, args = arguments;
                        var later = function(){
                            timeout = null;
                            func.apply(context, args);
                        };
                        clearTimeout(timeout);
                        timeout = setTimeout(later, wait);
                    }
                }
                var iladLoaded = false;
                var throttledIlad = throttleIlad(function(){
                    if( !iladLoaded ){
                        iladLoaded = __tnt.elementOnScreen('#lee-deal-container');
                        if(iladLoaded){
                            $('#lee-deal-container').append('<iframe src="https://richmond.scoreyourdeal.com/widgets.php?cat_id=all" width="300" height="250" frameborder="0" scrolling="no" allowtransparency="true"></iframe>');
                            window.removeEventListener('scroll', throttledIlad);
                        }
                    }   
                }, 100);
                window.addEventListener('scroll', throttledIlad);
            });
         </script>
    </div>


</section></div></div></div>
