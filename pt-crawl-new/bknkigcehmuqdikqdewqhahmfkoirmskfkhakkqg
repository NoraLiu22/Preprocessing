                <div id="gallery_item_24" class="gallery-item image-item">
                    
                        <h3>'Record Year' by Eric Church</h3>
                    <figure class="photo layout-horizontal letterbox-style-default">
            <div class="image">
                
                
                
                
        <div itemprop="image" itemscope itemtype="https://schema.org/ImageObject">
            <meta itemprop="width" content="1010">
            <meta itemprop="height" content="720">
            <meta itemprop="contentUrl" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/9/57/95703413-edb1-5a09-a00d-72ec49f4c5fb/65733b142fa81.image.jpg">
            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/richmond.com/content/tncms/assets/v3/editorial/9/57/95703413-edb1-5a09-a00d-72ec49f4c5fb/65733b142fa81.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
                            alt="'Record Year' by Eric Church"
                            class="img-responsive lazyload full default"
                            width="1010"
