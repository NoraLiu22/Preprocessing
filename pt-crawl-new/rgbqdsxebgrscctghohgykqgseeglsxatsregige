    <script>
        Q4Login.IframeRedirectURI = "https://login.q4inc.com/authorize?client_id=gKrDY1E9BUSWfU7DGuW2CO4rjdskiNg7&scope=openid+profile+email&response_type=code&response_mode=query&nonce=DEgCRFYznLeAu4XMcdkUgQdHuw&state=eyJhbGciOiJodHRwOi8vd3d3LnczLm9yZy8yMDAxLzA0L3htbGRzaWctbW9yZSNobWFjLXNoYTI1NiIsInR5cCI6IkpXVCJ9.eyJyZXR1cm5VcmwiOiJodHRwczovL2lyLmFib3V0YW1hem9uLmNvbS9xNGxvZ2luY2FsbGJhY2suYXNweCIsImV4cCI6MTcyMDI3ODAzOC4wLCJpYXQiOjE3MTUwMTg0MzguMCwiaXNzIjoiUTQuV2ViLlB1YmxpYy5TaXRlIiwiYXVkIjoidXBzdHJlYW0uY2RuLndlYi5wcmQucTRpbmMuY29tIn0.iMK0LrtqxJmgKhrjQZ8kx2-cJ5CV7qabKYBEgvh6UNg&redirect_uri=https%3a%2f%2fauth.platform.q4inc.com%2fauth%2fpublicAuthRedirect&prompt=none";
        (function () {
            if (window.location.hash.includes("login=true")) {
                // q4pid is set in the modal so it should already be there
                const pendoId = window.localStorage.getItem("q4pid");

                // email is also from modal
                const email = window.localStorage.getItem("q4l_email");

                let redirectUrl = $('#hdnRedirectToLoginUrl').val();
                redirectUrl = redirectUrl.replace("_q4pidval_", pendoId);
                redirectUrl = redirectUrl.replace("_q4lemail_", email);

                window.location.replace(redirectUrl);
            }
        })();
    </script>
