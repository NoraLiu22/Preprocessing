                <div class="pane_inner"><span class='Q4FooterDiv16'><div id="_ctrl0_ctl51_divModuleContainer" class="module module-embed hidden">
    <div class="module_container module_container--outer">
        
        <div class="module_container module_container--inner">
            <script>
var q4App = $.extend(true, q4Defaults, {
    options: {
        headerOffset: $('.layout_header').height() + 20
    },
    scrollingTable: function(selector) {
        $(selector).not(selector + ' table').wrap('<div class="table-wrapper" />');
    },
    meetingFancy: function($selector, trigger){
        var bCookie = document.cookie.replace(/(?:(?:^|.*;\s*)bCookie\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        console.log(bCookie, bCookie.length);
        if ( !bCookie.length ) {
            $selector.removeClass('js--hidden');
        }
        $selector.on('click', '.button--cookie',function(){
            $selector.addClass('js--hidden');
            document.cookie = 'bCookie=true; path=/';
        });

        $selector.on('click', trigger, function(e){
            e.preventDefault();
            $.fancybox.open({
                type: 'iframe',
                src: '/request-meeting-iframe/default.aspx',
                opts: {
                    smallBtn: false,
                    margin: [10, 0],
                    slideClass: 'fancybox-slide--form',
                    parentEl: 'form'
                }
            });
        });
        window.addEventListener('message', function (event) {
            if (event.data == 'close-fancybox') {                
                $.fancybox.close();
            }
        }, false);
    },
    closeLangMenu: function(){
        $('.module-languages_dropdown_header').on('click', function(){
            $('.module-languages_toggle').removeClass('js--active');
            $('.module-languages_dropdown').removeClass('js--revealed').attr('aria-hidden', 'false');
        });
    },
    closeSearch: function(){
        $('.module-search_text').on('click', function(){
            $('.module-search h2').removeClass('js--active');
            $('.module-search .module_container--inner').removeClass('js--revealed').attr('aria-hidden', 'false');
            $('body').removeClass('js--mobile');
        });
    },
    init: function() {
        var app = this;

        app.cleanUp();
        app.submitOnEnter('.module-unsubscribe');
        app.submitOnEnter('.module-search');
        app.validateSubmit('.module-search');
        app.cleanQuickLinks($('.module-links'));
        app.copyright($('.copyright_year'));
        app.docTracking();
        app.fancySignup();
        app.resetDate(['.nav a[href*="s4.q4web.com"]:not([href$=".pdf"])']);
        app.previewToolbar();
        app.sections();
        app.contrast.init();
        app.meetingFancy($('.module-cta'),'.button--meeting');
        
        app.reveal('.module-links--header', 'h2', '.module_container--inner');
        
        app.mobileMenuToggle($('.layout'), '.pane--navigation', '.layout_toggle');
        
        app.reveal('.module-search', '.module_title', '.module_container--inner');
        app.mobileMenuToggle($('body'), '.pane--navigation', '.module-search h2');
        app.closeSearch();
        
        app.reveal('.module-languages', '.module-languages_toggle', '.module-languages_dropdown');
        app.closeLangMenu();
    }
});

q4App.init();
</script>
        </div>
