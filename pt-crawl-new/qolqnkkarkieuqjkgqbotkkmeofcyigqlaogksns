      <script>
  // Get the value of the 'lang' attribute from the body element
  const langAttribute = document.body.getAttribute('lang');

  // Function to display the div based on the detected language
  function showDivForLanguage(lang) {
    const divs = document.querySelectorAll('.feedback-form');

    for (const div of divs) {
      if (div.classList.contains(lang + '-feedback-form')) {
        div.style.display = 'block';
      } else {
        div.style.display = 'none';
      }
    }
  }

  // Call the function with the detected language
  showDivForLanguage(langAttribute);
</script>      <!-- End Custom Markup Metabox: Append to Footer -->
