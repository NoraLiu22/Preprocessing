			<script>
			function submitSearch() {
				switch ($('#trSearch').val()) {
					case "Songs":
						cat='s';
						break;
					case "Albums":
						cat='a';
						break;
					case "Alben":
						cat='a';
						break;
					case "Compilaties":
						cat='c';
						break;
					case "DVDs":
						cat='d';
						break;
					case "Interpret":
						cat='i';
						break;
					case "EAN":
						cat='ean';
						break;
					case "Edelmetall":
						cat='e';
						break;
					case "Komp./Prod.":
						cat='p';
						break;
					case "Filme":
						cat='m';
						break;
					case "Bücher":
						cat='b';
						break;
					case "Games":
						cat='g';
						break;
					case "Events":
						cat='ev';
						break;
					case "Forum":
						cat='f';
						break;
					case "Fundgrube":
						cat='fg';
						break;
					case "Member":
						cat='members';
						break;
				}
				document.location='/search.asp?search='+escape($('#searchField').val()).replace(/\+/g,"%2B")+"&cat="+cat;
			}
			</script>





