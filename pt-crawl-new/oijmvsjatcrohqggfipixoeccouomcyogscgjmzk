<script src="/bundles/js?v=gpTPlL-skNsMHPdT2v-CRv-PKRyQUwu9eCfXhTYq3_s1"></script>



    <script type="text/javascript">
        var images = ['assembly', 'senate', 'government', 'interiordome', 'legislation', 'skylight', 'tulips'];

        var switchTimer;
        var bufferTimer;
        var rotationSpeed = 6000;
        var rotationStart = 2000;
        var currentImage = "";
        var goSlow = true;

        function buildImageUrl(name) {
            return "/Content/images/snazz-" + name + ".jpg";
        }

        function randomImageUrl() {
            return buildImageUrl(images[Math.floor(Math.random() * images.length)]);
        }

        // Change the image
        function rotateImage() {
            goSlow = true;
            setImage(randomImageUrl());
        }

        function setImageByName(name) {
            currentImage = name;
            goSlow = false;
            setImage(buildImageUrl(name));
        }

        function setImage(newImageUrl) {
            clearTimeout(switchTimer);
            var pic1 = $(".front-picture-1");
            var pic2 = $(".front-picture-2");
            var oldImageUrl = pic1.attr("src");

            pic2.hide();
            pic2.removeClass("fade-picture");
            pic2.removeClass("fade-picture-fast");
            pic2.attr("src", oldImageUrl);
            pic2.show();

            pic1.attr("src", newImageUrl);
            clearTimeout(switchTimer);
            switchTimer = setTimeout(fadeImage, 100);
        }

        function fadeImage() {
            var pic2 = $(".front-picture-2");
            if (goSlow) {
                pic2.addClass("fade-picture");
            } else {
                pic2.addClass("fade-picture-fast");
            }

            clearTimeout(switchTimer);
            switchTimer = setTimeout(rotateImage, rotationSpeed * 2);
        }

        $(document).ready(function() {
            bootstrap_tab_bookmark();

            $('[data-toggle=tooltip]').tooltip({ container: 'body' });

            switchTimer = setTimeout(rotateImage, rotationSpeed + rotationStart);

            var wrapper = $(".front-picture-wrapper");

            wrapper.click(function() {
                rotationSpeed = rotationSpeed * 0.9;
                if (rotationSpeed <= 1000) {
                    wrapper.text("THIS SPACE IS VERY USEFUL");
                }
                rotateImage();
            });


            $(".car a.btn").hover(function(e) {
                var pic = $(e.toElement).data("image");
                if (pic && currentImage !== pic) {
                    setImageByName(pic);
                }
            });

            $(".bg-switcher").click(function(e) {
                var thing = $(e.toElement).text();
                $("body").css("background-image", "url(/content/images/background_top_" + thing + ".jpg");
            });


        });
    </script>


</body>
