  window.addEventListener('mainScripts', function() {
    if (!'BrockmanPoll' in window) return;
    const wrappers = document.querySelectorAll('.poll_wrapper');
    if (wrappers) {
      wrappers.forEach((wrapper) => {
        const poll = wrapper.querySelector('.poll');
        new BrockmanPoll({
          'poll': poll,
          'wrapper': wrapper,
          'postEndpoint': '/poll/votes',
          'getEndpoint': '/poll/next',
          'tags': [],
          'loc': {
            'upNext': "Up next",
            'genericError': "Sorry, but an error occurred. Please try again.",
            'votedMessage': "You\u0027ve already voted on this poll.",
          },
        });
      })
    }
  });
</script>
 
