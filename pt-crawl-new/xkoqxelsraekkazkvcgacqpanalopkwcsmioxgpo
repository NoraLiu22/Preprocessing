<script>
  function appendAnonymised() {
    if(window.anonymisedDone) return;
    const anonymisedEl = document.createElement('script');
    anonymisedEl.type = "text/javascript"
    anonymisedEl.async = true;
    anonymisedEl.src = "https://static.anonymised.io/light/loader.js";
    anonymisedEl.id = "idward-plugin-js";
    anonymisedEl.setAttribute("idw_client_id", "MjE1Mw=="); // Network ID
    anonymisedEl.setAttribute("idw_hide_button", "true");
    document.getElementsByTagName('head')[0].appendChild(anonymisedEl);
    window.anonymisedDone = true;
  }
  window.addEventListener('BrockmanTCFConsent', (e) => {
    const tcData = e.detail;
    if(tcData.vendor?.consents?.[1116]) { // TCF ID for Anonymised.io
      appendAnonymised();
    }
  });
</script>

