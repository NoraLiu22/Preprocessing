<script>
  function getGroupPmId(isSubscribed) {
    let ukRowGroupPmId = norwayGroupPmId = 742930, europeGroupPmId = 823472, subGroupPmId = 823449;

        ukRowGroupPmId = 832785;

        europeGroupPmId = 832786;


        subGroupPmId = 832785;

    if(isSubscribed) {
      return subGroupPmId;
    }

    if (Intl) {
      const { timeZone } = Intl.DateTimeFormat().resolvedOptions();

      if(timeZone.includes('Europe/')) {
        if(timeZone.includes('Europe/London')) {
          return ukRowGroupPmId;
        }

        if(timeZone.includes('Europe/Oslo')) {
          return norwayGroupPmId;
        }

        return europeGroupPmId;
      }
      return ukRowGroupPmId
    }
    return ukRowGroupPmId
  }

  function enableElements() {
    if (!window?.BrockmanAllowedCookies?.targeting) return;
    const elements = document.querySelectorAll('iframe[data-src], script[data-src], img[data-src]');
    for (let i = 0; i < elements.length; i++) {
      const element = elements[i];
      if (element.src !== element.dataset.src) element.setAttribute('src', element.dataset.src);
    }
  }

  function showEmbedPlaceholders() {
    if (window.embedPlaceholdersDone) return;
    const elements = document.querySelectorAll('.embed_placeholder');

    for (let i = 0; i < elements.length; i++) {
      const element = elements[i];
      element.style.display = '';
    }
    window.embedPlaceholdersDone = true;
  }

  function hidePerformanceEmbedPlaceholders() {
    hideEmbedPlaceholders('performance');
  }

  function hideFunctionalEmbedPlaceholders() {
    hideEmbedPlaceholders('functional');
  }

  function hideTargetingEmbedPlaceholders() {
    hideEmbedPlaceholders('targeting');
  }

  function hideEmbedPlaceholders(type) {
    // Removes script and iframe placeholders when targeting has been accepted
    const identifier = '.embed_placeholder' + (type ? '[data-type="' + type + '"]' : '');
    const elements = document.querySelectorAll(identifier);
    for (let i = 0; i < elements.length; i++) {
      const element = elements[i];
      element.style.display = 'none';
    }
  }

  function performanceCookiesAllowed() {
    window.dispatchEvent(new Event('BrockmanPerformanceCookiesAllowed'));
    hidePerformanceEmbedPlaceholders();
  }

  function performanceCookiesDisallowed() {
    window.dispatchEvent(new Event('BrockmanPerformanceCookiesDisallowed'));
  }

  function functionalCookiesAllowed() {
    window.dispatchEvent(new Event('BrockmanFunctionalCookiesAllowed'));
    hideFunctionalEmbedPlaceholders();
  }

  function functionalCookiesDisallowed() {
    window.dispatchEvent(new Event('BrockmanFunctionalCookiesDisallowed'));
  }

  function targetingCookiesAllowed() {
    window.dispatchEvent(new Event('BrockmanTargetingCookiesAllowed'));
    enableElements();
    hideTargetingEmbedPlaceholders();
  }

  function targetingCookiesDisallowed() {
    window.dispatchEvent(new Event('BrockmanTargetingCookiesDisallowed'));
  }

  function getSubscriber3pCookie() {
    const decodedCookie = decodeURIComponent(document.cookie);
    const cookieArray = decodedCookie.split(';');
    return cookieArray.some((cookie) => {
      const [cookieKey, cookieValue] = cookie.trim().split('=')
      return cookieKey === 'subscriber_3p' && cookieValue === 'true'
    })
  }

  let isSubscribed = false;


  window._sp_queue = [];
  window._sp_ = {
    config: {
      accountId: 347,
        baseEndpoint: 'https://cmpsp.eurogamer.net',
      propertyHref: 'https://www.eurogamer.net',
      joinHref: true,

      gdpr: {},
      groupPmId: getGroupPmId(isSubscribed),
      targetingParams: {
        subscribed: isSubscribed
      },
      events: {
        onConsentReady: function () {
          if (isSubscribed && getSubscriber3pCookie()) {
            // "64899c52b4b95206a108e62c" is the _id for targeting cookies
            __tcfapi('postRejectAll', 2, () => {
              __tcfapi('postCustomConsent', 2, () => { }, [], ['64899c52b4b95206a108e62c'], []);
            });
          }

          __tcfapi('getCustomVendorConsents', 2, function (data, success) {
            if (success) {
              const { consentedPurposes } = data;
              window.BrockmanAllowedCookies = {
                performance: consentedPurposes.some(p => p._id === '64899c52b4b95206a108e614'),
                functional: consentedPurposes.some(p => p._id === '64899c52b4b95206a108e61a'),
                targeting: consentedPurposes.some(p => p._id === '64899c52b4b95206a108e62c'),
              }

              showEmbedPlaceholders();

              window.BrockmanAllowedCookies.performance ?
                performanceCookiesAllowed() : performanceCookiesDisallowed();

              window.BrockmanAllowedCookies.functional ?
                functionalCookiesAllowed() : functionalCookiesDisallowed();

              window.BrockmanAllowedCookies.targeting ?
                targetingCookiesAllowed() : targetingCookiesDisallowed();
            }
          });

          __tcfapi('addEventListener', 2, (tcData, success) => {
            if (success) {
              window.consentString = tcData.tcString;
              window.dispatchEvent(new CustomEvent('BrockmanTCFConsent', { detail: tcData }));
            } else {
              window.dispatchEvent(new Event('BrockmanTCFOptOut'));
            }
          });
        },
      }
    }
  }
