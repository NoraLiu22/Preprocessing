    <script>
            $(document).ready(function () {
                $('.mobile_search').on('click touch', function () {
                    $('#arama-form2').submit();
                    return false;
                });
                if (getCookie("v") == null || getCookie("v") == ''){
                    $('.uiVersion').addClass('oldVersion').removeClass('newVersion').text('بگه‌ڕێنه‌وه‌ بۆ باری پێشوو');
                }else if(getCookie("v")=='1'){
                    $('.uiVersion').addClass('oldVersion').removeClass('newVersion').text('بگه‌ڕێنه‌وه‌ بۆ باری پێشوو');
                }else{
                    $('.uiVersion').addClass('newVersion').removeClass('oldVersion').text('بۆچوونی نوێ');
                }



                $('.newVersion').on('click', function () {
                    setCookie("v", 1, 999);
                    $(this).addClass('oldVersion').removeClass('newVersion');
                    window.location.href = 'https://www.aa.com.tr/ks';
                    return false;
                });

                $('.oldVersion').on('click', function () {
                    setCookie("v", 0, 999);
                    $(this).addClass('newVersion').removeClass('oldVersion');
                    window.location.href = 'https://www.aa.com.tr/ks';
                    return false;
                });
            });

    </script>

