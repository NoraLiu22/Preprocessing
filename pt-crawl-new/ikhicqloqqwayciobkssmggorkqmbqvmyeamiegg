<script type="text/javascript">
  <!-- Start OneTrust replace headers -->
  function OptanonWrapper() {
    var tagsH2 = document.querySelectorAll('#onetrust-pc-sdk h2');
    for(i=0; i < tagsH2.length; i++) { otreplace(tagsH2[i], "ot-h2"); }
    var tagsH3 = document.querySelectorAll('#onetrust-pc-sdk h3');
    for(i=0; i < tagsH3.length; i++) { otreplace(tagsH3[i], "ot-h3"); }
    var tagsH4 = document.querySelectorAll('#onetrust-pc-sdk h4');
    for(i=0; i < tagsH4.length; i++) { otreplace(tagsH4[i], "ot-h4"); }

    var otImg = document.querySelectorAll('#onetrust-pc-sdk img'); 

    for(i=0; i < otImg.length; i++) { otImg[i].setAttribute("loading", "lazy"); }

    var otImg = document.querySelectorAll('#onetrust-pc-sdk img'); for(i=0; i < otImg.length; i++) { otImg[i].setAttribute("loading", "lazy"); }var otImg = document.querySelectorAll('#onetrust-pc-sdk img'); for(i=0; i < otImg.length; i++) { otImg[i].setAttribute("loading", "lazy"); }
  }       

  function otreplace(tag, otclass) {
    var parent = tag.parentNode;
    var newDiv = document.createElement("div");
    if (tag.id != undefined) newDiv.setAttribute("id", "" + tag.id + "");
    newDiv.classList = tag.classList;
    newDiv.innerHTML = tag.innerHTML;
    newDiv.classList.add(otclass);
    parent.insertBefore(newDiv, tag);
    parent.removeChild(tag);
  }
  <!-- End OneTrust replace headers -->
</script>
