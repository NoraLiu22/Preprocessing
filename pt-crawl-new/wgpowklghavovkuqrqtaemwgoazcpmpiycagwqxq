		var siteCanonical = document.querySelector("link[rel='canonical']");
		var urlref = '';

		if(null !== siteCanonical) {
			urlref = siteCanonical.href;
		} else {
			var fullPath = location.pathname.split('/');
			var endPath = fullPath[2];
			urlref = location.href.replace(endPath,'');
