ope, Hysope Officinale, Jufa, Rabo De Gato, Ysop.\u003Cbr\u002F\u003E\u003Cbr\u002F\u003E","activetab":"overview","route":{"fullPath":"\u002Fvitamins\u002Fai\u002Fingredientmono-258\u002Fhyssop","path":"\u002Fvitamins\u002Fai\u002Fingredientmono-258\u002Fhyssop","query":{},"hash":"","params":{"id":"258","name":"hyssop","articleid":"091e9c5e81e0c9c5","channelid":"1169"},"matched":[{"path":"\u002Fvitamins\u002Fai\u002Fingredientmono-:id\u002F:name","meta":{},"props":{"default":false},"children":[],"instances":{},"leaveGuards":[],"updateGuards":[],"enterCallbacks":{},"components":{"default":{}}}],"meta":{},"href":"\u002Fvitamins\u002Fai\u002Fingredientmono-258\u002Fhyssop"},"cms":{"friendlyURLData":null,"siteFronts":false,"modules":{"091e9c5e820d1a4b":{"module_settings":{"chronic_id":"091e9c5e820d1a4b","title":"webmd-vitamins-monograph-store","object_type":"wbmd_pb_module","wbmd_pb_module_category":"","wbmd_pb_module_xsl":{"path":"\u002Fwebmd\u002FPageBuilder_Assets\u002FXSL\u002FHTML\u002Fvue-webmd-vitamins-monograph-store.xsl","object_type":"wbmd_pb_xsl","chronic_id":"091e9c5e82079da7"},"Class":"HTML"}},"091e9c5e820d0b5c":{"module_settings":{"chronic_id":"091e9c5e820d0b5c","title":"webmd-vitamins-brandseeker-router","object_type":"wbmd_pb_module","wbmd_pb_module_category":"","wbmd_pb_module_xsl":{"path":"\u002Fwebmd\u002FPageBuilder_Assets\u002FXSL\u002FHTML\u002Fvue-webmd-vitamins-brandseeker-router.xsl","object_type":"wbmd_pb_xsl","chronic_id":"091e9c5e82079da8"},"Class":"HTML"}},"091e9c5e813b6dde":{"module_data":{"shared_Modules":{"SharedModules":[{"module_settings":{"title":"hicbn"},"module_data":{"groups":{"group":[{"group_name":"Conditions","link_items":{"link_item":[{"link_text":"ADD\u002FADHD","link_friendlyUrl":"https:\u002F\u002Fwww.webmd.com\u002Fadd-adhd\u002Fdefault.htm"},{"link_text":"Allergies","link_friendlyUrl":"https:\u002F\u002Fwww.webmd.com\u002Fallergies\u002Fdefault.htm"},{"link_text":"Arthritis","link_friendlyUrl":"https:\u002F\u002Fwww.webmd.c