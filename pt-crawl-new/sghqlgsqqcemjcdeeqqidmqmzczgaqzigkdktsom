                            }
                        });
                    }
                    
                        jQuery(document).ready(function($){
                            function throttleEvvnt(func, wait){
                                var timeout;
                                return function(){
                                    var context = this, args = arguments;
                                    var later = function(){
                                        timeout = null;
                                        func.apply(context, args);
                                    };
                                    clearTimeout(timeout);
                                    timeout = setTimeout(later, wait);
                                }
                            }
                            var evvntLoaded = false;
                            var throttledEvvnt = throttleEvvnt(function(){
                                if(!evvntLoaded){
                                    evvntLoaded = __tnt.elementOnScreen('#evvnt-widget-1269760');
                                    if(evvntLoaded){
                                        var s = document.createElement('script');
                                            s.setAttribute('src', '//discovery.evvnt.com/prd/evvnt_discovery_plugin-latest.min.js');
                                            s.onload = evvntDiscoveryInit;
                                        document.body.appendChild(s);
                                        window.removeEventListener('scroll', throttledEvvnt);
                                    }
                                }   
                            }, 100);
                            window.addEventListener('scroll', throttledEvvnt);
                        });
                    
                </script>
            </div></div></section></div><div id="tncms-block-1556422" class="tncms-block style-title overflow-scroll"><section id="block-1556422" class="block emphasis-h3   " ><div class="clearfix">
    
    <div class="block-title title-1556422 ">
        <div class="block-title-inner">
            <h3>
                
                    <a href="/entertainment/movies">
                
                Movies
                
                    </a>
                
            </h3>
        </div>
    </div></div><div class="card-grid   card-img-md">
        
        
