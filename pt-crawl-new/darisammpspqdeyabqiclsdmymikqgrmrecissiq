            <meta itemprop="url" content="https://bloximages.newyork1.vip.townnews.com/enformeva.com/content/tncms/assets/v3/editorial/5/6d/56d1efc0-bf95-11ee-aa5a-0b8df75101df/65b93278128bc.image.jpg">
                        <img
                            src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAADCAQAAAAe/WZNAAAAEElEQVR42mM8U88ABowYDABAxQPltt5zqAAAAABJRU5ErkJggg=="
                            alt="Yvonne Goad Pamunkey Artist"
                            class="img-responsive lazyload full default"
                            width="1175"
