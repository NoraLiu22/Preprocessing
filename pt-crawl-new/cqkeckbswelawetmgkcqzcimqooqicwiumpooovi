				})
			(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

			window.pmcGATriggered = false;
			window.dataLayer = window.dataLayer || [];

			
			if ( 'undefined' !== typeof Rollbar && 'undefined' !== typeof window.pmcGARollbar ) {
