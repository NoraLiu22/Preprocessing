            this.showPaisModal = function () {
                // Muestra modal
                $('#modalFilter').modal('show');
                // check radio pais selectedd
            }
            this.printChart = function () {
                // Manda a imprimir
                // working
                $('#modalPDF').modal('show');
                $('#modalFilter').modal('hide');
                // ========= Seccion para PDF
                var result_img;
                var fN = this.itemsArray()[0].current_week.replace(/-/g, '');
                var fullUri = 'http://charts.monitorlatino.com:88/top20/Print/Nicaragua/General/' + fN + '/' + this.orden();
                // var fullUri = 'http://charts.monitorlatino.com/top20/Print/Nicaragua/General/' + fN + '/' + this.orden();
                $.ajax({
                    url: 'http://charts.monitorlatino.com/recursos/ws_PDF.asmx/getIMGfromHTML?uri=%22' + fullUri + '%22',
                    success: function (data) {
                        result_img = data["childNodes"][0].innerHTML;
                        // Si es IE EDGE o inferior
                        if (result_img == undefined || result_img == null) {
                            result_img = data.lastChild.childNodes['0'].data;
                        }
                        if (result_img != undefined && result_img != null) {
                            result_img = result_img.replace("http://charts.monitorlatino.com/recursos/tops/", '');
                        }

                        var webMethod = "http://charts.monitorlatino.com/recursos/getpdf.asmx/HtmltoPDF";
                        var lk = "http://charts.monitorlatino.com/recursos/pdf_img_top20.aspx?img=" + result_img;

                        $.ajax({
                            url: webMethod,
                            type: "POST",
                            data: "{ html: '" + encodeURI(lk) + "' }",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function (msg) {
                                $('#modalPDF').modal('hide');
                                window.open('http://charts.monitorlatino.com/recursos/pdf/' + msg.d, '_blank');
                            },
                            error: function (e) {
                                alert("WebSerivce unreachable");
                            }
                        });

                    }
                });
                // ========= Seccion para PDF

            }
            this.setPaisFilter = function () {
                //debugger;
                // Aplica Orden solo actualiza modelo.
                if (this.pais() == $('#selectPais option:selected').val() && this.formato() == $('#selectedFormat option:selected').val()) {
                    // Revisa Orden
                    if (this.orden() == $('#selectedOrd option:selected').val()) {
                        //console.log('Mismo Orden' + this.orden() + ' - ' + $('#selectedOrd option:selected').val());
                        //debugger;
                    } else {
                        // Cambia orden()
                        this.orden($('#selectedOrd option:selected').val());
                        var ordenText = this.ordenArray().filter(function (_item) {
                            if (_item.idOrden == self.orden()) {
                                return true;
                            }
                        });

                        this.labelHeader(this.pais() + ' / ' + this.formato() + ' / ' + ordenText[0].Orden);
                        this.ordenTxt(ordenText[0].Orden);


                        // Actualiza itemsArray
                        if (this.orden() == 0) {
                            self.itemsArray(self.audienceArray());
                        } else {
                            self.itemsArray(self.spinsArray());
                        }
                    }
                    $('#modalFilter').modal('hide');


                } else {

                    this.pais($('#selectPais option:selected').val());
                    this.formato($('#selectedFormat option:selected').val());
                    wopen(this.formato(), this.pais());

                }

            }
        }

        function ajustaTabla() {
            var tl = $('div.orden0 table').height();
            var tr = $('div.orden1 table').height();
            if (tl > tr) {
                $('div.orden1 table').height(tl);
            } else if (tl < tr) {
                $('div.orden0 table').height(tr);
            }
        }

        var items = new itemsModel();
        /*$(document).click(function(){
            ajustaTabla();
            // si lo deseamos podemos eliminar el evento click
            // una vez utilizado por primera vez
            $(document).unbind("click");
        });*/

        $(document).ready(function () {
            $('tr span.circle').removeClass('invisible');
            $('tr span.circleYT').removeClass('invisible');
            items.fecha(dateFormatMMM_DD_YYYYY(items.itemsArray()[0].current_week));
            ko.applyBindings(items);

            $('#selectPais').val(items.pais());
            $('#selectedFormat').val(items.formato());
            // cambia formatos
            $('#selectPais').change(function (obj) {
                var p = $(this).find('option:selected').val();
                $.ajax({
                    url: '/top20/retFormats/Nicaragua',
                    data: { "pais": p },
                    type: 'POST',
                    success: function (data) {
                        items.formatsArray($.parseJSON(data.replace(/&quot;/g, '"').replace(/&#241/g, '')));

                        // Orden
                        $.ajax({
                            url: '/top20/retOrden/Nicaragua',
                            data: { "pais": p, "extra": 0 },
                            type: 'POST',
                            success: function (data) {
                                items.ordenArray($.parseJSON(data.replace(/&quot;/g, '"').replace(/&#241/g, '')));
                            }
                        });

                    }
                });

            });
            $('td.total').html(''), $('div.special').html('')
        });

        if (md.mobile() != null && md.mobile().length >= 0) {

            // mobile
            items.EsMobile(true);
            $('div#fullScreen').addClass('hide'); // Oculta
            $('#filterDesktop').addClass('hide'); // Oculta
            $('div#fullMobile').removeClass('hide'); // Muestra
            $('div#filterMobile').removeClass('hide'); // Muestra
            $('#btnPrint_').removeClass('hide');

            var w = $(document).width();
            $('ins.adsbygoogle').width(w);


        } else {
            // DesckTop
            items.EsMobile(false);
            $('div#fullScreen').removeClass('hide'); // Muestra
            $('#filterDesktop').removeClass('hide'); // Muestra

            $('div#fullMobile').addClass('hide'); // Oculta
            $('div#filterMobile').addClass('hide'); // Oculta

            $('#btnPrint_').addClass('hide');

        }

        function wopen(fmt, p, fecha) {
            fecha = fecha || 'nd';

            //window.open('http://localhost:56744/top20/' + p + '/' + fmt, '_self');
            var arrH = window.location.href.split("/");
            if (arrH[4] == 'anual') {
                window.open(urlPath() + '/top20/anual/' + p + '/' + fmt, '_self');
            } else {
                if (fecha != 'nd') {
                    window.open(urlPath() + '/top20/' + p + '/' + fmt + '/' + fecha, '_self');
                } else {
                    window.open(urlPath() + '/top20/' + p + '/' + fmt, '_self');
                }
            }



        }

        function urlPath() {
            var path = '';
            var arrH = window.location.href.split("/");
            path = arrH[0] + "//" + arrH[2];

            return path;
        }

