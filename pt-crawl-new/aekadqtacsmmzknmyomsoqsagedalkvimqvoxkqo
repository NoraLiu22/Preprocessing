                //alert(fechaN);
                wopen(this.formato(), this.pais(), fechaN);
            }
            this.nwChartD = function () {
                if (this.itemsArray()[0].next_week == this.itemsArray()[0].current_week) {
                    swal("Atencin", "Este es el ltimo chart generado.", "warning");
                } else {
                    var fechaN = this.itemsArray()[0].next_week.replace(/-/g, '');
                    wopen(this.formato(), this.pais(), fechaN);
                }
            }
