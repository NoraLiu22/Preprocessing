    <script>

        var md = new MobileDetect(window.navigator.userAgent);

        var monthShortNames = ["Ene", "Feb", "Mar", "Abr", "May", "Jun",
        "Jul", "Ago", "Sep", "Oct", "Nov", "Dic"
        ];

        function parseDate(input) {
            var parts = input.match(/(\d+)/g);
            //' 00:01'
            return new Date(parts[0], parts[1] - 1, parts[2], '00', '01');
        }




        function dateFormatMMM_DD_YYYYY(d) {
            var t = new Date(d + ' 00:01');
            t = parseDate(d);
            //alert(parseDate(d));
            return monthShortNames[t.getMonth()] + ' ' + t.getDate() + ', ' + t.getFullYear();
            //return monthFullNames[t.getMonth()] + ' ' + t.getDate() + ', ' + t.getFullYear();

        }

