    $('#site-search-1075949-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1075949').submit();
        return false;
    });

    $('#site-search-1075949').on('submit', function(){
        // Filter query
        var elem = document.querySelector("#site-search-1075949 input[name=q]"),
        sQueryFiltered = elem.value.replace(/\?/g, '');
    elem.value = sQueryFiltered;

        if( $('#site-search-1075949-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
