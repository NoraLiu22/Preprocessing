            });

            var $landing_feature = $('.lndg_carousel_main').flickity({
                accessibility: false,
                cellAlign: 'center',
                pageDots: false,
                imagesLoaded: true,
                draggable: false,
                cellSelector: '.main_cell',
                adaptiveHeight: true
            });
        </script>
    </div>

        
