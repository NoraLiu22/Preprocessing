        if(__tnt?.video?.vastUrl) __tnt.video.vastUrl = false;
    }
    jQuery(document).ready(function($){
        $(document).find('[data-vast-url]').each(function(){
            if(__tnt?.video?.vastUrl){
                $(this).attr('data-vast-url', __tnt.video.vastUrl);
