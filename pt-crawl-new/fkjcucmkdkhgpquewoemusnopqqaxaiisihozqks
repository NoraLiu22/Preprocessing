<script>
    
    function openOne(target){
        // get list of nested subnavs, and if no subnavs are open, open the first
        var subnavs = target.querySelectorAll(`.hh-subnav`);
        var opened = [...subnavs].filter((e)=> e.classList.contains('open'));
        if(opened.length < 1) subnavs[0].classList.add('open');
    }
    
    async function swapMobile(btn,target){
    
        // get slide duration for timing purposes
        var slideDuration = parseFloat(getComputedStyle(target)['transitionDuration'])*1000;
            
        // get height for open / close animation
        var height = target.getBoundingClientRect().height;
        
        // check for any already opened btns
        var old = {};
        var old_target = '.'+btn.className+'.open';
        if(btn.dataset.id) old_target += `[data-topic="${btn.dataset.topic}"]`;
        old.btn = document.querySelector(old_target);
    
        // remove open state from currently open element
        if(old.btn){
            old.content = document.querySelector(old_target+' + div');
            if(old.content) old.content.style.maxHeight = old.content.getBoundingClientRect().height + 'px';
            setTimeout(()=>{
                old.btn.classList.remove('open');
            },50);
        }
    
        // if the btn is open already... close it
        if(btn.classList.contains('open')) {
    
            // set height
            target.style.maxHeight = height + 'px';
    
            // wait 50ms to trigger slide animation
            setTimeout(()=> {
                
                // remove open class
                btn.classList.remove('open');
                
                // scroll into view...
                setTimeout(()=> { 
                
                    // if there is an open nav button...
                    var open = document.querySelector('.hh-nav.open');
                    var slideTarget = open ? open : document.querySelector('.hh-main');
                    slideTarget.scrollIntoView({behavior: "smooth", block: "start", inline: "nearest"});
                }, 0);
            
            },50);    
            
        } 
    
        // if the btn is closed...
        else {
    
            // get height (cause closed state is always 0)
            // need to replicate surrounding environment as close as possible to account for padding / etc
            var clone = document.createElement('div');
                clone.innerHTML = 
                    `<div class="container">
                        <div class="hh-main">
                            <div class="hh-topics">
                                ${target.outerHTML}                        
                            </div>                    
                        </div>                
                    </div>`;
                clone.style.position = 'static';
            document.body.appendChild(clone);
            height = clone.getBoundingClientRect().height;
            clone.remove();
    
            // apply height
            target.style.maxHeight = height + 'px';
    
            // apply open class
            btn.classList.add('open');
            
            // after slide animation...
            setTimeout(()=> {
            
                // if btn is main nav, remove maxheight declaration
                if(btn.classList.contains('hh-nav')) target.style.maxHeight = 'inherit';
            
                // scroll to top of the now active button  
                btn.scrollIntoView({behavior: "smooth", block: "start", inline: "nearest"});
            
            }, slideDuration);
        
        }  
        clickable = true;
        
        // declare swapMobile as finished
        return;
    }
    
    function swapDesktop(btn,target) {
        
        // ensure at least one subnav is already visible for this topic
        if(btn.classList.contains('hh-nav')) openOne(target);
        
        // get siblings
        var siblings = document.querySelectorAll('.'+btn.className);
        var old = [...siblings].filter((e)=>e.classList.contains('open'));
        
        // get transition duration
        var duration = parseFloat(getComputedStyle(target)['transitionDuration'])*1000;
        
        // if one is already open...
        if(old.length > 0){
            
            // fade out old
            old.forEach((e)=> e.classList.add('transition-out'));        
            btn.classList.add('transition-in');
            
            // after a duration, fade in the new
            setTimeout(()=>{
                
                target.classList.add('fading');
                setTimeout(()=>{
    
                    setTimeout(()=> {
                        btn.classList.remove('transition-in');
                        target.classList.remove('fading');
                        btn.classList.add('open');
                        old.forEach((e)=> e.classList.remove('open','transition-out'));
                        clickable = true;
                    },duration);
    
                },5);
                
            },duration/2);
            
        }
        
        // if none are currently open already
        else {
            
            target.classList.add('fading');
            setTimeout(()=>{
                btn.classList.add('transition-in');
                
                setTimeout(()=> {
                    btn.classList.remove('transition-in');
                    target.classList.remove('fading');
                    btn.classList.add('open');
                    clickable = true;
                },duration);
                
            },5);
            
        }
            
    
    }
    
    
    const btns = document.querySelectorAll('button[data-target]');
    let clickable = true;
    btns.forEach((btn)=>{
        
        btn.addEventListener('click',(e)=> {
            if(!clickable) return;
            clickable = false;
            var target = document.querySelector(btn.dataset.target);
            if(window.innerWidth>=768) {
                if(btn.classList.contains('open')){
                    clickable = true;
                    return;                
                } else swapDesktop(btn,target);
            }
            else swapMobile(btn,target);
        });
    });
    
    const links = document.querySelectorAll('.hh-content [data-nav]');
    links.forEach((link)=>{
        
        link.addEventListener('click',(e)=> {
        
            e.preventDefault();
        
            if(link.dataset.nav === 'contact') document.getElementById('tncms-block-3007975').scrollIntoView({behavior: "smooth", block: "start", inline: "nearest"});
            else {
                if(!clickable) return;
                clickable = false;
                
                // get currently open menu button
                var currentNav = document.querySelector(`.hh-nav.open`);
                
                // get subnav
                var subnav = document.querySelector(`.hh-subnav[data-id="${link.dataset.nav}"]`);
                var subnavTarget = document.querySelector(subnav.dataset.target);
                
                // get nav
                var nav = document.querySelector(`.hh-nav[data-topic="${subnav.dataset.topic}"]`);
                var navTarget = document.querySelector(nav.dataset.target);
                
                if(window.innerWidth>=768) {
                    
                    // first open main nav
                    swapDesktop(nav,navTarget);
                        
                    // then open subnav}
                    swapDesktop(subnav,subnavTarget);
                }
                else {
                
                    async function swapBothMobile() {
                    
                        // first open main nav (if not already open)
                        if(currentNav !== nav) await swapMobile(nav,navTarget);
                        
                        // then open subnav}
                        swapMobile(subnav,subnavTarget);
                    }
                    
                    swapBothMobile();
                } 
                
                document.querySelector(".hh-main").scrollIntoView({behavior: "smooth", block: "start", inline: "nearest"});
            }
        });
    });
</script>

    
</div><div id="tncms-block-3008029" class="tncms-block">


