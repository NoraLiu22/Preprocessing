		<script>
		    /*<![CDATA[*/
		    function updateOnLoad(){
		    	if ($('#membAddlGiftInput').length > 0 && $('#membAddlGiftInput').val().trim().length > 0) {
			        $('#membAddlGiftInput').change();
			    }
		    	if ($('.js-gift-other-amt').length > 0 && $('.js-gift-other-amt').val().trim().length > 0) {
		        	$('.js-gift-other-amt').change();
		    	}
		    	
		    	olgGift2.setGiftCommentVisibility();
		    }
		
		    if(window.addEventListener){
		        window.addEventListener('load',updateOnLoad,false); //W3C
		    }
		    else{
		        window.attachEvent('onload',updateOnLoad); //IE
		    }
		    /*]]>*/
		</script>

