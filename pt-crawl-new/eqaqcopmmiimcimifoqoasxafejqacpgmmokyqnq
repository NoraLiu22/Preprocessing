<div class="taxonomy-tree-wrap collapsible-facet">

        <header class="sf-group-header">
            <h4 class="title">GeoRef Subject</h4>
            <i class="sf-group-header-icon icon-general_arrow-right"></i>
        </header>
        <div class="taxonomy-node" data-id="GroupType/30">
            <!-- NODE PARENT -->
            
            <!-- NODE CHILDREN -->
