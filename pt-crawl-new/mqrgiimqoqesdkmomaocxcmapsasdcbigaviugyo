var iSize=1;
function BiggerFont(){
	var x = document.getElementsByTagName("BODY")[0];
	iSize=iSize+.3
	x.style.zoom=iSize;
	//-moz-transform:x.style.scale(2);
}
function SmallerFont(){
	var x = document.getElementsByTagName("BODY")[0];
	iSize=iSize-.3
	x.style.zoom=iSize;
	//-moz-transform:x.style.scale(2);
}

function ShowFileList(){
	var oDocList = document.getElementById("tocListFloat");
	if(oDocList.className=="HideDocList"){
		oDocList.className="ShowDocList";
	}else{
		oDocList.className="HideDocList";
	}
	oDocList.focus();
}
