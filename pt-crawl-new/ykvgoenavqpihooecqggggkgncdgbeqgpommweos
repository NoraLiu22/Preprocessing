           data-id="1024474"
           data-url="/recipes/1024474-chicken-karaage"
           data-uri="nyt://recipe/6fa39029-6830-5cf9-9677-5012442eafdd"
           data-seo-image-url="https://static01.nyt.com/images/2023/09/07/multimedia/EK-Chicken-Karaage-gvfq-copy/EK-Chicken-Karaage-gvfq-articleLarge.jpg"
