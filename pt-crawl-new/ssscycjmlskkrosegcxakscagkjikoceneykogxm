      <script>
        try {
          var selected = localStorage.getItem('theme') || 'light';
          var dd = document.currentScript.previousElementSibling;
          if (dd.offsetParent !== null) {
            dd.querySelectorAll('.dropdown__link').forEach(function(opt) {
              if (opt.dataset.value === selected) {
                dd.querySelector('.dropdown__label').innerHTML = opt.innerHTML;
              }
            })
          }
        } catch (e) {}
