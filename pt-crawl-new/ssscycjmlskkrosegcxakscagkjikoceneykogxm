            <svg width="18" height="18" class="mag-lens">
              <use href="/media/components/masthead/sprite.svg#search"></use>
            </svg>
            Search
          </a>

          <div class="mobile-sections__link">
    <div class="dropdown dropdown--subtle theme-picker" 
      ><button class="dropdown__button theme-picker__button" disabled>
        <span class="dropdown__label"></span>
        <svg
          class="dropdown__icon"
          width="10" height="6" viewBox="0 0 10 6"
          xmlns="http://www.w3.org/2000/svg"
          role="img"
        >
          <title>Activating this button will toggle the display of additional content</title>
          <path d="M0 0l5 6 5-6z"/>
        </svg>
      </button>
      <div class="dropdown__content">
        
    <button  class="dropdown__link theme-picker__option"  data-value="light">
      <svg width="16" height="16">
        <use href="/media/sites/slate-com/global-sprite.svg#light"></use>
      </svg>
      Light

    </button>
  
    <button  class="dropdown__link theme-picker__option"  data-value="dark">
      <svg width="16" height="16">
        <use href="/media/sites/slate-com/global-sprite.svg#dark"></use>
      </svg>
      Dark

    </button>
  
    <button  class="dropdown__link theme-picker__option"  data-value="auto">
      <svg width="16" height="16">
        <use href="/media/sites/slate-com/global-sprite.svg#auto"></use>
      </svg>
      Auto

    </button>
  
      </div>
    </div>
    
      <script>
        try {
          var selected = localStorage.getItem('theme') || 'light';
          var dd = document.currentScript.previousElementSibling;
          if (dd.offsetParent !== null) {
            dd.querySelectorAll('.dropdown__link').forEach(function(opt) {
              if (opt.dataset.value === selected) {
                dd.querySelector('.dropdown__label').innerHTML = opt.innerHTML;
              }
            })
          }
        } catch (e) {}
      </script>
    
  </div>

