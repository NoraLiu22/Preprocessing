<script>
$(document).ready(function() {
  $('img.nocache').each(function () {
      var $this = $(this);
      $this.attr('src', noCache($this.attr('src')));
  });
});
function noCache(url) {
  return url + (url.indexOf('?') >= 0 ? '&nocache=' : '?') + $.now();
}
</script>

