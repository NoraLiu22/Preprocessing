                    <div>
                        <div class="ms-webpart-zone ms-fullWidth">
		<div id="MSOZoneCell_WebPartctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e" class="s4-wpcell-plain ms-webpartzone-cell ms-webpart-cell-vertical ms-fullWidth ">
			<div class="ms-webpart-chrome ms-webpart-chrome-vertical ms-webpart-chrome-fullWidth ">
				<div class="ms-webpart-chrome-title" id="WebPartctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_ChromeTitle">
					<span title="Homepage Banner Image - PAI Enterprise Web Parts - Homepage Banner Image" id="WebPartTitlectl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e" class="js-webpart-titleCell"><h2 style="text-align:justify;" class="ms-webpart-titleText">&nbsp;<span>Homepage Banner Image</span><span id="WebPartCaptionctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e"></span></h2></span>
				</div><div WebPartID="500eeaf9-9035-49f9-9330-5447b185916e" HasPers="false" id="WebPartctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e" width="100%" class="ms-WPBody noindex ms-wpContentDivSpace " allowDelete="false" allowExport="false" style="" ><div id="ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e">
					

<style id="carouselStyles">
</style>

<script>
    var currentIndex = 1;
    var allCarousels;
    var allbuttonLinks;
    var allButtonLabels
    var aryCarousels;
    var count;
    var arybuttonLinks;
    var aryButtonLabels;
    var aryBannerLabels;
    var aryHeaderLabels;
    var arySubHeaderLabels;
    var headerLabel;
    var subHeaderLabel;
    var bannerLabel
    $(document).ready(function () {
        allCarousels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfCarousel").val());
        allbuttonLinks = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hdButtonLinks").val());
        allButtonLabels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfButtonlabels").val());
        allBannerLabels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfBannerLabels").val());
        allHeaderLabels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfHeaderTextLabels").val());
        allSubHeaderLabels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfHeaderSubTextLabels").val());

        var header = document.getElementById('bannerHeader');
        header.style.display = "";
        var subHeader = document.getElementById('bannerSubheader');
        subHeader.style.display = "";
        if (allCarousels) {
            //Carousel
            var nextButton = document.getElementById('BannerNext');
            nextButton.style.display = "";
            var PrevButtom = document.getElementById('BannerPrev');
            PrevButtom.style.display = "";
            aryCarousels = allCarousels.split(";");

            if (aryCarousels.length > 6) {
                aryCarousels = aryCarousels.splice(0, 6);
            }

            count = aryCarousels.length;

            for (var i = 0; i < count; i++) {
                var preloadIndex = i + 1;
                $('#imgPreload' + preloadIndex).attr('src', aryCarousels[i]);
            }

            var lastIndex = 1;
            var isBeginning = true;

            if (allbuttonLinks) {
                arybuttonLinks = allbuttonLinks.split(";");
            }
            if(allButtonLabels) {
                aryButtonLabels = allButtonLabels.split(";");
            }
            if(allBannerLabels) {
                aryBannerLabels = allBannerLabels.split(";");
            }
            if(allHeaderLabels) {
                aryHeaderLabels = allHeaderLabels.split(";");
            }
             if(allSubHeaderLabels) {
                arySubHeaderLabels = allSubHeaderLabels.split(";");
            }

            
            $("#buttonText").html(aryButtonLabels[0]);
            $('.banner').attr('aria-label', aryBannerLabels[0]);
            
            $('#bannerHeader').html(aryHeaderLabels[0]);
            $('#bannerSubheader').html(arySubHeaderLabels[0]);
            $('#BannerButtonLink').attr('href', arybuttonLinks[0]);
           checkExternalLink();

          }
          else {
              var nextButton = document.getElementById('BannerNext');
              nextButton.style.display = "none";
              var PrevButtom = document.getElementById('BannerPrev');
              PrevButtom.style.display = "none";

              var buttonLink = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hdButtonLinks").val());
              var buttonLabel = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfButtonlabels").val());
              bannerLabel = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfBannerLabels").val());
            if (buttonLink) {
                $('#BannerButtonLink').attr('href', buttonLink);
               
            }
            if (buttonLabel) {
              
                    $("#buttonText").html(buttonLabel);
            }
             if (bannerLabel) {
                $('.banner').attr('aria-label', bannerLabel);
            }

              headerLabel = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfHeaderTextLabels").val());
              subHeaderLabel = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfHeaderSubTextLabels").val());
            
            if (headerLabel) {
                $('#bannerHeader').html(headerLabel);
            }
            if (subHeaderLabel) {
                $('#bannerSubheader').html(subHeaderLabel);
            }
            checkExternalLink();
        
        }
    });
    function checkExternalLink()
    {
        var currentLink = document.getElementById("BannerButtonLink");
        currentLink.innerHTML = ""; 
        if (($(currentLink).has('img').length) === 0) { // && (!$(currentLink).hasClass('external'))
                if (currentLink.hostname !== location.hostname && (linkIsEnterpriseApproved(currentLink.href) === false)) {
                    $(currentLink).addClass('external');
                    $(currentLink).attr('target', '_blank');
                    $(currentLink).append("<span class='show-for-sr'>Opens In A New Window</span>");
                    if ($(currentLink).hasClass('contentTile')) {
                        var currentP = $(currentLink).find('p');
                        $(currentP).append("<i class='pais pai-external-link'></i>");
                    }
                    else {
                        $(currentLink).append("<i class='pais pai-external-link'></i>");
                    }
                }
            }
    }
    function setCustIndex(toDo) {

        if (typeof toDo != 'undefined' && toDo) {
            var Carousels = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_hfCarousel").val());

            if (allCarousels) {

                if (toDo === "next") {
                    var prevIndex = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfNavigation").val());
                    if (prevIndex === "") {
                        prevIndex = 1;
                    }
                    prevIndex = parseInt(prevIndex, 10)
                    currIndex = prevIndex + 1;
                    if (currIndex > count) {
                        currIndex = 1;
                    }

                    document.getElementById('ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfNavigation').value = currIndex;
                    $('#BannerButtonLink').attr('href', arybuttonLinks[currIndex-1]);
                    
                    $("#buttonText").html(aryButtonLabels[currIndex-1]);
                    $('.banner').attr('aria-label', aryBannerLabels[currIndex-1]);
                    $('#bannerHeader').html(aryHeaderLabels[currIndex-1]);
                    $('#bannerSubheader').html(arySubHeaderLabels[currIndex-1]);
                    

                    $('.banner').removeClass('homepage-banner-' + prevIndex);
                    $('.banner').addClass('homepage-banner-' + currIndex);
                    $('.BannerHeading').removeClass('HPBannerHeading-' + prevIndex);
                    $('.BannerHeading').addClass('HPBannerHeading-' + currIndex);

                    $('.BannerSubtext').removeClass('HPBannerSubText-' + prevIndex);
                    $('.BannerSubtext').addClass('HPBannerSubText-' + currIndex);

                    $('.BannerButton').removeClass('HPBannerButton-' + prevIndex);
                    $('.BannerButton').addClass('HPBannerButton-' + currIndex);
                    currentIndex = currIndex + 1;
                    checkExternalLink();
                }
                if (toDo === "prev") {
                    var prevIndex = ($("#ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfNavigation").val());
                    if (prevIndex === "") {
                        prevIndex = 1;
                    }
                    prevIndex = parseInt(prevIndex, 10)
                    currIndex = prevIndex - 1;
                    if (currIndex === 0) {
                        currIndex = count;
                    }

                    document.getElementById('ctl00_ctl50_g_500eeaf9_9035_49f9_9330_5447b185916e_HfNavigation').value = currIndex;
                    $('#BannerButtonLink').attr('href', arybuttonLinks[currIndex-1]);
                    
                    $("#buttonText").html(aryButtonLabels[currIndex-1]);
                    $('.banner').attr('aria-label', aryBannerLabels[currIndex-1]);
                   
                    $('#bannerHeader').html(aryHeaderLabels[currIndex-1]);
                    $('#bannerSubheader').html(arySubHeaderLabels[currIndex-1]);

                    $('.banner').removeClass('homepage-banner-' + prevIndex);
                    $('.banner').addClass('homepage-banner-' + currIndex);
                    $('.BannerHeading').removeClass('HPBannerHeading-' + prevIndex);
                    $('.BannerHeading').addClass('HPBannerHeading-' + currIndex);

                    $('.BannerSubtext').removeClass('HPBannerSubText-' + prevIndex);
                    $('.BannerSubtext').addClass('HPBannerSubText-' + currIndex);

                    $('.BannerButton').removeClass('HPBannerButton-' + prevIndex);
                    $('.BannerButton').addClass('HPBannerButton-' + currIndex);
                    currentIndex = currIndex + 1;
                    checkExternalLink();
                }
            }
        }
    }
</script>

