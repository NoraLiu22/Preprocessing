<script>
	var myModal = new bootstrap.Modal(document.getElementById('myModal'), {
		keyboard: false,
		backdrop: true
	})

	var myModalEl = document.getElementById('myModal')
	myModalEl.addEventListener('hide.bs.modal', function(event) {
		// do something...
		sessionStorage.setItem('stack-modal', 'shown');
	})

	//if(sessionStorage.getItem('stack-modal') != 'shown') {
	//    $(window).on('load', function() {
	//        myModal.show();
	//        $('subs-body').hide();
	//    });
	//}

	$("#stack-hide").click(function(){
		$("#mktbar").css({"display":"none",});
		sessionStorage.setItem('stack-footer', 'hidden');

	});
	$('#td-header-search-button').click(function(){
		$('.td-drop-down-search').addClass("td-drop-down-search-open");
	});
	$('#search-close').click(function(){
		$('.td-drop-down-search').removeClass("td-drop-down-search-open");
	});
</script>
