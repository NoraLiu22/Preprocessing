	$(document).ready(function() {
		$("#frmContact").submit(function(){
			var tel = $("#phone").val();
			if (isNaN(tel)) 
			{
				alert("Phone must be numbers only");
				return false;
			}

			$("#frmContact").submit();
		});

		sports_images = ['https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23181489.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23180560.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23179320.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23173287.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23173016.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23168431.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23173479.jpg','https://www.gannett-cdn.com/usatsimg/image/thumb/900-600ac/23199043.jpg',];
		setInterval(forwardSportsImage, 5000);


		//This function will find the key for the current Image
		function currentSportsImageKey() {
		    i = jQuery.inArray($('#sports_slideshow').attr('src'), sports_images);
		    return i;
		}


		//This function will move the slideshow forward one
		function forwardSportsImage() {
		    currentSportsImageKey();
		    if (i < sports_images.length - 1) {
		        changeSportsImage(i + 1);
		    } else {
		        changeSportsImage(0);
		    }
		}


		//This function will change to image to whatever the variable i passes to it
		function changeSportsImage(i) {
		    $("#sports_slideshow").attr('src',sports_images[i]);

		}

		news_images = [		 
