<footer>If you want to update this page <pre>ssh tarsier.reading-web-staging.eqiad1.wikimedia.cloud</pre> and go to the /var/www/html/index.html folder.
<script>
fetch( `summary.json` )
    .then((d) => d.json())
    .then((j) => {
        Array.from(document.querySelectorAll('li a')).map((a)=> {
            const href = a.getAttribute( 'href' );
            const rows = j[href.replace(/\//, '')];
            a.dataset.errors = rows;
            const span = document.createElement( 'span' );
            span.textContent = ` ${rows} errors`;
            a.parentNode.appendChild( span );
        } );
    } ).then( () => {
        // make green if night is better than light mode.
        Array.from(document.querySelectorAll('li a')).map((a)=> {
            const li = a.parentNode;
            const href = a.getAttribute( 'href' );
            if ( href.indexOf( 'night' ) > -1 && li.previousElementSibling ) {
                console.log( li );
                const lightErrors = li.previousElementSibling.querySelector( 'a' ).dataset.errors;
                if ( parseInt( a.dataset.errors, 10 ) <= parseInt( lightErrors, 10 ) ) {
                    li.style.color = 'green';
                } else {
                    li.style.color = 'red';
                }
            }
        } );
    } );
</script>

