        $('#classifieds-search-1269781 .classifieds-map-toggle').on(__tnt.client.clickEvent, function(){
            if( $('#classifieds-search-1269781 .classifieds-search-map-container').hasClass('closed') ){
                $('#classifieds-search-1269781 .classifieds-search-map-container').removeClass('closed');
