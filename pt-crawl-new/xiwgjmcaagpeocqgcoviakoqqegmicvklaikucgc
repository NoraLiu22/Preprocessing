                        /*******************/
// Shared variables
/*******************/

var currentFileset,
    currentManifest = 1,
    firstLoad = true,
    hash = window.location.hash.replace('#track/',''),
    hasHash = false,
    meta_id = "metadc19781",
    offset,
    physicalDescription = $("#physical-description").text(),
    playerInstance = jwplayer("ark-player"),
    $playlist = $("#visual-playlist"),
    $playlistRow = $(".playlist__row"),
    silent = " si.,",
    $timestampInput = $("#copy-timestamp"),
    $timestampSuccess = $("#timestamp-success"),
    totalPlaytime = 0,
    totalDuration = Math.ceil(3391.895508,0),
    $viewDescriptionHere = $("#view-description-here");



/*******************/
// Utils
/*******************/

//
// get the current time offset in the playing video and insert into input for user copy
//
function updatePositions(){
    var position =  Math.floor(playerInstance.getPosition()),
        timeStamValue = $timestampInput.data('original-value') +'?s=' + position + window.location.hash ;
    $timestampInput.val(timeStamValue);
}


// percentage calculator for timeings. roundeded up.
function calculatePercentOfTotal(num, total){
    var calculated = (num/total)*100;
    return Math.ceil(calculated, 0);
}


//
// look in physical description for 'code' representing silence. if it exists, add human readable note.
//
if (physicalDescription.indexOf(silent) >= 0) {
    $viewDescriptionHere.prepend("Note: Our records indicate this item has no sound. ")
}




/*******************/
// AUDIO PLAYER SETUP
/*******************/

playerInstance.setup({
    "autostart": false,
    "playlist":currentPlaylist,
    "width": '100%',
    "height": '40',
    "displaytitle": false,
    "displaydescription": false,
    "playbackRateControls": [0.25, 0.50, 0.75, 1, 1.25, 1.5, 2, 3, 4],
    "skin": {
        "name": "unt-audio",
        "controlbar": {
          "background": "rgba(0,0,0,0.35)",
        }
    },
});






/*******************/
// On Ready
/*******************/

playerInstance.on('ready',function(){

    var playlist = playerInstance.getPlaylist();

    // did the hash string return a number?
    // decrement it, test if it's in the playlist
    // if so set the playlist to that index.
    if ($.isNumeric( hash )){
        var fileIndex = hash - 1;

        if ( fileIndex in playlist ){
            hasHash = true;
            playerInstance.playlistItem(fileIndex).stop();
        };
    };

    //
    // Buttons must be added to ready function to appear in dynamicly generated playlists
    //
    if (addedButtons) {
        // add button to ui to show time offsets
        playerInstance.addButton(
            "/media/assets/public/images/clock.svg",
            "Get a link to the current time.",
            function() {
                updatePositions();
                $timestampInput.select();
                document.execCommand('copy');
                $timestampSuccess.show().delay(5000).fadeOut(1000);
            },
            "offset-link",
            "unt-extension"
        );

        // add button to ui to jump user to sharing section;
        // Sharing API not available in commercial-free version of jwplayer
        playerInstance.addButton(
            "/media/assets/public/images/share.svg",
            "Share/Embed this Video.",
            function() {
                document.getElementById('cite').scrollIntoView();
            },
            "cite-link",
            "unt-extension"
        );
    }
    // If this is a playlist with more than 1 item, add next/prev buttons
    if (playerInstance.getPlaylist().length > 1) {
        playerInstance.addButton(
            "/media/assets/public/images/angle-right.svg",
            "Next Playlist Item.",
            function() {
                playerInstance.playlistItem( Math.max(0, playerInstance.getPlaylistIndex() + 1) );
            },
            "playlist-next",
            "unt-extension"
        );
        playerInstance.addButton(
            "/media/assets/public/images/angle-left.svg",
            "Previous Playlist Item.",
            function() {
               if (playerInstance.getPlaylistIndex() === 0) {
                    // is at video #1. Loop back to [-1]
                    playerInstance.playlistItem( Math.max(0, playerInstance.getPlaylist().length - 1) );
                }
                else {
                    // Otherwise, go to the previous
                    playerInstance.playlistItem( Math.max(0, playerInstance.getPlaylistIndex() - 1) );
                }
            },
            "playlist-previous",
            "unt-extension"
        );
    }


});

/*******************/
// On PlaylistItem
/*******************/

// On play of an item in the playlist:
// update the URL to include the track # (for bookmarking), don't effect history.
// Change value of time offset copy utility
// highlight the current active video

playerInstance.on('playlistItem', function(i){

    // utility, set current item for referencing during on.('time')
    currentFileset = playerInstance.getPlaylistIndex() + 1;

    // update the hash for playlist item plays with a fileset > 1.
    if (hasHash) {
        hasHash = false;
    } else {
        var prettyIndex = i.index+1;
        if (currentFileset > 1) {
            history.replaceState(null, null, "#track/"+prettyIndex);
        } else {
            var noHash = window.location.href.replace(/#.*$/, '');
            history.replaceState(null, null, noHash);
        }
        updatePositions();
    };

    // update visual playlist visual styles, highlight currently playhing item.
    $("[data-playlist-index='" + i.index + "']")
        .addClass('list-group-item-success')
        .siblings('.list-group-item-success')
        .removeClass('list-group-item-success');

});



/*******************/
// On Play
/*******************/

/*
playerInstance.on('play', function(){
});
*/

/***************************************************/
//  On First Frame
//  Applicable to all playlist items
/***************************************************/

playerInstance.on('firstFrame', function() {
    // Does the URL contain a query param?
    // Get the offset in seconds, test if numeric.
    // On pass, when played, make the 1st frame at the time offset.
    if (window.location.search && firstLoad) {
        var params = new URLSearchParams(window.location.search),
            offset = params.get('s');
        if ($.isNumeric( offset )){
            playerInstance.seek(offset);
            firstLoad = false;
        };
    }
});



/*******************/
// On "time"
/*******************/


playerInstance.on('time', _.throttle(function(e) {
    // current position
    // var p = e.position;
    // incrementally log the total played time for final beforeUnload reporting.
    totalPlaytime++
}, 1000));


/*******************/
// On Seek (complete)
/*******************/

// User seeks on the player control bar:
// update the time offset displayed in the copy utility
playerInstance.on('seeked', function(){
    updatePositions();
});



/*******************/
// On FullScreen
/*******************/


playerInstance.on('fullscreen', function(fs){
    // track user toggle of fullscreen viewer
    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Fullscreen: " + fs.fullscreen,
      "eventLabel": meta_id,        // Object ID
      "eventValue": 1               // incr. val to determine higher value content.
    });

});

