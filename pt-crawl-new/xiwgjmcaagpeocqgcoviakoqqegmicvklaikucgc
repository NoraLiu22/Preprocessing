// On Captions change
/*******************/


playerInstance.on('captionsChanged', function(cap){
    var changedIndex = cap.track;

    // Track user interaction: captions/subtitles toggled off <-> languages
    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Captions Toggled: " + cap.tracks[changedIndex].label,
      "eventLabel": meta_id,        // Object ID
      "eventValue": 1               // incr. val to determine higher value content.
    });
});


