// Using beacon, send GA data about the total played time for the A/V item.
$(window).on('beforeunload', function(){
    var durationPct = calculatePercentOfTotal(totalPlaytime,totalDuration);
        ga('send', {
          "hitType": "event",
          "eventCategory": window.gaPrefix + ": A/V Interactions",
          "eventAction": "Played",
          "eventLabel": meta_id,        // Object ID
          "eventValue": 1,              // count for totals of all plays
          "metric3": totalPlaytime,     // The actual played time of object (sec.). Can be longer than total Duration if user skips/rewinds.
          "metric4":  durationPct     // percent played as a whole number.
        });
});



                        
                            


