// Visual Playlist (fileset view)
/*******************************/

$playlistRow.on('click', function(){
    var playIndex = $(this).data("playlist-index");
    playerInstance.playlistItem(playIndex);

    // Track user interaction: click/change of visual playlist item (differs from automatic advancement)
    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Visual Playlist Update",
      "eventLabel": meta_id,        // Object ID
      "eventValue": 1               // incr. val to determine higher value content.
    });
});







