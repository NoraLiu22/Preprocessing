var tabsListSource = [

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }
,

    {"language": "English",
     "lang_code": "eng",
     "kind": "captions"
    }


]

var tabsListReduced = []
$.each(tabsListSource, function(i, el){
    var reduced = $.grep(tabsListReduced, function (item) {
       return item.language === el.language && item.kind === el.kind;
    });
    if (reduced.length === 0){
        tabsListReduced.push(el);
    }
});
console.log("reduced:", tabsListReduced);

$("#vtt-tabs").html(
    $.map(tabsListReduced, function(el) {
      switch (el.kind){
        case "captions":
         label = el.language + " Transcript"
         break;
        case "subtitles":
         label = el.language + " Translation"
         break;
        case "chapters":
         label = "Sections (" + el.language + ")"
         break;
        case "descriptions":
         label = el.language + " Description"
         break;
        case "metadata":
         label = "Metadata"
         break;
        default:
         label = el.language 
      }
      var link = $('<a >', {
                        "id" : el.lang_code + '-' + el.kind + '-trigger',
                        "href": "#" + el.lang_code + '-' + el.kind,
                        "aria-controls" : el.lang_code + '-' + el.kind,
                        "role" : "tab",
                        "class" : "vtt-tabber",
                        "data-which" : el.lang_code + '-' + el.kind,
                        "data-toggle" : "tab"
                  }).text(label);
      return $('<li/>', { role: "presentation" }).html(link);
    }) 
)
$('#vtt-tabs').find("a").first().tab('show');

var numbericSorter = function(key,asc,a,b){
    var num = asc? 1: -1;
    if(a[key] < b[key]) return -num;
    if(a[key] > b[key]) return  num;
    return 0;
}

/************************************/
// Captioning/Transcription variables
/************************************/

var captions = [],
    chapters = [],
    subtitles = [],
    caption = -1,
    query = "",
    $vttBlock = $("#vtt-block"),
    $search = $(".vtt-search"),
    matches = [],
    resultOffsetScrollHeight = $("#site-navigation").height() + $search.height() + 40,
    captionsVTTList,
    chaptersVTTList,
    descriptionsVTTList,
    metadataVTTList,
    subtitlesVTTList;

// Highlight the text that corresponds to the current time
// (throttle to 1 time per second) since on 'time' occurs multiple times.
playerInstance.on('time', _.throttle(function(e) {
    var p = e.position;
    highlightCurrentTimestamp(p);
}, 1000));

// When the user seeks, find the nearest caption timestamp and scoll to  it
playerInstance.on('seek', function(e) {
    var o = e.offset;
    highlightCurrentTimestamp(o);
});


// Filter the vtts array of objects into discreat lists based on the 'kind' attribute
captionsVTTList = vtts.filter(function (entry) { return entry.kind === "captions"; });
chaptersVTTList = vtts.filter(function (entry) { return entry.kind === "chapters"; });
descriptionsVTTList = vtts.filter(function (entry) { return entry.kind === "descriptions"; });
metadataVTTList = vtts.filter(function (entry) { return entry.kind === "metadata"; });
subtitlesVTTList = vtts.filter(function (entry) { return entry.kind === "subtitles"; });

captionsVTTList.sort(numbericSorter.bind(null, "fileset", true));
chaptersVTTList.sort(numbericSorter.bind(null, "fileset", true));
descriptionsVTTList.sort(numbericSorter.bind(null, "fileset", true));
metadataVTTList.sort(numbericSorter.bind(null, "fileset", true));
subtitlesVTTList.sort(numbericSorter.bind(null, "fileset", true));

// parse a webvtt timestamp to seconds
function parse_timestamp(s) {
    var match = s.match(/^(?:([0-9]+):)?([0-5][0-9]):([0-5][0-9](?:[.,][0-9]{0,3})?)/);
    if (match == null) {
        throw 'Invalid timestamp format: ' + s;
    }
    var hours = parseInt(match[1] || "0", 10),
        hourMatch = match[1] || "00",
        minutes = parseInt(match[2], 10),
        seconds = parseFloat(match[3].replace(',', '.')),
        totalSeconds = Math.floor( seconds + 60 * minutes + 60 * 60 * hours),
        timeObject = {};

    timeObject.seconds = totalSeconds;
    timeObject.stamp = hourMatch + ":" + match[2] +":" + match[3].substr(0, 2);
    return timeObject;
}

// build html output and push to DOM
function buildHTMLTranscript(vttIndex, vttList){
    // function repeats on each ajax complete until all vtt files have been processed

    // No more vtt lists to process, break the loop
    if (vttList.length == vttIndex) {
        return;
    }

    // set up current loop data
    var currentList = vttList[vttIndex], // current vtt item
        $domElement = $("#" + currentList.language_code + "-" + currentList.kind + "-content"), // Match to a DOM wrapper for all VTTs of this kind and language
        $vttFileSetContainer = $('<div class="' + currentList.kind + '-fileset-group" id="' + currentList.kind + '-' + currentList.language_code + '-' + currentList.manifest + '-' + currentList.fileset + '"></div>'), // generate a div for the current filset/vtt
        fileMeta = {}; // array of vtt metadata

    //console.log(currentList)
    //console.log($domElement)
    //console.log($vttFileSetContainer)

    $domElement.append($vttFileSetContainer);

    $.ajax({
        url: currentList.path,
        dataType: "text",  // jQuery will infer this, but you can set explicitly
        success: function( data, textStatus, jqXHR ) {
            var lines = data.replace('\r\n|\n+', '\n').split(/[\n]/).map(function(line) {
                return line.trim();
            });
            var line = null,
                blurb = {},
                fileMeta = {},
                start = null,
                end = null,
                dialog = null,
                id= null,
                reTextEnds = new RegExp("[\.\?\:](<\/v>)?$|^(\\[|\\(|\u{266a})(.*?)(\\]|\\)|\u{266a})$"), // delimiate text by either a period at the end of the line or a line surrounded by [ ].
                htmlString = "",
                wordcount = 0,
                isNewSentence = true,
                stamp = "",
                associatedPlaylist = playerInstance.getPlaylistItem(currentList.fileset - 1),
                kind = currentList.kind,
                language = currentList.language,
                language_code = currentList.language_code,
                numWords = 0,
                fallbackCounter = 0;

            for (var i = 0; i < lines.length; i++) {
                line = lines[i];

                // line contains key/value metadata about the file, store to object for future use
                if (line.startsWith('vtt_')) {
                    var line_meta = line.trim().split(":",2),
                        meta_key = line_meta[0].replace("vtt_","").trim(),
                        meta_val = line_meta[1].trim();
                        fileMeta[meta_key] = meta_val
                    if (meta_key == "method" && meta_val == "automated") {
                        $("#" + language_code + "-" + kind + " .automatic-caption-notice").show();
                    }
                }

                // timestamp lines
                if (line.indexOf('-->') >= 0) {
                    // Is this the timestamp line? If so, split set start/stop times
                    var splitted = line.split(/[ \t]+-->[ \t]+/);
                    start = parse_timestamp(splitted[0]);
                    end = parse_timestamp(splitted[1]);

                    // rational fallback for extremly long lines with bad punctuation
                    if (fallbackCounter >= 5) {
                        isNewSentence = true;
                    }

                    // captions and subtitles, we want a button to show timestamps at beginning of each new 'sentence/section'
                    if (isNewSentence || kind == "chapters") {
                        stamp = "<button data-ts='" + start.seconds +
                                "' data-manifest='" + currentList.manifest +
                                "' data-fileset='" + currentList.fileset +
                                "' data-lng='" + language_code +
                                "' data-kind='" + kind +
                                "' class='btn btn-xs btn-primary--ghost timestamp-btn'>" + start.stamp + "</button>";
                        isNewSentence = false;
                    } else {
                        stamp = "";
                    }


                    htmlString += stamp;


                } else if (line == '' || i == lines.length - 1) {
                    // is this a blank line or the last line?
                    if (start && end && dialog || start === 0 && end && dialog ) {
                        // has a time in/out been set? If so, we are in a cue.
                        // add data to the blurb object for later reference
                        blurb.id = language_code + "-" + kind + "-" +currentList.manifest + "-" + currentList.fileset + "-" + i;
                        blurb.manifest = currentList.manifest;
                        blurb.fileset = currentList.fileset;
                        blurb.lng = language_code
                        blurb.in = start.seconds;
                        blurb.out = end.seconds;
                        blurb.dialog = dialog.trim();

                        // construct an html line for this cue
                        htmlString += "<span class='" + kind + "-line' id='" + language_code + "-" + kind + "-" +
                            currentList.manifest + "-" + currentList.fileset + "-" + i +
                            "' data-ts='" + start.seconds +
                            "' data-title='" + start.stamp +
                            "' data-manifest='" + currentList.manifest +
                            "' data-fileset='" + currentList.fileset +
                            "' data-lng='" + language_code +
                            "' data-kind='" + kind +
                            "'>" +
                            dialog +
                            " </span>";
                        // add the cue to the captions array and unset loop variables.
                        switch(kind) {
                            case "captions":
                                captions.push(blurb);
                                break;
                            case "chapters":
                                chapters.push(blurb);
                                break;
                            case "subtitles":
                                subtitles.push(blurb);
                                break;
                        }
                        blurb = {};
                        id = null;
                        start = null;
                        end = null;
                        dialog = null;
                    }
                } else if( start && end || start === 0 && end) {
                    // iterate fallback counter to deal with possible bad punctuation.
                    fallbackCounter++
                    
                    numWords += line.split(' ').length;

                    // does the line end in a period or bracket? add line breaks.
                    if ( reTextEnds.test(line) || kind == "chapters" || fallbackCounter >= 5 ) {
                        line += "<br class='ts-break' />";
                        isNewSentence = true;
                    } else {
                        isNewSentence = false;
                    }
                    line.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
                    if (dialog == null) {
                        dialog = line + " ";
                    } else {
                        dialog += line + " ";
                    }
                }
                if (fallbackCounter >= 5) {
                    fallbackCounter = 0;
                }
            }
            // done looping through vtt

            // round number of words to nearest 100.
            if (numWords > 100) {
                numWords = Math.ceil(numWords/100)*100;
                var countString = " (about " + numWords.toLocaleString() + " words" +")";
            } else {
                var countString = ""
            }
            // generate a title for the vtt
            clipTitle = "<h3 class='h5'>" + associatedPlaylist.title +  countString + "</h3>"
            // add the generated html to the associated div on the page
            $vttFileSetContainer.html(clipTitle + htmlString);
        },
        /// re-run for each item in the list on complete.
        complete: function() {
            buildHTMLTranscript(++vttIndex, vttList);
        }
    });
};

// build captions-based HTML transcripts.
if (captionsVTTList.length) {
    buildHTMLTranscript(0,captionsVTTList)
}
// build chapter-based HTML transcripts.
if (chaptersVTTList.length) {
    buildHTMLTranscript(0,chaptersVTTList)
}
// build translation/subtitle-based HTML transcripts.
if (subtitlesVTTList.length) {
    buildHTMLTranscript(0,subtitlesVTTList)
}

// DRY chaining to add .current to an item
function toggleCurrentClass($selector){
    $selector.addClass("current")
              .siblings()
              .removeClass("current")
              .parent()
              .siblings()
              .find("span")
              .removeClass("current");
}

// utility: loop through captions/subtitle/chapter array:
// takes 1 argument: an interval representing a second in the a/v item.
// loop through the captions array, find object where arg < start > end time
// remove sibling .current, add .current to matching span
function highlightCurrentTimestamp(p){
    for(var j=0; j<captions.length; j++) {
        if(captions[j].in < p && captions[j].out > p && captions[j].manifest === currentManifest && captions[j].fileset === currentFileset) {
            var $c = $('#' + captions[j].id);
            toggleCurrentClass($c);
          break;
        }
    }

    for(var j=0; j<subtitles.length; j++) {
        if(subtitles[j].in < p && subtitles[j].out > p && subtitles[j].manifest === currentManifest && subtitles[j].fileset === currentFileset) {
            var $c = $('#' + subtitles[j].id);
            toggleCurrentClass($c);
        }
    }

    for(var j=0; j<chapters.length; j++) {
        if(chapters[j].in < p && chapters[j].out > p && chapters[j].manifest === currentManifest && chapters[j].fileset === currentFileset) {
            var $c = $('#' + chapters[j].id);
            toggleCurrentClass($c);
          break;
        }
    }

}

// Utility: Execute search
function searchTranscript(q, which) {
  matches = [];
  query = q;
  for(var i=0; i<which.length; i++) {
    var element = which[i],
        dialog = _.deburr(element.dialog),  // use lodash deburr to latinize strings.
        m = dialog.toLowerCase().indexOf(q),
        itm = {};
    if( m > -1) {
      document.getElementById(element.id).classList.add("result");
      itm.id = i,
      itm.cid = element.id,
      itm.dialog = element.dialog,
      matches.push(itm);
    }
  }
  if(matches.length) {
    cycleSearch(0, q);
  } else {
    resetSearch(q, which);
  }
};


// cyle through search results
function cycleSearch(i, q) {
    var $c = $('#' + matches[i].cid);

    $('html, body').animate({
       scrollTop: $c.offset().top - resultOffsetScrollHeight
    }, 500)

    cycle = i;
};

// reset the search
function resetSearch(q, which) {
    if(matches.length) {
        for(var i=0; i<which.length; i++) {
          document.getElementById(which[i].id).classList.remove("result");
        }
    }
    query = "";
    matches = [];
    cycle = -1;
};

// seek to time offset start-time of webvtt cue on html line click
$vttBlock.on('click', '.captions-line, .chapters-line, .subtitles-line', function(e){
    var $this = $(this),
        stamp = $this.data('ts'),
        man = $this.data('manifest') -1,
        fs = $this.data('fileset') -1;
    playerInstance.playlistItem(fs).seek(stamp);
    $(e.delegateTarget).find(".current").removeClass("current");
    $this.addClass('current');

    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Transcript - Text Clicked",
      "eventLabel": meta_id,        // Object ID
      "eventValue": 1               // incr. val to determine higher value content.
    });

});

// seek to time offset start-time of webvtt cue on html line click
$vttBlock.on('click', '.timestamp-btn', function(e){
    var $this = $(this),
        stamp = $this.data('ts'),
        man = $this.data('manifest') -1,
        fs = $this.data('fileset') -1;
    playerInstance.playlistItem(fs).seek(stamp);
    $(e.delegateTarget).find(".current").removeClass("current");

    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Transcript - Timestamp Clicked",
      "eventLabel": meta_id,        // Object ID
      "eventValue": 1               // incr. val to determine higher value content.
    });

});


// Search transcript on form submit
// Routes to search/cyle/rest functions
$search.submit( function(e) {
    // override basic form submission
    e.preventDefault();
    // get query string
    var $this = $(this),
        q = $this.find("input.form-control").val().toLowerCase(),
        searches = $this.data("searches"),
        which;

    // which search is being executed, reference correct data list
    switch(searches) {
      case "captions":
        which = captions
        break;
      case "subtitles":
        which = subtitles
        break;
      case "description":
        which = description
        break;
      default:
        which = captions
    }

    // is there a query string?
    if(q.length > 0) {
      // query var is originally a blank var.  on searchTranscript, set to q
      // so 1st submission, nothing happens visally.
      if (q == query) {
        if(cycle >= matches.length - 1) {
          // loop through results with cycle function.
          cycleSearch(0, q);
        } else {
          cycleSearch(cycle + 1, q);
        }
      // this is the 1st time with this query. Reset the search and then run it.
      } else {
        resetSearch(q, which);
        searchTranscript(q, which);
      }
    } else {
      resetSearch(q, which);
    }

    ga('send', {
      "hitType": "event",
      "eventCategory": window.gaPrefix + ": A/V Interactions",
      "eventAction": "Transcript - Searched",
      "eventLabel": meta_id,         // Object ID
      "eventValue": 1                // incr. val to determine higher value content.
    });

});


// Bootstrap tabs. Clear search inputs.
$("a.vtt-tabber").on('show.bs.tab', function (e) {
  var target = $(e.target).attr("href"),
      targetSearch = $(target).find("form.vtt-search"),
      dataSearches = targetSearch.data("searches");

  matches = [];
  resetSearch("", dataSearches);

});


// Bootstrap Affix Search on Scroll
playerInstance.on('ready',function(){
    $search.affix({
        offset: {
            top: $search.offset().top
        }
    });

});


                        


                    } else {
                        setTimeout(function() { defer() }, 50);
                    }
                }());

            </script>
        
        





        
        
        
    
    

    



