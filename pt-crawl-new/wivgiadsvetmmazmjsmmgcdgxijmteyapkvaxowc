<script type="text/javascript">
	$(function() {
		$('#state').select2({placeholder: 'State'});

		$('#locationType').change(function() {
			($(this).val() == 'In Person Training') ? $('#group-location').slideDown(300) : $('#group-location').slideUp(300);
		});

		$("#trainingForm").validate({
			// debug: true,
			rules: {
				reqOrg: 'required',
				nameFirst: 'required',
				nameLast: 'required',
				address1: 'required',
				city: 'required',
				zip: {
					required: true,
					pattern: /^\d{5}(-\d{4})?$/
				},
				phone: {
					pattern: /^(\d{3}-){2}\d{4}$/
				},
				fax: {
					pattern: /^(\d{3}-){2}\d{4}$/
				},
				email: {
					email: true
				},
				reqDate: 'required',
				locationType: 'required',
				attendees: {
					required: true,
					digits: true
				},
				trainingType: 'required'
			},
			messages: {
				reqOrg: 'The Requesting Organization field is required.',
				nameFirst: 'The First Name field is required.',
				nameLast: 'The Last Name field is required.',
				address1: 'The Address 1 field is required.',
				city: 'The City field is required.',
				zip: {
					required: 'The Zip Code field is required.',
					pattern: 'Invalid Zip Code.  Must match the pattern 12345 or 12345-6789.'
				},
				phone: {
					pattern: 'Invalid Phone number.  Must match the pattern 123-456-7890.'
				},
				fax: {
					pattern: 'Invalid Fax number.  Must match the pattern 123-456-7890.'
				},
				email: {
					email: 'Invalid email address.'
				},
				reqDate: 'The Requested Date field is required.',
				locationType: 'The Location Type field is required.',
				attendees: {
					required: 'The Attendees field is required.',
					digits: 'The Attendees field must be a number.'
				},
				// trainingType: 'Select either "Initital Training" or "Re-training".'
				trainingType: 'The Training Type field is required.'
			}
		});
	});
</script>







<h1>Right-to-Know Training Request Form</h1>



<div class="alert alert-info">
	The Office of Open Records routinely conducts training on the Right-to-Know Law for both agencies and requesters.  <a href="/RTKL/TrainingAbout.cfm">Learn more</a> about our training programs.
</div>

