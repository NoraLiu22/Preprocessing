			geolocationResponse: {
				stateCode: pmc_fastly_geo_data.region,
				countryCode: pmc_fastly_geo_data.country,
			}
		}
	}

	function OptanonWrapper() {
		window.pmc_google_consent_mode?.update();		if ( 'undefined' !== typeof Rollbar && 'undefined' !== typeof window.pmcGARollbar ) {
			// Rollbar GA OneTrust Callback.
			window.pmcGARollbar.setParam( 'otCallback', performance?.now() || true );
			Rollbar.debug( 'oneTrustCallback', window.pmcGARollbar.getParams() );
		}

		if ( true !== window.pmc_meta?.is_eu ) {
			const activeGroups = ( 'string' === typeof window.OnetrustActiveGroups ) ? window.OnetrustActiveGroups : '';
			window.triggerGA( activeGroups.includes( 'C0002' ) ); // This is for non EU traffic.
		}

		if ( 'object' === typeof window.pmc_onetrust ) {
			window.pmc_onetrust.init();
			console.log('PMC OneTrust initialized');
		} else {
			console.warn('PMC custom OneTrust logic was not loaded due to user region');
		}
	}
</script>
