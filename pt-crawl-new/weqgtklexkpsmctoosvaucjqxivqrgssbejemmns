<meta class="swiftype" name="content_type" data-type="string" content="List" />
<meta class="swiftype" name="post_id" data-type="integer" content="1234824872" />
<meta class="swiftype" name="title" data-type="string" content="The 50 Worst Decisions in Movie History" />
<meta class="swiftype" name="author" data-type="string" content="Andy Greene" />
<meta class="swiftype" name="published_at" data-type="date" content="2023-09-25 10:33:22" />
<meta class="swiftype" name="image" data-type="enum" content="https://www.rollingstone.com/wp-content/uploads/2023/09/WORSTIDEAS_FLAT.jpg?w=1024" />
<meta class="swiftype" name="comment_count" data-type="integer" content="0" />
