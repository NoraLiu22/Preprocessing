<dialog
  class="
    glue-modal
    
    
      is-themed
      theme-red-100
    
  "
  data-g-module_name="modal"
  data-modal-close="Close dialog window"
  
    data-g-module_id="module-modal-di-more-authentic-representation-disabilities"
    id="module-modal-di-more-authentic-representation-disabilities"
  
  
    aria-labelledby="module-modal-di-more-authentic-representation-disabilities-heading"
  >
  <div class="glue-modal__content-wrapper" role="presentation">
