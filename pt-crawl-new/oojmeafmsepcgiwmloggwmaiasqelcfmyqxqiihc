  <div class="module-card-collage__card-inner glue-card__inner">
    
      
    <picture>
        <source
        srcset="https://lh3.googleusercontent.com/yTQnd_tAVnXDQo2zQiTvejCDutb6c10qfxe-FZrQVcU4WMCaGtjjOKwEzkQi_m6wrFeM1Dq7TDmh5S86jZdCGuekre6LsmtttVglK_coy9hRBvhK4xo=w800"
