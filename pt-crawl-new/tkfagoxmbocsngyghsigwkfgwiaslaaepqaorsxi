  <source srcset="https://cdn.vox-cdn.com/thumbor/474hJLCUamQJzXVz0Hmn79AX0C0=/0x0:1998x3064/320x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 320w, https://cdn.vox-cdn.com/thumbor/zDiOFzTroRzlBPJc09qtidP9tLs=/0x0:1998x3064/520x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 520w, https://cdn.vox-cdn.com/thumbor/BLvk1yT_kfP6QX0WxyzurUgKccE=/0x0:1998x3064/720x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 720w, https://cdn.vox-cdn.com/thumbor/Q7818lMW7-L4ZQCBq7EjneMLOMU=/0x0:1998x3064/920x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 920w, https://cdn.vox-cdn.com/thumbor/Hm88p46Wn2OlF-cFouuteacfYcE=/0x0:1998x3064/1120x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 1120w, https://cdn.vox-cdn.com/thumbor/0L7zcuAGWhe_VGCnLuD77VVrHkQ=/0x0:1998x3064/1320x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 1320w, https://cdn.vox-cdn.com/thumbor/pEyNzhx2TIciInoHJx3YdrCMeaw=/0x0:1998x3064/1520x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 1520w, https://cdn.vox-cdn.com/thumbor/EnvfoQDyR1p0XfEKqfxxLWTw0po=/0x0:1998x3064/1720x0/filters:focal(0x0:1998x3064):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/6714657/the-devil-wears-prada-emily-blunt.0.jpg 1720w, https://cdn.vox-cdn.com/thumbor/srtZNhSbPT4CNmJs5yLnM8p7jaY=/0x0:1998x3064/1920x0/filters:focal(0x0:1998x3064):format(