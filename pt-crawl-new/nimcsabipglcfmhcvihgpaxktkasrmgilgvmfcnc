 <script type="text/javascript">
  function disableEnterKey(e)

	{
	     var key;
	     if(window.event)
     key = window.event.keyCode;     //IE
	     else
	          key = e.which;     //firefox
	     if(key == 13)
	          return false;
	     else
	          return true;

	}
  function CallWindow1(CtrlName) 
    {
        //       window.open('cal.aspx?CtrlName=' + CtrlName, 'PopUpCalendar', 'width=235,height=220,top=500,left=500,toolbars=no,scrollbars=no,status=no,resizable=no');  //textbox id passed as querystring

        newwindow = window.open('cal.aspx?CtrlName=' + CtrlName, 'PopUpCalendar', 'width=235,height=220,top=500,left=500,toolbars=no,scrollbars=no,status=no,resizable=no');
     if (window.focus) { newwindow.focus(); }
    }

    function CallWindow2(CtrlName) {
        //       window.open('cal.aspx?CtrlName=' + CtrlName, 'PopUpCalendar', 'width=235,height=220,top=500,left=500,toolbars=no,scrollbars=no,status=no,resizable=no');  //textbox id passed as querystring
       
//  & TotalFee & "&SubmitID=" & SubmitId
        var EventDate = document.getElementById('ctl00_ContentPlaceHolder1_txtEventDate').value;
     
      newwindow = window.open('cal2.aspx?CtrlName=' + CtrlName + '&EventDate=' +EventDate, 'PopUpCalendar', 'width=235,height=220,top=500,left=500,toolbars=no,scrollbars=no,status=no,resizable=no');
      if (window.focus) { newwindow.focus(); }
    }


//        Response.AppendHeader("Refresh", Convert.ToString((Session.Timeout * 60) + 10) & "; URL=errorPage.aspx")

    function CallVerifyAddress() {
      document.getElementById('ctl00_ContentPlaceHolder1_txtResult').innerHTML = '';
        document.getElementById('ctl00_ContentPlaceHolder1_Image1').style.display = "block";
        document.getElementById('ctl00_ContentPlaceHolder1_Image1').style.color = 'red';
        document.getElementById('ctl00_ContentPlaceHolder1_Image1').innerHTML = 'Address verification in process...';
        document.getElementById('ctl00_ContentPlaceHolder1_LblError').innerHTML = '';
        var Houseno = document.getElementById('ctl00_ContentPlaceHolder1_txtHouse').value;
        var Address1 = document.getElementById('ctl00_ContentPlaceHolder1_txtAddress1').value;
        var Address1 = Houseno + ' ' + Address1;
        AddWindow = window.open('VerifyAddress.aspx?Add=' + Address1, 'VerifyAddress', 'width=465,height=290,top=500,left=500,toolbars=no,scrollbars=no,status=no,resizable=no');
        if (Address1.length > 0) {
            if (window.focus) { AddWindow.focus(); }
             }
       else {
           alert("Enter a valid Permit Location");
       }

      
    }

    

function hidePanel() {
    window.opener.document.getElementById('ctl00_ContentPlaceHolder1_AddEvent').style.visibility = 'hidden';
}



//hides addevent when form is loaded
onLoad = "HidePanel()";



function validate() {
    var t1 = Page_ClientValidate("CheckAddress");
    var t2 = Page_ClientValidate("submit");

    //    if (!t1 || !t2) return false;
if  (t1== false) 
{
return false;
}
//else if (t2 == false) 
//{
//    return false;
//}
else
{
    return true;
}

}
// To check if houseno is numeric
function checkText(source, args) {

    var houseno = document.getElementById('ctl00_ContentPlaceHolder1_txtHouse').value;
     if (isNaN(houseno))
 
    {
        args.IsValid = false;
        
    
    }

    else 
    {
        args.IsValid = true;

    }
}
//compare email address with confirm email address
function EmailValidate(source, args) {
    var e = document.getElementById('ctl00_ContentPlaceHolder1_txtconfirmemail').value

    e = e.toLocaleLowerCase();
    var n = document.getElementById('ctl00_ContentPlaceHolder1_txtemail').value;
    n = n.toLocaleLowerCase();
    if (e == n) 
   {
       args.IsValid = true;
      
    }
    else 
    {
        //for each condition that fails set IsValid to false and choose the error message:
        args.IsValid = false;
   }

     }


     function showFile() {
         var file = document.getElementById("ctl00_ContentPlaceHolder1_FileUpload1");
         //var path = file.value;

         //document.getElementById('ctl00_ContentPlaceHolder1_lblfilenames').innerHTML = path;

         var files = [];
         var indFiles = "Files: ";
         for (i = 0; i < file.files.length; i++) {
             files[i] = file.files[i].name;
             indFiles = indFiles + i + ":" + files[i] + "; ";
         }
         document.getElementById('ctl00_ContentPlaceHolder1_lblfilenames').innerHTML = indFiles;
    }
     
