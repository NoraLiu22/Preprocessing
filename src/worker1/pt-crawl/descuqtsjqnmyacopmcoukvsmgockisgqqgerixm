\":true,\"perm.render-web.registration-wall\":true,\"perm.render-web.registration-wall.supress-verification-modal\":false,\"perm.render-web.resolve-political-monitor-cta.politics-cu\":false,\"perm.render-web.resolve-political-monitor-cta.section-pages\":false,\"perm.render-web.rugby-union-index.popular-content-unit\":true,\"perm.render-web.rugby-union.third-party-widget\":true,\"perm.render-web.save-for-later\":true,\"perm.render-web.save-onboarding-part-1\":true,\"perm.render-web.save-onboarding-part-2\":true,\"perm.render-web.snowplow.page.view\":true,\"perm.render-web.sport-index.popular-content-unit\":true,\"perm.render-web.state-index.popular-content-unit\":true,\"perm.render-web.styled-components-ssr\":true,\"perm.render-web.subscriber-notification\":true,\"perm.render-web.swg-entitlement\":true,\"perm.render-web.swg-purchase\":true,\"perm.render-web.tennis-live-scores-links\":true,\"perm.render-web.todays-paper.unavailable\":false,\"perm.render-web.travel.popular-content-unit\":true,\"perm.render-web.weather\":true,\"perm.render-web.world-index.popular-content-unit\":true,\"temp.render-web.article.audio-experiment\":true,\"temp.render-web.beyondwords-accordion-disabled\":true,\"temp.render-web.beyondwords-waitfor-grapeshot.enabled\":true,\"temp.render-web.brisbanetimes.theming.enabled\":false,\"temp.render-web.bt.revamp.paywall.promotions\":false,\"temp.render-web.collections.enabled\":true,\"temp.render-web-metro.goodfood.coral-8-comments\":true,\"temp.render-web.election.current-results-index\":true,\"temp.render-web.ads.enable-admiral\":false,\"temp.render-web.ads.enable-blockthrough\":false,\"temp.render-web.europa.prioritise-load\":false,\"temp.render-web.fwwc.scorecard\":false,\"temp.render-web.ga4.pageview-enabled\":false,\"temp.render-web.hard-refresh-if-new-release-available\":false,\"temp.render-web.liar-liar.long-playlist\":true,\"temp.render-web.localstorage.cleaner.enabled\":true,\"temp.render-web.cookie.cleaner.enabled\":true,\"temp.render-web.olympics-content-units.enabled\":false,\"temp.render-web.olympics-content-unit-navigation.enabled\":false,\"temp.render-web.olympics-content-unit-widget.enabled\":false,\"temp.render-web.olympics-custom-header.enabled\":false,\"temp.render-web.olympics-index.enabled\":false,\"temp.render-web.olympics-index-medal-tally.enabled\":false,\"temp.render-web.olympics-subpages.enabled\":false,\"temp.render-web.olympics-widgets.enabled\":true,\"temp.render-web.olympics-widgets-iframe.enabled\":true,\"temp-render-web.olympics-sponsors-strip.enabled\":false,\"temp.library-editorial-navigation.afr-mobile-link.enable\":true,\"temp.library-editorial-navigation.board-runsheet-search.enable\":false,\"temp.library-editorial-navigation.clippings-preview.enable\":true,\"temp.api-ink-images.enable-api-cutover\":false,\"temp.render-planning.boards.enable\":false,\"temp.library-editorial-navigation.gallery-asset-type-link.enabled\":true,\"temp.library-editorial-navigation.immersive-asset-type-link.enabled\":false,\"temp.library-editorial-navigation.new-video-link.enable\":true,\"temp.render-pagebuilder.smart-dashes.enable\":false,\"temp.render-web.paywall.rule-per-asset\":true,\"temp.render-web.piano-homepage-header-button.enabled\":true,\"temp.render-web.log-piano-conversion.enabled\":false,\"temp.render-web.piano.member-details.enabled\":true,\"temp.render-web.piano-premium-paywall\":true,\"temp.render-web.pong-su-index.v2\":true,\"temp.render-web.puzzles.target\":false,\"temp.render-web.referendum.iframe\":true,\"temp.render-google-amp.remove-amphtml-link\":true,\"temp.render-web.resolve-political-monitor.homepage-widget\":false,\"temp.render-web.search-improvements\":true,\"temp.render-web.snowplow.gcp-enabled\":false,\"temp.render-web.sticky-billboard-ads\":true,\"temp.render-web.traveller.onboarding-banner\":false,\"temp.render-web.united-cup.live-video\":true,\"temp.render-web.swg-check-entitlement\":true,\"temp.render-web.swg-prompt-disabled.brisbanetimes\":false,\"temp.render-web.swg-prompt-disabled.watoday\":false,\"temp.render-web.watoday.2024-tagline.enabled\":true}},\"header\":{\"isSticky\":false},\"identification\":{\"deviceID\":null,\"nineUniqueID\":null,\"pendingVerify\":null,\"refreshTrigger\":false,\"session\":null,\"token\":null,\"user\":null},\"iframe\":{},\"liveArticle\":{\"keyPosts\":[],\"morePosts\":false,\"newPosts\":[],\"posts\":[],\"pageInfo\":null,\"pinnedPosts\":[]},\"loadingBar\":0,\"location\":null,\"login\":{\"emailAddress\":\"\",\"isLoggedIn\":false,\"isVisible\":false,\"justLoggedIn\":false,\"justLoggedInAction\":\"\",\"loginAction\":\"\",\"showModal\":\"none\",\"swgEntitlementStatus\":null},\"meterPrompt\":{\"isVisible\":false,\"limit\":0},\"myAccount\":{\"isVisible\":false},\"page\":{\"/national/she-was-told-her-fiction-wasn-t-australian-enough-then-she-won-the-miles-franklin-20240304-p5f9mi.html\":{\"asset\":{\"ads\":{\"suppress\":false},\"assetType\":\"featureArticle\",\"categories\":[\"National\"],\"categoryId\":40,\"asset\":{\"about\":\"Working with a \\u201Cbillable hours\\u201D mindset, lawyer, mum of four and author Shankari Chandran weaves sharp social justice critiques through her loving family tales.\",\"allowComments\":true,\"body\":\"<p>I\\u2019m about halfway through a jab, cross, body-hook combo when I realise I might have underestimated Shankari Chandran. As I struggle to master the most feeble of punches, I look across to the 49-year-old standing next to me, who is letting fly a savage kick to the boxing bag. Barefoot, wearing a tank top, track pants and now slightly fogged glasses, a sweaty halo of frizz above her face, boxing gloves that are almost the size of her torso, she\\u2019s in full fighter mode. \\u201CWhat\\u2019s your favourite move?\\u201D I gasp at one point. The most recent Miles Franklin Literary Award winner flashes me a smile \\u2013 the vast, white, straight kind that changes a whole face \\u2013 then answers. \\u201CHitting the bag in the balls.\\u201D Patrick White, eat your heart out.</p><p>\\u201CShe called me up before she first came to the class and said, \\u2018I just want you to know the only thing I have stuck at is writing. I will probably come once and you\\u2019ll never see me again,\\u2019\\u2009\\u201D says Bang Bang Muay Thai trainer Sarah Clisdell of her client before I join their Thursday evening class on Sydney\\u2019s lower north shore. \\u201CAnd she\\u2019s still here.\\u201D</p><p>Of course she is. Chandran, as you\\u2019ll soon learn, can withstand more than a few rounds in the ring. While the aches of ageing nagged her to take up exercise, the violence of boxing offered a satisfaction of its own. \\u201CI suspect I have a lot of suppressed rage about life and the state of the world,\\u201D she says. She\\u2019s named after a Hindu goddess, the destroyer and creator of worlds. The mortal Shankari does a little of this, too \\u2013 in her novels, legal work and community advocacy, she\\u2019s always breaking things with a view to rebuilding them for the better. The morning after the class, Chandran sends me a check-in email (that\\u2019s how poorly I performed). \\u201CYou are an absolute legend,\\u201D she writes in one of her trademark full-of-personality missives. \\u201CSo game. I hope you\\u2019re feeling well and ready to hit something again.\\u201D</p><x-placeholder id=\\\"aa680fda3bb03abf63fa8e43557a4de02f6bfafa\\\"></x-placeholder><p>Indefatigable. If there\\u2019s one theme that emerges from speaking to dozens of Chandran\\u2019s friends, family and colleagues, it\\u2019s her sheer relentlessness. \\u201CYou don\\u2019t want to be compared to Shanks,\\u201D says former law colleague, Jilly Field. \\u201CI probably don\\u2019t know anyone else who is that high-achieving. She\\u2019s quite the \\u2013 I don\\u2019t know if this is the right word \\u2013 but a prodigy.\\u201D</p><p>Shankari Chandran wears many hats. There\\u2019s Writer Chandran, whose third published novel <em>Chai Time at Cinnamon Gardens</em> <x-placeholder id=\\\"ccd160f1710564e56e827dfe8034368a88c2fae6\\\"></x-placeholder>, the $60,000 Miles Franklin Literary Award, last year. Her Audible Original <em>Unfinished Business</em> was released in February (and will be out in book form later this year), and her new novel <em>Safe Haven</em> will be published by Ultimo Press this month. There are also screen deals and scripts in the works, with her debut novel <em>Song of the Sun God</em> slated for a six-parter, featuring <em>Bridgerton</em> star Charithra Chandran, and <em>Unfinished Business</em> in the initial stages of development as a television series.</p><p>Then there\\u2019s Lawyer Chandran. She\\u2019s overseen pro bono and law reform work at two major firms, Allen &amp; Overy in the UK and Ashurst in Australia, including steering a team that represented Guant\\u00E1namo Bay detainees and serving on an advisory committee to the then-UK prime minister Gordon Brown. Now, Chandran is head of sustainability for one of Australia\\u2019s largest retailers, Super Retail Group (owners of Supercheap Auto, Rebel, BCF and Macpac). I remember this a little too late when we meet for the first time, and absentmindedly toss my half-drunk coffee into a bin. I then watch as Chandran patiently disassembles her lid, flattens her cup and deposits each in their appropriate receptacles. \\u201CIt can be very confusing,\\u201D she offers, her wry wit never far from the polished surface.</p><p>Then there\\u2019s Family Chandran, the mother, wife, sister and daughter. She has four children (Ellora, 20; Kailash, 18; Hari, 15; and Siddharth, 14) with her<br />husband of nearly 25 years, Haran Siva, a former director at Deloitte, now a country manager at the not-for-profit Climate Bonds Initiative. They even seem to still like each other, with Siva, 52, describing Chandran as \\u201Chugely loving\\u201D and recounting a recent birthday getaway where they \\u201Ctalked the whole way and didn\\u2019t bring the kids up once\\u201D.</p><x-placeholder id=\\\"6c1194a656a29339af590b2341899d3e43643589\\\"></x-placeholder><p>That\\u2019s not to mention all the other Chandrans \\u2013 the one who appears on panels, delivers speeches, writes essays, organises Tamil community events, makes eggplant curries that win the praise of her mother (even if she does follow a slightly different recipe), attends book clubs, oversees writing organisations, and reads over her neighbour\\u2019s daughter\\u2019s high school creative-writing stories.</p><p>\\u201CIf I have to write a tricky email, I\\u2019ll send her an email and say, \\u2018This is what I\\u2019m writing, can you please give me a fun last line?\\u2019 and she\\u2019ll supply me with about three different options,\\u201D says television producer Karen Radzyner, who has optioned two of Chandran\\u2019s novels.</p><p>Chandran rolls her eyes at such praise, twisting the conversation to credit her support networks at work and home and to highlight her fortune and privilege. She half-jokes that she sees life in terms of billable hours; that she separates her day into units \\u2013 a shower, for example, takes six \\u2013 and finds herself unconsciously tapping her watch as a reminder that time is money. \\u201CI see it as maths now,\\u201D she says. \\u201CIf the day is finite and my life is finite, where is the best place to put my time? And my instinct is to push myself on everything \\u2013 but I [also] really try hard to challenge that instinct.\\u201D</p><x-placeholder id=\\\"1c03aaf74d529fd7b10b192a46118d8d1e88e334\\\"></x-placeholder><p>You might think winning the Miles Franklin would grant Chandran permission to slow down, at least a little. After all, she\\u2019s joined an elite club of Australian authors that includes the 1957 inaugural winner Patrick White, George Johnston, Thomas Keneally, Ruth Park, Peter Carey, Tim Winton, Elizabeth Jolley and Michelle de Kretser, to name but a few. Good things flow automatically after a prestigious win like that. But what becomes clear when spending any time in Chandran\\u2019s orbit, is that she very determinedly has no time to waste. There will be no resting on any Miles Franklin laurels here.</p><p>\\u201CIf I have an idea that needs to be externalised into a novel, I am obsessive, I actually cannot stop,\\u201D she says. \\u201CI don\\u2019t know how to stop until I\\u2019ve written it.\\u201D</p><x-placeholder id=\\\"50b05ade84c6ba0e25e3a53b844e9b71ff3819a9\\\"></x-placeholder><hr /><p><strong>Perhaps</strong> Chandran\\u2019s parents have heard about the heat I copped at boxing, because they seem to have gone blessedly light on the chilli in the Tamil dishes they pile onto my plate when I visit for dinner. Beetroot, eggplant, dhal, chickpeas, rice. I\\u2019m at their apartment, overlooking Sydney\\u2019s Parramatta River at Breakfast Point, along with Chandran and her younger brother, Narendran Nadanachandran, whom she describes as one of her favourite people.</p><x-placeholder id=\\\"484fdd228ac8ecbc1dcca4d34a1d4e0c7ae18e96\\\"></x-placeholder><p>Food is a symbol of love and tradition in both this household and in Chandran\\u2019s novels, so fittingly the evening begins with a debate about who was responsible for making the murukku, a crunchy fried noodle dish offered to me when I arrive, and ends with a factory-style line-up for packing leftovers, which seems so familiar to them that their bodies weave around each other in an unconscious dance. I\\u2019ve never been so well-fed while working on a story; I leave with two containers of food, plus fresh papadams.</p><p>This dance is present in their words, too; they bounce off each other, finishing sentences and derailing conversations with backstories, memories and questions, as they write and rewrite the family lore. \\u201CShankari was always very keen on reading and learning. From the very early days she would be with books,\\u201D says her father, Nadana Chandran. \\u201CShe was a very easy child to manage.\\u201D Her mother, Rathy, picks up the thread. \\u201CShankari was a very organised child. So I actually had nothing very much to do with organising her.\\u201D</p><x-placeholder id=\\\"5fca5bbc9b37533ca9c0d8587e0e0f3fa0477dfa\\\"></x-placeholder><p>\\u201CAlmost OCD,\\u201D quips her brother bestie, Narendran. \\u201CLet\\u2019s call it a pathology.\\u201D</p><p>Sitting at this table, I can trace the cornerstones of Chandran\\u2019s life up the branches of the family tree. Her parents are both doctors, her father a neurosurgeon and her mother a general practitioner, who were married by arrangement in Colombo, Sri Lanka. They left Sri Lanka for London in 1971, part of a generation of Tamils to flee as civil war loomed, with tensions between Sri Lanka\\u2019s Sinhalese and Tamil populations growing in lockstep with anti-Tamil legislation. If you had the money or education to get out, the feeling was, you should.</p><x-placeholder id=\\\"890a00509cca7c4274c51e2e6b72a8bba43a433d\\\"></x-placeholder><p>Chandran was born in London, before the family of three followed relatives to Australia in 1977, eventually settling in Canberra (Narendran, who is now a doctor, was born the following year). The White Australia Policy had ended just a few years earlier and her parents knew of only three other Tamil families in the city at the time.</p><p>\\u201CI think [my parents\\u2019] approach was that they wanted us to be able to fit in and do well and contribute to Australian society and not be especially visible,\\u201D Chandran says. \\u201CAt the same time, they were desperate to retain their culture and tradition and Tamil values and closeness to the Tamil community. So we would have almost like a double life. I\\u2019d go out and hang with my Australian friends and code-switch into white Australia, then code-switch back at home into Tamil Australia, whatever was unfolding there.\\u201D</p><p>Not that they had to worry about Chandran. She was then much as she is now: organised, disciplined, determined. A high achiever. Her parents were conservative and strict, but supportive. Sex, alcohol and dates were off-limits; learning and education, which had allowed her parents to leave Sri Lanka, were paramount. Chandran remembers watching her father perform neurosurgery in the operating theatre, and videos of his successful operations being interspersed with holiday pictures on family slide nights. \\u201CYou just got up on time. You did your chores. You did your homework. I don\\u2019t think we were particularly good children or submissive children, we were just efficient as a family. Why wouldn\\u2019t you do things the way that had been optimally suggested to you?\\u201D Chandran says. \\u201CI have definitely been a good girl. I am a pleaser. I\\u2019m a typical first child. I really wanted to make them proud. I was overly concerned with gold stars.\\u201D</p><x-placeholder id=\\\"a994d3a36281aa09b866111c3d86c4490876e82a\\\"></x-placeholder><p>There were family trips back to Sri Lanka but when the civil war started in 1983, her parents\\u2019 former home seemed suddenly both closer and further away. Chandran\\u2019s father was heavily engaged with politics, lobbying the Australian government to open the borders to Tamils seeking refuge, attending protest marches, forming the Canberra Tamil Association and helping sponsor families to come to Australia. Chandran remembers falling asleep on the sofa as a girl to the sound of aunties and uncles swapping stories of lost loves and lives destroyed. Her father (who was made a member of the Order of Australia in 2015, for his medical and cultural contributions to the community) received threats on the phone and in the mail. \\u201CAs children, we shouldn\\u2019t be hearing these things but you are hearing these things,\\u201D she says, \\u201Cbecause the community in Australia is collectively grieving and collectively asking what they can do.\\u201D</p><p>Chandran and her brother laugh when I ask if she could have told her parents she wanted to be a writer. Doctor, engineer, lawyer, accountant, that\\u2019s the professional hierarchy, her father tells me over dinner. Even now, if someone asks what their daughter does, her parents will say \\u201Clawyer\\u201D first. No one in the family had a clue what the Miles Franklin was before Chandran won it. But the two passions \\u2013 lawyer and novelist \\u2013 aren\\u2019t as strange bedfellows as they might appear. For Chandran, social justice and storytelling are inextricably linked.</p><x-placeholder id=\\\"40891d1c0e214e50b230a571a547dba872c77ad5\\\"></x-placeholder><p>After studying law at the University of NSW and travelling, Chandran moved to London at 25 to be with British-born Siva, a family friend. She had met him when her parents offered him a lift while he was visiting Australia. Legend has it she didn\\u2019t speak a word to him because she was in such a bad mood after a fight with her father over a sleeveless shirt she\\u2019d worn that left her bra strap showing.</p><p>In London, she turned a maternity role as a pro bono officer at Allen &amp; Overy into a decade-long position, extending the company\\u2019s pro bono reach at such a rate they joked it was the fastest-growing team in the firm. \\u201CWhen she wants something, she will do it,\\u201D says Jilly Field, her former colleague at Ashurst (the law firm where Chandran worked when she returned to Australia). \\u201CShe has just got the capacity, the tenacity, the commitment and the absolute wild intelligence to do it.\\u201D</p><x-placeholder id=\\\"3d0eb0a596d3ec36145ee7acb85e19e270d177a1\\\"></x-placeholder><p>Chandran worked on projects in more than 30 countries, channelling the power and reach of the firm into social justice issues, including working on cases dealing with freedom of speech, unlawful detention and women\\u2019s right to access abortions. By the time she and Siva relocated to Australia at the end of 2009, with three children and a fourth on the way, she was burnt out. While she\\u2019d attended fiction workshops and created plans for a novel over the years, her desire to write intensified with the politics and polemic around border security, as Tamil asylum seekers began arriving here via boat.</p><p>\\u201CIt was an educated, privileged class that could leave [Sri Lanka] in the \\u201970s and as a result, we became educated and privileged,\\u201D she says. \\u201CAnd we are growing up seeing this and thinking, \\u2018What is our responsibility? What is our duty to these people that have been left behind? They are our people.\\u2019\\u2009\\u201D She felt guilty that she hadn\\u2019t done enough while working in London. \\u201CAnd so when I came to Australia, I thought, \\u2018Well, let me try writing this story because terrible things have happened. And no one knows and no one cares. And we\\u2019re complicit in it \\u2026 What is my duty from my position of privilege?\\u2019 And I chose to write.\\u201D</p><x-placeholder id=\\\"72d87295a6caf5b8e5f8ddc1fe1baa70a5dddd54\\\"></x-placeholder><hr /><p><strong>\\u201CSee,</strong> <strong>no one knows me here,\\u201D</strong> Chandran whispers as we arrive at the Sydney Writers\\u2019 Festival program launch at the cavernous Carriageworks in early March. I have just enough time to shoot her a bemused look over the heads of two young women who approach and introduce themselves, almost emotional with excitement to tell her how much they love her novels, and her. Chandran stands at the back during the speeches, as her face flashes on the screen as one of the upcoming festival\\u2019s highlight events (Chandran will appear at both the Melbourne and Sydney writers\\u2019 festivals this month). She\\u2019s wearing sneakers and carrying a canvas tote bag imprinted with an image of a library borrowing card, her hair half held in place by a mini claw clip. Chandran rarely spends money on herself, thanks to a mix of frugality and sustainability, leaving her cousins to dress her with hand-me-downs, and friends to convince her to treat herself to new dresses.</p><x-placeholder id=\\\"71f74d82ddeb7b107d4870f05a51620992cbf983\\\"></x-placeholder><p>At one point, she whispers to me that she loves another festival guest, Richard Flanagan, whom she references not-so-covertly in all her novels (there\\u2019s a Flanagan River in her latest). Flanagan no longer enters the Miles Franklin, having asked his publishers to withdraw him from contention in 2017 after being shortlisted five times, and publicly questioning the value of literary prizes. The award \\u2013 established by the <em>My Brilliant Career</em> novelist Miles Franklin for \\u201Ca novel which is of the highest literary merit and presents Australian life in any of its phases\\u201D \\u2013 has come under increasing scrutiny in recent years, as has the entire Australian literary industry, which is small and tight-knit at best and insular at worst. The overwhelming majority of winners have been of Anglo-Saxon and European descent, with Noongar man Kim Scott the first Indigenous person to win, sharing the prize in 2000, more than four decades after Patrick White\\u2019s win. (<x-placeholder id=\\\"a3e9a98a0379c5aba702b25e0823595bf4b98bcb\\\"></x-placeholder>)</p><p>Aside from Michelle de Kretser, who was born in Sri Lanka, immigrated to Australia as a teen and <x-placeholder id=\\\"2c75ed35038f6bc0d6c4446c2385c77bfc4b84ac\\\"></x-placeholder>, no other South-East Asian writer had received the award before.</p><x-placeholder id=\\\"7507bf545ab34aa76f0970b62e6765f24dd6d67c\\\"></x-placeholder><p>Chandran says she\\u2019s not noticed people treating her differently since the win \\u2013 mostly. \\u201CThe other day, we were walking to the temple just around the corner, and there was a Sri Lankan lady there, and she wanted to meet me rather than my mother. If anything, that\\u2019s been the most significant change \\u2013 that people are now more interested in talking to me than they are in talking to my mother.\\u201D</p><p>But something has changed. Not that long ago, publishers would not touch her debut novel <em>Song of the Sun God</em>, a multi-generational, multi-continent epic based on Chandran\\u2019s family history, about a young Tamil couple who wed on the eve of Sri Lanka\\u2019s independence from Britain before being caught in the civil war. It was written (and rewritten) over the years in between moving, work, maternity leave and mothering.</p><p>During the past decade, Australian publishing has been forced to reckon with its lack of diversity at all levels, from agents to publishers to authors. The first <x-placeholder id=\\\"fab700c766652ac55b5d6b2db61b3e7ade77e135\\\"></x-placeholder> a project run by the Australian Publishers Association and the University of Melbourne, released in 2022, confirmed what is obvious to anyone in the industry \\u2013 it is largely white, with less than 10 per cent of workers identifying as Asian, less than 11 per cent with a European (non-British) heritage and fewer than one per cent as First Nations. And that\\u2019s now.</p><x-placeholder id=\\\"ed166f8646058342649431073f2475fdd0557fdc\\\"></x-placeholder><p>In 2014, before the conversation around publishing diversity went mainstream, and when Chandran was trying to sell <em>Song of the Sun God</em>, those stats were worse. Tara Wynne, from the prestigious literary agency Curtis Brown, tried and failed to get the novel published in Australia. She had fallen in love with <em>Sun God</em>, as well as with its \\u201Cexceptionally charming, hugely bright\\u201D author. Chandran was repeatedly told her story was beautiful but wouldn\\u2019t sell. She remembers calling an editor directly, desperate to understand the rejection, pushing her until she explained that it wasn\\u2019t considered \\u201CAustralian enough\\u201D. It was a devastating blow.</p><x-placeholder id=\\\"02a981ec3656bcb4aa28166e051ae52abe29f3a1\\\"></x-placeholder><p>\\u201CI was angry and also deeply saddened by it. It\\u2019s a kind of grief,\\u201D Chandran says. \\u201CBecause you effectively have your place in Australia, and as the teller of Australian stories, the validity and value of that is repudiated. And you\\u2019ve always known, you\\u2019ve always sensed that you are on the margins of Australian society, but then to have it reflected back so directly about a story that to me spoke so honestly to an Australian experience.\\u201D She adds, \\u201CI remember lying down and putting my head on my dad\\u2019s lap and just having a good cry because of the frustration of wanting something so much and the barrier to entry was not the quality of the work that I was producing.\\u201D</p><p><em>Song of the Sun God</em> was eventually published in Sri Lanka in 2017, in a deal Chandran says wasn\\u2019t favourable for her. It was around this point that she put on her boxing gloves and made up her mind: she would keep writing until she could bring <em>Song of the Sun God</em> home.</p><hr /><p><strong>Chandran\\u2019s</strong> novels are an all-in affair. She\\u2019s not one of those writers who have a death grip on manuscripts until they\\u2019re finished or are superstitious about speaking of them too early. She invites friends and family to read her drafts and give her feedback. As a kind of thank you, they\\u2019ll often find themselves making cameos in her novels. \\u201CI think everyone in book club is there somehow,\\u201D says Su Lin Ho, who has been in a book club with Chandran for more than a decade. \\u201CI think one is a butcher, another is a sassy bartender.\\u201D</p><x-placeholder id=\\\"c6f407ed3f6a0d6feb24bff5d5dc7e63520d2f07\\\"></x-placeholder><p>Her writing room is tucked between the living and dining rooms in her circa 1920s home in Roseville, the walls dotted with tall bookshelves, Hindu gods and goddesses, and Tamil art. A map of Indigenous Australia hangs above her desk \\u2013 she\\u2019s worked significantly on issues of incarceration here \\u2013 alongside portraits of her and Siva drawn by their children, a framed letter of thanks from Gordon Brown for serving on his Council on Social Action, artwork by her daughter of Siva\\u2019s ancestral home in Jaffna, and framed images of her novel covers.</p><p>Blu-Tacked to the bottom of her laptop screen is a quote that reads, \\u201C<em>We need to redefine \\u2018hard work\\u2019 to include \\u2018hard thinking\\u2019 ... The hardest work is thinking of a better way to do it.</em>\\u201D</p><p>Chandran says it\\u2019s encouragement for her corporate role, but it also speaks to her willingness to fight. After the blow of <em>Song of the Sun God</em>, she decided to give publishers exactly what they were telling her they wanted. And it worked.</p><p>\\u201CI thought, \\u2018What novels are getting published? If it\\u2019s a sweeping saga in the tradition of, you know, Isabel Allende, it is not getting published. What is getting published? Apocalypse novels are getting published.\\u2019 And I love a good apocalypse. I am absolutely obsessed with apocalypses, and so I was like, \\u2018OK, I can write an apocalypse.\\u2019\\u2009\\u201D</p><x-placeholder id=\\\"d8f18654c4924ad1b38dfa47cc54c98fef26ca22\\\"></x-placeholder><p>In 2017,<em> The Barrier</em> \\u2013 a sci-fi thriller set in a future world struck by a religious war and an Ebola epidemic, where issues of vaccination have divided the West from the East \\u2013 became Chandran\\u2019s first novel published in Australia. Based on the feedback she\\u2019d received about <em>Song of the Sun God</em>, Chandran had decided to rewrite her scientist-spy protagonist, Noah Williams, as white. While acknowledging that regret is a waste of billable hours, she wishes today that she\\u2019d left her lead as South Asian.</p><p>\\u201CI should have backed myself and my style of storytelling more. I think that having a South Asian protagonist made for a much more intriguing story.\\u201D While it offered her a crash course in publishing, she was disappointed by both the novel\\u2019s reception and its sales (which she suspects were largely fuelled by her dozens of cousins).</p><p>After <em>The Barrier</em>, Chandran accepted that she\\u2019d probably not have a publishing career in Australia. The liberation from expectation gave her the freedom to write <em>Chai Time at Cinnamon Gardens</em> as she wanted to write it. The story feels like a spiritual sequel to her debut, telling the story of a nursing home in western Sydney bought by Maya and Zakhir after they fled Sri Lanka during the civil war, now run by their daughter, Anji. The residents are diverse, many of them Tamil, and their stories toggle between the past and the present as the novel explores racism, trauma, colonisation and identity. At its heart, Chandran says, <em>Chai Time</em> is about what it means to \\u201Cbe Australian\\u201D.</p><p>When the<em> Chai Time</em> manuscript was sent to publishers in 2020, Chandran received a very different reception from that she\\u2019d received several years earlier. \\u201CWhen I read it, I was in love with it,\\u201D says her publisher at Ultimo Press, Robert Watkins. \\u201CI thought there was a really broad readership for it ... From the very beginning, I really was sold on the idea of a novel that feels warm and comforting but has kind of an edge to it.\\u201D</p><p>Watkins describes the book as a \\u201CTrojan horse\\u201D \\u2013 the twee cover and name belying a story that doesn\\u2019t pull its punches. Readers are made comfortable, then confronted. It\\u2019s a reflection of Chandran\\u2019s own approach to the world. While she can have a forceful directness, her criticisms of social injustices are bubble-wrapped in kindness and humour. There\\u2019s a gentleness and grace that belies her fierceness.</p><p>Friend Indumathi Balachandran, a third-generation veena player, Carnatic singer, and producer, who is Tamil-Indian and has collaborated with Chandran on cultural projects, says the novel felt important for migrant communities like hers. \\u201CI remember thinking, \\u2018You are so brave.\\u2019 To put this in a book and to put your name to it. It\\u2019s different when you\\u2019re talking over dinner or coffee, but this is really saying it and standing by it.\\u201D</p><p>Chandran\\u2019s daughter, Ellora, says since she left home to study at university, she\\u2019s started to fully understand the impact her mum\\u2019s books have had on Tamils in Australia. \\u201CFor me, and my brothers and my cousins, all the books she\\u2019s writing and all the talks she does, I very much feel that they are for us because we don\\u2019t know a lot of that stuff,\\u201D Ellora says. \\u201CWe\\u2019ve only been to Sri Lanka once, and this is very much our way of connecting with that lost culture and that lost history.\\u201D</p><x-placeholder id=\\\"58530167484e2891d7a9e9100a0d0e9997b82561\\\"></x-placeholder><p>Chandran missed the multiple calls from Richard Neville, the State Library of NSW\\u2019s Mitchell Librarian and chair of the Miles Franklin Literary Award judging panel, as he attempted to tell her she had won the 2023 prize. When he finally found her, Chandran, in disbelief, had to ask him multiple times to repeat himself. \\u201CIt was pretty emotional,\\u201D she says. \\u201CIt\\u2019s a prize about the reflection of Australia, and for so much of my life, I\\u2019ve been uncertain of my place here. The books themselves speak to this.\\u201D</p><p>The biggest prize of all, perhaps, was that it finally helped bring <em>Song of the Sun God</em> home; it was published in Australia in November 2022 and then issued in a new format in July 2023 after the Miles Franklin win. And the flow-on continues. The Audible Original, <em>Unfinished Business</em>, a reworking of a political thriller Chandran had completed years earlier but could not find a publisher for, will be published in book form this year. Notably, Chandran has switched the novel\\u2019s protagonist, CIA agent and human rights lawyer Ellie Harper, from being white to having South Asian ancestry.</p><p><em>Safe Haven</em>, her first new novel to be published since the win, takes on Australia\\u2019s offshore detention policy and community advocacy. Inspired by high-profile cases like that of the Nadesalingam family (often known as \\u201C<x-placeholder id=\\\"ebc78843e9ecaf6127851978314bd42570b2a4c4\\\"></x-placeholder>\\u201D), it tells the stories of a refugee threatened with deportation and the community that activates to help her, and a lawyer investigating the death of a security officer at a fictional detention centre. Readers will recognise the same quiet fury and gentle humour that drives all of Chandran\\u2019s novels, as well as her ongoing interest in representing the Sri Lankan civil war and the experience of Tamils in her fiction.</p><x-placeholder id=\\\"359c8de2dd59774d63a56a428f989e265bb14ba6\\\"></x-placeholder><p>Chandran wrote the novel before she won the Miles Franklin but knows the prize means there will be increased expectation and attention. Her belief in her work is tempered by nerves, particularly that the book might be politicised given the recent High Court ruling that indefinite detention is unlawful. \\u201CI would say, \\u2018Read the book,\\u2019\\u2009\\u201D she says preemptively. \\u201C\\u2018Think about the questions that it\\u2019s actually asking.\\u2019\\u2009\\u201D</p><p>The promotional campaign for <em>Safe Haven</em> adds more talks, events and deadlines to Chandran\\u2019s to-do list. Each time we speak, there\\u2019s a long list of commitments ahead as well as behind her. She admits she\\u2019s unsure how much longer she can sustain this intense pace. After a long seminar in Melbourne one day recently, she returned home and did something unremarkable to many of us but almost unheard of for her: she zoned out. \\u201CI literally lay down in the foetal position, with children either lying around or ignoring me completely, depending on the child, and watched the second half of <em>The Equalizer</em>. I then proceeded to watch <em>The Equalizer 2</em>; I had already seen both movies but I rewatched them.\\u201D</p><p>Her law-school friend, Rebekah Cheney, has been watching the demands pile up since the Miles Franklin win with mild concern. \\u201CI\\u2019m always saying to her, \\u2018If you want to be remarkable, you can\\u2019t be busy. They don\\u2019t work well together. And for you to continue to be remarkable and for your next book to be your best yet, you need the headspace,\\u2019\\u2009\\u201D Cheney says. \\u201CShe has everything that a person could need to be remarkable. And I want her to continue to be remarkable.\\u201D</p><p>How to be less busy, though? Chandran is reluctant to leave her corporate role \\u2013 she enjoys the social purpose. And she won\\u2019t take her success as a writer for granted either, nor the ability to make a living wage from novels. Nor does she assume she\\u2019ll always be published. The \\u201Cmigrant mentality\\u201D, as she calls it, the sense that scarcity is just around the corner, is always with her. But there\\u2019s something else, too: a deep sense of duty and desire to help others. And a hopeful sense \\u2013 perhaps emboldened by her publishing turnaround of recent years \\u2013 that things can get better.</p><p>\\u201CI do wake up every morning thinking that social change is possible, and I will die thinking that social change is possible, and there is an optimism and a hopefulness in that. I think the fact that I\\u2019ve brought four children into the world, knowing what the world is, suggests that I am optimistic.\\u201D</p><p><b>To read more from</b><b><i> Good Weekend </i></b><b>magazine, visit our page at</b><x-placeholder id=\\\"03faa790c12ccdaff7eef84d5ae34a49d56c7899\\\"></x-placeholder><b>,</b><x-placeholder id=\\\"c1b1d977907585672bda5fb543e2209ed7dc7e02\\\"></x-placeholder><b> and</b><x-placeholder id=\\\"0b3db0cc539dcef8b3037e96474b63f5776063e0\\\"></x-placeholder>.</p><x-placeholder id=\\\"4ac06aa6d310ba4424bc54a5998e1d914af5a877\\\"></x-placeholder>\",\"bodyPlaceholders\":{\"02a981ec3656bcb4aa28166e051ae52abe29f3a1\":{\"data\":{\"altText\":\"\",\"animated\":false,\"aspect\":1.5,\"autoCrop\":false,\"caption\":\"Three of Chandran\\u2019s novels. Her latest, Safe Haven, takes on Australia\\u2019s off shore detention policy and community advocacy and was inspired by high-profile cases\\nincluding that of \\u201Cthe Biloela family\\u201D.\",\"credit\":\"Supplied\",\"cropWidth\":756,\"fileName\":\"027c9ef2b61d169c1d446da1c83e1cef694e29a5\",\"id\":\"027c9ef2b61d169c1d446da1c83e1cef694e29a5\",\"mimeType\":\"image/jpeg\",\"offsetX\":0,\"offsetY\":0,\"originalHeight\":null,\"originalWidth\":null,\"source\":\"Supplied\",\"zoom\":0.252},\"type\":\"image\"},\"03faa790c12ccdaff7eef84d5ae34a49d56c7899\":{\"data\":{\"newTab\":false,\"noFollow\":false,\"text\":\" <b><i>The Sydney Morning Herald</i></b>\",\"url\":\"https://www.smh.com.au/topic/good-weekend-1qq\"},\"type\":\"linkExternal\"},\"0b3db0cc539dcef8b3037e96474b63f5776063e0\":{\"data\":{\"newTab\":false,\"noFollow\":false,\"text\":\" <b><i>Brisbane Times</i></b>\",\"url\":\"https://www.brisbanetimes.com.au/topic/good-weekend-1qq\"},\"type\":\"linkExternal\"},\"1c03aaf74d529fd7b10b192a46118d8d1e88e334\":{\"data\":{\"altText\":\"\",\"aspect\":1.5,\"autoCrop\":false,\"caption\":\"Chandran with publisher Robert Watkins, who describes Chai Time at Cinnamon Gardens as \\u201Ca novel that feels warm and comforting but has kind of an edge to it\\u201D.\",\"credit\":\"Alicia Hetherington Photography\",\"cropWidth\":756,\"fileName\":\"7b2fd560735b79b87606ebd75d5bb1b8fc5ae7a4\",\"id\":\"7b2fd560735b79b87606ebd75d5bb1b8fc5ae7a4\",\"offsetX\":65,\"offsetY\":72,\"originalHeight\":1067,\"originalWidth\":1600,\"source\":\"Supplied\",\"zoom\":0.54},\"type\":\"image\"},\"2c75ed35038f