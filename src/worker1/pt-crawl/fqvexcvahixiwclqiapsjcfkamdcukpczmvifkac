    <!-- related issue -->
     



    <!-- headline -->
    
    <h1>NRE to overhaul Connecticut DOT commuter rail locomotives</h1>
    



    <!-- subheading -->
    
        <!-- webcam -->
    



    <!-- byline and publish date -->
    
    <div class="byline">

        
        <span class="publishDate">
            March 20, 2017
        </span>
        
    </div>
    



    <!-- tags -->
    
    <div class="tags">
        <span class="russoOne">RELATED TOPICS:</span> 
        
        <a href="/web/20180410141400/http://trn.trains.com/tags/locomotives">LOCOMOTIVES</a>
         | 
        <a href="/web/20180410141400/http://trn.trains.com/tags/suppliers">SUPPLIERS</a>
         | 
        <a href="/web/20180410141400/http://trn.trains.com/tags/passenger">PASSENGER</a>
         | 
        <a href="/web/20180410141400/http://trn.trains.com/tags/commuter">COMMUTER</a>
         | 
        <a href="/web/20180410141400/http://trn.trains.com/tags/northeast">NORTHEAST</a>
        
    </div>
    



    <!-- Trains Industry Newsletter signup -->
    
    <div class="TINDNewsletterBox">
        <div class="form">
        <div class="errorMessageSection">
        </div>
