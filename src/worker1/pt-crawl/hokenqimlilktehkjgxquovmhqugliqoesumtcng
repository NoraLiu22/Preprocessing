function map_init_basic (leafmap, options) {
    var map = leafmap;

    	// Replace comma with dot and parse as float
        var lat = "54.494784".replace(',', '.');
        var lng = "18.538443".replace(',', '.');
		map.setView([lat, lng], 6)
		new L.marker([lat,lng]).addTo(map);
	}
</script>
