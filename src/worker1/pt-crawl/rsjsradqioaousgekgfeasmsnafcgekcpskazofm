        outbrainWidgets = [].slice.call(outbrainWidgets)
        if (outbrainWidgets && outbrainWidgets.length > 0) {
            outbrainWidgets.forEach(function(widget) {
                widget.setAttribute('data-external-id', pageViewId);
            });
        }

        window.PAGE_VIEW_ID = pageViewId;
