        const getRandomValues = function(arr) {
            if (typeof msCrypto !== 'undefined') {
                return msCrypto.getRandomValues(arr);
            }
            return crypto.getRandomValues(arr);
        };

        const toHexString = function(bytes) {
            byteArray = [].slice.call(bytes)
            return byteArray.reduce(function(hex, byte) {
                hex += ('0' + byte.toString(16)).slice(-2);
                return hex;
            }, '');
        };
        const randomBytes = getRandomValues(new Uint8Array(16));
        const pageViewId = toHexString(randomBytes);

