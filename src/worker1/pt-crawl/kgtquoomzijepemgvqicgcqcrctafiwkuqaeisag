<script type="text/JavaScript">
	
var default_map=1;

function my(n) {
	switch(n)
		{
			case 1:
  				document['imgMap'].src='images/maps/Aragon1050.jpg';
				document['imgMap'].useMap='#map2';
  			break;    
			case 2:
			  document['imgMap'].src='images/maps/Aragon1120.jpg';
			  document['imgMap'].useMap='#map';
			  break;
			case 3:
			  document['imgMap'].src='images/maps/Aragon1150.jpg';
			  document['imgMap'].useMap='#map';
			  break;
			 case 4:
			  document['imgMap'].src='images/maps/Aragon1250.jpg';
			  document['imgMap'].useMap='#map3';
			  break;
			  case 5:
			  document['imgMap'].src='images/maps/Aragon1285.jpg';
			  document['imgMap'].useMap='#map4';
			  break;
			case 6:
			  document['imgMap'].src='images/maps/Aragon1330.jpg';
			  document['imgMap'].useMap='#map1';
			  break;
			  case 7:
			  document['imgMap'].src='images/maps/Aragon1415.jpg';
			  document['imgMap'].useMap='#map5';
			  break;
			   case 8:
			  document['imgMap'].src='images/maps/Aragon1450.jpg';
			  document['imgMap'].useMap='#map';
			  break;
			default:
			  document['imgMap'].src="images/maps/Aragon1330.jpg";
				document['imgMap'].useMap='#map1';
			}
}
</script>

