				aTemp[3] = '<a href="/news/newsletters/edreview/?src=ln" title="Go to \'ED Review\' page">ED Review</a>';
	
				aTemp[4] = '<a href="/news/newsletters/innovator/?src=ln" title="Go to \'Education Innovator\' page">Education Innovator</a>';
	
				aTemp[5] = '<a href="/news/newsletters/ovaereview/index.html?src=ln" title="Go to \'OVAE Review\' page">OVAE Review</a>';
	
				aTemp[6] = '<a href="/pubs/archivednewsletters.html?src=ln" title="Go to \'Archive\' page">Archive</a>';

		secondaryNavitems[7] = aTemp;

		aTemp = new Array;
	
				aTemp[0] = '<a href="/news/fedregister/proprule/?src=ln" title="Go to \'Proposed Regulations\' page">Proposed Regulations</a>';
	
				aTemp[1] = '<a href="/news/fedregister/finrule/?src=ln" title="Go to \'Final Regulations\' page">Final Regulations</a>';
	
				aTemp[2] = '<a href="/news/fedregister/announce/?src=ln" title="Go to \'Announcements\' page">Announcements</a>';
	
				aTemp[3] = '<a href="/news/fedregister/other/?src=ln" title="Go to \'Other\' page">Other</a>';

		secondaryNavitems[8] = aTemp;

	
	
	
	function navChange(item){
	//function to change DHTML to new nav title, but keep current page loaded
		if(item != null){	
			for(n=0; n<numberOfItems; n++){
				if(alwaysTrue != null && alwaysTrue == n){
				//if n is the landing page nav title, it must stay expanded
					expandedItems[n] = true;
				}
				else{
					// if this is the newly selected nav title, expand/contract it based on current state
					if(item == n){
						if(expandedItems[n] == false){
							expandedItems[n] = true;
						}
						else{
							expandedItems[n] = false;
						}
					}
					else{
					// if this is not the selected nav title and was not the landing page "always expanded" title,
					// set it to "not expanded"
						expandedItems[n] = false;
					}
				}
			}	
		}
				
		serve = false;
				
		if(myDOM('leftNav')){
			serve = true;
		}
		if(browser.indexOf('ie') >=0 && navigator.userAgent.indexOf('Mac') >=0){
			serve = false;		
		}
			
		if(serve){
			text = '<table summary="This table is for layout only" cellpadding=0 cellspacing=0 border=0 width="170" align="left">';
				
			for(y = 0; y<numberOfItems; y++){
				text += '<tr>';
				if(expandedItems[y] == true){
					if(alwaysTrue != null && alwaysTrue == y){	
					//if this item is always expanded under the current landing page, highlight it and show selected bullet
					//There will be no link to click on for the header				
						text += '<td><img src="/images/spacer.gif" alt=" " width="24" height="5" hspace="0" vspace="0" border="0"></td>';		 
						text += '<td valign="top"><img src="/images/ed_inf_snav_bullet_r.gif" alt="\'' + primaryNavitems[y] + '\' subtopics menu is expanded" width="9" height="9" hspace="0" vspace="0" border="0" name="' + 'bullet' + y + '"></td>';
						text += '<td><img src="/images/spacer.gif" alt=" " width="4" height="9" hspace="0" vspace="0" border="0"></td>';
							
						text += '<td class="leftNavTextHighlight" valign="top" align="left" width="124">' + primaryNavitems[y] + '</td>';	
						text += '<td><img src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0"></td>';
						text += '</tr>';
					}
					else{
					//if the expanded item is one that has been selected, show selected bullet and create link for requested url with its expansion number
						text += '<td><img src="/images/spacer.gif" alt=" " width="24" height="5" hspace="0" vspace="0" border="0"></td>';		 
						text += '<td valign="top"><a href="/news/pressreleases/2004/01/01142004.html?exp='+y+'" onClick="navChange(' + y + '); return false;"><img src="/images/ed_inf_snav_bullet_r.gif" alt="Collapse \'' + primaryNavitems[y] + '\' subtopics menu" width="9" height="9" hspace="0" vspace="0" border="0" name="' + 'bullet' + y + '"></a></td>';
						text += '<td><img src="/images/spacer.gif" alt=" " width="4" height="9" hspace="0" vspace="0" border="0"></td>';
							
						text += '<td class="leftNavText" valign="top" align="left" width="124"><a href="/news/pressreleases/2004/01/01142004.html?exp='+y+'" onClick="navChange(' + y + '); return false;" title="Collapse \'' + primaryNavitems[y] + '\' subtopics menu">' + primaryNavitems[y] + '</a></td>';	
						text += '<td><img src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0"></td>';
						text += '</tr>';
					}
				}
				else{
				//if this item is not currently expanded, set it up with normal bullet and link to current requested url with the appropriate expansion number
					text += '<td><img src="/images/spacer.gif" alt=" " width="24" height="5" hspace="0" vspace="0" border="0"></td>';
					text += '<td valign="top"><a href="/news/pressreleases/2004/01/01142004.html?exp='+y+'" onClick="navChange(' + y + '); return false;"><img src="/images/ed_inf_snav_bullet.gif" alt="Expand \'' + primaryNavitems[y] + '\' subtopics menu" width="9" height="9" hspace="0" vspace="0" border="0" name="' + 'bullet' + y + '"></a></td>';
					text += '<td><img src="/images/spacer.gif" alt=" " width="4" height="9" hspace="0" vspace="0" border="0"></td>';
						
					text += '<td class="leftNavText" valign="top" align="left" width="124"><a href="/news/pressreleases/2004/01/01142004.html?exp='+y+'" onClick="navChange(' + y + '); return false;" title="Expand \'' + primaryNavitems[y] + '\' subtopics menu">' + primaryNavitems[y] + '</a></td>';	
					text += '<td><img src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0"></td>';
					text += '</tr>';
						
				}
					
					
				if(expandedItems[y] == true){
				//for each expanded item, search through its children to create headers for each
					for(z=0; z<secondaryNavitems[y].length; z++){
					
						text += '<tr>';
						text += '<td class="leftSubNavText"><img src="/images/spacer.gif" alt=" " width="24" height="5" hspace="0" vspace="0" border="0"></td>';
						
						if(alwaysTrue != null && alwaysTrue ==y && alwaysTrueSubItem != null && alwaysTrueSubItem == z){
						// if this is the child of an "always selected" item for the current topic page, give it the subnavtext highlighting and insert the 
						// link and header set up for this item
							text += '<td valign=top class="leftSubNavText"><img src="/images/ed_stu_snav_subselect.gif" alt="\'' + secondaryNavitems[y][z] + '\' page is open" width="2" height="17" hspace="3" vspace="0" border="0" name="' + y + 'subBullet' + z + '"></td>';
							text += '<td class="leftSubNavText"><img src="/images/spacer.gif" alt=" " width="4" height="17" hspace="0" vspace="0" border="0"></td>';	
							text += '<td class="leftSubNavTextHighlight" align="left" width="124">' + secondaryNavitems[y][z] + '</td>';
						}
						else{
						// if this is just a selected header (ie, not a current topic page header), insert the header in the non-highlighted section
							text += '<td valign=top class="leftSubNavText"><img src="/images/spacer.gif" alt=" " width="2" height="17" hspace="3" vspace="0" border="0"></td>';
							text += '<td class="leftSubNavText"><img src="/images/spacer.gif" alt=" " width="4" height="17" hspace="0" vspace="0" border="0"></td>';	
							text += '<td class="leftSubNavText" align="left" width="124">' + secondaryNavitems[y][z] + '</td>';
						}
						
						text += '<td class="leftSubNavText"><img src="/images/spacer.gif" alt=" " width="4" height="9" hspace="0" vspace="0" border="0"></td>';
						text += '</tr>';
						
						text += '<tr><td colspan="5"><img src="/images/spacer.gif" alt=" " width="1" height="1" hspace="0" vspace="0" border="0"></td></tr>';
					}			
				}					
					
				text += '<tr><td colspan="5"><img src="/images/spacer.gif" alt=" " width="1" height="7" hspace="0" vspace="0" border="0"></td></tr>'	
				}
				
				text += '</table>';
			
				myDOM('leftNav').innerHTML=text;
		}
		else{
		// if we are not using DHTML, the page needs to be reloaded with the URL and attached currently selected item
			document.location = degradationURL[item];
		}		
	}

</script>



