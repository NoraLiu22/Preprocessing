<!--IMPORTANT: Each Quick Click link appears in 3 separate places below.  All changes must be made to all 3 occurrences.-->
<script>
function checkKeyPressedForEnter(eEvent){
	var cPressed;
	if(eEvent && eEvent.which){//get char code in netscape
		cPressed = eEvent.which;
	}
	else{
		eEvent = event;
		cPressed = eEvent.keyCode;
	}
	if(cPressed == 13){
		if (document.quickClickForm.where.selectedIndex > 0) {
			document.quickClickForm.submit();
		}
		return false;
	}
	return true;
}
</script>
<!--  If netscape 6.x, start form -->
