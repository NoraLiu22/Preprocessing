    document.getElementsByTagName('html')[0].className += ' js-support ' + navigator.platform;
    var SNI = window.SNI || {};
    SNI.JS_RESOURCE_PATH="//cook.fnr.sndimg.com/etc/clientlibs/assets/v3/js/"
