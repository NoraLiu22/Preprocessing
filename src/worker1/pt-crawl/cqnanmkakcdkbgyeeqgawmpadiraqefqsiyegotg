    <div class="o-Ingredients__m-Body">
      <!-- Terms are food and cook, isFood is food OR cook -->
      
      
      
        
        

        
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 stick (8 tablespoons) salted butter</span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">4 blocks (8 ounces each) sharp cheddar cheese, such as Cracker Barrel, cut into 1-inch cubes </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 egg </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 teaspoon olive oil </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">Pinch kosher salt </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 pound elbow macaroni </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 can cream of mushroom soup </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1/2 can evaporated milk, such as Carnation </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">5 tablespoons sour cream </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">8 ounces extra-sharp cheddar cheese, shredded </span>
            </label>
          </p>
        
          <p class="o-Ingredients__a-Ingredient">
            <label class="checkbox-text">
              
              <span class="o-Ingredients__a-Ingredient--CheckboxLabel">1 cup crumbled butter crackers, such as Ritz </span>
            </label>
          </p>
        
      
