<div id="block-chronicle-content">
  
    
      <article  class="node node--type-stories node--view-mode-full ds-1col clearfix container">
  
    
  
  <div class="field field--name-dynamic-twig-fieldnode-category-identifier field--type-ds field--label-hidden field__items">
