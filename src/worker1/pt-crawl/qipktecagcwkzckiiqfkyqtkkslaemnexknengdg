            <script type="text/x-mathjax-config">
                MathJax.Hub.Config({
                    "HTML-CSS": {
                        preferredFont: "TeX",
                        availableFonts: [ "STIX", "TeX" ],
                        linebreaks: { automatic: true },
                        EqnChunk: (MathJax.Hub.Browser.isMobile ? 10 : 50)
                    },
                    tex2jax: {
                        processEscapes: true,
                        ignoreClass: "tex2jax_ignore|dno"
                    },
                    TeX: {
                        Macros: {
                            Aut: "\\operatorname{Aut}",
                            Hom: "\\operatorname{Hom}"
                        },
                        noUndefined: {
                            attributes: {
                                mathcolor: "#039", //"red",
