                        style="float:left;display:inline;width:556px;height:8px;position: relative; z-index:1001;margin-top:-20px;">
                        <div style="float:right;display:inline;width:60px;" class="slidernav">
                            <div id="ec1" class="icon_a" onclick="clickNum(1);"></div>
                            <div id="ec2" class=" icon_b" onclick="clickNum(2);"></div>
                        </div>
