                    <script type="text/javascript">
                        var homeBannerNum = $('.slidernav div').size();//首页BAR总数----后台提供   
                        var curBannerNum = 1;//当前的BAR位置   
                        var preBannerNum = homeBannerNum;
                        $(document).ready(function () {
                            $('.homeJs_banner_img a img').each(function () {
                                $(this).css('display', 'block');
                            });
                            $('.homeJs_banner_textBox').each(function () {
                                $(this).css('display', 'block');
                            });
                            reHomeBannerButton();//按钮初始化   
                            var sliderNavs = [];
                            $('.slidernav div').each(function (index) {
                                sliderNavs[index] = '#' + $(this).attr('id');
                            });
                            $("#jCarouselLite2").jCarouselLite({
                                speed: 3000, //滚动速率,数值越小越快   
                                visible: 1,
                                start: 0,
                                auto: 5000,
                                beforeStart: function (a) {
                                    if (homeBannerNum > 1) {
                                        curBannerNum++;
                                        if (curBannerNum > homeBannerNum) {
                                            curBannerNum = 1;
                                        }
                                        preBannerNum = curBannerNum - 1;
                                        if (preBannerNum == 0) {
                                            preBannerNum = homeBannerNum;
                                        }
                                        reHomeBannerButton();
                                    }
                                },
                                btnGo: sliderNavs,
                                easing: "easeinout"
                            });
                        });
                        function reHomeBannerButton() {
                            $("#ec" + curBannerNum).removeClass("icon_b").addClass("icon_a");
                            if (homeBannerNum > 1) {
                                $("#ec" + preBannerNum).removeClass("icon_a").addClass("icon_b");
                            }
                        }
                        function clickNum(num) {
                            preBannerNum = curBannerNum;
                            curBannerNum = num;
                            reHomeBannerButton();
                        }
                        function showMenu(type, MoreUrl) {
                            $("#divRecommend").hide();
                            $("#btnRecommend").removeClass("leftBox_title_text_b");
                            $("#btnRecommend").addClass("leftBox_title_text_a");
                            $("#divQuickRecipe").hide();
                            $("#btnQuickRecipe").removeClass("leftBox_title_text_b");
                            $("#btnQuickRecipe").addClass("leftBox_title_text_a");
                            $("#div" + type).show();
                            $("#btn" + type).removeClass("leftBox_title_text_a");
                            $("#btn" + type).addClass("leftBox_title_text_b");
                            $("#aMore").attr("href", MoreUrl);
                        }
                        function showCookInfo(type, obj) {
                            if (type == 1) {
                                $(obj).find('.homeCookInfo').show();
                                //j("#aMore").attr("href","/zh-CN/Weibo?u="+uid);   
                            } else {
                                $(obj).find('.homeCookInfo').hide();
                                //j("#aMore").attr("href","/zh-CN/Weibo?u="+uid);   
                            }
                        } 
                    </script>
