    <script>
        $(function(){
            $('.foot1 .more1').click(function(){
                if($('.foot1').css('height')!='30px'){
                    $(this).addClass('a').removeClass('b');
                    $('.foot1').css('height','30px');
                }else{
                    $(this).addClass('b').removeClass('a');
                    $('.foot1').css('height','auto');
                }
                return false;
            })
            $('.foot1 .lit').mouseover(function(){
                $(this).addClass('b').removeClass('a').siblings('.lit').addClass('a').removeClass('b')
                var i=$(this).index('.lit');
                $('.link_more').eq(i).show().siblings('.link_more').hide();
            })
            $('.link_more').mouseleave(function(){
                $('.foot1 .lit').addClass('a').removeClass('b')
                $(this).hide();
            })

            $('.foot').mouseleave(function(){
                $('.foot1 .lit').addClass('a').removeClass('b')
                $('.link_more').hide();
            })
            $('.link_more').eq(0).css('left','120px')
            $('.link_more').eq(1).css('left','190px')
            $('.link_more').eq(2).css('left','500px')
            $('.link_more').eq(3).css('left','630px')
            //$('.link_more').eq(3).css('left','413px')
            //$('.link_more').eq(3).css('left','569px')
            $('#idStartDate').val($('#divT').html().replace(/年|月|日/g,'.').substr(0,10))
        })
        function AddFavorite(title, url) {
            try {
                window.external.addFavorite(url, title);
            }
            catch (e) {
                try {
                    window.sidebar.addPanel(title, url, "");
                }
                catch (e) {
                    alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请进入新网站后使用Ctrl+D进行添加");
                }
            }
        }

    </script>
