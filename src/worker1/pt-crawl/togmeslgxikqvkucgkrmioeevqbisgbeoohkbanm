        <script>
            $(function ()
            {
                $(".pk_tabboxt_2019 .pk_xuan2019 li").hover(function ()
                {
                    //获取点击的元素给其添加样式，讲其兄弟元素的样式移除
                    $(this).addClass("pk_active").siblings().removeClass("pk_active");
                    //获取选中元素的下标
                    var index = $(this).index();
                    $(this).parent().siblings().children().eq(index).addClass("pk_active")
                        .siblings().removeClass("pk_active");
                });

                $(".pk_lijie_weixin li span").each(function(){
                    var txtr=$(this).text();
                    if(txtr.length>6){
                        $(this).addClass("xiaozihaoti");
                    }
                });

                $(".piao_2019_kuai dt").hover(function(){
                    $(this).find(".pk_2019left_xian").show();
                },function(){
                    $(this).find(".pk_2019left_xian").hide();
                })

                $(".piao_2019_kuai dt").hover(function(){
                    $(this).find(".pk_2019left_xian2").show();
                },function(){
                    $(this).find(".pk_2019left_xian2").hide();
                })
            });
        </script>
