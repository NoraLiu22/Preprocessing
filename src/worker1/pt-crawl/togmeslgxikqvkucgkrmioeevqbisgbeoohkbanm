                    function submitSearchnew(){
                        $("#loginname").val($("#loginname").val().replace(/[\-\_\,\!\|\~\`\(\)\#\$\%\^\&\*\{\}\:\;\"\L\<\>\?]/g, ''));
                        var search_input = document.getElementById("loginname").value;
                        if(search_input == "")
                        {
                            alert("请输入检索词！");
                            return false;
                        }
                    }
