      -function () {
        $('#input-search').on('keypress', function (e) {
          if (e.keyCode == 13) {
            window.open('http://search.people.cn/s?st=0&keyword=$$$'.replace('$$$', encodeURIComponent(this.value)))
          }
        })
        $('.icon-search').click(function () {
          window.open('http://search.people.cn/s?st=0&keyword=$$$'.replace('$$$', encodeURIComponent($('#input-search').val())))
        })
      }()
