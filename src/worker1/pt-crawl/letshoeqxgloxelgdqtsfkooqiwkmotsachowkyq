      <script>
        -function () {
          var $allSlides = $('.bk-keji .swiper-slide')
          var $pageNum3 = $('.page-num3')
          var swiper3 = new Swiper('.bk-keji .swiper-container', {
            pagination: '.bk-keji .pagination1',
            loop: true,
            grabCursor: true,
            nextButton: '.bk-keji .ar3',
            prevButton: '.bk-keji .al3',
            paginationClickable: true,
            onInit: function(swiper) {
              $pageNum3.html('1/' + $allSlides.length)
            },
            onSlideChangeEnd: function(swiper) {
              $pageNum3.html(swiper.realIndex + 1 + '/' + $allSlides.length)
            }
          })
        }();
      </script>

