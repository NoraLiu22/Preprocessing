<script>
    // 需要隐藏的板块，填写 '隐藏' ，否则填写 '显示'，默认都显示
    var hiddenCongif = {
      '焦点图': '显示',
      '大观': '显示',
      '青年说': '显示',
      '强国新闻': '显示',
      '科技名人堂': '显示',
      '强国微视频': '显示',
      '人民e财经': '显示',
      '专题': '显示',
    }
    // 编辑填写，会覆盖默认值
    // 如果需要板块隐藏，则在对应板块右侧填写 '隐藏'    

       var hiddenCongif = {
      '焦点图': '显示',
      '大观': '隐藏',
      '青年说': '显示',
      '强国新闻': '显示',
      '科技名人堂': '显示',
      '强国微视频': '显示',
      '人民e财经': '显示',
      '专题': '显示',
    }
  
    // 如果需要板块隐藏，则在对应板块右侧填写 '隐藏'    

    var hiddenHandler = function() {
      if (hiddenCongif['焦点图'] === '隐藏') {
        $('.bk-jdt').hide()
      }
      if (hiddenCongif['大观'] === '隐藏') {
        $('.bk-daguan .common-title').first().hide()
        $('.bk-daguan .swiper2-outer').hide()
      }
      if (hiddenCongif['青年说'] === '隐藏') {
        $('#qingnianshuo').hide()
        $('.bk-fengqin').hide()
      }
      if (hiddenCongif['强国新闻'] === '隐藏') {
        $('.bk-guancha').hide()
      }
      if (hiddenCongif['科技名人堂'] === '隐藏') {
        $('.bk-keji .common-title').first().hide()
        $('.bk-keji .swiper1-out').hide()
      }
      if (hiddenCongif['强国微视频'] === '隐藏') {
        $('#weishipin').hide()
        $('.bk-duihua').hide()
        
      }
      if (hiddenCongif['人民e财经'] === '隐藏') {
        $('.bk-ec').hide()
      }
      if (hiddenCongif['专题'] === '隐藏') {
        $('.bk-zhuanti').first().hide()
      }

    }

    hiddenHandler()
  </script>





