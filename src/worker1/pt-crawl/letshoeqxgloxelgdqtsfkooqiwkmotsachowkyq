    <script>
        - function () {
          var isMobile = /Android|iPhone|iPad|iPod/i.test(navigator.userAgent)
          var $allSlides = $('.swiper-container-jdt .swiper-slide')

          var tabHTML = ''
          for (var i = 0; i < $allSlides.length; i++) {
            tabHTML += '<div class="swiper-slide">$text</div>'.replace('$text', $allSlides.eq(i).find('p').text())
          }
          $('.swiper-containertab .swiper-wrapper')[0].innerHTML = tabHTML

          $('.bk-jdt .swiper-containertab .swiper-slide').each(function(index) {
            $(this).attr('data-idx', index)
          })

          // 自动生成左侧右侧焦点图
          $('#swiper-left .swiper-wrapper')[0].innerHTML = $('.swiper-container-jdt .swiper-wrapper')[0].innerHTML
          $('#swiper-right .swiper-wrapper')[0].innerHTML = $('.swiper-container-jdt .swiper-wrapper')[0].innerHTML
          $('#swiper-left .swiper-wrapper p,#swiper-left .swiper-wrapper video,#swiper-left .swiper-wrapper .btn-play-jdt').remove()
          $('#swiper-right .swiper-wrapper p,#swiper-right .swiper-wrapper video,#swiper-right .swiper-wrapper .btn-play-jdt').remove()


          var $pageNo = $('.bk-jdt .page-no')

          var html = ''
          var width = 100 / $allSlides.length
          for (var i = 0; i < $allSlides.length; i++) {
            html += '<span style="width: $width"></span>'.replace('$width', width + '%')
          }

          $('.bar-wrap').html(html)

          var $barSpan = $('.bar-wrap span')
          var $allV = null
          var stopAllV = function () {
            $allV.hide()
            $allV.each(function (index, v) {
              v.pause()
            })
          }



          new Swiper('.bk-jdt .swiper-container-jdt', {
            pagination: '.pagination1',
            loop: true,
            grabCursor: true,
            slidesPerView: 1,
            prevButton: '.arrow-jdt-left',
            nextButton: '.arrow-jdt-right',

            paginationClickable: true,
            onInit: function (swiper) {
              window.swiper1 = swiper
              $allV = $('.bk-jdt video')
              $barSpan.first().addClass('active')
              $pageNo.html('1/' + $allSlides.length)
              $('.btn-play-jdt').click(function () {

                $(this).siblings('video').first().show().get(0).play();
                $('.bar-wrap').hide()
              })
              // 视频暂停播放时，关闭视频原生播放器
              if (isMobile) {
                var $allVideo = $('.bk-jdt video')
                $allVideo.each(function(index2, video) {
                  video.onpause = function() {
                    $(this).hide().siblings().show()
                  }
                })
              }
            },
            onSlideChangeEnd: function (swiper) {
              $pageNo.html(swiper.realIndex + 1 + '/' + $allSlides.length)
              $barSpan.eq(swiper.realIndex).addClass('active').siblings().removeClass('active')
              console.log('SWIPER111: ' + swiper.realIndex)
              $allV && stopAllV()
              $('.bar-wrap').show()
              if (window.swiperLeft && window.swiperRight) {
                swiperLeft.slideTo(swiper.realIndex + 1, 0)
                setTimeout(function() {
                  swiperLeft.slidePrev()
                }, 50);
                swiperRight.slideTo(swiper.realIndex + 1, 0)
                setTimeout(function(){
                  swiperRight.slideNext()
                }, 50);
              }
              if (window.swiper66) {
                setTimeout(function () {
                  var index2 = swiper.realIndex + 4
                  // if (swiper.realIndex == 0) {
                  //   index2 = 0
                  //   console.log('索引修改', index2)
                  // }
                  console.log('目标索引: ' + index2)
                  swiper66.slideTo(index2)
                }, 50)
              }

            }
          })

          window.swiper66 = new Swiper('.bk-jdt .swiper-containertab', {
            // pagination: '.pagination1',
            loop: true,
            grabCursor: true,
            slidesPerView: 4,
            direction: 'vertical',
            spaceBetween: 1,
            speed: 100,
            preventClicks : false,
            preventClicksPropagation : false,
            // slideToClickedSlide: true,
            paginationClickable: true,
            onInit: function (swiper) {
              $('.bk-jdt .swiper-containertab .swiper-slide').click(function () {
                var index = $(this).attr('data-swiper-slide-index')
                console.log('滑动去往：' + index)
                window.swiper66.slideTo(index - 0 + 4)
                window.swiper1.slideTo(index - 0 + 1, 500)
              })
              swiper.detachEvents()
            },
            onSlideChangeEnd: function (swiper) {
              console.log('SWIPER@@@: ' + swiper.realIndex)
              // swiper1.slideTo(swiper.realIndex)
            }
          })

          new Swiper('.bk-jdt .swiper-left', {
            // pagination: '.pagination1',
            loop: true,
            grabCursor: true,
            slidesPerView: 1,
            // effect: 'fade',
            speed: 0,
            onInit: function (swiper) {
              swiper.slidePrev()
              window.swiperLeft = swiper
              // console.log('swiperLeft init')
              // setTimeout(function() {

              // }, 100)
            },
            onSlideChangeEnd: function (swiper) {

            }
          })

          new Swiper('.bk-jdt .swiper-right', {
            // pagination: '.pagination1',
            loop: true,
            grabCursor: true,
            slidesPerView: 1,
            // effect: 'fade',
            speed: 0,
            onInit: function (swiper) {
              swiper.slideNext()
              window.swiperRight = swiper
            },
            onSlideChangeEnd: function (swiper) {

            }
          })



        }();
    </script>

