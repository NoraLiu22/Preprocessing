 <script type="text/javascript">
    (function ($, window) {

      $('.Tabs').each(function (idx, el) {
        var el_li = $(el).find('>ul>li'),
          el_cont = $(el).siblings('.choose').find('.Content'),
          el_cur = 0;

        el_cont.eq(0).show();
        el_li.on('mouseenter', function () {
          el_cur = $(this).index();
          el_li.removeClass('Cur');
          $(this).addClass('Cur');
          el_cont.hide();
          el_cont.eq(el_cur).show();
        });
      });

    })(window.jQuery, window);
  </script>
