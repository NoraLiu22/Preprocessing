<script type="text/javascript">
var ss = 10;
var timestatus=1;
function countdown() {
ss = ss-1;
if (timestatus ==0){
	
	return;
}



if (ss<0) {
window.location="http://www.famous-trials.com/sheriffshipp";
}
else {
document.getElementById("countdown").innerHTML=ss;
window.setTimeout("countdown()", 1000);
}
}

    function closePopup(){
        
         if(timestatus==1)
    {
        clearInterval(ss);
        timestatus=0;
    }
        
        
}


</script>
