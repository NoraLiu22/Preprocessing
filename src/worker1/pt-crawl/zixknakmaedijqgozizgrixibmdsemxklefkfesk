        doHeadlineNews();
        function doHeadlineNews() {
            var _html = '';
            var _headlineAbstract = '';
            
                var _sideTitleYN = '是';

                if (_sideTitleYN && _sideTitleYN == '是') {
                    var _headlineUrl = "./xxfb/tt/202404/t20240429_845455.html";
                    var _headlineTitle = "";
                    if (_headlineTitle.length == 0) {
                        _headlineTitle = '《习近平谈“一带一路”（2023年版）》英文版出版发行';
                    }
                    if (_headlineAbstract.length == 0) {
                        _headlineAbstract = '该书英文版的出版发行，有助于国外读者深入了解共建“一带一路”的理念、举措、目标和成果';
                    }
                    var _headlineSource = "36";
                    var _relHref = "";
                    var _relTitle = "";
                    var _refSource = "";
                    
                    if (_headlineSource && _headlineSource.length == 0) {
                        _headlineSource = '32';
                    }
                    _html += "<div><a target='_blank' style='font-size:"+_headlineSource+"px;' href='" + _headlineUrl + "' >";
                    _html += _headlineTitle.replace('  ', '<br>');
                    _html += "</a></div>";
                    if (_relHref && _relHref.length > 0) {
                        _html += "<div><a target='_blank' style='font-size:"+_refSource+"px;' href='" + _relHref + "' >";
                        _html += _relTitle;
                        _html += "</a></div>";
                    }
                    var _htmlAbstract = _headlineAbstract.length>0?'[' + _headlineAbstract + ']':'';
                }
            
                var _sideTitleYN = '';

                if (_sideTitleYN && _sideTitleYN == '是') {
                    var _headlineUrl = "./xxfb/tt/202404/t20240425_844883.html";
                    var _headlineTitle = "习近平在重庆考察强调";
                    if (_headlineTitle.length == 0) {
                        _headlineTitle = '习近平在重庆考察时强调 进一步全面深化改革开放 不断谱写中国式现代化重庆篇章';
                    }
                    if (_headlineAbstract.length == 0) {
                        _headlineAbstract = '习近平：进一步全面深化改革开放 不断谱写中国式现代化重庆篇章 | 蔡奇陪同考察';
                    }
                    var _headlineSource = "36";
                    var _relHref = "";
                    var _relTitle = "";
                    var _refSource = "";
                    
                    if (_headlineSource && _headlineSource.length == 0) {
                        _headlineSource = '32';
                    }
                    _html += "<div><a target='_blank' style='font-size:"+_headlineSource+"px;' href='" + _headlineUrl + "' >";
                    _html += _headlineTitle.replace('  ', '<br>');
                    _html += "</a></div>";
                    if (_relHref && _relHref.length > 0) {
                        _html += "<div><a target='_blank' style='font-size:"+_refSource+"px;' href='" + _relHref + "' >";
                        _html += _relTitle;
                        _html += "</a></div>";
                    }
                    var _htmlAbstract = _headlineAbstract.length>0?'[' + _headlineAbstract + ']':'';
                }
            
            $("#hotnews-tit").append(_html);
            $("#hotnews-abs").append(_htmlAbstract);
        }
        var _searchWord = {
            "spmc" : '',
            "page" : 1,
            "size" : 2
        };
        $.ajax({
            type:"post",
            url:"/was5/web/search?channelid=267045",
            //dataType:"JSON",
            data : _searchWord,
            success : function(data1){
                var jsondata = JSON.parse(data1);
                $("#dataCenter").html("");
                var _data = jsondata.data;
                if (_data != null && _data.length > 0) {
                    for (var i=0;i<2;i++) {
                        var _spmc = _data[i].spmc;
                        var docpuburl = _data[i].docpuburl;
                        if (_spmc!= null && _spmc.length>0) {
                            var _listr = "<li>";
                            _listr = _listr + '<a className="nzsc3u1_a" href="'+ docpuburl + '">';
                            _listr = _listr + '<span>【许可结果】</span>';
                            _listr = _listr + _spmc;
                            _listr = _listr + '</a>';
                            _listr = _listr + '</li>';
                            $('#ul-resultpub').append(_listr);
                        }
                    }
                }
            },
            error:function(jqXHR){
                console.log("Error: "+jqXHR.status);
            }
        });
    });
