            .txtMarquee-left{ width:auto;  position:relative; margin-bottom:20px; font-size:15px;background: #edf1f3; height:30px;  height: 35px;  padding: 5px; }
            .rollss { width:1140px; height: 24px;  position: relative;  float:left;}
