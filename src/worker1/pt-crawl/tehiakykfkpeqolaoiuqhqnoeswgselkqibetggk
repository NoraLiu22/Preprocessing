				if (ad) {
					if (ad.out) {
						adSlots[adId] = googletag.defineOutOfPageSlot(ad.name, adId).addService(googletag.pubads());
					}
					else if (ad.collapse) {
						adSlots[adId] = googletag.defineSlot(ad.name, ad.sizes, adId).addService(googletag.pubads()).setCollapseEmptyDiv(true, true);
