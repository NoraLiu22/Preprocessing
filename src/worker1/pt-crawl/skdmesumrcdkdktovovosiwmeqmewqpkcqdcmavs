				<TD ALIGN=left colspan=2 WIDTH=300>
					<!--pull text and 
graphic--><!--left side graphic-->
					<IMG alt="illustration" align="left" height="450" src="/art/Jenna_and_me_final.jpg" width="300" border="0"><br clear=all>
					<IMG align=right height=8 
src="/art/ART.CREDIT.MAVRIDES.gif" width=79>
