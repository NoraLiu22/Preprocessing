                            var W1 = $("#nr1");
                            var W2 = $("#nr2");
                            var T1 = $("#tb1");
                            var W3 = $("#nr3");
                            var T2 = $("#tb2");
                            var W4 = $("#nr4");
                            var T3 = $("#tb3");
                            var G2 = $("#gg2");
                            var W5 = $("#nr5");
                            var T4 = $("#tb4");
                            if (gg1 > 0 && gg2 > 0) {
                                W1.html(G1.html());
                                W2.html(T1.html());
                                W3.html(T2.html());
                                W4.html(T3.html());
                                W5.html(G2.html());
                            } else if (gg1 > 0) {
                                W1.html(G1.html());
                                W2.html(T1.html());
                                W3.html(T2.html());
                                W4.html(T3.html());
                                W5.html(T4.html());
                            } else if (gg2 > 0) {
                                W5.html(G2.html());
                            }
                        });
                </script></div></div></div> <div class="guanggao T3"><div class="gdad"><div class="scroll"><div id="LeftArr" class="scroll_btn"></div> <div id="scrollPic" class="pic"><ul><li><script sid="1279" type="text/javascript" src="//a2.news.cn/js/xadn.js"></script></li> <li><script sid="1280" type="text/javascript" src="//a2.news.cn/js/xadn.js"></script></li> <li><script sid="1277" type="text/javascript" src="//a2.news.cn/js/xadn.js"></script></li></ul></div> <div id="RightArr" class="scroll_btn"></div></div></div></div> <div class="box"><div class="box_left szzq"><div class="titl"><span class="more"><a target="_blank" href="http://hq.news.cn/hngov/hnld/index.htm" style="color:#FF0000; font-weight:normal;">领导活动报道集</a></span> <h3>政情人事</h3></div> <h4 style="margin-top:-12px;"><span class="more" style="line-height:36px;"><a target="_blank" href="http://hq.news.cn/news/zy.htm">更多&gt;&gt;</a></span>海南政要</h4> <ul class="list"><li data="datasource:1755f20ec5c54e5188d39da32c6c7f0e" datatype="ds" preview="ds_"><a href="20240504/4f114e3ed8544f48a36e39f9c2548cb0/c.html" target="_blank">冯飞看望慰问值班值守人员和一线劳动者</a></li><li><a href="20240501/f2a58ab8de9740f992917f0eae55e16e/c.html" target="_blank">刘小明：不断改善群众生活居住条件</a></li></ul> <h4><span class="more" style="line-height:36px;"><a target="_blank" href="http://hq.news.cn/news/rs.htm">更多&gt;&gt;</a></span>人事任免</h4> <ul class="list"><li data="datasource:fd3a7067060f4a1bb2f9496dc821591e" datatype="ds" preview="ds_"><a href="20240312/f61a5d38cbff4c079e8a3ec2d30c039e/c.html" target="_blank">李爱华同志任东方市委书记</a></li><li><a href="20240110/c1ef1f5b63764a459274544e32aaa8a3/c.html" target="_blank">中共海南省委决定唐守兵同志任临高县委书记</a></li></ul> <h4><span class="more" style="line-height:36px;"><a target="_blank" href="http://hq.news.cn/news/lz.htm">更多&gt;&gt;</a></span>廉政</h4> <ul class="list" style="margin-bottom:0px;"><li data="datasource:4f4c5d924c6142cf94ed924024727a36" datatype="ds" preview="ds_"><a href="20240403/3e7251b07df041b3a4ae5c0e5a13c149/c.html