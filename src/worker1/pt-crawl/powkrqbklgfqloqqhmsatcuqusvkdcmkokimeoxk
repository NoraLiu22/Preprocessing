    <td width="952" align="left" valign="top" bgcolor="#eaeaea" height="503"> 
      <div align="left"> 
        <p><font color="#663399" face="Verdana,Arial,Helvetica,sans-serif" size="3"><b>Basic 
          Throw-in Training</b></font></p>
        <p align="center"><font size="2" face="Verdana, Arial, Helvetica, sans-serif"> 
          <script type="text/javascript"><!--
google_ad_client = "pub-3607693930582854";
google_ad_width = 728;
google_ad_height = 90;
google_ad_format = "728x90_as";
