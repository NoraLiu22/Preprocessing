  <script>
      /**
       * @file
       * Attaches several event listener to a web page.
       */

      (function ($, Drupal, drupalSettings) {

          /* eslint max-nested-callbacks: ["error", 4] */

          'use strict';

          Drupal.google_analytics = {};

          $(document).ready(function () {

              // Attach mousedown, keyup, touchstart events to document only and catch
              // clicks on all elements.
              $(document.body).on('mousedown', function (event) {
                 // console.group('Running Google Analytics for Drupal.');
                  if($(event.target).is('button') && $(event.target).attr('data-behavior') == "subjectsOpen subjects") {
                      gtag('event', 'Subjects', {
                          event_category: 'Subjects',
                          event_action: 'click'
                      });
                  }
                  if(event.target.closest('.embed-video__play') ) {
                    //  console.log("video played");
                      gtag('event', 'Video', {
                          event_category: 'Videos',
                          event_action: 'play',
                          event_label: book_title,

                      });
                  }

                  if(event.target.closest('.webform-button--submit') ) {
                     // console.log("newsletter-submit");
                      gtag('event', 'Newsletter', {
                          event_category: 'Newsletter',
                          event_action: 'signup',
                          event_label: window.location.pathname

                      });
                  }
                  if(event.target.closest('.g-header__nav-item--search')) {
                     // console.log('search open');
                      gtag('event','Search Open', {
                          event_category: 'Search',
                          event_action: 'open'

                      })
                  }
                  if(event.target.closest('.o-book__cta-list-item')) {
                     // console.log('download cover click');
                      gtag('event','Download Cover', {
                          event_category: 'Book detail',
                          event_action: 'download',
                          event_label: 'Cover'

                      })
                  }


                  if(event.target.closest('.m-book-cover__actions')) {
                      //console.log('look inside');
                      gtag('event','Look Inside', {
                          event_category: 'Book detail',
                          event_action: 'click',
                          event_label: 'Look inside'
                      })
                  }
                  if(event.target.closest('.m-editions__trigger')) {
                      var edition_type = $(event.target).attr('id').slice(0,$(event.target).attr('id').indexOf('_'));
                     // console.log(edition_type)
                      gtag('event','Edition', {
                          event_category: 'Book detail',
                          event_action: 'click',
                          event_label: edition_type
                      })
                  }
                  if(event.target.closest('.copyISBN')) {
                     // console.log('copy isbn');
                      gtag('event','Copy Isbn', {
                          event_category: 'Book detail',
                          event_action: 'copy',
                          event_label: 'ISBN'
                      })
                  }


                  if(event.target.closest('.o-book__buy-button')) {
                     // console.log('buy this');
                      gtag('event','Buy This', {
                          event_category: 'Book detail',
                          event_action: 'add to cart',
                          event_label: window.location.pathname
                      })
                  }



                  if(event.target.closest('.o-book__nav-scroller')) {
                      var tab = $(event.target).attr('href');
                     // console.log('book tab: ' + tab);
                      gtag('event','Book Tab', {
                          event_category: 'Book detail',
                          event_action: 'click',
                          event_label: tab
                      })
                  }


                  if(event.target.closest('.o-look-inside__buy-button')) {
                      var book_title = $(event.target).closest('.o-look-inside').find('.f-h7' ).html();
                      gtag('event','Look Inside Buy', {
                          event_category: 'look inside',
                          event_action: 'add to cart',
                          event_label: book_title,
                      })
                  }

                  if(event.target.closest('.o-look-inside__close')) {
                     // console.log('look inside close')
                      gtag('event','Look Inside Close', {
                          event_category: 'look inside',
                          event_action: 'close'
                      })
                  }

                  if(event.target.closest('.o-buy-book__editions')) {
                      var type = $('input[name=type]:checked', '#bookForm').val();
                      if(type == "Hardcover_") {
                          type = "Paperback_";
                      } else if (type == "Paperback_") {
                          type = "Hardcover_";
                      }
                      //console.log('radio edition clicked: ' + type);
                       gtag('event','ToggleFormat', {
                           event_category: 'Purchase Flow',
                           event_action: 'click',
                           event_label: type
                       })
                  }

                  if(event.target.closest('.o-buy-book__buy-button')) {
                      //console.log('buy button');
                      gtag('event','Buy Button', {
                          event_category: 'Purchase Flow',
                          event_action: 'click'
                      })
                  }

                  if(event.target.closest('.o-buy-book__external')) {
                     // console.log( 'external buy link');
                      gtag('event','externalbuy', {
                          event_category: 'Purchase Flow',
                          event_action: 'click',
                          event_label: $(event.target).closest('.f-secondary').attr('href')
                      })
                  }


                  if(event.target.closest('.g-modal__close')) {
                      //console.log( 'buy modal closed');
                      gtag('event','externalbuy', {
                          event_category: 'Purchase Flow',
                          event_action: 'close'
                      })
                  }



                  if(event.target.closest('.m-book-detail__link')) {
                     // console.log( 'sidebar buy');
                      gtag('event','sidebar buy', {
                          event_category: 'Sidebar',
                          event_action: 'add to cart',
                          event_label: $(event.target).closest('.m-book-detail__buy-button').attr('href')
                      })
                  }



                  if(event.target.closest('#panel_date-time')) {
                      //console.log("add to cal");
                      gtag('event','addtocal', {
                          event_category: 'Event',
                          event_action: 'click',
                          event_label: 'Calendar'
                      })
                  }

                  if(event.target.closest('#panel_address')) {
                    //  console.log("event external");
                      gtag('event','external-link', {
                          event_category: 'Event',
                          event_action: 'click',
                          event_label: 'External Link'
                      })
                  }



                  if(event.target.closest('.m-sharer__links')) {
                      gtag('event','event share', {
                          event_category: 'Event',
                          event_action: 'share',
                          event_label: $(event.target).attr('data-sharepagelinks')

                      })
                  }


              });


          });



      })(jQuery, Drupal, drupalSettings);

  </script>
