<section class="ingredients block col-sm-12 col-md-5">
    <h4>Ingrédients</h4>

                        
                            <ul>
                                            <li class="itemListe">
                            <input type="checkbox" name="eau" id="eau" />
                            <label for="eau">
                                <span class="qty">
                                                                                                                                                    8 tasses
                                                                    </span>
                                
                                eau
                                
                            </label>
                        </li>
                                            <li class="itemListe">
                            <input type="checkbox" name="gourgane" id="gourgane" />
