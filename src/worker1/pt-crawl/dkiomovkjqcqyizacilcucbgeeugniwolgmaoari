                        }







// ROLLOVERS SCRIPT







// Preload images







if (document.images) {







var imageUrls = new  Array("/history/scottishhistory/images/independence/history_independence_history_on.gif","/history/scottishhistory/images/independence/history_independence_history_on.gif","/history/scottishhistory/images/independence/history_independence_media_on.gif","/history/scottishhistory/images/independence/history_independence_oddities_on.gif","/history/scottishhistory/images/independence/history_independence_web_guide_on.gif");







        var pic = new Array(imageUrls.length);







        for (i=0; i<imageUrls.length; i++) {



                pic[i] = new Image();



                pic[i].src = imageUrls[i];



        }



}















// Rolling over



function hiLite(imageName) {



        if (document.images) {



                top.document.images[imageName].src = "/history/scottishhistory/images/independence/" + imageName + "on.gif";



        }



}







// Rolling out



function backNormal(imageName) {



        if (document.images) {



                top.document.images[imageName].src = "/history/scottishhistory/images/independence/" + imageName + "off.gif";



        }



}



