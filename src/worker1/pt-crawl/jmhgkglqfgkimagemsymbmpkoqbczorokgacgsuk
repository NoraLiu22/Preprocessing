<script>
$(function () {
    lhr.part01Swiper(); //顶部焦点图
    lhr.part02Swiper(); //顶部焦点图
    lhr.textTabsSwiper(); //顶部焦点图
lhr.lwautoTabsSwiper(); //瞭望汽车tab
    lhr.voicer(); //顶部焦点图
    lhr.backtop(); //顶部焦点图

});
var lhr = {
    part01Swiper: function () {
        var part01swiper = new Swiper('.part01_p3 .swiper-container', {
            pagination: '.part01_p3 .swiper-pagination',
            paginationClickable: '.part01_p3 .swiper-pagination',
            spaceBetween: 30
        });
    },
    //顶部焦点图
    part02Swiper: function () {
        var part02swiper = new Swiper('.part02_right .swiper-container', {
            pagination: '.part02_right .swiper-pagination',
            paginationClickable: '.part02_right .swiper-pagination',
            nextButton: '.part02_right .swiper-button-next',
            prevButton: '.part02_right .swiper-button-prev',
            spaceBetween: 30
        });
    },
    textTabsSwiper: function () {
        var _textTabsSwiper = new Swiper('.part03_left .swiper-container', {
            onlyExternal: true,
            speed: 500,
            initialSlide: 0,
        })
        $(".TextTabs a").on('mouseover touchstart mousedown', function (e) {
            // e.preventDefault()
            $(".TextTabs .active").removeClass('active')
            $(this).addClass('active')
            // _textTabsSwiper.swipeTo($(this).index())
            _textTabsSwiper.slideTo($(this).index())
        })
        $(".TextTabs a").click(function (e) {
            // e.preventDefault()
        })

    },
lwautoTabsSwiper: function () {
        var _lwautoTabsSwiper = new Swiper('.lwautoCon .swiper-container', {
            onlyExternal: true,
            speed: 500,
            initialSlide: 0,
        })
        $(".lwautoTabs a").on('mouseover touchstart mousedown', function (e) {
            // e.preventDefault()
            $(".lwautoTabs .active").removeClass('active')
            $(this).addClass('active')
            // _lwautoTabsSwiper.swipeTo($(this).index())
            _lwautoTabsSwiper.slideTo($(this).index())
        })
        $(".lwautoTabs a").click(function (e) {
            // e.preventDefault()
        })

    },
    voicer: function voicer() {
        var swiper = new Swiper('#voicerSwpier', {
            nextButton: '.voicer-swiper .swiper-button-next',
            prevButton: '.voicer-swiper .swiper-button-prev',
            slidesPerView: 5,
            // spaceBetween: 20,
            loop: true
            // autoplay: 5000
        });
    },
    backtop: function voicer() {
        $(".backTop").click(function () {
            $('html,body').animate({
                scrollTop: (0)
            }, 500);
        });
    },

}
</script>
