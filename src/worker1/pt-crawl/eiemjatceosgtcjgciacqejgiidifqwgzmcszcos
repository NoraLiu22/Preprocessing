<script>
(function(window, $) {
$(".top_li li a").mouseenter(function(){
	$(".two").show();
})
$(".top_li li a").mouseleave(function(){
	$(".two").hide();
})


$(".swiper-container").mouseenter(function(){
	$(".s1").show();
})
$(".swiper-container").mouseleave(function(){
	$(".s1").hide();
})

var swiper = new Swiper('.swiper-container',{
  autoplay : 3000,
  loop: true,
  prevButton:'.swiper-button-prev.s1',
  nextButton:'.swiper-button-next.s1',
  pagination:'.swiper-pagination',
})	

/*无缝滚动*/
	var demo = $("#demo")[0];
	var demo1 = $("#demo1")[0];
	var demo2 = $("#demo2")[0];
	demo2.innerHTML=demo1.innerHTML;
	function Marquee(){
		if(demo.scrollLeft-demo2.offsetWidth>=0){
			demo.scrollLeft-=demo1.offsetWidth;
		}
		else{
			demo.scrollLeft += 1;
		}
	}
	var myvar=setInterval(Marquee,30);
	demo.onmouseout=function (){myvar=setInterval(Marquee,30);};
	demo.onmouseover=function(){clearInterval(myvar);};


var Swiper4 = new Swiper('.swiper-container4',{
  slidesPerView: 4,
  loop : true,
  prevButton:'.swiper-button-prev.s4',
  nextButton:'.swiper-button-next.s4',
  spaceBetween : 20,
})

var Swiper5 = new Swiper('.swiper-container5',{
  direction: 'vertical',
  slidesPerView: 3,
  prevButton:'.swiper-button-prev.s5',
  nextButton:'.swiper-button-next.s5',
  spaceBetween : 10,
})
$(".swiper-container5 .swiper-slide").click(function(){
	var index = $(this).index(".swiper-container5 .swiper-slide");
	$(".block5 .left .photo3").eq(index).addClass("hover").siblings(".block5 .left .photo3").removeClass("hover")
})


var Swiper7 = new Swiper('.swiper-container7',{
  effect : 'coverflow',
  slidesPerView: 2,
  centeredSlides: true,
  loop : true,
  prevButton:'.swiper-button-prev.s7',
  nextButton:'.swiper-button-next.s7',
  coverflow: {
            rotate: 0,
            stretch: 0,
            depth: 500,
            modifier: 1,
            slideShadows : true
        },
})	

var Swiper8 = new Swiper('.swiper-container8',{
  slidesPerView: 1,
