window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        let profileDetails = document.querySelector('.profile_container--main');
let profileLink = document.querySelector('.social-author__name'); 
let profileClose = document.querySelector('.profile___button--close');
let bodyElement = document.querySelector('body');

if (window.innerWidth < 768) {
  profileLink.href = "javascript:void(0)";
  profileLink.addEventListener('click', function(e) {
    profileDetails.classList.add("hovered");
    bodyElement.classList.add("no-scroll");
  }); 
  profileClose.addEventListener('click', function(e) {
    profileDetails.classList.remove("hovered");
    bodyElement.classList.remove("no-scroll");
  });  
} else {
  profileLink.addEventListener('mouseover', function(e) {
    profileDetails.classList.add("hovered");
  }); 
  profileLink.addEventListener('mouseout', function(e) {
    profileDetails.classList.remove("hovered");
  }); 
  profileDetails.addEventListener('mouseover', function(e) {
    profileDetails.classList.add("hovered");
  }); 
  profileDetails.addEventListener('mouseout', function(e) {
    profileDetails.classList.remove("hovered");
  }); 
}
    

});

