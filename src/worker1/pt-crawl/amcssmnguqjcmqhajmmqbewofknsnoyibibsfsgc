"/history/scottishhistory/images/media_museum/media_museum_floorplan_10_on.gif");        
   
var pic = new Array(imageUrls.length);

        for (i=0; i<imageUrls.length; i++) {
                pic[i] = new Image();
                pic[i].src = imageUrls[i];
        }
}



// Rolling over
function hiLite(imageName) {
        if (document.images) {
                top.document.images[imageName].src = "/history/scottishhistory/images/media_museum/" + imageName + "on.gif";
        }
}

// Rolling out
function backNormal(imageName) {
        if (document.images) {
                top.document.images[imageName].src = "/history/scottishhistory/images/media_museum/" + imageName + "off.gif";
        }
}


