                <script type="text/javascript">
    
    $('#showMoreComments').click(function () { 
       
       $.get("/admin/comments/RenderComments", { referenceID: "860171", referenceType: "2", page: 1, authorID: 0, disableDeleting : "False" },
       function(data){
          $(data).insertBefore("#showMore")
       });

       $(this).remove();
    });

    function LoadReplyBox(selectedComment){
            var replyTo = $(selectedComment).attr('replyTo');
            var name = $('#PostCommentForm #Name').val();
            var link = $('#PostCommentForm #Link').val();
            var redirectUrl = $('#PostCommentForm #RedirectUrl').val();
            var thumbUrl = $('#PostCommentForm #thumbUrl').val();
            var replyToLink = $(selectedComment);

            $.get("/admin/comments/LoadReplyBox", { ReplyTo : replyTo,  ReferenceTypeID: "860171", ReferenceType: "2", Name : name, Link : link, ThumbUrl : thumbUrl, RedirectUrl : redirectUrl },
        function(data){
            $(data).insertAfter($(replyToLink))
        });
    }

    function LoadParent(selectedComment){
        var id  = selectedComment.attr('itemID');
        var commentHolder = selectedComment.parents('.comment-wrapper');

         $.get("/admin/comments/GetReplyParent", { id : id,  authorID: "0"},
        function(data){
            selectedComment.remove();
            commentHolder.addClass('indent-comment-parent');
            $(data).insertBefore(commentHolder);
        });
    }
</script>

