                        noUndefined: {
                            attributes: {
                                mathcolor: "#039", //"red",
                                mathbackground: "white", //"#FFEEEE",
                                mathsize: "90%"
                            }
                        }
                    },
                    messageStyle: "none"
                });
            </script>
        <script type="text/javascript">
                $(document).ready(function() {
                    $("#MathInput").stop(true, true).keyup(function() {
                        $.ajax({
                            url: "/mwsq/",
                            type: "POST",
                            data: { query : $("#MathInput").val() },
                            dataType: "text"
                        })
                        .done(function(xml) {
                            $("#MathPreview").html(xml);
                            $(window).resize();
                        });
                    });
                    var press = jQuery.Event("keyup");
                    press.ctrlKey = false;
                    press.which = 40;
                    $("#MathInput").trigger(press);
                });
            </script>
