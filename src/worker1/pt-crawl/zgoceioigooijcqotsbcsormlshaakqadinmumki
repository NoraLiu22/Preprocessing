<title>Great Circle Distances</title>
<script>
function round(n) {
   if(n < 0.01) return 0
   if(n <= 10) mult = 100
   if((n > 10) && (n < 100))  mult = 10
   if(n >= 100) mult = 1
   return parseInt((n * mult) + 0.5) / mult
}
function calc() {
   rad = 0.01745566
   LoAdeg = document.a.aLongdeg.value
   LoAmin = document.a.aLongmin.value
   if((LoAdeg == "") || (LoAmin == "")) return
   LoAdeg = parseFloat(LoAdeg); LoAmin = parseFloat(LoAmin)
   LoA = LoAdeg + (LoAmin / 60)
   if(LoA > 180) return
   if(document.a.aLong[1].checked) LoA = -LoA

   LoBdeg = document.b.bLongdeg.value
   LoBmin = document.b.bLongmin.value
   if((LoBdeg == "") || (LoBmin == "")) return
   LoBdeg = parseFloat(LoBdeg); LoBmin = parseFloat(LoBmin)
   LoB = LoBdeg + (LoBmin / 60)
   if(LoB > 180) return
   if(document.b.bLong[1].checked) LoB = -LoB
   Lo = Math.abs(LoA - LoB); 

   LatAdeg = document.a.aLatdeg.value
   LatAmin = document.a.aLatmin.value
   if((LatAdeg == "") || (LatAmin == "")) return
   LatAdeg = parseFloat(LatAdeg); LatAmin = parseFloat(LatAmin)
   LatA = LatAdeg + (LatAmin /60)
   if(LatA > 90) return
   if(document.a.aLat[1].checked) LatA = -LatA

   LatBdeg = document.b.bLatdeg.value
   LatBmin = document.b.bLatmin.value
   if((LatBdeg == "") || (LatBmin == "")) return
   LatBdeg = parseFloat(LatBdeg); LatBmin = parseFloat(LatBmin)
   LatB = LatBdeg + (LatBmin /60)
   if(LatB > 90) return 
   if(document.b.bLat[1].checked) LatB = -LatB

   with(Math) {
      Drad = acos((sin(LatA*rad)*sin(LatB*rad))+
            (abs(cos(LatA*rad))*abs(cos(LatB*rad))*cos(Lo*rad)))
   }
   document.result.miles.value = " "+round(Drad * 57.2881 * 69.06)
   document.result.k.value = " "+round(Drad * 57.2881 * 111.2)
}
</script>
</head>
