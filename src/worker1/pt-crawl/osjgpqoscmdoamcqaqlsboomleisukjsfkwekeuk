        <!--
        function prepare_attr_jl(str) {
            return str.replace(/\ /g, "___");
        }
        function reaction_buttons_increment_button_ajax(post_id, button){
            var already_voted_text = 'Ya votaste en esta noticia';
            var only_one_vote = false;
            var show_after_votes = false;
            $feedback =  jQuery(".feedback");
                                    
            $single_vote_reaction_button = jQuery("#reaction_buttons_post" + post_id + " .reaction_button");
            $several_vote_reaction_button= jQuery("#reaction_buttons_post" + post_id + " .reaction_button_" + prepare_attr_jl(button) + "_count");
                               
            // remove the href attribute before sending the request to make
            // sure no one votes more than once by clicking ten times fast
            if(only_one_vote){
                // remove all the href's from the posts and replace it by the
                // alert not to vote twice if set
                if(already_voted_text){
                    $single_vote_reaction_button.attr('href', 'javascript:$feedback.text("'+already_voted_text+'").fadeIn(400).delay(5000).fadeOut(400);');
                } else{
                    $single_vote_reaction_button.removeAttr('href');
                }
            }else{
                // remove/replace only on the clicked button
                if(already_voted_text){
                    if ($several_vote_reaction_button.parent().hasClass('voted'))
                        $several_vote_reaction_button.parent().attr('href', 'javascript:$feedback.text("'+already_voted_text+'").fadeIn(400).delay(5000).fadeOut(400);');
                } else {
                    $several_vote_reaction_button.parent().removeAttr('href');
                }
            }                  
            jQuery.ajax({
                type: "post",
                url: "https://web.archive.org/web/20111202054932/http://www.lapatilla.com/site/wp-admin/admin-ajax.php",
                dataType: 'json',
                data: { action: 'reaction_buttons_increment_button_php', post_id: post_id, button: button, _ajax_nonce: '8bceac186a' },
                success: function(data){
                    $feedback.text("Su voto ha sido recibido").fadeIn(400).delay(5000).fadeOut(400);                        
                    $several_vote_reaction_button.parent().next('.count').html(""+data['count']+"");
                    if(only_one_vote){
                        $single_vote_reaction_button.addClass('voted'); 
                    } else{
                        $several_vote_reaction_button.parent().addClass('voted');
                    }
                    if(show_after_votes){
                        $single_vote_reaction_button.parent().next().removeAttr('style');
                    }
                },
                error: function(jqXHR, textStatus, errorThrown){
                    $feedback.text(textStatus+": Ha ocurrido un error mientras intentabamos registrar su voto. ").fadeIn(400).delay(5000).fadeOut(400);                    
                }
            });
        }
            
