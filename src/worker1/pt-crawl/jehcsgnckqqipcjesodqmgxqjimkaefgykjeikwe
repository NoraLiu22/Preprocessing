    <script>
        function ToggleSelected(selIdx) {
            var id;
            for (var x = 0; x <= 1; x++) {
                id = "#navList>li:eq(" + x + ")>a"
                $(id).removeClass("selected");
            }
            id = "#navList>li:eq(" + selIdx + ")>a";
            $(id).addClass("selected");

            return false;
        }
    </script>

