    <script>
        $(document).ready(function () {

            getForumResponsesNumber();

            function getForumResponsesNumber() {
                var request = $.ajax(
                    {
                        type: "GET",
                        url: "/getForumResponsesNumber",
                        data: {

                        },
                        success: function (response) {
                            var forumresponses = response.forumresponses
                            if (forumresponses > 0) {
                                $("#forumHeader").html('<a class="nav-link main" href="/forum">FORUM<sup>' + forumresponses + '</sup></a>');
                            }
                        }
                    }
                );
            }

        });
    </script>

    <!-- Google Analytics -->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-139349847-1', 'auto');
        ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->

    <script>
        $(".navbar-toggler-icon").click(function () {
            if ($(".ribbon-top-left").is(":visible")) {
                $(".ribbon-top-left").hide();
            } else {
                $(".ribbon-top-left").show();
            }
        });

        $('#mysearch').click(function () {
            $('#finder').submit();
        });
        $('#backsign').click(function () {
            $('#keywords').val('');
            $('#backsign').html('')
        });

        $('.basicAutoComplete').keyup(function () {
            if (($(".basicAutoComplete").val().length) > 0) {
                $('#backsign').html('<i class="fas fa-backspace"></i>')
            } else {
                $('#backsign').html('')
            }
        });

    </script>

    <!-- Script for sidebar interaction -->
    <script>
        $(document).ready(function () {
            // Open sidebar on menu button click
            $('.menu-button').on('click', function () {
                $('#sidebar-menu').addClass('active');
            });

            // Close sidebar on close button click
            $('#close-sidebar').on('click', function () {
                $('#sidebar-menu').removeClass('active');
            });

            // Close sidebar when clicking outside of it
            $(document).on('click', function (event) {
                if (!$(event.target).closest('#sidebar-menu, .menu-button').length) {
                    $('#sidebar-menu').removeClass('active');
                }
            });
        });
    </script>
    
<script src="/static/site/js/bootstrap-autocomplete.js"></script>
<script>
    $('.basicAutoComplete').autoComplete(
	{ minLength: 3 }
	);

	$('.basicAutoComplete').on('autocomplete.select', function (evt, item) {
		$('#searchOrganisations').submit();
	});

	$(function () {
		$('.custom-select').on('change', function () {
			$('#searchOrganisations').submit();
		});
        $('#pill-country').on('click', function() {
            $('#country').val('')
            $('#searchOrganisations').submit();
        });
        $('#pill-orgTypes').on('click', function() {
            $('#orgTypes').val('')
            $('#searchOrganisations').submit();
        });

	});
</script>


