<script type="text/javascript" src="/web/20141129153421js_/http://www.summit-americas.org/js/menu.js"></script>
<!-- include jQuery library -->
<script type="text/javascript" src="/web/20141129153421js_/http://www.summit-americas.org/js/jquery-1.4.1.min.js"></script>

<!-- include Cycle plugin -->
<script type="text/javascript" src="/web/20141129153421js_/http://www.summit-americas.org/js/jquery.cycle.all.min.js"></script>

<!--  initialize the slideshow when the DOM is ready -->
<script type="text/javascript">
$(document).ready(function() {
//    $('.slideshow').cycle({
//		fx: 'fade' // choose your transition type, ex: fade, scrollUp, shuffle, etc...
//	});
//slideshow functions	
// redefine Cycle's updateActivePagerLink function 
	$.fn.cycle.updateActivePagerLink = function(pager, currSlideIndex) { 
 	  $(pager).find('li').removeClass('activeLI') 
		.filter('li:eq('+currSlideIndex+')').addClass('activeLI'); 
	}; 
			if ( $('.slideshow').length > 0 ) {		
			$('.slideshow')
			.after('<ul class="pager-nav">') 
			.cycle({
			fx:'none',
			cleartype:1,
			//timeout:20000,
			timeout:2000,
			speed:'5000',
			pager:'.pager-nav',	
			pagerAnchorBuilder: function(idx, slide) { 
				var navlabel = $(slide).find("img").attr('alt');
				return '<li><a href="#">' + navlabel + '</a></li>';		
				}
			}
			);
			// hide this snippet of script if inserting a single video to remove play and pause buttons 
			$('.pager-nav').append("<li><a id='playbtn' class='pause'>&nbsp;</a></li>");
			var bplplay = true;
			$('#playbtn').click(function() {
			
				if (bplplay) {
			    	$('.slideshow').cycle('pause');
					$('#playbtn').removeClass('pause').addClass('play')
					bplplay = false;
				}
				else {
					$('.slideshow').cycle('resume');
					$('#playbtn').removeClass('play').addClass('pause')
					bplplay = true;
				}
			});
		}


});
</script>
