    <td width="940" align="left" valign="top" bgcolor="#eaeaea"> 
      <div align="left"> 
        <font color="#663399" face="Verdana,Arial,Helvetica,sans-serif" size="4"><b>Coaching 
        the High Jumper</b></font></div>
      <div align="left"> 
        <p align="center"><font size="2" face="Verdana, Arial, Helvetica, sans-serif"> 
          <script type="text/javascript"><!--
google_ad_client = "pub-3607693930582854";
google_ad_width = 728;
google_ad_height = 90;
google_ad_format = "728x90_as";
google_ad_type = "text_image";
google_ad_channel ="";
google_color_border = "FFFFFF";
google_color_bg = "EAEAEA";
