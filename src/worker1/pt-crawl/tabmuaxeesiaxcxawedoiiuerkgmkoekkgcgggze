e&quot;:&quot;Why replacing politicians with experts is a reckless idea&quot;,&quot;format&quot;:{&quot;display&quot;:1,&quot;theme&quot;:0,&quot;design&quot;:10},&quot;context&quot;:&quot;SubMeta&quot;}" config="{&quot;renderingTarget&quot;:&quot;Web&quot;,&quot;darkModeAvailable&quot;:false,&quot;assetOrigin&quot;:&quot;https://assets.guim.co.uk/&quot;}"><a href="mailto:?subject=Why replacing politicians with experts is a reckless idea&amp;body=https://www.theguardian.com/news/2018/may/01/why-replacing-politicians-with-experts-is-a-reckless-idea?CMP=share_btn_url" type="button" class="dcr-1l6r7m">Share<div class="src-button-space"></div><svg viewBox="-3 -3 30 30" xmlns="http://www.w3.org/2000/svg" aria-hidden="true"><path fill-rule="evenodd" clip-rule="evenodd" d="M19 16c1.675 0 3 1.35 3 3 0 1.675-1.325 3-3 3a2.972 2.972 0 0 1-2.975-3c0-.125 0-.275.025-.375L7.1 14.15a2.908 2.908 0 0 1-2.075.825C3.325 14.975 2 13.65 2 12a3.004 3.004 0 0 1 3.025-3.025c.775 0 1.5.35 2.075.875l8.95-4.475c-.025-.1-.025-.225-.025-.4A2.966 2.966 0 0 1 19 2c1.675 0 3 1.325 3 2.975C22 6.65 20.675 8 19 8c-.8 0-1.475-.325-2.05-.825l-8.975 4.45C8 11.7 8 11.825 8 12s0 .3-.025.375l8.975 4.45c.575-.5 1.25-.825 2.05-.825Zm0-12.8c-.975 0-1.8.775-1.8 1.775 0 1 .825 1.825 1.8 1.825 1 0 1.825-.825 1.825-1.825S20 3.2 19 3.2Zm0 17.6c1 0 1.825-.825 1.825-1.8 0-1-.825-1.8-1.825-1.8-.975 0-1.8.8-1.8 1.8 0 .975.825 1.8 1.8 1.8Z"></path></svg></a></gu-island><div class="dcr-ssfe7f"><a data-link-name="meta-syndication-article" href="https://syndication.theguardian.com/?url=https%3A%2F%2Fwww.theguardian.com%2Fnews%2F2018%2Fmay%2F01%2Fwhy-replacing-politicians-with-experts-is-a-reckless-idea&amp;type=article&amp;internalpagecode=news/2018/may/01/why-replacing-politicians-with-experts-is-a-reckless-idea" target="_blank" rel="noreferrer" title="Reuse this content" class="dcr-1ugckbi">Reuse this content</a></div></div></div></div></div><div data-gu-name="right-column" class="dcr-cneztv"><div class="dcr-jwg3xc"><section class="dcr-1hko8ef"><div class="dcr-1nzopul"><div class="ad-slot-container dcr-1jweyjf"><div id="dfp-ad--right" data-link-name="ad slot right" data-name="right" aria-hidden="true" class="js-ad-slot ad-slot ad-slot--right ad-slot--mpu-banner-ad ad-slot--rendered js-sticky-mpu"></div></div></div></section></div></div></div></div></article><aside class="dcr-1susrlh"><div class="dcr-u0uasy"><div class="ad-slot-container dcr-117mnry"><div id="dfp-ad--merchandising-high" data-link-name="ad slot merchandising-high" data-name="merchandising-high" data-refresh="false" aria-hidden="true" class="js-ad-slot ad-slot ad-slot--merchandising-high dcr-13rbi4"></div></div></div></aside><gu-island name="OnwardsUpper" priority="feature" deferUntil="visible" props="{&quot;ajaxUrl&quot;:&quot;https://api.nextgen.guardianapps.co.uk&quot;,&quot;hasRelated&quot;:true,&quot;hasStoryPackage&quot;:false,&quot;isAdFreeUser&quot;:false,&quot;pageId&quot;:&quot;news/2018/may/01/why-replacing-politicians-with-experts-is-a-reckless-idea&quot;,&quot;isPaidContent&quot;:false,&quot;showRelatedContent&quot;:true,&quot;keywordIds&quot;:&quot;politics/politics,uk/uk,society/society,politics/eu-referendum,world/eu,us-news/donaldtrump,books/plato&quot;,&quot;contentType&quot;:&quot;Article&quot;,&quot;tags&quot;:[{&quot;id&quot;:&quot;news/series/the-long-read&quot;,&quot;type&quot;:&quot;Series&quot;,&quot;title&quot;:&quot;The long read&quot;},{&quot;id&quot;:&quot;politics/politics&quot;,&quot;type&quot;:&quot;Keyword&quot;,&quot;title&quot;:&quot;Politics&quot;},{&quot;id&quot;:&quot;uk/uk&quot;,&quot;type&quot;:&quot;Keyword&quot;,&quot;title&quot;:&quot;UK news&quot;},{&quot;id&quot;:&quot;society/society&quot;,&quot;type&quot;:&quot;Keyword&quot;,&quot;title&quot;:&quot;Society&quot;},{&quot;id&quot;:&quot;politics/eu-referendum&quot;,&quot;type&quot;:&quot;Keyword&quot;,&quot;title&quot;:&quot;Brexit&quot;},{&quot;id&quot;:&quot;world/eu&quot;,&quot;type&quot;:&quot;Keyword&quot;,&quot;titl