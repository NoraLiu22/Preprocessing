// Preload images







if (document.images) {







var imageUrls = new  Array("/history/scottishhistory/images/home_page/history_home_history_on.gif","/history/scottishhistory/images/home_page/history_home_history_on.gif","/history/scottishhistory/images/home_page/history_home_media_on.gif","/history/scottishhistory/images/home_page/history_home_oddities_on.gif","/history/scottishhistory/images/home_page/history_home_web_guide_on.gif");







