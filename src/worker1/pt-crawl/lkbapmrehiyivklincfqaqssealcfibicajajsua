        // 滚动新闻
        function marquee() {
            var scowllWhit = $('.infoList').width()
            var tl = gsap.timeline({
                repeat: -1
            });
            tl.to('.infoList', {
                x: -scowllWhit / 2,
                duration: 280,
                ease: 'none'
            })
            $('.infoList').on('mouseenter', function () {
                tl.pause()
            })
            $('.infoList').on('mouseleave', function () {
                tl.play()
            })
        }

        var xpagegd = new XHW.Xpage('.bd .infoList', {
            mode: 'listView', // 插件模式
            nid: 115033, // 栏目id
            version: 2, // 新josn
            pageSize: 20, // 每页条数
            moreButton: '.xpage-more-btn', // 更多按钮
            renderItem: function (item, index, list) {
                var itemHtml = ['<li>',
                    '   <a href="{{LinkUrl}}" target="_blank">{{Title}}</a>',
                    '</li>'
                ].join("");
                return itemHtml;
            },
            onPageRender: function () {
                marquee();
            }


        });


