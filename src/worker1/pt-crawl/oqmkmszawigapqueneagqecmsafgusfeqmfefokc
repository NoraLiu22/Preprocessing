       <a href="morph?l=an&amp;la=la&amp;can=an0&amp;prior=machinaris" onclick="m(this,1,0); return false" class="text" target="morph">an</a> <a href="morph?l=vero&amp;la=la&amp;can=vero1&amp;prior=an" onclick="m(this,2,0); return false" class="text" target="morph">vero</a> <a href="morph?l=vir&amp;la=la&amp;can=vir0&amp;prior=vero" onclick="m(this,1,0); return false" class="text" target="morph">vir</a> <a href="morph?l=amplissimus&amp;la=la&amp;can=amplissimus0&amp;prior=vir" onclick="m(this,1,0); return false" class="text" target="morph">amplissimus</a>, <a href="morph?l=P&amp;la=la&amp;can=p0&amp;prior=amplissimus" onclick="m(this,1,0); return false" class="text" target="morph">P</a>.


<a href="morph?l=Scipio&amp;la=la&amp;can=scipio0&amp;prior=P" onclick="m(this,1,0); return false" class="text" target="morph">Scipio</a>, <a href="morph?l=pontifex&amp;la=la&amp;can=pontifex0&amp;prior=Scipio" onclick="m(this,1,0); return false" class="text" target="morph">pontifex</a> <a href="morph?l=maximus&amp;la=la&amp;can=maximus0&amp;prior=pontifex" onclick="m(this,1,0); return false" class="text" target="morph">maximus</a>, 
<a href="morph?l=Ti&amp;la=la&amp;can=ti0&amp;prior=maximus" onclick="m(this,1,0); return false" class="text" target="morph">Ti</a>. <a href="morph?l=Gracchum&amp;la=la&amp;can=gracchum0&amp;prior=Ti" onclick="m(this,1,0); return false" class="text" target="morph">Gracchum</a> <a href="morph?l=mediocriter&amp;la=la&amp;can=mediocriter0&amp;prior=Gracchum" onclick="m(this,1,0); return false" class="text" target="morph">mediocriter</a> <a href="morph?l=labefactantem&amp;la=la&amp;can=labefactantem0&amp;prior=mediocriter" onclick="m(this,1,0); return false" class="text" target="morph">labefactantem</a> <a href="morph?l=statum&amp;la=la&amp;can=statum0&amp;prior=labefactantem" onclick="m(this,1,0); return false" class="text" target="morph">statum</a> <a href="morph?l=rei&amp;la=la&amp;can=rei1&amp;prior=statum" onclick="m(this,2,0); return false" class="text" target="morph">rei</a> <a href="morph?l=publicae&amp;la=la&amp;can=publicae1&amp;prior=rei" onclick="m(this,2,0); return false" class="text" target="morph">publicae</a>  
<a href="morph?l=privatus&amp;la=la&amp;can=privatus0&amp;prior=publicae" onclick="m(this,1,0); return false" class="text" target="morph">privatus</a> <a href="morph?l=interfecit&amp;la=la&amp;can=interfecit0&amp;prior=privatus" onclick="m(this,1,0); return false" class="text" target="morph">interfecit</a>: <a href="morph?l=Catilinam&amp;la=la&amp;can=catilinam0&amp;prior=interfecit" onclick="m(this,1,0); return false" class="text" target="morph">Catilinam</a><a id="note-link7" href="#note7"><sup>7</sup></a> <a href="morph?l=orbem&amp;la=la&amp;can=orbem0&amp;prior=Catilinam" onclick="m(this,1,0); return false" class="text" target="morph">orbem</a> <a href="morph?l=terrae&amp;la=la&amp;can=terrae0&amp;prior=orbem" onclick="m(this,1,0); return false" class="text" target="morph">terrae</a> <a href="morph?l=caede&amp;la=la&amp;can=caede0&amp;prior=terrae" onclick="m(this,1,0); return false" class="text" target="morph">caede</a> <a href="morph?l=atque&amp;la=la&amp;can=atque0&amp;prior=caede" onclick="m(this,1,0); return false" class="text" target="morph">atque</a> 
<a href="morph?l=incendiis&amp;la=la&amp;can=incendiis0&amp;prior=atque" onclick="m(this,1,0); return false" class="text" target="morph">incendiis</a> <a href="morph?l=vastare&amp;la=la&amp;can=vastare0&amp;prior=incendiis" onclick="m(this,1,0); return false" class="text" target="morph">vastare</a> <a href="morph?l=cupientem&amp;la=la&amp;can=cupientem0&amp;prior=vastare" onclick="m(this,1,0); return false" class="text" target="morph">cupientem</a> <a href="morph?l=nos&amp;la=la&amp;can=nos3&amp;prior=cupientem" onclick="m(this,4,0); return false" class="text" target="morph">nos</a> <a href="morph?l=consules&amp;la=la&amp;can=consules0&amp;prior=nos" onclick="m(this,1,0); return false" class="text" target="morph">consules</a> <a href="morph?l=perferemus&amp;la=la&amp;can=perferemus0&amp;prior=consules" onclick="m(this,1,0); return false" class="text" target="morph">perferemus</a><a id="note-link8" href="#note8"><sup>8</sup></a>? <a href="morph?l=nam&amp;la=la&amp;can=nam0&amp;prior=perferemus" onclick="m(this,1,0); return false" class="text" target="morph">nam</a> 
<a href="morph?l=illa&amp;la=la&amp;can=illa0&amp;prior=nam" onclick="m(this,1,0); return false" class="text" target="morph">illa</a> <a href="morph?l=nimis&amp;la=la&amp;can=nimis0&amp;prior=illa" onclick="m(this,1,0); return false" class="text" target="morph">nimis</a> <a href="morph?l=antiqua&amp;la=la&amp;can=antiqua0&amp;prior=nimis" onclick="m(this,1,0); return false" class="text" target="morph">antiqua</a> <a href="morph?l=praetereo&amp;la=la&amp;can=praetereo0&amp;prior=antiqua" onclick="m(this,1,0); return false" class="text" target="morph">praetereo</a>, <a href="morph?l=quod&amp;la=la&amp;can=quod0&amp;prior=praetereo" onclick="m(this,1,0); return false" class="text" target="morph">quod</a><a id="note-link9" href="#note9"><sup>9</sup></a> <a href="morph?l=C&amp;la=la&amp;can=c0&amp;prior=quod" onclick="m(this,1,0); return false" class="text" target="morph">C</a>. <a href="morph?l=Servilius&amp;la=la&amp;can=servilius0&amp;prior=C" onclick="m(this,1,0); return false" class="text" target="morph">Servilius</a> <a href="morph?l=Ahala&amp;la=la&amp;can=ahala0&amp;prior=Servilius" onclick="m(this,1,0); return false" class="text" target="morph">Ahala</a> <a href="morph?l=Sp&amp;la=la&amp;can=sp0&amp;prior=Ahala" onclick="m(this,1,0); return false" class="text" target="morph">Sp</a>. 
<a href="morph?l=Maelium&amp;la=la&amp;can=maelium0&amp;prior=Sp" onclick="m(this,1,0); return false" class="text" target="morph">Maelium</a> <a href="morph?l=novis&amp;la=la&amp;can=novis0&amp;prior=Maelium" onclick="m(this,1,0); return false" class="text" target="morph">novis</a> <a href="morph?l=rebus&amp;la=la&amp;can=rebus0&amp;prior=novis" onclick="m(this,1,0); return false" class="text" target="morph">rebus</a> <a href="morph?l=studentem&amp;la=la&amp;can=studentem0&amp;prior=rebus" onclick="m(this,1,0); return false" class="text" target="morph">studentem</a> <a href="morph?l=manu&amp;la=la&amp;can=manu0&amp;prior=studentem" onclick="m(this,1,0); return false" class="text" target="morph">manu</a> <a href="morph?l=sua&amp;la=la&amp;can=sua0&amp;prior=manu" onclick="m(this,1,0); return false" class="text" target="morph">sua</a> <a href="morph?l=occidit&amp;la=la&amp;can=occidit0&amp;prior=sua" onclick="m(this,1,0); return false" class="text" target="morph">occidit</a>. <a href="morph?l=fuit&amp;la=la&amp;can=fuit0&amp;prior=occidit" onclick="m(this,1,0); return false" class="text" target="morph">fuit</a>, 
<a href="morph?l=fuit&amp;la=la&amp;can=fuit1&amp;prior=fuit" onclick="m(this,2,0); return false" class="text" target="morph">fuit</a> <a href="morph?l=ista&amp;la=la&amp;can=ista0&amp;prior=fuit" onclick="m(this,1,0); return false" class="text" target="morph">ista</a> <a href="morph?l=quondam&amp;la=la&amp;can=quondam0&amp;prior=ista" onclick="m(this,1,0); return false" class="text" target="morph">quondam</a> <a href="morph?l=in&amp;la=la&amp;can=in3&amp;prior=quondam" onclick="m(this,4,0); return false" class="text" target="morph">in</a> <a href="morph?l=hac&amp;la=la&amp;can=hac0&amp;prior=in" onclick="m(this,1,0); return false" class="text" target="morph">hac</a> <a href="morph?l=re&amp;la=la&amp;can=re0&amp;prior=hac" onclick="m(this,1,0); return false" class="text" target="morph">re</a> <a href="morph?l=publica&amp;la=la&amp;can=publica0&amp;prior=re" onclick="m(this,1,0); return false" class="text" target="morph">publica</a> <a href="morph?l=virtus&amp;la=la&amp;can=virtus0&amp;prior=publica" onclick="m(this,1,0); return false" class="text" target="morph">virtus</a> <a href="morph?l=ut&amp;la=la&amp;can=ut0&amp;prior=virtus" onclick="m(this,1,0); return false" class="text" target="morph">ut</a> <a href="morph?l=viri&amp;la=la&amp;can=viri1&amp;prior=ut" onclick="m(this,2,0); return false" class="text" target="morph">viri</a> <a href="morph?l=fortes&amp;la=la&amp;can=fortes1&amp;prior=viri" onclick="m(this,2,0); return false" class="text" target="morph">fortes</a> 
<a href="morph?l=acrioribus&amp;la=la&amp;can=acrioribus0&amp;prior=fortes" onclick="m(this,1,0); return false" class="text" target="morph">acrioribus</a> <a href="morph?l=suppliciis&amp;la=la&amp;can=suppliciis0&amp;prior=acrioribus" onclick="m(this,1,0); return false" class="text" target="morph">suppliciis</a> <a href="morph?l=civem&amp;la=la&amp;can=civem0&amp;prior=suppliciis" onclick="m(this,1,0); return false" class="text" target="morph">civem</a> <a href="morph?l=perniciosum&amp;la=la&amp;can=perniciosum0&amp;prior=civem" onclick="m(this,1,0); return false" class="text" target="morph">perniciosum</a> <a href="morph?l=quam&amp;la=la&amp;can=quam2&amp;prior=perniciosum" onclick="m(this,3,0); return false" class="text" target="morph">quam</a> <a href="morph?l=acerbissimum&amp;la=la&amp;can=acerbissimum0&amp;prior=quam" onclick="m(this,1,0); return false" class="text" target="morph">acerbissimum</a> 
<a href="morph?l=hostem&amp;la=la&amp;can=hostem0&amp;prior=acerbissimum" onclick="m(this,1,0); return false" class="text" target="morph">hostem</a> <a href="morph?l=coercerent&amp;la=la&amp;can=coercerent0&amp;prior=hostem" onclick="m(this,1,0); return false" class="text" target="morph">coercerent</a>. <a href="morph?l=habemus&amp;la=la&amp;can=habemus0&amp;prior=coercerent" onclick="m(this,1,0); return false" class="text" target="morph">habemus</a><a id="note-link10" href="#note10"><sup>10</sup></a> <a href="morph?l=senatus&amp;la=la&amp;can=senatus2&amp;prior=habemus" onclick="m(this,3,0); return false" class="text" target="morph">senatus</a> <a href="morph?l=consultum&amp;la=la&amp;can=consultum0&amp;prior=senatus" onclick="m(this,1,0); return false" class="text" target="morph">consultum</a> <a href="morph?l=in&amp;la=la&amp;can=in4&amp;prior=consultum" onclick="m(this,5,0); return false" class="text" target="morph">in</a> <a href="morph?l=te&amp;la=la&amp;can=te3&amp;prior=in" onclick="m(this,4,0); return false" class="text" target="morph">te</a>, 
<a href="morph?l=Catilina&amp;la=la&amp;can=catilina2&amp;prior=te" onclick="m(this,3,0); return false" class="text" target="morph">Catilina</a>, <a href="morph?l=vehemens&amp;la=la&amp;can=vehemens0&amp;prior=Catilina" onclick="m(this,1,0); return false" class="text" target="morph">vehemens</a> <a href="morph?l=et&amp;la=la&amp;can=et1&amp;prior=vehemens" onclick="m(this,2,0); return false" class="text" target="morph">et</a> <a href="morph?l=grave&amp;la=la&amp;can=grave0&amp;prior=et" onclick="m(this,1,0); return false" class="text" target="morph">grave</a><a id="note-link11" href="#note11"><sup>11</sup></a>, <a href="morph?l=non&amp;la=la&amp;can=non2&amp;prior=grave" onclick="m(this,3,0); return false" class="text" target="morph">non</a> <a href="morph?l=deest&amp;la=la&amp;can=deest0&amp;prior=non" onclick="m(this,1,0); return false" class="text" target="morph">deest</a> <a href="morph?l=rei&amp;la=la&amp;can=rei2&amp;prior=deest" onclick="m(this,3,0); return false" class="text" target="morph">rei</a> <a href="morph?l=publicae&amp;la=la&amp;can=publicae2&amp;prior=rei" onclick="m(this,3,0); return false" class="text" target="morph">publicae</a> <a href="morph?l=consilium&amp;la=la&amp;can=consilium0&amp;prior=publicae" onclick="m(this,1,0); return false" class="text" target="morph">consilium</a> 
