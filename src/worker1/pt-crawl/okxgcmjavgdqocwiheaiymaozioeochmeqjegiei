$(function(){
    $('#scrl').fadeTo(0,0.6);
    $('#scrr').fadeTo(0,0.6);
    
    $("#number p:first").addClass("cur")
            var b1s = $('#box1a li').size();
            var b1w = $('#box1a li').width();
            $('#box1a ul').append($("#box1a li:first").clone())
            b1s = b1s + 1
            $('#box1a ul').css('width',b1s * b1w);
            var ncur = 0;
            var wid = null;
            
            $("#number p").on("mouseover",function(){
                    ncur = $(this).index()
                    $(this).addClass("cur")
                    $(this).siblings().removeClass("cur")
                    $('#box1a ul').stop(true,true).animate({'margin-left':-b1w*ncur}) 
                })
            
            $('#scrl').on('click', function() {
                            ncur = (ncur - 2 + b1s) % (b1s - 1);
                            $('#box1a ul').stop(true,true).animate({'margin-left':-b1w*ncur}) 
                            $("#number p:eq(" + ncur + ")").addClass("cur")
                            $("#number p:eq(" + ncur + ")").siblings().removeClass("cur")
                            })
                            
            $('#scrr').on('click', function() {
                            ncur = (ncur + b1s) % (b1s - 1);
                            $('#box1a ul').stop(true,true).animate({'margin-left':-b1w*ncur}) 
                            $("#number p:eq(" + ncur + ")").addClass("cur")
                            $("#number p:eq(" + ncur + ")").siblings().removeClass("cur")
                            })
            
            $('#scrl').on('mouseenter',function(){
                        window.clearInterval(wid);
                    }).on('mouseleave',function(){
                        wid = window.setInterval(autoPlay,5000);
                    })
                    $('#scrr').on('mouseenter',function(){
                        window.clearInterval(wid);
                    }).on('mouseleave',function(){
                        wid = window.setInterval(autoPlay,5000);
                    })
                    $('#number p').on('mouseenter',function(){
                        window.clearInterval(wid);
                    }).on('mouseleave',function(){
                        wid = window.setInterval(autoPlay,5000);
                    })
            
            function autoPlay(){
                        ncur++
                        if(ncur<b1s-1) 
                        {
                            $("#number p:eq(" + ncur + ")").addClass("cur")
                            $("#number p:eq(" + ncur + ")").siblings().removeClass("cur")
                            $('#box1a ul').stop(true,true).animate({'margin-left':-b1w*ncur}) 

                        }
                        else
                        {
                            $("#number p:eq(0)").addClass("cur")
                            $("#number p:eq(0)").siblings().removeClass("cur")
                            $('#box1a ul').stop(true,true).animate({'margin-left':-b1w*ncur}).animate({'margin-left':0},0)
                            ncur = 0
                        }
                    }
            wid = window.setInterval(autoPlay,4000);
});

$('.photos li').each(function(){
                var php = $(this).find('p');
                var phimg = $(this).find('img');
                var phpH = php.innerHeight();
                php.css('bottom', -phpH);
                $(this).on('mouseenter',function(){
                    php.stop(true,true).animate({bottom:0},300);
                    phimg.fadeTo(0,0.3);
                }).on('mouseleave',function(){
                    php.animate({bottom:-phpH},200);
                    phimg.fadeTo(0,1);
                })
            })
-->
</script>

