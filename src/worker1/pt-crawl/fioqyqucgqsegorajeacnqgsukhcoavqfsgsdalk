  <!-- <script src="/img/MAIN/2020/07/120320/js/jquery-1.11.1.min.js" type="text/javascript" charset="UTF-8"></script> -->
  <script src="/img/MAIN/2020/07/120320/js/swiper.4.5.0.min.js" type="text/javascript" charset="UTF-8"></script>
  <script src="/img/MAIN/2020/07/120320/js/main.js" type="text/javascript" charset="UTF-8"></script>
  <!-- 框架script文件 -->
  <script src="/img/MAIN/2020/07/120320/js/owo.core.b2n9k20v8fd.js" type="text/javascript" charset="UTF-8"></script>
  <script type="text/javascript" charset="UTF-8">
    // 模块 page 的script代码
    function randomNum(minNum, maxNum) {
      switch (arguments.length) {
        case 1:
          return parseInt(Math.random() * minNum + 1, 10);
          break;

        case 2:
          return parseInt(Math.random() * (maxNum - minNum + 1) + minNum, 10);
          break;

        default:
          return 0;
          break;
      }
    }

    owo.script = {
      "page": {
        "data": {
          "swiper": null
        },
        "created": function created() {
          var editList = this.queryAll('.tab-bar .edit'); // 随机一个项目

          console.log(this);
          var randNum = randomNum(0, editList.length - 1); // 高亮条目
          document.querySelector('.more-button').href = this.queryAll('.tab-bar .more-link')[randNum].href
          this.queryAll('.tab-bar .item')[randNum].classList.add('active');
          this.query('.show-box ul').innerHTML = editList[randNum].innerHTML;
        },
        "changeSwiper": function changeSwiper() {
          var itemList = this.queryAll('.tab-bar .item');

          for (var index = 0; index < itemList.length; index++) {
            var element = itemList[index];
            element.classList.remove('active');
          }

          if (this.$target.querySelector('.more-link')) {
            document.querySelector('.more-button').href = this.$target.querySelector('.more-link').href
          }

          console.log(this.$target);
          this.$target.classList.add('active');
          this.query('.show-box ul').innerHTML = this.$target.querySelector('.edit').innerHTML;
        },
        "template": {
          "bigImage": {
            "created": function created() {
              new Swiper(this.query('.swiper-container'), {
                pagination: {
                  el: this.query('.pagination')
                },
                loop: true,
                autoplay: true,
                paginationClickable: true
              });
            }
          },
          "imageShow": {
            "data": {
              "swiper": null
            },
            "created": function created() {
              this.data.swiper = new Swiper(this.query('.swiper-container'), {
                loop: true,
                autoplay: false,
                slidesPerView: 5,
                paginationClickable: true,
                on: {
                  transitionStart: function transitionStart() {
                    for (var index = 0; index < this.slides.length; index++) {
                      var element = this.slides[index];
                      element.classList.remove('active');
                    }

                    this.slides[this.activeIndex + 2].classList.add('active');
                  },
                  init: function init() {
                    // this.slides[7].style.display = 'none'
                    // alert()
                    this.slides[7].classList.add('active');
                  }
                }
              });
            },
            "next": function next() {
              this.data.swiper.slideNext();
            },
            "prev": function prev() {
              this.data.swiper.slidePrev();
            }
          }
        }
      }
    };

setTimeout(function() {
  if (_owo.isMobi) {
    $('.image-show .swiper-container-box')[0].style.right = 'auto'
    $('.image-show .swiper-container-box')[0].style.left = -1400 + (document.body.offsetWidth / 2) + 'px'
  }
}, 500);



  </script>
<script src="http://tools.people.com.cn/css/2010tianrun/webdig_test.js" language="javascript" type="text/javascript" async></script>

