<script>
    /* 搜索处理 */
    - function () {
      var $searchLink = $('#search-link')
      $('#keyword').on('input change', function () {
        if (this.value) {
          $searchLink.attr('href', 'http://search.people.cn/getNewsResult/?channel=pic&keyword=$$$'.replace('$$$', encodeURIComponent(this.value)))
        } else {
          $searchLink.attr('href', 'http://search.people.cn/getNewsResult/?channel=pic&keyword=')
        }
      })

      $('#keyword').on('keypress', function (e) {
        if (e.keyCode === 13 && this.value) {
          window.open('http://search.people.cn/getNewsResult/?channel=pic&keyword=$$$'.replace('$$$', encodeURIComponent(this.value)))
        }
      })

    }();
</script>

