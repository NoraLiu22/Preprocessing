        function paly(vurl) {
            document.getElementById("mask").style.display = "block"
            document.getElementById("close").style.display = "block"
            document.getElementById("mask").innerHTML = '<iframe width=762 height=430 frameborder=0 scrolling=no border=0 src="' + vurl + '"></iframe>'
        }
        function closeiframe() {
            document.getElementById("mask").style.display = "none"
            document.getElementById("close").style.display = "none"
            document.getElementById("mask").innerHTML = ''
        }
        function checkvideo() {
            thd = $(".v1").length
            for (i = 0; i < thd; i++) {
                curr_thd = $(".v1").eq(i).html()
                if (curr_thd.length > 0) {
                    if (curr_thd.length < 6) {//小于6个字符，显示小引题，大于则显示iframe
                        $(".v2").eq(i).html('<em>' + curr_thd + '</em>')
                    }
                    else {
                        $(".v2").eq(i).html('<a target="_self" href="javascript:void(0);" onClick=paly("' + curr_thd + '")><i></i></a>')
                    }
                }
            }
        }
        checkvideo()//检查V1
