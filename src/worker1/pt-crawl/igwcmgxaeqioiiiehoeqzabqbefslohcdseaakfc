<script>
	
	function cerrartell(elem){					
		elem.parentElement.parentElement.parentElement.className = elem.parentElement.parentElement.parentElement.className +  " cerrar";
		setTimeout(function(){ 
			elem.parentElement.parentElement.parentElement.style.display="none";
			document.getElementById("btnrecomendar").className="btn_tell";			
		}, 1500);
		
	}
	
	
	jQuery(document).ready(function($){
		
		$("#btnrecomendar").removeClass("noshow");
		
		
		$("#btnrecomendar .face").click(function(){	
			window.open("https://www.facebook.com/ArtDailyNewspaper/","_blank");
		});
		
		$("#btnrecomendar .twitter").click(function(){	
			window.open("https://twitter.com/artdaily","_blank");
		});
		
		
		
		$("#btnrecomendar .mail").click(function(){	
			$("#frmtellafriend .formulario").trigger("reset");
			$("#frmtellafriend").removeClass("noshow");
			
			$("#msgtellafriend").addClass("noshow");
			$("#msgtellafriendok").addClass("noshow");
			
			$("#btnrecomendar").addClass("noshow");
			$("#tellafriend").removeClass("cerrar");
			$("#tellafriend").css("display","");
			$("#tellafriend").addClass("active");
		}); 
		
		$("#fomulariotell").submit(function(event){
			event.preventDefault(); //prevent default form submit
			$("#frmtellafriend").addClass("noshow");
			$("#msgtellafriend").removeClass("noshow");
			
			var datos=$("#frmtellafriend .formulario").serialize();
			
			 $.ajax({
                type: "GET",
                url: "/Mail/Correo.aspx?" + datos,
				//url: "/Mail/Correo.aspx?correo=info@artdaily.org&nombre=manuel",
                //data: JSON.stringify([{correo:"info@artdaily.org"},{nombre:"angel"}]),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function(response){
					try{
						if (response=="1"){
							$("#msgtellafriend").addClass("noshow");
							$("#msgtellafriendok").removeClass("noshow");
							$("#tellafriend").addClass("cerrar");
							setTimeout(function(){ 
								$("#tellafriend").css("display","none");
								document.getElementById("btnrecomendar").className="btn_tell";			
							}, 5000);
						}
					}catch(e){
						
					}
				},
                failure: function(response) {
                    alert(response.d);
                }
            });
			return false;
		});
		
	});
	
	
	
</script>
<script type="text/javascript">
	function cerrarpopup(elem){					
		elem.parentElement.parentElement.className = elem.parentElement.parentElement.className +  " cerrar";
		setTimeout(function(){ 
			elem.parentElement.parentElement.style.display="none";
			document.getElementById("btnrecomendar").className="btn_tell";			
		}, 1500);
		
	}
	
	
	 

		if (window.addEventListener) {
		  window.addEventListener('load', function(){
				var d = document.getElementById("popup");
				d.className = d.className + " active";
			});

		} else if (window.attachEvent) {
		  window.attachEvent('onload', function(){
				var d = document.getElementById("popup");
				d.className = d.className + " active";
			});			
		}
		/*window.addEventListener("load", function(){
				var d = document.getElementById("popup");
			d.className = d.className + " active";
		});	*/	
		
		
			
			
	
			
			
</script>

