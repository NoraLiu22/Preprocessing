<script>
    console.log("NAVIGA INIT Apr 09 2024 Update v5");

    let loginButton = $(".MG2login"); 
    let logoutButton = $(".MG2logout"); 
    let activationButton = $(".MG2activation"); 
    let myAccountButton = $(".MG2MyAccount"); 
    let subscribeButton = $(".MG2Subscribe");
    let nameContainer = $("#nav-user-firstName");

//Generate GUID for session id. Used for troubleshooting
let sessionId = (function () {
  let generatePart = function () {
    return Math.floor(65536 * (1 + Math.random()))
      .toString(16)
      .substring(1);
  };
  return (
    generatePart() +
    generatePart() +
    generatePart() +
    generatePart() +
    generatePart() +
    generatePart() +
    generatePart() +
    generatePart()
  );
})();

//Function wrapping plugin initalization. Accepts plugin name(string) and initialization settings object. Returns promise
let initializePlugin = function (pluginName, initSettings) {
  try {
    let plugin = window[pluginName];
    if (!plugin) {
      throw new Error(pluginName + " object not found");
    }
    let initResult = plugin.init(initSettings);
    return initResult && initResult.then ? initResult : Promise.resolve(true);
  } catch (e) {
    return Promise.reject(e.message);
  }
};

//Initialization of the plugins in the right order. Init settings from original init script splitted between plugins
initializePlugin("Fingerprint", {
  version: "nola",
  siteCode: "TA",
  environment: "prod", //or "stage"
  resourceUrl: "https://nola.com", //if plugin is loaded from "https://websitedomain.com/static/plugin.min.js" value should be https://websitedomain.com"
  sessionId: sessionId,
})
  .then(function () {
    return initializePlugin("G2Insights", {
      environment: "prod", //or "stage"
      version: "nola",
      collectors: ["connext"],
      tagManager: "GTM",
      containerId: "GTM-W6K8XP3",
      layoutCode: "TA",
      siteCode: "TA",
      resourceUrl: "https://nola.com", //if plugin is loaded from "https://websitedomain.com/static/plugin.min.js" value should be https://websitedomain.com"
      sessionId: sessionId,
    });
  })
  .then(function () {
    return initializePlugin("Connext", {
      clientCode: "nola",
      environment: "prod", //or "stage"
      siteCode: "TA",
      configCode: "ta_Default",
      debug: false,
      silentmode: false,
      resourceUrl: "https://nola.com", //if plugin is loaded from "https://websitedomain.com/static/plugin.min.js" value should be https://websitedomain.com"
      sessionId: sessionId,
      publicEventHandlers: {
        //public event handlers

        onNotAuthorized: function (eventData) {
      
        
          $(document).ready(function () {
          
            /** Multi-newsletter signup page */
            
            if (window.location.href.indexOf("newsletters") > -1) {
              // Reset email input field
              const emailInput = document.getElementById("newsletters_emailAddress");
              if ( typeof emailInput != "undefined" && emailInput != null && emailInput ) {
                document.getElementById("newsletters_emailAddress").value = "";
              }
              
              // Personalized newsletter preferences (show unsubscribe buttons)
              const personalizedSettings = new PersonalizedSettingsRenderer();
              personalizedSettings.removeUnsubscribeButtons();
              personalizedSettings.disableEeditionSignUp();
              personalizedSettings.loggedOut_specialText();
              
            }
            /** END Multi-newsletter signup page */
            
            
            $(".nav-account-container").css({ visibility: "hidden" });
            $(".open-setting-container").css({ visibility: "hidden" });
            $(".nav-signIn-container").css({ visibility: "visible" });

            $(".MG2login").show();
            $(".MG2Subscribe").show();
            $(".MG2activation").hide();
            $(".MG2MyAccount").hide();
            $(".MG2logout").hide();
                      /** Marfeel User Journey Tracking - Added by Nina Tran 7/24/23 */
          window.marfeel.cmd.push(['compass', function(compass) {
	    compass.setUserType(0);
          }]);
          /** END Marfeel User Journey Tracking */
          });

        },
        onAuthorized: function (eventData) {
          $(document).ready(function () {
            $(".MG2login").hide();
            $(".MG2Subscribe").show();
            $(".MG2activation").show();
            $(".MG2MyAccount").hide();
            $(".MG2logout").show();
            
              /** Multi-newsletter sign-up page */
              console.log("ADDING E-EDITION SIGN-UP", newsletterCategory);
              newsletterCategory.push({EeditionNotice:"news"});
              /** END Multi-newsetter sign-up page */
            
              /** Marfeel User Journey Tracking - Added by Nina Tran 7/24/23 */
              let userEmail = eventData.EventData.MG2AccountData.Email;
              window.marfeel.cmd.push(['compass', function(compass) {
                compass.setUserType(2);
              }]);
              window.marfeel.cmd.push(['compass', function(compass) {
                compass.setSiteUserId(userEmail);
              }]);
              /** END Marfeel User Journey Tracking */
              
          });

        },
        onHasNoActiveSubscription: function (eventData) {
          $(document).ready(function () {
            $(".MG2login").hide();
            $(".MG2Subscribe").show();
            $(".MG2activation").show();
            $(".MG2MyAccount").hide();
            $(".MG2logout").show();
            /** Marfeel User Journey Tracking - Added by Nina Tran 7/24/23 */
          let userEmail = eventData.EventData.MG2AccountData.Email;
          window.marfeel.cmd.push(['compass', function(compass) {
	    compass.setUserType(2);
          }]);
          window.marfeel.cmd.push(['compass', function(compass) {
                compass.setSiteUserId(userEmail);
              }]);
          /** END Marfeel User Journey Tracking */
          });    
        },
        onHasAccessNotEntitled: function (eventData) {
          $(document).ready(function () {
            $(".MG2login").hide();
            $(".MG2Subscribe").show();
            $(".MG2activation").show();
            $(".MG2MyAccount").hide();
            $(".MG2logout").show();
            
            /** Marfeel User Journey Tracking - Added by Nina Tran 7/24/23 */
          let userEmail = eventData.EventData.MG2AccountData.Email;
          window.marfeel.cmd.push(['compass', function(compass) {
	    compass.setUserType(3);
          }]);
          window.marfeel.cmd.push(['compass', function(compass) {
                compass.setSiteUserId(userEmail);
              }]);
          /** END Marfeel User Journey Tracking */
          });
          
        },
        onHasAccess: function (eventData) {
          $(document).ready(function () {
            let userEmail = eventData.EventData.MG2AccountData.Email;
            console.log("ON HAS ACCESS");
            
            /** Multi-newsletter signup page */
            
            // Autofill email input field
            if (window.location.href.indexOf("newsletters") > -1) {
              const emailInput = document.getElementById("newsletters_emailAddress");
              if ( typeof emailInput != "undefined" && emailInput != null && emailInput ) {
                document.getElementById("newsletters_emailAddress").value = userEmail;
              }
              
              // Personalized newsletter preferences (Show unsubscribe buttons)
              const personalizedSettings = new PersonalizedSettingsRenderer();
              personalizedSettings.enableEeditionSignUp();
              personalizedSettings.createUnsubscribeButtons();
              personalizedSettings.loggedIn_emailInputListener();
            }
            
            /** END Multi-newsletter signup page */
            
            let firstName =
              eventData.EventData.MG2AccountData.DigitalAccess.Subscription
                .FirstName;
            let formattedName = `Hi, ${firstName[0]}${firstName
              .slice(1)
              .toLowerCase()}!`;

            $("#nav-user-firstName").html(formattedName);

            $(".nav-signIn-container").css({ visibility: "hidden" });
            $(".nav-account-container").css({ visibility: "visible" });
            $(".open-setting-container").css({ visibility: "visible" });

            $(".MG2login").hide();
            $(".MG2Subscribe").hide();
            $(".MG2activation").hide();
            $(".MG2MyAccount").show();
            $(".MG2logout").show();
            
            /** Marfeel User Journey Tracking - Added by Nina Tran 7/24/23 */
          window.marfeel.cmd.push(['compass', function(compass) {
            compass.setUserType(3);
          }]);
          window.marfeel.cmd.push(['compass', function(compass) {
                compass.setSiteUserId(userEmail);
              }]);
          /** END Marfeel User Journey Tracking */
          });
          
        },
      },
      //other settings if needed
    });
  })
  .catch(function (err) {
  
    console.error(err);
  });
  

</script>   
 
    <!-- end of scripts Naviga Subscribe -->

    <!---- Remove scroll@ cookie  -->
      <script>
        function getAndDeleteCookies(cookieWord) {
           //const cDecoded = decodeURIComponent(document.cookie); //to be careful
           const cDecoded = document.cookie;
           const cookiesArr = cDecoded.split('; ');
           const foundCookies = cookiesArr.filter((cookie) => cookie.includes(cookieWord)).map((word) => word.split("=")[0])
             return foundCookies.forEach((cookieName) =>  {
              let pathUrl = cookieName.split("/")[1];
           // let cookieNameClean = cookieName.replace(/\//g, "%2F");
            let cookieNameClean = cookieName
         document.cookie = `${cookieNameClean}=;path=/${pathUrl};expires=${new Date(0).toUTCString()}`
             document.cookie = `${cookieNameClean}=;path=/;expires=${new Date(0).toUTCString()}`
        }) 
       }
       
       getAndDeleteCookies("scroll@");
        </script>
    <!---- End of Remove scroll@ cookie  -->    
  

