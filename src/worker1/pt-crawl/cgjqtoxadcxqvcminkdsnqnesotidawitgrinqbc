                gptAdSlots['fixed-breakout-desktop-3'] = googletag.defineSlot(adUnit, [[320,50],[300,50],[300,250],[2,1],[468,60],[728,90],[970,90],[970,250]], 'fixed-breakout-desktop-3').defineSizeMapping(mapping).setCollapseEmptyDiv(true).addService(googletag.pubads());
