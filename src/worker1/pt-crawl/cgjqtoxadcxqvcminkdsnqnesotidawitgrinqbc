    <div class="asset-breakout-ads visible-md visible-lg overridden">
        <div class="tnt-ads-container text-center  " >
        <div id="fixed-breakout-desktop-3" class="tnt-ads dfp-ad" data-lazy="true" data-refresh="true" >
            <script>
                googletag.cmd.push(function() {
                        googletag.display('fixed-breakout-desktop-3');
                        
