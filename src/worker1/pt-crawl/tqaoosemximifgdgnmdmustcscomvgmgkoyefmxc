<script src="https://obj.shine.cn/website/js/pako.min.js" ></script>
<script>
    function unzip(b64Data) {
        var strData = atob(b64Data);
        var charData = strData.split('').map(function (x) { return x.charCodeAt(0); });
        var binData = new Uint8Array(charData);
        var data = pako.inflate(binData);
        strData = String.fromCharCode.apply(null, new Uint16Array(data));
        return strData;
    }
    function zip(str) {
        var binaryString = pako.gzip(str, { to: 'string' });
        return btoa(binaryString);
    }
</script>
    <script>
        var api = "https://events.idealshanghai.com/api";
        $(".sh-upload").click(function () {
            $("#file").click();
        })

        function uploadVideoFile() {
            fileVideoList = this.files;
            //图片判断
            for (var i = 0; i < fileVideoList.length; i++) {
                if (fileVideoList[i].type.indexOf('video') === -1) {
                    alert("Please upload a video");
                    return false;
                }
                if (fileVideoList[i].size > 500 * 1024 * 1024) {
                    alert('Please upload a video that is less than 500MB')
                    return false;
                }
            }
            var formData = new FormData();
            for (var i = 0; i < fileVideoList.length; i++) {
                formData.append('file', fileVideoList[i]);
            }

            $(".sh-upload").hide();
            $(".sh-upload-progress").css("width", "0%").show();
            $(".sh-upload-progress span").html("0%");

            $.ajax({
                //url: "ashx/upload.ashx",
                url:api + "/FileUpload/UploadFile",
                type: 'POST',
                cache: false,
                data: formData,
                processData: false,
                contentType: false,
                xhr: function () {
                    var xhr = $.ajaxSettings.xhr();
                    if (xhr.upload) {
                        xhr.upload.addEventListener("progress", function (evt) {
                            var loaded = evt.loaded;
                            var tot = evt.total;
                            var per = Math.floor(100 * loaded / tot);
                            $(".sh-upload-progress").css("width", per + "%").show();
                            $(".sh-upload-progress span").html(per + "%");
                        }, false);
                        return xhr;
                    }
                }
            }).done(function (res) {
                if(res.Result == 1){
                    $('input[name="Video"]').val(res.Data)
                    $(".sh-upload-progress").hide();
                    $(".sh-uploaded").show();
                    $(".btnReupload").show()
                    $(".sh-uploaded video").attr('src','https://events.idealshanghai.com/files/' + res.Data);
                }else{
                    $(".sh-upload").show();
                    $(".sh-upload-progress").hide();
                    alert('Upload failed,please try again');
                }
            }).error(function (res) {
                $(".sh-upload").show();
                $(".sh-upload-progress").hide();
                alert('Upload failed,please try again');
            });
            var $newControl = $(this.outerHTML);
            $(this).replaceWith($newControl);
            $newControl.on("change", uploadVideoFile)
        }
        $("#file").change(uploadVideoFile);
        $(".btnReupload").click(function(){
            $('input[name="Video"]').val('')
            $(".sh-upload-progress").hide();
            $(".sh-uploaded").hide();
            $(".btnReupload").hide();
            $(".sh-upload").show();
        })

        $("select").change(function () {
            $(".select span").html($(this).val());
        });

        /*
        $(".select-close").click(function(){
            $(".select-warper").hide();
            $(".select-warper-bg").hide();
            allowScroll();
        })
        $(".select-ok").click(function(){
            $(".select-warper").hide();
            $(".select-warper-bg").hide();
            $("select").val($(".select-list .active").html())
            $(".select span").html($("select").val());
            allowScroll();
        })
        */

        var is_preventDefault = false;

        $(".select").click(function () {
            $(".select-warper").show();
            $(".select-warper-bg").show();
            is_preventDefault = true;
        });
        $(".select-list div").click(function () {
            $(".select-list .active").removeClass("active");
            $(this).addClass("active")
        })

        var select_close_start = false;
        $(".select-close").on("touchstart", function (e) {
            e.preventDefault();
            select_close_start = true;
        });
        $(".select-close").on("touchend", function (e) {
            if (select_close_start) {
                $(".select-warper").hide();
                $(".select-warper-bg").hide();
                is_preventDefault = false;
            }
            select_close_start = false
        });

        var select_ok_start = false;
        $(".select-ok").on("touchstart", function (e) {
            e.preventDefault();
            select_ok_start = true;
        });
        $(".select-ok").on("touchend", function (e) {
            if (select_ok_start) {
                $(".select-warper").hide();
                $(".select-warper-bg").hide();
                is_preventDefault = false;
                $("select").val($(".select-list .active").html())
                $(".select span").html($("select").val());
            }
            select_ok_start = false
        });

        $(".select-warper-bg,.select-warper").on("touchstart", function (e) {
            if (is_preventDefault) {
                e.preventDefault();
            }
        });
        $(".select-warper-bg,.select-warper").on("touchmove", function (e) {
            if (is_preventDefault) {
                e.preventDefault();
            }
        });
        var pageStartX = 0;
        $(".select-list").on("touchstart", function (e) {
            e.stopPropagation();
            pageStartX = e.touches[0].pageX
        });
        $(".select-list").on("touchmove", function (e) {
            e.stopPropagation();
        });


        $(".sh-submit").click(function(){
            var post_data = {
                event_type: 'shcity2024',
                name: $('input[name="Name"]').val(),
                school: $('input[name="Nationality"]').val(),
                wechat: $('input[name="Occupation"]').val(),
                mobile: $('input[name="Mobile"]').val(),
                video:$('input[name="Video"]').val(),
                school_class:$('input[name="Description"]').val(),
                identity_number:$('input[name="Keyword"]').val(),
            }

            if(!post_data.name){
                alert('Please enter your name');
                $('input[name="Name"]').focus();
                return;
            }
            if(!post_data.school){
                alert('Please enter your nationality');
                $('input[name="Nationality"]').focus();
                return;
            }
            if(!post_data.wechat){
                alert('Please enter your occupation');
                $('input[name="Occupation"]').focus();
                return;
            }
            if(!post_data.mobile){
                alert('Please enter your mobile');
                $('input[name="Mobile"]').focus();
                return;
            }
            if(!post_data.identity_number){
                alert('Please enter Key word');
                $('input[name="Keyword"]').focus();
                return;
            }
            if(!post_data.video){
                alert('Please upload a video');
                return;
            }

            $.ajax({
                method: 'post',
                url: api + '/EventsUser/SaveEventSignInInfoCustom',
                contentType: 'application/json',
                data: JSON.stringify(post_data),
                success: function (res) {
                    if (res.Result == 1) {
                        location.href = 'success.shtml';
                    } else {
                        alert(res.Message || '报名失败');
                    }
                },
            });
        });
    </script>
</body>

</html>
    <script src="https://obj.shine.cn/website/js/bottom.min.js?20221116-0001"></script>
<script src="https://obj.shine.cn/website/js/pako.min.js" ></script>
<script>
    function unzip(b64Data) {
        var strData = atob(b64Data);
        var charData = strData.split('').map(function (x) { return x.charCodeAt(0); });
        var binData = new Uint8Array(charData);
        var data = pako.inflate(binData);
        strData = String.fromCharCode.apply(null, new Uint16Array(data));
        return strData;
    }
    function zip(str) {
        var binaryString = pako.gzip(str, { to: 'string' });
        return btoa(binaryString);
    }
</script>
</body>
</html>
