        function blockScripts() {
            // Block scripts within specified divs
            var divsToBlockScripts = document.querySelectorAll("#sideAd, #sideAd2, #hideOnMobile, #spacer");
            divsToBlockScripts.forEach(function(div) {
                var scripts = div.querySelectorAll("script");
                scripts.forEach(function(script) {
                    script.remove();
                });
            });
        }

        // Run blockScripts function when the window has fully loaded
        window.onload = function () {
            // Check if the screen width is less than or equal to 768 pixels (adjust as needed)
            if (window.innerWidth <= 768) {
                blockScripts();
            }
        };
    </script>


