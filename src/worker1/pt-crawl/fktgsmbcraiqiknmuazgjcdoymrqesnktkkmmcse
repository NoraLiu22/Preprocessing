<section data-event="" id="smart-tv"
         class="checkerboard-betta py-16 relative w-full py-10"
    >
        <div class="md:container">
            <div class="md:w-10/12 mx-auto grid h-full place-items-center grid-cols-1 md:grid-cols-2 gap-8 md:gap-5">

