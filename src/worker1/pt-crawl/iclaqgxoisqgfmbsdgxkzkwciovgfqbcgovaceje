    <script type="text/javascript">

        var myDate = new Date(),
            day = myDate.getDay(),
            week = new Array("星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六"),
            str = myDate.getFullYear() + '年' + (myDate.getMonth() + 1) + '月' + myDate.getDate() + '日 ';
        $('#time').html(str);

        function initMenu() {
            var _speed = 400; // in ms
            var _hold = $('.Nav ul.nav');
            var _t = null;
            if (_hold.length) {
                var _list = _hold.children();
                _list.each(function () {
                    var _el = $(this);
                    var _box = _el.children('div.nav-w');
                    var _sub = _box.children();
                    _box.show().width(_sub.outerWidth()).hide();
                    _el.mouseenter(function () {
                        var _this = this;
                        _t = setTimeout(function () {
                            $(_this).css('zIndex', 10);
                            _el.addClass('hover');
                            if (_box.is(':hidden')) {
                                _box.show();
                                _sub.css('marginTop', -_sub.outerHeight());
                            }
                            _sub.stop().animate({ marginTop: 0 }, _speed);
                        }, 300)
                    }).mouseleave(function () {
                        if (_t) clearTimeout(_t);
                        var _this = $(this);
                        $(this).css('zIndex', 5);
                        if (_sub.length) {
                            _sub.stop().animate({ marginTop: -_sub.outerHeight() }, _speed, function () {
                                _box.hide();
                                _el.removeClass('hover');
                                _this.css('zIndex', 1);
                            });
                        } else {
                            _el.removeClass('hover');
                        }
                    });
                });
            }
        }

        $(document).ready(function () {

            initMenu();

        });
    </script>

    <script type="text/javascript">
        (function ($, window) {

            changepic('.pic1');
            changepic('.pic2');

            function changepic(id) {

                var $_id = $(id),
                    cur = 0,
                    sum = $_id.find('li').size(),
                    auto = null;

                function roll(No) {
                    $_id.find('li').hide();
                    $_id.find('li').eq(No).fadeIn(200);
                    $_id.find('.menu>p').removeClass('cur');
                    $_id.find('.menu>p').eq(No).addClass('cur');
                }

                function autoroll() {
                    cur = (cur + 1) % sum;
                    roll(cur);
                }

                $_id.find('li:first').show();
                $_id.find('li').each(function () {
                    $_id.find('.menu').append('<p></p>');
                });
                $_id.find('.menu>p:first').addClass('cur');
                $_id.find('.sr').on('click', function () {
                    cur = (cur + 1) % sum;
                    roll(cur);
                });
                $_id.find('.sl').on('click', function () {
                    cur = (cur - 1 + sum) % sum;
                    roll(cur);
                });
                $_id.find('.menu>p').on('click', function () {
                    cur = $(this).index();
                    roll(cur);
                });
                auto = window.setInterval(autoroll, 5000);
                $_id.on('mouseenter', function () {
                    window.clearInterval(auto);
                }).on('mouseleave', function () {
                    auto = window.setInterval(autoroll, 5000);
                });
            }

            var liveSum = $('.roll li').size(),
                liveW = 0,
                liveCur = 0,
                liveAuto = null,
                liveArr = [0];

            $('.roll ul').append($('.roll li').clone());
            var len = $('.roll li').eq('1').position().left;

            function liveRoll() {
                liveCur += 1;
                $('.roll ul').stop(true, true).animate({
                    'margin-left': -liveCur
                }, 'slow', function () {
                    if (liveCur >= len) {
                        $('.roll ul').css('margin-left', '0');
                        liveCur = 0;
                    }
                });
            }

            liveAuto = window.setInterval(liveRoll, 20);
            $('.roll').on('mouseenter', function () {
                window.clearInterval(liveAuto);
            }).on('mouseleave', function () {
                liveAuto = window.setInterval(liveRoll, 20);
            });

        })(window.jQuery, window);

    </script>

