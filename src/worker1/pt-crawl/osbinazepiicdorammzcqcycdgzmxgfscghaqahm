  const pathItems = pathname.split('/');
  let pid;
  switch (pathItems[1]) { // intel and strat don't have their own subdomains, so we set manually
    case 'intelligencer':
      pid = 1073;
      break;
    case 'strategist':
      pid = 1079;
      break;
    default:
      pid = pids.find(({ key }) => hostname.includes(key)).value;
      break;
  }

  // Subx canvases
  const subXAnimationElements = `
    <div class="zeddit-rcm" data-pid="${pid}" data-type="canvas-panel" animation-type="popup"></div>
    <div class="zeddit-rcm" data-pid="${pid}" data-type="canvas-panel" animation-type="slide"></div>

    <div class="zeddit-rcm" data-pid="${pid}" data-type="canvas-email" animation-type="popup"></div>
    <div class="zeddit-rcm" data-pid="${pid}" data-type="canvas-email" animation-type="slide"></div>
  `;

  // integrate Sub(x) scripts and elements
  if (hostname !== 'subs.nymag.com') { // do not integrate on this subdomain
    document.head.appendChild(trackingScript);
    document.body.insertAdjacentHTML('beforeend', subXAnimationElements);
  }
</script>

