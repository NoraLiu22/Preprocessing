        <script>
            async function getConfig() {
                const parsedUrl = new URL(window.location.href);
                const institution = parsedUrl.searchParams.get('institution');
                const response = await fetch(
                    '/esplorows/rest/research/configuration' + (institution ? `?institution=${institution}` : '')
                );
                return response.json();
            }

            function OptanonWrapper() {}

            function useVariables() {
                if (localStorage.getItem('espHideLogo') !== 'true') {
                    document.getElementById('esp-init-loader-message').style.display = 'flex';
                }

                const otKey = localStorage.getItem('espOneTrustKey');
                if (localStorage.getItem('espOneTrust') === 'true' && otKey !== 'null') {
                    const script1 = document.createElement('script');
                    script1.id = 'otScript1';
                    script1.src = `https://cdn.cookielaw.org/consent/${otKey}/OtAutoBlock.js`;
                    script1.type = 'text/javascript';
                    const script2 = document.createElement('script');
                    script2.id = 'otScript2';
                    script2.src = 'https://cdn.cookielaw.org/scripttemplates/otSDKStub.js';
                    script2.type = 'text/javascript';
                    script2.charset = 'UTF-8';
                    script2.dataset.domainScript = otKey;
                    if (document.getElementById('otScript1') === null) {
                        document.head.appendChild(script1);
                    }
                    if (document.getElementById('otScript2') === null) {
                        document.head.appendChild(script2);
                    }
                }

                const primaryColor = localStorage.getItem('espPrimary');
                const accentColor = localStorage.getItem('espAccent');
                if (primaryColor) {
                    document.getElementById('esp-init-loader').style.setProperty('--custom-primary', primaryColor);
                } else {
                    document.getElementById('esp-init-loader').style.removeProperty('--custom-primary');
                }
                if (accentColor) {
                    document.getElementById('esp-init-loader').style.setProperty('--custom-accent', accentColor);
                } else {
                    document.getElementById('esp-init-loader').style.removeProperty('--custom-accent');
                }
            }

            useVariables();

            getConfig().then((config) => {
                const colors = config?.customization?.colors;
                const keyMap = {
                    espPrimary: colors?.primary,
                    espAccent: colors?.accent,
                    espHideLogo: config.customerParameters['hide_esploro_logo'],
                    espOneTrust: config.featureFlags['ENABLE_ONETRUST'],
                    espOneTrustKey: config.customerParameters['esploro_onetrust_licence_key'],
                };
                for (const key in keyMap) {
                    if (keyMap[key]) {
                        localStorage.setItem(key, keyMap[key]);
                    } else {
                        localStorage.removeItem(key);
                    }
                }
                useVariables();
            });
        </script>
