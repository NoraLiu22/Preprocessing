		$(document).ready(function () {
			// External anchor fix for Firefox
			var h = window.location.hash;
			if (h) {
				$('html, body').stop();
				setTimeout(function () {
					$('.wrapper').scrollTop(0);
					$('html, body').animate({
						scrollTop: $('[name="' + h.slice(1) + '"]').offset().top
					}, 500);
				}, 10);
