function _gam_kv_(s) {
    function pbtoval(a) {
      if (a.length === 0) return 0;
      return parseFloat(a[0]);
    }
    function apstoval(a) {
      if (a.length === 0) return 0;
      a=a[0].replace("amp_","").replace("v_","").substring(2,7).padEnd(5,'#');
      var pos = _gam_mapaps.indexOf(a);
      if (pos === -1) {
          return -1;
      }
      var order = Math.floor(pos / 5);
      return Math.min(order, 300)*0.01+
          Math.max(Math.min(order - 300, 100), 0)*0.05+
          Math.max(Math.min(order - 400, 48), 0)*0.25+
          Math.max(Math.min(order - 448, 20), 0)*0.5+
            Math.max(order - 468, 0)*1;
    }
    function valtosj(n) {
      if (n < 0.5) return 0;
      if ((0.5 <= n) && (n <= 1)) return 1;
      if (n >= 7) return 4;
      return 1 + Math.floor((n - 1) / 2)
    }
    
    s.clearTargeting('sj');
    s.clearTargeting('rndp');
    try {
      const t_pb = pbtoval(s.getTargeting('hb_pb'));
      const t_aps = apstoval(s.getTargeting('amznbid'));
      const m = Math.max(t_pb, t_aps);
      const random1 = googletag.pubads().getTargeting('random1')[0];
      if (m > 0) {
        s.setTargeting('rndp', m.toFixed(2));
        if ((random1 !== '19') && (random1 !== '20')) {
          s.setTargeting('sj', valtosj(m).toString());
        }
      } else {
        s.setTargeting('rndp', '-1');
        if ((random1 !== '19') && (random1 !== '20')) {
          s.setTargeting('sj', '-1');
        }
      }
    } catch (e) { }
}
    googletag.cmd.push(function () {
        
