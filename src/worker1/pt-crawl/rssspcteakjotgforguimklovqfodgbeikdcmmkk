                        if (slotData.sizes) {
                            slot = googletag.defineSlot('/' + data.networkId + '/' + slotData.adUnit, slotData.sizes, entry.target.id);
                        } else {
                            slot = googletag.defineOutOfPageSlot('/' + data.networkId + '/' + slotData.adUnit, entry.target.id);
