<script type="text/javascript" src="http://images.china.cn/images1/resource/scripts/jquery/jquery-1.12.4.min.js" ></script>
<script type="text/javascript" >
	var num = 0;
	var time;
	$('.section2 .left span').click( function(){
		var bFlag = $(this).hasClass('r');		
		if( bFlag ){
			num++;
			if( num == 4 ){ num = 0 }
		}else{
			num--;
			if( num == -1 ){ num = 3 }
		}
		play();		
	} );	
	time = setInterval( autoPlay,3000 );	
	$('.section2 .left').hover( function(){
		clearInterval( time );
	},function(){
		time = setInterval( autoPlay,3000 );
	} );		
	function play(){
		$('.section2 .left .list').stop().animate( {'left':-720*num} );
	}
	function autoPlay(){	
		num++;
		if( num == 4 ){ num = 0 }
		play();	
	}
</script> contentType 9 text/html url 54 http://union.china.com.cn:80/zhuanti/node_1012280.html responseCode 3 200 