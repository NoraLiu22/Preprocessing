      function initializeRakuten(url) {
        /*Tracking Bootstrap Set Up DataLayer objects/properties here*/
        if (!window.DataLayer) {
          window.DataLayer = {};
        }
        if (!DataLayer.events) {
          DataLayer.events = {};
        }
        DataLayer.events.SPIVersion = DataLayer.events.SPIVersion || "3.4";
        DataLayer.events.SiteSection = "1";
        var loc, ct = document.createElement("script");
        ct.type = "text/javascript";
        ct.async = true;
        ct.src = url;
        loc = document.getElementsByTagName('script')[0];
        loc.parentNode.insertBefore(ct, loc);
      }

      window._rakutenInitialized = false;
      document.addEventListener('scroll', function () {
        if (!window._rakutenInitialized) {
          initializeRakuten(document.location.protocol + "//web.archive.org/web/20220928170755/https://tag.rmp.rakuten.com/121786.ct.js");
          window._rakutenInitialized = true;
        }
      });
      document.addEventListener('click', function () {
        if (!window._rakutenInitialized) {
          initializeRakuten(document.location.protocol + "//web.archive.org/web/20220928170755/https://tag.rmp.rakuten.com/121786.ct.js");
          window._rakutenInitialized = true;
        }
      });
    </script>

