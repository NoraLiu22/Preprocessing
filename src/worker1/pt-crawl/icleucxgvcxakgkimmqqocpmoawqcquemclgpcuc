<script>
$( document ).ready(function() {
  
    skipLink(); 
    function skipLink () {
    var link = $('#skip-link a');
    var elm = $('h1:visible, h2:visible');
    if (!elm.length) {
        return;
    }
    link.on('click', function (e) {
        e.preventDefault();
        e.stopPropagation();
        $(elm[0]).attr('tabindex', '-1');
        $(elm[0]).focus();
        $('html, body').animate({
            scrollTop: $(elm[0]).offset().top - 120
        }, 500);
    });
}
    
});



(function($) {
        
    var size;


    // VERIFY WINDOW SIZE
    function windowSize() {
        size = $(document).width();
        if (size >= 991) {
            $('body').removeClass('open-menu');
            $('.hamburger-menu .bar').removeClass('animate');
        }
    };

     // ESC BUTTON ACTION
    $(document).keyup(function(e) {
        if (e.keyCode == 27) {
            $('.bar').removeClass('animate');
            $('body').removeClass('open-menu');
            $('#accessible-menu #navigation .menu-item-has-children a ul').each(function( index ) {
                $(this).removeClass('open-sub');
            });
        }
    });

    $('.msl_navbar ul').addClass('sub-menu dropdown');
    $('.msl_navbar li:has(ul)').addClass('menu-item-has-children');
    $("#msl_admin > h3").attr('role', 'button');
    $("#msl_admin > h3").attr('tabindex', '0');
    
    $('.msl_navbar li.menu-item-has-children > a').each(function() {
        $(this).attr('aria-expanded','false');
        $(this).attr('aria-haspopup','true');
    });
    
    $('#mobileNavigation li.menu-item-has-children > a').click(function() {
        $(this).attr('aria-expanded','true');
    });
     
    
    $("#accessible-menu #navigation > ul > li").each(function() {
        var sectionTitle = $(this).find("> .msl-itemhassubnav .nav-text").text();
        var sectionLink  = $(this).find("> .msl-itemhassubnav").attr("href");
        $(this).find('> ul').prepend('<a href=" ' + sectionLink + ' " class="navButton" tabindex="0" aria-pressed="false">Visit '+ sectionTitle +'</a>');
    });
    
    
    /*$('#accessible-menu #navigation > ul > li').hover(function() {
        $whidt_item = $(this).width();
        $whidt_item = $whidt_item-8;

        $prevEl = $(this).prev('li');
        $preWidth = $(this).prev('li').width();
        var pos = $(this).position();
        pos = pos.left+4;
    });*/


     // ANIMATE HAMBURGER MENU
    $('.hamburger-menu').on('keypress click', function() {
        $(this).toggleClass('animated');
        $('.hamburger-menu .bar').toggleClass('animate');
        if($('body').hasClass('open-menu')){
            $('body').removeClass('open-menu');
        }else{
            $('body').toggleClass('open-menu');
        }
    });

    $('#accessible-menu #navigation .menu-item-has-children ul').each(function(index) {
        $(this).prepend('<li class="backTo" role="none"><a href="#" role="menuitem">Back</a></li>');
    });

    // RESPONSIVE MENU NAVIGATION
    $('#accessible-menu #navigation .menu-item-has-children > a').on('keypress click', function(e) {
        if(size <= 991){
            e.preventDefault();
            $(this).siblings('ul').addClass('open-sub');
        }
    });
    
    
    //Open menu on desktop
    $('#accessible-menu #navigation .msl_navbar > .menu-item-has-children > a').on('keypress click', function(e) {
      if(size >= 992){
        if (!$(this).parent().hasClass("opened")) {
            e.preventDefault();
        }
        
        $("#accessible-menu #navigation .menu-item-has-children").removeClass('opened');
        $(this).parent().addClass('opened');
        
        if ( $('#accessible-menu #navigation .msl_navbar > li' ).hasClass("opened")) {
          $("header").addClass("opened");
          $("body").mouseup(function(i) {
            if (!$(i.target).closest("a").length > 0 ) {
                $("#accessible-menu #navigation .menu-item-has-children").removeClass('opened');
                $("header").removeClass("opened");
            }
          });
        } 
        
      }
    });
    

    // CLICK FUNCTION BACK MENU RESPONSIVE
    $('#accessible-menu #navigation .menu-item-has-children ul .backTo').on('keypress click', function(e) {
        e.preventDefault();
        $(this).closest('ul').removeClass('open-sub');
    });

    $('body .over-menu').on('click', function() {
        $('body').removeClass('open-menu');
        $('.bar').removeClass('animate');
    });

  
    
    $(document).keyup(function(e) {
     if (e.key === "Escape") { // escape key maps to keycode `27`
         $('.msl_navbar li.menu-item-has-children').each(function() {
            $(this).removeClass('opened');
            $(this).find('> a').attr('aria-expanded','false');
        });
        $('.hamburger-menu').removeClass('animated');
     }
    });
                 
    
    

    $(document).ready(function(){
        windowSize();
    });

    $(window).scroll(function(){
        //smallerMenu();
    });

    $(window).resize(function(){
        windowSize();
    });
    

})(jQuery);
    
</script>
<!-- end of BCUSU22-header snippet -->


