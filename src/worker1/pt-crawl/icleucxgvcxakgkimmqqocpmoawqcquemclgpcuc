<script type="text/javascript">
    $(document).ready(function() {
        
<!-- start of BCUSU22-scripts snippet -->
//Adminbar -controls show/hide
if ( $('.msl-loggedin').length > 0 ) { 
 $('header #admin-tools, header .bskt').show();
 $('header .login').hide();
} else {
 $('header #admin-tools .msl-admin, header #admin-tools .user, header .msl-basket-summary').css('display','none');
 $('body').addClass('loggedOut');
}


//Slider wrap content
$('#sliderStyling .msl-item').each(function() {
    $(this).find('.msl-title, .msl-leader').wrapAll('<div class="details" />');
    $(this).find('.details').insertBefore( $(this).find('.msl-image') );
});


//Newslist wrap content
$('.newsStyles .msl-item').each(function() {
    $(this).find('.msl-item-details, .msl-title, .msl-leader').wrapAll('<div class="details" />');
    $(this).find('.msl-item-details').insertBefore( $(this).find('.msl-title') );
    var imgLink = $(this).find('.msl-image').attr('href');
    $(this).find('.details').append('<a href="'+ imgLink +'" role="button" tabindex="-1" class="readmore" >Read More <i class="fa-solid fa-right-long"></i></a>');
    
    $(this).find('.msl-title > a, .msl-morelink').attr('tabindex', '-1');
});


//Newslist wrap content 2
$('.newsContainer .news_item').each(function() {
    $(this).find('h5, .leader').wrapAll('<div class="details" />');
    $(this).find('.news_image').insertBefore( $(this).find('.details') );
    $(this).find('h5 > a').attr('tabindex', '-1');
    
    var imgLink2 = $(this).find('h5 a').attr('href');
    $(this).find('.details').append('<a href="'+ imgLink2 +'" role="button" tabindex="-1" class="readmore" >Read More <i class="fa-solid fa-right-long"></i></a>');
    
    
    if ( $(this).find('img').length ) {
        var imgSrc = $(this).find('img').attr('src');
        imgSrc = imgSrc.split('?')[0];
        $(this).find('img').attr('src', imgSrc + '?thumbnail_width=300&thumbnail_height=430&resize_type=CropToFit&fill_colour=ffffff');
        
    } else {
        $(this).find('.news_image').prepend('<a id="ctl00_Main_ctl00_rptNews_ctl03_lnkImage" href="'+ imgLink2 +'"><img src="/asset/news/default/ee5012d098794b2e8e40f78dd4cfd4a5.png?thumbnail_width=300&amp;thumbnail_height=430&amp;resize_type=CropToFit&amp;fill_colour=ffffff" id="ctl00_Main_ctl00_rptNews_ctl03_imgImage" loading="lazy" alt=""></a>');
    }
});


//Eventlist wrap content
$('.squares-events .event_item').each(function() {
    $(this).find('.msl_event_name, .msl_event_description, .msl_event_time, .msl_event_organisation, .msl_event_location').wrapAll('<div class="details" />');
    $(this).find('.msl_event_time').insertBefore( $(this).find('.msl_event_name') );
    $(this).find('.msl_event_organisation').insertBefore( $(this).find('.msl_event_name') );
    $(this).find('.msl_event_description').insertBefore( $(this).find('.msl_event_location') );
    var eventLink = $(this).find('.msl_event_name').attr('href');
    $(this).find('.details').append('<a href="'+ eventLink +'" role="button" tabindex="-1" class="readmore" >Read More <i class="fa-solid fa-right-long"></i></a>');
    
    $(this).find('.msl_event_name').attr('tabindex', '-1');
});

//Event details
    var maxHeight = 0;
    $(".squares-events .details").each(function(){
       if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
    });
    $(".squares-events .details").height(maxHeight);


//Officers image nav wrap 
$('.officers li').each(function() {
    $(this).find('.msl-imagenav-description, .msl-imagenav-title').wrapAll('<div class="details" />');
    $(this).find('.msl-imagenav-description').insertBefore( $(this).find('.msl-imagenav-title') );
});


//Wrapping up the org items
$('.msl_organisation_list li.msl-listing-logo').each(function () {
    img = $(this).find('a').addClass('gl-logo');
    $(this).next().prepend(img);
    $(this).remove();
 });



//search slideout
$(".searchfun a").click(function(e) {
  e.preventDefault();
  $(this).toggleClass('open');
  $('.search-form').slideToggle();
  
  if ( $(this).hasClass('open') ) {
     $(this).html('<i class="fas fa-times"></i>');
  } else {
     $(this).html('<i class="fas fa-search"></i>');
  }

});


//Image nav Read more
$('.imageNav li').each(function() {
    var link = $(this).find('.msl-imagenav-page').attr('href');
    $(this).append('<a href="'+ link +'" role="button" tabindex="-1" class="readmore" >Read More <i class="fa-solid fa-right-long"></i></a>');
});


//Comments
$('.comment_panel .comments dd').each(function() {
    $(this).wrapInner( "<div class='commentContent'></div>");
    $(this).prev('dt').prependTo(  $(this) )
});
<!-- end of BCUSU22-scripts snippet -->

    });
</script>
            </div>

            
