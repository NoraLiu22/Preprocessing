 tp.push(["addHandler", "showOffer", (offerParams) => {
   console.log("__PEXP offer", offerParams);
   
   if (offerParams.containerSelector == '.piano-content-lock' || offerParams.containerSelector == '.piano-content-lock-upper') {
     
     // ...we add a gradient strip above our template container (it's name should correspond to a specified one in Composer)...
     document.querySelector(offerParams.containerSelector).classList.add("template-shown");
     
     // ...and hide everything after it within the same HTML container (also, the name should be checked)
     //document.querySelectorAll(offerParams.containerSelector + " ~ *").forEach(function (x) {
     //  x.hidden = true;
     //});
     
     // expand the content area to include the lock content
      var $btext = $('.bigtext');
     if(offerParams.containerSelector == ".piano-content-lock") {
         $btext.find("p").slice(6).remove();
     } else if (offerParams.containerSelector == ".piano-content-lock-upper") {
       $btext.find("p").slice(3).remove();
     }
     
