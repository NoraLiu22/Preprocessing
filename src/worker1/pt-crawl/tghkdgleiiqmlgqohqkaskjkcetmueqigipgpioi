     elemsToRemove.forEach(function(item){
      if (item) {
         item.remove();
      }  
     });
     
     var newHeight = 0;
     var visibleContent = $btext.find('p:visible');
     for (var i=0; i<visibleContent.length; i++) {
       newHeight += visibleContent.eq(i).outerHeight();
     }
     
     if ($('.piano-in-article-reco').length) {
       newHeight += $('.piano-in-article-reco').outerHeight();
     }
     
     $btext.css('height', newHeight);
     
