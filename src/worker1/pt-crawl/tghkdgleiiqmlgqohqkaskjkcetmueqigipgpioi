  <script>
    tp = window.tp || [];
    tp.push(["setContentAuthor", "Jennifer Harper"]);         // The content author
    tp.push(["setContentSection", "News"]);       // The content section
    tp.push(["setTags", ["Inside the Beltway", "Politics"]]);                 // The content tags
