	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="/misc/jquery-1.8.3.min.js"><\/script>')</script>
	<script src="/misc/sh/jquery.searchabledropdown-1.0.8.min.js"></script>
	<script>
		$(document).ready(function () {

			// make 2 lists searchable
			$("#ctl00_MainContent_listCountries").searchable();
			$("#ctl00_MainContent_listYears").searchable();

			$(document).on("click", ".pagination a", function (event) {
				event.preventDefault ? event.preventDefault() : (event.returnValue = false);
				GetCoins($(this).attr("href").match(/\-?[\d]+/));
			});

			$("#PaginationPage").removeAttr("onchange");
			$(document).on("change", "#PaginationPage", function (event) {
				$("#PaginationPage").removeAttr("onchange");
				event.preventDefault ? event.preventDefault() : (event.returnValue = false);
				GetCoins($(this).val());
			});

			$("#ctl00_MainContent_listCountries").change(function (event) {
				event.preventDefault ? event.preventDefault() : (event.returnValue = false);
				GetCoins(1);
			});
			$("#ctl00_MainContent_listYears").change(function (event) {
				event.preventDefault ? event.preventDefault() : (event.returnValue = false);
				GetCoins(1);
			});
		});

		function GetCoins(page) {
			// get filters
			var country = $("#ctl00_MainContent_listCountries").val();
			var year = $("#ctl00_MainContent_listYears").val();

			// set-up ajax search
			$.ajax({
				beforeSend: function () {
					// indicate to user that an operation is being performed
				    $("#AjaxMessage").html("Retrieving coins from the database - please wait... <img src='/images/spinner.gif' alt='Loading' width='16' height='16' />");
				    $("#AjaxMessage").removeClass("alert");
					$("#AjaxMessage").addClass("information");
					$("#AjaxMessage").fadeIn("fast");
				},
				type: "POST",
				url: "/WebServices/GetVaultItems.asmx/getCoins",
				data: "{'db': '1" + 
					"', 'country': '" + country +
					"', 'year': '" + year +
					"', 'page': '" + page +
					"', 'grade': '"  +
					"', 'collector': '" + 
					"', 'itemsPerPage': '10" +
					"' }",
				cache: true,
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				success: function (response) {
					$('#ItemResults').html(response.d);
					$("#AjaxMessage").hide();
					$("#PaginationPage").removeAttr("onchange");
				},
				error: function (xmlHttpRequest, textStatus, errorThrown) {
					$("#AjaxMessage").removeClass("information");
					$("#AjaxMessage").addClass("alert");
					$("#AjaxMessage").html("We encountered an error; please report it to the <a href='/contact'>Administrator</a> ...");
					$("#AjaxMessage").fadeOut(10000);
				}
			});
		}	
	</script>

	
		<div class="msgGrowl-container bottom-right">
