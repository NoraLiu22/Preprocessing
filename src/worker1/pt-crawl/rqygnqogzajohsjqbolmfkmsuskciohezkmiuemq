    box-shadow: 20px 2px #000000;
    right: 0;
  }
}

.date {
  background-color: #e8e5df;
  display: flex;
  grid-column: 1 / span 2;
  grid-row: 2;
  padding: 16px 22px;
  position: relative;
}

.date p {
  margin: 0 0;
}

.content-box--intro {
  background-color: #e8e5df;
  font-weight: 400;
}

/* .content-box--intro .cp-text--intro p {
  font-weight: 400;
} */

.content-box--main {
  background-color: #e8e5df;
}

@media screen and (min-width: 700px) {
  .content-box--main .cp-video {
    max-width: 824px;
    margin-left: auto;
    margin-right: auto;
  }
}

/* .pullquote {
