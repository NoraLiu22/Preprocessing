    case 'chartbeat_mab':
      return ['data-store', 'content-person', 'measure-content'];
    case 'optimizely':
      return ['vendor', 'data-store', 'content-person-prof', 'content-person', 'measure-ads', 'measure-content', 'product-develop'];
