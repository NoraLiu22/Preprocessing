  switch (serviceName) {
    case 'adobe':
      return ['data-store', 'content-person-prof', 'content-person', 'measure-content', 'measure-market', 'product-develop'];
    case 'bombora':
      return ['vendor', 'data-store', 'ads-person-prof', 'measure-ads', 'measure-content', 'product-develop'];
