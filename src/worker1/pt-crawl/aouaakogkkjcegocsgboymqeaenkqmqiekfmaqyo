<script type="text/javascript">
$(function () {
    var chart;
	var data = new Array();
	var units = "million"
	 var sCategory = "2020;2021;2022;2023;2024;"
	var aCategory2 = new Array();
	aCategory2 = sCategory.split(";");

	var sNames = "Other;Delta ;Southwest ;"
	var aNames= new Array();
	aNames = sNames.split(";");

	var stringa = "8.240825;14.287959;13.582498;16.59828;16.582107"
	var aStringA= new Array();
	aStringA = stringa.split(";");
	for(var i=0; i<aStringA.length; i++) {
		aStringA[i] = Math.round(aStringA[i]);
	}

	var stringb = "25.569877;46.968719;60.217293;64.856024;64.837576"
	var aStringB= new Array();
	aStringB = stringb.split(";");
	for(var i=0; i<aStringB.length; i++) {
		aStringB[i] = Math.round(aStringB[i]);
	}

	var stringc = "4.14426;6.524499;7.342917;8.066611;8.110474"
	var aStringC= new Array();
	aStringC = stringc.split(";");
	for(var i=0; i<aStringC.length; i++) {
		aStringC[i] = Math.round(aStringC[i]);
	}

		if (aNames[0] != "")
			{
				data.push({data: aStringA, name: aNames[0]});
			}
		if (aNames[1] != "")
			{
				data.push({data: aStringB, name: aNames[1]});
			}

		if (aNames[2] != "")
			{
				data.push({data: aStringC, name: aNames[2]});
			}

    $(document).ready(function() {
        chart = new Highcharts.Chart({
            chart: {
                renderTo: 'container2',
                type: 'column'
            },
			credits: {
            	enabled: false
        	},
            title: {
                text: null
            },
            xAxis: {
                categories: aCategory2
            },
            yAxis: {
                min: 0,
                title: {
                    text: null
                }
            },
          	legend: {
				align: 'center',
				width: 250,
        		floating: false,
        		layout: 'horizontal',
        		borderWidth:0,
				style:{
					font: 'Arial sans-serif',
					fontSize: '5px',
					padding: '5px'
				}
    		},
            tooltip: {
                formatter: function() {
                    return '<b>'+ this.x +'</b><br/>'+
                        this.series.name +': '+ Highcharts.numberFormat(this.y, 0) + '  ' + units + '<br/>'+
                        'Total: '+ Highcharts.numberFormat(this.point.stackTotal, 0) + '  ' + units;
                }
            },
            plotOptions: {
                column: {
                    stacking: 'normal',
                    dataLabels: {
                        enabled: true,
                        color: (Highcharts.theme && Highcharts.theme.dataLabelsColor) || 'black'
                    }
                }
            },
            series: data
			,
			exporting: {
            enabled: false
        }
        });
    });

