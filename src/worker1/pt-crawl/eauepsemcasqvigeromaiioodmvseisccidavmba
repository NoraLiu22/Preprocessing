                <h1 id="parent-fieldname-title" class="documentFirstHeading">
                    EEA greenhouse gases — data viewer
                </h1>
            
            
        
                    


                    <div class="content-header-body">
                      
                        
                          

    


<!-- LANGUAGE SWITCHER DEFINITION -->


    

        

    



    <div class="documentByLine" id="plone-document-byline">

        <noscript>
          
          <div class="portalMessage warning">This website has limited functionality with javascript off. Please make sure javascript is enabled in your browser.</div>
        </noscript>

        
            <span class="portalType">Dashboard (Tableau)</span>

            <div class="inline-block" id="version-global-id">
                
                    <strong>Prod-ID:</strong>
                
                <span>DAS-270-en</span>
            </div>

            

            
            <span class="docDate publishDate">
                Published 15 Apr 2024
                <span class="byline-separator"></span>
            </span>
            
        


        
        

      
        <div class="documentByLineReadingStats inline-block">
          <span class="documentByLineReadingTime">1</span>
          <span>min read</span>
          
        </div>
      

      

    </div>





    
    
    
            
        





    
      <div class="documentByLineThemes" id="themes-tags">
            <span class="listHead">Topics:</span>
            
              <span>
                <a class="link-category" rel="tag" href="https://www.eea.europa.eu/themes/climate">Climate change mitigation</a>
              </span>
            
      </div>
    




                        
                      

                      
                        
                        
            
                <div id="parent-fieldname-description" class="documentDescription">
                   Data viewer on greenhouse gas emissions and removals, sent by countries to UNFCCC and the EU Greenhouse Gas Monitoring Mechanism (EU Member States).

                </div>
            
            
        
                      


                      
                        
                      
                    </div>

                  </div>

