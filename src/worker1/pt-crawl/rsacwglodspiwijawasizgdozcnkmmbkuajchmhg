        <script type="text/javascript">
            var googletag = window.googletag || {};
            googletag.cmd = window.googletag.cmd || [];
            var slot = slot || {}; //some modules call this more than once so we check if slot is already set.
            googletag.cmd.push(function () {
              gptSlots.push(
                slot['94a7a7ca-433a-46b7-9447-5b1badd8dfc7'] = googletag
                  .defineSlot(adUnit, gptconfig.adSizes, '94a7a7ca-433a-46b7-9447-5b1badd8dfc7')
                  .setTargeting('partner', 'mailbag_content_tray_homepage')
                  .setTargeting('slot', 'logo')
                  .defineSizeMapping(gptconfig.responsiveMappings['presented-by']).addService(googletag.pubads())
              );
            });
          
              window['94a7a7ca-433a-46b7-9447-5b1badd8dfc7_refreshAdv'] = function() {
                googletag.pubads().refresh([slot['94a7a7ca-433a-46b7-9447-5b1badd8dfc7']],{changeCorrelator: false});
              };
        if (typeof(__iasPET) !== 'undefined') {
          googletag.cmd.push(function() {
            var newSlot = window.slot['94a7a7ca-433a-46b7-9447-5b1badd8dfc7'];
            if(typeof(newSlot) === 'undefined'){
              return;
            }

            var iasPETSlots = [];
            var newSlotSizes = newSlot.getSizes().map(function(size) {
              if (size.getWidth && size.getHeight)
                return [size.getWidth(), size.getHeight()];
              else
                return [1, 1];
            });
 
            iasPETSlots.push({
              adSlotId: newSlot.getSlotElementId(),
              size: newSlotSizes,
              adUnitPath: newSlot.getAdUnitPath()
            });
 
            __iasPET.queue.push({
              adSlots: iasPETSlots,
              dataHandler: iasSingleSlotDataHandler('94a7a7ca-433a-46b7-9447-5b1badd8dfc7')
            });
          });
        }
