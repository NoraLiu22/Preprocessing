            	jQuery('#modal_signin_rememberme').removeAttr('checked');
            } else {
            	jQuery(this).text("X");
            	jQuery('#modal_signin_rememberme').attr('checked', 'checked');
            }
            return false;
        });
    	jQuery('input, textarea').placeholder();
    });
