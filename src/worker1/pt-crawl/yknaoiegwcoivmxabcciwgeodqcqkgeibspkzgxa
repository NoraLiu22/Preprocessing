		<script language="javascript">

 jQuery(document).ready(function () {
        jQuery('#modal_signin_form').on('submit', function (e) {
            var jQuerythis = jQuery(this);

            //prevent the form from submitting normally
            e.preventDefault();

            //show the default loading message while the jQuery.post request is sent
            //jQuery.mobile.showPageLoadingMsg();

            var data = jQuerythis.serialize()+'&_='+new Date().getTime()+'&callback=?';

            jQuery.ajax({url: jQuerythis.attr('action'), data: data, dataType: 'jsonp'}).done(function (respObj) {
                //jQuery.mobile.hidePageLoadingMsg();
                if ( respObj.status == 'ok' ) {
                    jQuery('#modal_signin_div').hide();
                    jQuery('#modal_signin_password').val('');
                    jQuery('#modal_signin_error_div').hide();
                    var eventMsg = 'User was logged in successfully.';
                    if (typeof(getAtUser) != 'undefined') {
                        eventMsg = getAtUser();
                    }
                    jQuery.event.trigger({
                        type: "hdnLoginSuccessful",
                        message: eventMsg,
                        time: new Date()
                    });
                } else {
                    // We want to make sure the sign-in form is shown
                    jQuery('#modal_signin_div').show()
                    jQuery( "#modal_signin_error_div" ).html(respObj.message);
                    jQuery( "#modal_signin_error_div" ).show();
                    jQuery( "#modal_signin_data_span" ).html("");
                }
            });
        });
    });
</script>

