                $(document).ready(function() {
                    $("#MathInput").stop(true, true).keyup(function() {
                        $.ajax({
                            url: "/mwsq/",
                            type: "POST",
                            data: { query : $("#MathInput").val() },
                            dataType: "text"
                        })
                        .done(function(xml) {
                            $("#MathPreview").html(xml);
                            $(window).resize();
                        });
