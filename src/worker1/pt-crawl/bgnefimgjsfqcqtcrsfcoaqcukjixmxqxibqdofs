                            <a href="/classification/?q=cc%3A65" title="Mathematics Subject Classification">
                                Numerical analysis&nbsp;<span style="white-space: nowrap;">(65-XX)</span>
                            </a>
                        </td></tr><tr class="more">
                     
                        <td class="number">
                            <a href="/?q=ra%3Ahassouni.yassine+cc%3A74" title="Citing Publications in this Field">1</a>
                        </td>
                        <td class="text">
                            <a href="/classification/?q=cc%3A74" title="Mathematics Subject Classification">
                                Mechanics of deformable solids&nbsp;<span style="white-space: nowrap;">(74-XX)</span>
                            </a>
                        </td></tr><tr class="more">
                     
                        <td class="number">
                            <a href="/?q=ra%3Ahassouni.yassine+cc%3A92" title="Citing Publications in this Field">1</a>
                        </td>
                        <td class="text">
                            <a href="/classification/?q=cc%3A92" title="Mathematics Subject Classification">
                                Biology and other natural sciences&nbsp;<span style="white-space: nowrap;">(92-XX)</span>
                            </a>
                        </td></tr>
        </table>
    </div>


                    <div class="diagram svg-diagram">
                        <h3>Citations by Year</h3>
                        <div class="diagram ci-diagram" aria-label="citations by year"></div>
                        
    <style>
        rect:hover {
            fill: steelblue;
        }
        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }
        .bar {
            fill: steelblue;
        }
        a:hover .bar, a:focus .bar {
            stroke: black;
            fill-opacity: 60%;
        }
    </style>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".ci-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1997, frequency:3 },{ year:1999, frequency:1 },{ year:2000, frequency:2 },{ year:2002, frequency:1 },{ year:2003, frequency:2 },{ year:2004, frequency:1 },{ year:2005, frequency:1 },{ year:2006, frequency:1 },{ year:2008, frequency:1 },{ year:2009, frequency:2 },{ year:2010, frequency:5 },{ year:2011, frequency:10 },{ year:2012, frequency:4 },{ year:2013, frequency:18 },{ year:2014, frequency:10 },{ year:2015, frequency:10 },{ year:2016, frequency:13 },{ year:2017, frequency:11 },{ year:2018, frequency:13 },{ year:2019, frequency:19 },{ year:2020, frequency:21 },{ year:2021, frequency:19 },{ year:2022, frequency:24 },{ year:2023, frequency:14 },{ year:2024, frequency:3 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=ra%3Ahassouni.yassine+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                    </div>
                </div>
            
            
        </article>
    

    </div>
        </div>
    

