        <script type="text/javascript">
                $(document).ready(function() {
                    $("#MathInput").stop(true, true).keyup(function() {
                        $.ajax({
                            url: "/mwsq/",
                            type: "POST",
                            data: { query : $("#MathInput").val() },
                            dataType: "text"
                        })
                        .done(function(xml) {
                            $("#MathPreview").html(xml);
                            $(window).resize();
                        });
                    });
                    var press = jQuery.Event("keyup");
                    press.ctrlKey = false;
                    press.which = 40;
                    $("#MathInput").trigger(press);
                });
            </script>
        <div id="new_tab_icon" style="display: none">&nbsp;<span class="glyphicon glyphicon-new-window" aria-hidden="true"></span><span class="sr-only">(opens in new tab)</span></div>
    </body>
</html> contentType 9 text/html url 47 https://zbmath.org:443/authors/torretti.roberto responseCode 3 200 