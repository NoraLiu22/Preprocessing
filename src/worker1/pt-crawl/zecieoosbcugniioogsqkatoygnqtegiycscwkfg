                $(".flash_page b").html(flash_num);
            });
            var i = 0;
            $(".flash_btn li").eq(0).on("click", function () {
                i -= 1;
                if (i < 0) {
                    i = flash_num - 1;
                };
                $(".flash_img li").eq(i).show().siblings().hide();
                $(".flash_title li").eq(i).show().siblings().hide();
                $(".flash_page span").html(i + 1);
            });
            $(".flash_btn li").eq(1).on("click", function () {
                i += 1;
                if (i == flash_num) {
                    i = 0;
                };
                $(".flash_img li").eq(i).show().siblings().hide();
                $(".flash_title li").eq(i).show().siblings().hide();
                $(".flash_page span").html(i + 1);
            });
            function auto() {
                i += 1;
                if (i == flash_num) {
                    i = 0;
                };
                $(".flash_img li").eq(i).show().siblings().hide();
                $(".flash_title li").eq(i).show().siblings().hide();
                $(".flash_page span").html(i + 1);
            };
            var timer = setInterval(auto, 3000);
            $(".flash_box,.d1_right").hover(function () {
                clearInterval(timer);
            }, function () {
                timer = setInterval(auto, 3000);
            })

