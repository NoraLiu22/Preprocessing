<script language="JavaScript" type="text/javascript">
var za24_AdBaseURL = 'https://web.archive.org/web/20090106044608/http://ad.za.doubleclick.net/adj/'; 
var za24_AdSite = 'P4306.sa.24.com.news24'; 
var za24_AdZone = '/News'; 
var za24_PageUrl = location.href.substring(); 
var za24_FirstAd = 1; 
var za24_TileSeq = 0; 
var za24_AdSize = new Array();
var za24_AdPositionNo = new Array();

var za24_regexp=/.*[0-9]_(.*),.*/g;
var za24_match=za24_regexp.test(za24_PageUrl);
if (za24_match) {
  za24_ArticleParam=';artid='+za24_PageUrl.replace(za24_regexp,'$1'); 
} else {
	za24_ArticleParam=';artid='
}

za24_AdSize[1]='728x90'; za24_AdPositionNo[1]='1'; 
za24_AdSize[2]='728x90'; za24_AdPositionNo[2]='2'; 
za24_AdSize[3]='300x250'; za24_AdPositionNo[3]='1'; 
za24_AdSize[4]='160x600'; za24_AdPositionNo[4]='1'; 
za24_AdSize[5]='120x250'; za24_AdPositionNo[5]='1'; 
za24_AdSize[6]='120x250'; za24_AdPositionNo[6]='2'; 
za24_AdSize[7]='468x120'; za24_AdPositionNo[7]='1'; 

ord=Math.random()*10000000000000000; 

function DisplayDCAd(Size,PosNo,KwPair) {
	if (PosNo=='') {
	  PosNo='1';
	}
	for (var i = 1; i < za24_AdSize.length; i++) {
		if ((za24_AdSize[i]==Size) && (za24_AdPositionNo[i]==PosNo)) {

			DFP='';
			za24_TileSeq=za24_TileSeq+1;
			if (za24_FirstAd==1) {
				za24_FirstAd=0;
				DFP=';dcopt=ist';
			}
			
			
			document.write('<script type="text\/javascript" language="JavaScript" src="' + za24_AdBaseURL + za24_AdSite + za24_AdZone + ';sz=' + Size + ';posno=' + PosNo + ';kw=' + KwPair + za24_ArticleParam + DFP + ';tile=' + za24_TileSeq + ';ord=' + ord +'?"><\/script>');
			

			break;
		}
	}
}
</script>
