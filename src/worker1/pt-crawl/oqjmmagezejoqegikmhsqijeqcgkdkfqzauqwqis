  <article
           class="card recipe-card"
           data-type="recipe"
           data-id="1020596"
           data-url="/recipes/1020596-vegetarian-mushroom-wellington"
           data-uri="nyt://recipe/626806c3-dad0-5927-8355-f69c93145367"
           data-seo-image-url="https://static01.nyt.com/images/2019/11/05/dining/aw-mushroom-wellington/merlin_163633167_473a653a-a909-42e2-94c3-0788f950a163-articleLarge.jpg"
