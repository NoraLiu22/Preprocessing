                  <source srcset="/sites/default/files/styles/denomination_download_thumb/public/downloadable-materials/thumbnails/usc-downloads-brochure-and-poster-100-thumbnail_19.png.webp?itok=_FJ39YHh 1x" type="image/webp" width="240" height="240"/>
