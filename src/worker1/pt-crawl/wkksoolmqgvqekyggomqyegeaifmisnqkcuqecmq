			- function () {
if ($('.page_n a').length < 2) {
                    $('.a_left,.a_right').remove()
                }
				var zhengwenText = $('.picBox .content p').first().html() || $('#picG > p').first().html() || document.title
		console.log('正文是' + zhengwenText)

