<script>
       $(function(){

          //鼠标经过标签页切换
          $('.tab_lis li').mouseover(function(){
               $(this).addClass('tab_active').siblings().removeClass('tab_active');
               var i = $(this).index();
               $('.tab_content div').eq(i).css('display','block').addClass('show').siblings().css('display','none');
           });
		   $('.tab_lis2 li').mouseover(function(){
               $(this).addClass('tab_active2').siblings().removeClass('tab_active2');
               var i = $(this).index();
               $('.tab_content2 div').eq(i).css('display','block').addClass('show').siblings().css('display','none');
           });
       });

</script>
