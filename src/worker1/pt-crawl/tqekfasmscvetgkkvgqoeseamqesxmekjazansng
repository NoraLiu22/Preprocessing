    function onloaderror(message, url, line, col, errorObj) {
      window.caches.keys().then(function(keys) {
        if(keys.length>0){
          for (let i = 0; i < keys.length; i++) {
              window.caches.delete(keys[i]);
          }
          window.location.reload();
        }
      })
    };
