<script type="text/javascript">
"use strict";
$(document).ready(function(){
    var sectionName = location.href.split("#")[1];
    var initIndex = sectionName == "1"?1:0;
	setFocus(initIndex);

	var $buttons = $(".chart-switch-button");
	$buttons.click(function(){
		var idx = $buttons.index($(this));
		setFocus(idx);
	});
});

function setFocus() {	
   var chartData = datas;
   var seriesNames = ["人民网记者遍神州"];
   var options = setupOptions(chartData, seriesNames);
   var myChart = echarts.init($("#chart-1")[0]);
   myChart.setOption(options);
   /*mapclick*/
   //console.log(chartData);
	myChart.on('click', function (params) {
		var city = params.name;
        //console.log(params.name);
		for(var i=0;i<chartData.length;i++){
			if(chartData[i].name == params.name){
				if(chartData[i].urlvalue){
				window.open(chartData[i].urlvalue);
				}
				break;
			}
		}
	});
   /*mapclickend*/
}

function getMaxData(datas){
	var max = 0;
	for (var i = 0; i < datas.length; i++) {
		if (parseInt(datas[i].value) > max) {
			max = parseInt(datas[i].value);
		}
	}
	return Math.ceil(max/5)*5;
}

function setupOptions(datas, seriesName) {
	return {
	    	tooltip : {
				show: false,
	        	trigger: 'item',
				triggerOn: 'click',
				borderColor: '#ffffff',
				backgroundColor: 'rgba(0,0,0,0.6)',
				enterable: true,
				confine: true,
				//position: function (pos, params, dom, rect, size) {
      			//	return [pos[0] + 10, pos[1] + 50];
					// 鼠标在左侧时 tooltip 显示到右侧，鼠标在右侧时 tooltip 显示到左侧。
      				//var obj = {top: 60};
      				//obj[['left', 'right'][+(pos[0] < 500 / 2)]] = 5;
      				//return obj;
				//},
				/*formatter:function (params,ticket,callback) {
					if(params.data.wenzi){
						var res ='<div class=eBg>'+params.data.wenzi+'</div>'
						setTimeout(function (){callback(ticket, res);}, 1500);
						return 'loading...';
					}else{
						return '暂无内容'
					}
				}*/
	    	},
	    	dataRange: {
				show: false,
	        	min: 0,
	        	max: getMaxData(datas),
	        	x: 'left',
	        	y: 'bottom',
	        	text:['高','低'],
				formatter:function(v){
        			return Math.round(v);
			    },
	        	calculable : true,
	      		color:['#9fc1ed', '#9fc1ed'],
	      		precision:0
	    	},
	    	series : [
	        	{
	            	name: seriesName,
	            	type: 'map',
	            	mapType: 'china',
	            	roam: false,
	            	itemStyle:{
	                	normal:{label:{show:true, textStyle:{color:"#333"}}},
	                	emphasis:{label:{show:true}}
	            	},
	            	data:datas
	        	}
	    	]
		};
}
