<script>
    function handleVideo() {
        var $vid = $('#home-video').get(0);
        

        $("#vid-icons").click(function () {
            $('.pause-btn').toggleClass('active');
            $('.play-btn').toggleClass('active');

            if ($('.play-btn').hasClass('active')) {
                $vid.pause();
            } else {
                $vid.play();
            }
        })

        
        $(window).scroll(function () {
            var $scrollTop = $(document).scrollTop();
            if ($('.pause-btn').hasClass('active')) {
                if ($scrollTop <= 500) {
                    if ($vid.paused) {
                        $vid.play();
                        
                    }
                } else {
                    if (!$vid.paused) {
                        $vid.pause();
                    }
                }
            }
        })
    }

    $(document).ready(function () {

        $('#home-video').addClass('videoPlay');

        handleVideo();
    })
</script>





