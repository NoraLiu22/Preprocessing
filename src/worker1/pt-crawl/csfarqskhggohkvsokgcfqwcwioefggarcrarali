    <td width="941" align="left" valign="top" bgcolor="#eaeaea" height="1582"> 
      <div align="left"> 
        <p><font color="#663399" face="Verdana,Arial,Helvetica,sans-serif" size="4"><b>Coaching 
          the Sprinter</b></font></p>
        <p align="center"><font size="2" face="Verdana, Arial, Helvetica, sans-serif">
          <script type="text/javascript"><!--
google_ad_client = "pub-3607693930582854";
google_ad_width = 728;
google_ad_height = 90;
google_ad_format = "728x90_as";
google_ad_type = "text_image";
google_ad_channel ="";
google_color_border = "EAEAEA";
google_color_bg = "EAEAEA";
google_color_link = "0000FF";
