touchsurface.addEventListener('touchstart',function(e){var touchobj=e.changedTouches[0]
swipedir='none'
dist=0
startX=touchobj.pageX
startY=touchobj.pageY
startTime=new Date().getTime()},!1)
touchsurface.addEventListener('touchmove',function(e){},!1)
touchsurface.addEventListener('touchend',function(e){var touchobj=e.changedTouches[0]
distX=touchobj.pageX-startX
distY=touchobj.pageY-startY
elapsedTime=new Date().getTime()-startTime
if(elapsedTime<=allowedTime){if(Math.abs(distX)>=threshold&&Math.abs(distY)<=restraint){swipedir=(distX<0)?'left':'right'}else if(Math.abs(distY)>=threshold&&Math.abs(distX)<=restraint){swipedir=(distY<0)?'up':'down'}}
handleswipe(swipedir)},!1)}
jQuery("body").css("overflow","hidden");var el=document.getElementsByTagName('body')[0];swipedetect(el,function(swipedir){if(swipedir=='left'&&jQuery(".article-navigation .nav-next").length){jQuery(".next-slide").trigger('click');location.href=jQuery(".article-navigation .nav-next a").attr('href')}else if(swipedir=='right'&&jQuery(".article-navigation .nav-prev").length){jQuery(".prev-slide").trigger('click');location.href=jQuery(".article-navigation .nav-prev a").attr('href')}})})})</script> <script>doFir.push(function(){jQuery(document).ready(function(){var selectednewsletter=[];doItFooter=function(val,type){if(selectednewsletter.length>0){if(selectednewsletter.find((er)=>er===val)){$(`#${val}`).prop('checked',!1);selectednewsletter=selectednewsletter.filter((vr)=>vr!==val)}else{$(`#${val}`).prop('checked',!0);selectednewsletter.push(val)}}else{$(`#${val}`).prop('checked',!0);selectednewsletter.push(val)}};jQuery(".select-email-btn-footer").on('click',function(){var a=new Array();jQuery(".select-checkbox-footer").each(function(){if(jQuery(this).prop('checked')){a.push(jQuery(this).val())}});function getUrlParametersFooter(a){var b={},c,d=/=/.test(a),e=(d?a:location.search.substring(1)).split("&");for(p=0;p<e.length;p++)c=e[p].split("="),b[c[0]]=c[1];return!a||d?b:b[a]};function extractHostnameFooter(t){return(t.indexOf("//")>-1?t.split("/")[2]:t.split("/")[0]).split(":")[0].split("?")[0]};var registration_url=window.location.href;var utm_result=getUrlParametersFooter();var st_ref=(document.referrer=="")?"newsweek.com":extractHostnameFooter(document.referrer);var utm_source=(utm_result.utm_source)?utm_result.utm_source:"";var utm_medium=(utm_result.utm_medium)?" | "+utm_result.utm_medium:"";var utm_campaign=(utm_result.utm_campaign)?" | "+utm_result.utm_campaign:"";var st_source=(utm_source)?(utm_source+utm_medium+utm_campaign):st_ref;var form=jQuery("#formFooterNewsletter");var footer_email=jQuery('#newsletter-email-id').val();jQuery(".newsletter-email-footer").on('input',function(){jQuery(this).removeClass('error')});jQuery.ajax({type:"POST",url:'/tool/letter',data:{'email':footer_email,'letter':a,'utm_source':st_source,'registration_url':registration_url,'site_form':'footer-form'},success:function(d){var uemail=footer_email;sendToLiveRamp(uemail);if(d.msg=='success'){jQuery('.newsletter-email-footer').val('');alert("Congratulations, your newsletter preferences have been set. Changes can take 24 hours")}else if(d.msg==='empty_email'||d.msg==='wrong_email'||d.msg==='Invalid email'){{jQuery(".newsletter-email-footer",form).addClass('error');jQuery(".newsletter-email-footer",form).focus()}}else{alert('Please, insert your email to Sign Up.')}},error:function(d){alert("Congratulations, your newsletter preferences have been set. Changes can take 24 hours.")},dataType:'json'})})})})</script> <div class="main-footer"><div class="footer-main-container-bg"><div class="footer-100 footer-tablet-desktop-mix"><div class="footer-logo-and-socials"><div class="footer-logo-nw">
