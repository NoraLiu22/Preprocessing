(function(){            
  if(typeof Drupal.settings['timelineJS'] !== 'undefined'){      
    timelinejs_arry = Drupal.settings['timelineJS'];
    tmptimeline=[];
    for (i = 0; i < timelinejs_arry.length-1; i++) {       
