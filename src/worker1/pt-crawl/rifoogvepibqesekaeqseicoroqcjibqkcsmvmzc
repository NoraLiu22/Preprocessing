googletag.cmd.push(function () {
    var _YB = _YB || {
        ab: function () {
            return _YB.dool ? "b" : "a" + Math.floor(Math.random() * 10);
        },
        dool: Math.random() >= 0.1,
    };
    var yb_ab;
