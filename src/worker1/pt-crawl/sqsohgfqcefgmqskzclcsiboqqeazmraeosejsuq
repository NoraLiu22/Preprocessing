  slidesPerView: 3,
  centeredSlides: true,
  loop : true,
  prevButton:'.swiper-button-prev.s1',
  nextButton:'.swiper-button-next.s1',
  coverflow: {
            rotate: 0,
            stretch: 490,
            depth: 200,
            modifier: 1,
            slideShadows : false
        }
})


