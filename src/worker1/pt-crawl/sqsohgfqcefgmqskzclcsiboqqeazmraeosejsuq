	function auto3(){
		index3 = ( index3 + 1) % num3
        $(".slides3").stop().animate({left:"-" + width3*index3 + "px"},300);
		$(".dian3 li").eq(index3).addClass("hover").siblings(".dian3 li").removeClass("hover");
		$(".right3 li").eq(index3).addClass("hover").siblings(".right3 li").removeClass("hover");
    };
    var timer3=setInterval(auto3,3000);
