            "@type": "NewsArticle",
            "mainEntityOfPage": {
                "@type": "Webpage",
                "url": "https://www.euronews.com/next/2024/04/30/binance-founder-changpeng-zhao-facing-3-year-prison-sentence-for-allowing-money-laundering"
