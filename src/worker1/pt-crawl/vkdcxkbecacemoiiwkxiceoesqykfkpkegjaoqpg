    <script>
    $(document).ready(function () {
        if ($("#ShowSubNav").length > 0) {
            var showNav = $("#ShowSubNav").val();
            if ($("#" + showNav).length > 0) {
                //show the element and parent nav
                $("#" + showNav).css("display", "block");
                $(".subnavbluesub").css("display", "block");
            }
        }
    });
</script>

