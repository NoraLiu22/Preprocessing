<script type="text/javascript">

var defineAdSlotsCalled = false;
var displayAds = false;

//serve ads on mobile on scroll and onload on desktop
if( window.screen.width < 768 ) {
    $(document).scroll(function () {
        if ($(this).scrollTop() + $(window).height()  > 200) {

            load_google_ads_script();
            setTimeout(() => {
                if( !defineAdSlotsCalled ) {
                    define_ad_slots();
                }

                if( !displayAds ) {
                    display_ads();
                }
            }, 1000);

        }
    });
} else {

    setTimeout(() => {
        load_google_ads_script();
    }, 3000);
    setTimeout(() => {
        define_ad_slots();
        display_ads();
    }, 3200);

}

// Load google ads script
function load_google_ads_script() {

    if($('script[src="https://securepubads.g.doubleclick.net/tag/js/gpt.js"]').length < 1){

        // Get the head tag
        var head_ID = document.getElementsByTagName("head")[0];
        // Create script element
        var script_element = document.createElement('script');
        // Set the script type to JavaScript
        script_element.type = 'text/javascript';
        // External JS file
        script_element.src = 'https://securepubads.g.doubleclick.net/tag/js/gpt.js';
        head_ID.appendChild(script_element);
    }
}


function define_ad_slots() {

    defineAdSlotsCalled = true;
    window.googletag = window.googletag || {cmd: []};
    googletag.cmd.push(function() {
            googletag.pubads().setTargeting('staging', ['false']);
            googletag.pubads().setTargeting('postCat', ['australia']);
      googletag.pubads().setTargeting('postSubCat', ['']);
      googletag.pubads().setTargeting('postTag', ['dreaming', 'Eating Out', 'listicle']);
      googletag.pubads().setTargeting("cat_url", "/australia/best-high-teas-in-hotels/");
            googletag.pubads().setTargeting("parent_category", "australia");
      googletag.pubads().setTargeting("gparent_category", "australia");
            /*Wallpaper*/
      if( window.screen.width >= 1900 ) {
        if( $('#div-gpt-ad-1439274676992-0').length ) {
            googletag.defineSlot('/1050053/AT_wallpaper', [1900, 1000], 'div-gpt-ad-1439274676992-0').addService(googletag.pubads());
        }
      }

      /*Half page ad*/
      if( $('#div-gpt-ad-1463016610656-0').length ) {
        googletag.defineSlot('/1050053/AT-halfpage-ad', [300, 600], 'div-gpt-ad-1463016610656-0').addService(googletag.pubads());
      }

      /* MREC Ad*/
      if( $('#div-gpt-ad-1535418044998-0').length ) {
        googletag.defineSlot('/1050053/300x250_SideSlot', [300, 250], 'div-gpt-ad-1535418044998-0').addService(googletag.pubads());
      }

      /*Leader board*/
      if( $('#div-gpt-ad-1535414759216-0').length ) {
        googletag.defineSlot('/1050053/728_x_90_Top_Banner', [728, 90], 'div-gpt-ad-1535414759216-0').addService(googletag.pubads());
      }

      /* 970 X 250 */
      if( $('#div-gpt-ad-1535416871812-0').length ) {
        googletag.defineSlot('/1050053/AT_970X250', [970, 250], 'div-gpt-ad-1535416871812-0').addService(googletag.pubads());
      }

      /*Left wing*/
      if( $('#div-gpt-ad-1471835376688-0').length ) {
        googletag.defineSlot('/1050053/AT-left-Sky-scrapper', [160, 600], 'div-gpt-ad-1471835376688-0').addService(googletag.pubads());
      }

      /*Right wing*/
      if( $('#div-gpt-ad-1471836111806-0').length ) {
        googletag.defineSlot('/1050053/AT-right-sky-scrapper', [160, 600], 'div-gpt-ad-1471836111806-0').addService(googletag.pubads());
      }

      googletag.pubads().setTargeting('postID', ['237154']);
      googletag.enableServices();
    });
}

function display_ads() {

    displayAds = true;
    googletag.cmd.push(function() {

      /*Wallpaper*/
      if( window.screen.width >= 1900 ) {
          if( $('#div-gpt-ad-1439274676992-0').length ) {
            googletag.cmd.push(function() { googletag.display('div-gpt-ad-1439274676992-0'); });
          }
      }

      /*half page ad*/
      if( $('#div-gpt-ad-1463016610656-0').length ) {
        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1463016610656-0'); });
      }

      /*MREC ad*/
      if( $('#div-gpt-ad-1535418044998-0').length ) {
        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1535418044998-0'); });
      }

      /*Top leader board*/
      if( $('#div-gpt-ad-1535414759216-0').length ) {
        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1535414759216-0'); });
      }

      /* 970 X 250 */
      if( $('#div-gpt-ad-1535416871812-0').length ) {
        googletag.cmd.push(function() { googletag.display('div-gpt-ad-1535416871812-0'); });
      }


      if( window.screen.width >= 1440 ) {
        /*Right wing*/
        if( $('#div-gpt-ad-1471836111806-0').length ) {
            googletag.cmd.push(function() { googletag.display('div-gpt-ad-1471836111806-0'); });
        }

        /*Left wing*/
        if( $('#div-gpt-ad-1471835376688-0').length ) {
            googletag.cmd.push(function() { googletag.display('div-gpt-ad-1471835376688-0'); });
        }
      }

      googletag.pubads().enableSingleRequest();
      googletag.enableServices();
    });
}

</script>
