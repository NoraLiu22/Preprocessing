<script>
  function toggleToolbar(event) {
    if (event && event.target) {
      const adToolbar = event.target.closest('.ad-toolbar');

      if (adToolbar.classList.contains('open')) {
        adToolbar.classList.replace('open', 'close');
      } else if (adToolbar.classList.contains('close')) {
        adToolbar.classList.replace('close', 'open');
      } else {
        adToolbar.classList.add('close');
      }
    }
  }
</script>

<div data-module="ad-toolbar" id="BF_WIDGET_83" data-bfa="@l:Tolbar;">
  <div class="ad-toolbar loading ad-toolbar-bfn">
    <div class="ad-toolbar__arrow" onclick="toggleToolbar(event)">
      <svg width="15px" viewBox="0 0 38 38">
  <title>{title}</title>
  <path d="M11.5 36c-.5 0-1-.2-1.4-.6-.8-.8-.8-2 0-2.8L23.7 19 10.1 5.4c-.8-.8-.8-2 0-2.8.8-.8 2-.8 2.8 0L29.3 19 12.9 35.4c-.4.4-.9.6-1.4.6z"></path>
</svg>
    </div>
    <a href="https://www.buzzfeednews.com?origin=tb" class="ad-toolbar__images--bfn">
