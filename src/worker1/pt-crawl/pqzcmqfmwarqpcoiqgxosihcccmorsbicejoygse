const _gam_mapaps = '#####k0zk#a0hs#9u1hcn08w#e718gx0qo#5h1q8ti4g#ydj403im8#pnjlsgidc#u0jcwqiv4#lajuowr28#qgs1s6rk0#hqsjkjrb4#m3saotrsw#ddssgq96o#6aa6809og#xkao0d9fk#1xaf4n9xc#t7awwydj4#mieio8e0w#dsf0glds0#i5erkve9s#9ff9crvnk#2bwn41w5c#tlx4wevwg#xyww0owe8#p8xdsv4lc#uf5kw5534#lp62oi4u8#q25tss5c0#hc6bkomps#a8npcyn7k#1io74bmyo#5vny8lngg#x5og06rk##2j7r4979c#tt88wm70g#y6800w7i8#pg8hssow0#icpvk2pds#9mqdcfp4w#dzq4gppmo#59qm8vxts#afytc5ybk#1pzb4iy2o#62z28sykg#xczk0pfy8#q9gxszgg0#hjhfkcg74#lwh6omgow#d6hogxkao#6hla87ksg#xrls0kkjk#24lj4ul1c#tem0wr2f4#mb3eo12ww#dl3wge2o0#hy3nko35s#9845cubcw#eeccg4buo#5ocu8hbls#a1clcrc3k#1bd34nthc#u7ugwxtz4#lhuyoatq8#puupsku80#h4v7klds##jmdc#9lvk#jtmv4mlmo#o6mm8wm4g#fgn40t3i8#8d4hs3400#zn4zkg3r4#404qoq48w#va58gwcg0#0gdfk6cxs#rqdxcjcow#w3dogtd6o#nde68pukg#g9vk0zv28#7jw1scutc#bwvswmvb4#36waoxyww#whzwg7zeo#ns0e8kz5s#s505cuznk#jf0n4rh1c#cbi0w1hj4#3liioeha8#7yi9sohs0#z8irkupz4#4eqyo4qgw#vorgghq80#01r7krqps#rbrpco83k#k8934y8lc#bi9kwb8cg#fv9c0l8u8#759tss5c##cit4w8sn4#3stmolse8#85tdsvsw0#zftvksa9s#scb9c2ark#jmbr4faio#nzbi8pb0g#f9c00vj7k#kfk745jpc#bpkowijgg#g2kg0sjy8#7ckxsp1c0#092bkz1ts#rj2tcc1kw#vw2kgm22o#n6328x5og#gh6o07668#7r75sk5xc#c46wwu6f4#3e7eoqnsw#waosg0oao#nkpa8do1s#rxp1cnojk#j7pj4twqo#odxq83x8g#fny80gwzk#k0xz4qxhc#baygwnev4#47fuoxfcw#vhgcgaf40#zug3kkfls#r4glcsow##jtog#9t6o#etu68msxs#j6txcwtfk#aguf4tatc#3dbsw3bb4#uncaogb28#z0c1sqbk0#qacjkwjr4#vgkqo6k8w#mql8gjk00#r3kzktkhs#idlhcq1vk#ba2v402dc#2k3cwd24g#6x340n2m8#y73lsy680#ri77k86ps#is7pcl6gw#n57ggv6yo#ef7y8rocg#7bpc01ou8#ylptseolc#zey9sry0w#6igw0zgg##360ow2i2o#a9jb4iqrk#v99mom9ds#2regwqv40#mxwcg44jk#6b5s0amf4#jf28#wetc#4mxogw54w#136dcznr4#86ozkkruo#hbapsoagw#0ok5co0sg#kv20w8lc0#4fmkgf37k#pfcw0scn4#wivi8wydc#h3zls0gzk#o7i80gpog#978jkk8ao#cbxtsbegw#wifpconwg#fvp4wv5s0#p0av4dam8#w3thcd0xs#sk268gjk0#znksg43cw#kum807lz4#47vnk7cao#oedj4ph4w#vwidcvz0g#gw8ow98g0#nzrb4hq8##qoiyol0cg#xs1kw191c#irrwg4rnk#q9wqo9dds#agem8mmtc#tto1st4ow#f0phcdp8g#m483kdfk0#ikgsggy68#pnzeo229s#ysl4w5kw0#i5ukg5b7k#2ccg0rpj4#cyqrky7eo#xyh34bgu8#51zpcg2kg#hqcqonncw#l2sxs4ao0#jeku89fr4#mr11c8cu8#sz5s#97uo#my48wonb4#3djpctse8#omccg3o5c#tl9fkg4cg#wqeiosi68#47nr44ydc#jf8jkwkcg#6p4aoa8e8#a1khsksg0#q9ou8hdds#d69s01a8##u0bggv6dc#3dzi8btog#jm3uojegw#myk1sk5c0#v91q8paf4#agmio4pvk#wx9tsdiio#02ewwm6f4#whkw#4lxc#h24g#cbnk#oruo#ww74#b668#ju2o#smps#8268#d79c#w934#3tvk#kh6o#pm9s#ya68#ck5c#n474#0s8w#se80#8oow#0n4##fl6o#nwu8#9p8g#a2o0#un7k#2cxs#or9c#b37k#y8sg#2ayo#qj28#65ts#udxc#yg3k#5l34#x1c##ebcw#m134#';
function _gam_kv_(s) {
    function pbtoval(a) {
      if (a.length === 0) return 0;
      return parseFloat(a[0]);
    }
    function apstoval(a) {
      if (a.length === 0) return 0;
      a=a[0].replace("amp_","").replace("v_","").substring(2,7).padEnd(5,'#');
      var pos = _gam_mapaps.indexOf(a);
      if (pos === -1) {
          return -1;
      }
      var order = Math.floor(pos / 5);
      return Math.min(order, 300)*0.01+
          Math.max(Math.min(order - 300, 100), 0)*0.05+
          Math.max(Math.min(order - 400, 48), 0)*0.25+
          Math.max(Math.min(order - 448, 20), 0)*0.5+
            Math.max(order - 468, 0)*1;
    }
    function valtosj(n) {
      if (n < 0.5) return 0;
      if ((0.5 <= n) && (n <= 1)) return 1;
      if (n >= 7) return 4;
      return 1 + Math.floor((n - 1) / 2)
    }
    
    s.clearTargeting('sj');
    s.clearTargeting('rndp');
    try {
      const t_pb = pbtoval(s.getTargeting('hb_pb'));
      const t_aps = apstoval(s.getTargeting('amznbid'));
      const m = Math.max(t_pb, t_aps);
      const random1 = googletag.pubads().getTargeting('random1')[0];
      if (m > 0) {
        s.setTargeting('rndp', m.toFixed(2));
        if ((random1 !== '19') && (random1 !== '20')) {
          s.setTargeting('sj', valtosj(m).toString());
        }
      } else {
        s.setTargeting('rndp', '-1');
        if ((random1 !== '19') && (random1 !== '20')) {
          s.setTargeting('sj', '-1');
        }
      }
    } catch (e) { }
}

class GAMExp {

  request(data) {

    try {
      const f = this.#f();
      if (f === 1) {
        let s = this.#getslot(data);
        this.#current_resultado = (0.75 + 0.25 * Math.floor(Math.random() * 6)).toString();
        s.customTargeting.gexp_floor = [this.#current_resultado.toString()];
      } else if (f === 2) {
        let s = this.#getslot(data);
        s.customTargeting.gexp_result = [this.#info.resultado.toString()];
      }
    } catch (error) {
      console.error(error);
    }
  }


  response(event) {

    try {
      const slot = event.slot;
      if (!slot.getTargeting('p').includes(this.#p)) {
        return;
      }
      
      if (slot.getTargeting('gexp_floor').length !== 0) {
        slot.clearTargeting('gexp_floor');
      }

      const f = this.#f();
      if (f === 1) {
        // advertiserId campaignId lineItemId
        if (this.#adexIds.includes(event.campaignId)) {
          this.#info.resultado = this.#current_resultado;
          this.#info.fecha_resultado = this.#today();
          this.#info.fecha = this.#tomorrow();
          this.#save();
        } else if (this.#houseIds.includes(event.advertiserId)) {
          this.#info.resultado = 0;
          this.#info.fecha_resultado = this.#today();
          this.#info.fecha = this.#nexttest();
          this.#save();
        }
      }
    } catch (error) {
      console.error(error);
    }
  }

  #f() {
    this.#load();
    if (this.#info === null) {
      this.#info = {};
      this.#info.fecha = this.#today();
      this.#save();
      return 1;
    }

    if (typeof this.#info.resultado === 'undefined') {
      return this.#checkfecha();
    } else {
      if (this.#info.fecha_resultado === this.#today()) {
        return 2;
      } else {
        delete this.#info.fecha_resultado;
        delete this.#info.resultado;

        this.#save();

        return this.#checkfecha();
      }
    }
  }

  #load() {
    this.#info = JSON.parse(localStorage.getItem(this.#itemName));
  }

  #save() {
    localStorage.setItem(this.#itemName, JSON.stringify(this.#info));
  }

  #today() {
    return this.#datetostring(new Date());
  }

  #tomorrow() {
    let d = new Date();
    d.setDate(d.getDate() + 1);
    return this.#datetostring(d);
  }

  #nexttest() {
    let d = new Date();
    d.setDate(d.getDate() + 7);
    return this.#datetostring(d);
  }

  #checkfecha() {
    if (typeof this.#info.fecha === 'undefined') {
      this.#info.fecha = this.#today();
      this.#save();
      return 1;
    } else {
      if (this.#info.fecha === this.#today()) {
        return 1;
      } else {
        return 0;
      }
    }
  }

  #datetostring(d) {
    return d.getFullYear().toString() + (d.getMonth() + 1).toString().padStart(2, '0') + d.getDate().toString().padStart(2, '0');
  }

  #getslot(data) {
    return data.adSlots.find((e) => {
      return e.customTargeting.p.includes(this.#p)
    });
  }

  #itemName = 'GAMExp';
  #p = 'rb';
  #info;
  #adexIds = [2559776610];
  #houseIds = [45125937, 81003777, 4815141041];
  #current_resultado = '';
}

let _gam_exp = new GAMExp();

    googletag.cmd.push(function () {
        data.customTargeting.t = ['default'];
data.customTargeting.pl = ['stndr'];
