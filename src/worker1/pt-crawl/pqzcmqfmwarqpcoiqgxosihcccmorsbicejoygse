            }else{
                googletag.pubads().setRequestNonPersonalizedAds(1);
            }
            if (typeof IsAPSEnabled !== 'undefined' && IsAPSEnabled) {
                amz();
            }else{
                googletag.pubads().refresh();
            }
        });
    }, false);
}
