        var ht1 = $(this).next('div.hide').html();
        $('.f1').attr('src', ht1);
        $(this).parents('div.img-wh').css('display', 'none');
        $('.f1').css('display', 'block');
        $('.close').css('display', 'block')
    });
    $('.close').click(function () {
        $('.img-wh').css('display', 'block');
        $('.f1').attr('src', '');
        $('.f1').css('display', 'none');
        $('.close').css('display', 'none')
    });
</script></html> contentType 9 text/html url 31 http://csj.news.cn:80/index.htm responseCode 3 200 