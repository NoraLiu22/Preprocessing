function exitPollsFormSubmit(option) {
	
	
	if ( option == "pres" ) {
		var masterSelect = document.forms['exitPolls'].presList;
	}  if ( option == "governor" ) {
		var masterSelect = document.forms['exitPolls'].governorList;
	}  if ( option == "senate" ) {
		var masterSelect = document.forms['exitPolls'].senateList;
	}  if ( option == "house" ) {
		var masterSelect = document.forms['exitPolls'].houseList;
	}  if ( option == "ballot" ) {
		var masterSelect = document.forms['exitPolls'].ballotList;
	}  if ( option =="other" ) {
		var masterSelect = document.forms['exitPolls'].otherList;
	}
	
	if ( ( masterSelect.selectedIndex > 1 ) && ( masterSelect.options[masterSelect.selectedIndex].value != '' ) ) {
		location.href = "/ELECTION/2006/pages/results/states/" + masterSelect.options[masterSelect.selectedIndex].value;
	}
	return true;
}


function exitPollsRaceListHandler() {
	var governorSelect = document.forms['exitPolls'].governorList;
	var senateSelect = document.forms['exitPolls'].senateList;
	var houseSelect = document.forms['exitPolls'].houseList;
	var ballotSelect = document.forms['exitPolls'].ballotList;
	
	if ( document.senatePolls && document.senatePolls.length ) {
					addSelectOptions( senateSelect, document.senatePolls );
				} else {
					senateSelect.options[senateSelect.options.length] = new Option( "No exit polls yet", '' );
				}
				
	if ( document.housePolls && document.housePolls.length ) {
					addSelectOptions( houseSelect, document.housePolls );
				} else {
					houseSelect.options[houseSelect.options.length] = new Option( "No exit polls yet", '' );
				}
	
	if ( document.governorPolls && document.governorPolls.length ) {
					addSelectOptions( governorSelect, document.governorPolls );
				} else {
					governorSelect.options[governorSelect.options.length] = new Option( "No exit polls yet", '' );
				}	
	
	if ( document.ballotPolls && document.ballotPolls.length ) {
					addSelectOptions( ballotSelect, document.ballotPolls );
				} else {
					ballotSelect.options[ballotSelect.options.length] = new Option( "No exit polls yet", '' );
				}
		
	
}

function addSelectOptions( selectObj, arrayOfTextAndValue ) {
	for ( var i = 0; i < arrayOfTextAndValue.length; i = i + 2 ) {
		selectObj.options[selectObj.options.length] = new Option( arrayOfTextAndValue[i], arrayOfTextAndValue[i+1] );
	}
	
	
}
//-->

	</script>
	
