<a id="elink"><img id="ethumbnail"></a> 
<script>




// Months start at zero (January = 0, etc.)
//Code below explained. If current date at time of page load is after or equal to the date in parentheses, then the code in the {} will write. Else the code below will write instead. href links to e-edition. src points image. The 2 code sets point to the current thumbnail and e-edition, and to the scheduled thumbnail and e-edition.

if (new Date() >= new Date(2024, 4, 2)){
    
var a = document.getElementById('elink')
    a.setAttribute('href', "https://issuu.com/stlamerican/docs/may_2nd_2024_e-edition_smaller?fr=xKAE9_zU1NQ") 
    a.setAttribute('target', "_blank") 
    var img = document.getElementById('ethumbnail')
    img.setAttribute('src', "https://stlamerican.com/content/tncms/assets/v3/editorial/6/c1/6c165316-0820-11ef-a224-f3c15af11b57/6632e78ebe3e1.image.jpg") 
    img.setAttribute('width', "300px") 
    
    }

else {

    var a = document.getElementById('elink')
    a.setAttribute('href', "https://issuu.com/stlamerican/docs/april_25th_2024_e-edition_smaller?fr=xKAE9_zU1NQ")
    a.setAttribute('target', "_blank")
    var img = document.getElementById('ethumbnail')
    img.setAttribute('src', "https://stlamerican.com/content/tncms/assets/v3/editorial/0/b9/0b9aa0fa-029e-11ef-bd56-8ba93cf274ff/6629a96aede0f.image.jpg")
    img.setAttribute('width', "300px") 
}  
   
 
</script>
