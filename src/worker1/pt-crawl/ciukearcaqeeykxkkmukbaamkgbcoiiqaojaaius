<script type="text/javascript">
    // 滚动字幕
    (function ($, window) {
        var liveSum = $('.roll li').size(),
            liveW = 0,
            liveCur = 0,
            liveAuto = null,
            liveArr = [0];

        $('.roll ul').append($('.roll li').clone());
        var len = $('.roll li').eq('1').position().left;

        function liveRoll() {
            liveCur += 1;
            $('.roll ul').stop(true, true).animate({
                'margin-left': -liveCur
            }, 'slow', function () {
                if (liveCur >= len) {
                    $('.roll ul').css('margin-left', '0');
                    liveCur = 0;
                }
            });
        }

        liveAuto = window.setInterval(liveRoll, 20);
        $('.roll').on('mouseenter', function () {
            window.clearInterval(liveAuto);
        }).on('mouseleave', function () {
            liveAuto = window.setInterval(liveRoll, 20);
        });
        // 专题
        zhuantiFunc();
    })(window.jQuery, window);
    $('.str4').liMarquee({
        scrollamount: 100
    });
    // 专题
    function zhuantiFunc() {
        var offset = 0;
        var timer;

        $('#demo1').append($('#demo1 a').clone());
        function autoPlay() {
            timer = setInterval(function () {
                offset -= 1
                if (offset <= -1084) {
                    offset = 0
                }
                $('#demo1').css('margin-left', offset)
            }, 10)
        }
        autoPlay();
        $('#demo1 a').hover(function () {
            clearInterval(timer)
        }, function () {
            autoPlay();
        })
    }
</script>
