<script type="text/javascript">
(function($) {
/*滚动新闻*/
var demo = $("#demo")[0];
var demo1 = $("#demo1")[0];
var demo2 = $("#demo2")[0];
demo2.innerHTML=demo1.innerHTML;
function Marquee(){
    if(demo.scrollLeft-demo2.offsetWidth>=0){
        demo.scrollLeft-=demo1.offsetWidth;
    }
    else{
        demo.scrollLeft += 1;
    }
}
var myvar=setInterval(Marquee,30);
demo.onmouseout=function (){myvar=setInterval(Marquee,30);};
demo.onmouseover=function(){clearInterval(myvar);};

   var speed2=50
   demo5.innerHTML=demo4.innerHTML
   function Marquee2(){
   if(demo5.offsetTop-demo3.scrollTop<=0)
   demo3.scrollTop-=demo4.offsetHeight
   else{
   demo3.scrollTop++
   }
   }
   var MyMar2=setInterval(Marquee2,speed2)
   demo3.onmouseover=function() {clearInterval(MyMar2)}
   demo3.onmouseout=function() {MyMar2=setInterval(Marquee2,speed2)}
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('#slide_1 ul').size();

    function changePic() {
        $('#slide_1 ul').hide();
        $('#slide_1 ul').eq(picCur).fadeTo(300, 1);
        $('#menu_1 span').removeClass('cur');
        $('#menu_1 span').eq(picCur).addClass('cur');
    };

    $('#slide_1 ul').first().show();
    $('#menu_1 span:first()').addClass('cur');

    $('#menu_1 span').on('click', function () {
        picCur = $(this).index();
        changePic();
    });
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('#slide_2 ul').size();

    function changePic() {
        $('#slide_2 ul').hide();
        $('#slide_2 ul').eq(picCur).fadeTo(300, 1);
        $('#menu_2 span').removeClass('cur');
        $('#menu_2 span').eq(picCur).addClass('cur');
    };

    $('#slide_2 ul').first().show();
    $('#menu_2 span:first()').addClass('cur');

    $('#menu_2 span').on('click', function () {
        picCur = $(this).index();
        changePic();
    });
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('#picpart_1 li').size();

    function changePic() {
        $('#picpart_1 li').hide();
        $('#picpart_1 li').eq(picCur).fadeTo(300, 1);
    };

    $('#picpart_1 li').first().show();
    $('#rarr_1').on('click', function() {
        picCur += 1;
        picCur %= picSum;
        changePic();
    });

    $('#larr_1').on('click', function() {
        picCur -= 1;
        picCur += picSum;
        picCur %= picSum;
        changePic();
    });
})(jQuery);
