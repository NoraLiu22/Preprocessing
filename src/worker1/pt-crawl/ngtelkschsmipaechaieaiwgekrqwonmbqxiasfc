	var groupSubscriptions = document.querySelector('.group-subscriptions')
	var groupSubscriptionsArrow = document.querySelector('.group-subscriptions .arrow polygon')
	groupSubscriptions.addEventListener('mouseover', function() {
		groupSubscriptionsArrow.style.fill = "#fff"
	})
	groupSubscriptions.addEventListener('mouseout', function() {
		groupSubscriptionsArrow.style.fill = "#0F6CC9"
		groupSubscriptionsArrow.style.transition = "0.3s"
	})

	var footerCTA = document.querySelector('.footer-cta')
	footerCTA.addEventListener('mouseover', function() {
		footerCTA.style.border = '1px solid #0F6CC9'
		footerCTA.style.background = '#fff'
		footerCTA.style.color = '#0F6CC9'
	})
	footerCTA.addEventListener('mouseout', function() {
		footerCTA.style.color = '#fff'
		footerCTA.style.background = '#0F6CC9'
	})
	</script>

	<script>
		var afrPackages = [
			{
				name: 'premium digital',
				brand: 'afr',
				id: 'afr_premium_digital_weekly',
				price: '29.50',
				list_position: '1',
				variant: 'weekly'
			}
		]

		var otherSubscriptionPackages = [
			{
				name: 'other subscriptions',
				brand: 'afr',
				id: 'afr_other_subscriptions',
				price: '',
				list_position: '',
				variant: ''
			}
		]

		function trackOtherSubscriptions (data) {
			data = otherSubscriptionPackages[0]
			console.log('other subscription packages', otherSubscriptionPackages[0])
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'Product Impression',
				'eventLabel': promote_channel,
				'eventValue': 0
			})
			dataLayer.push({
				'ecommerce': {
				'currencyCode': 'AUD',
					'impressions': [
					{
						'name': data.name,
						'id': data.id,
						'price': data.price,
						'brand': data.brand,
						'variant': data.variant,
						'list_position': data.list_position
					}]
				}
			});
		}

		function trackUniDays (data) {
			data = afrPackages[0]
			console.log('package 1 weekly: ', afrPackages[0])
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'addToCart',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
				'event': 'addToCart',
				'currencyCode': 'AUD',
						'add': [
						{
							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
					}
				});
		}

		function getParameterByName (name, url) {
			if (!url) url = window.location.href;
			name = name.replace(/[\[\]]/g, '\\$&');
			var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
					results = regex.exec(url);
			if (!results) return null;
			if (!results[2]) return '';
			return decodeURIComponent(results[2].replace(/\+/g, ' '));
		}

	</script>

