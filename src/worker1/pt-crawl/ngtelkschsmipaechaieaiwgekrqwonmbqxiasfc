	<script>
		function mediaQueryDesktop(desktopWidth) {
			if (desktopWidth.matches) {
				document.querySelector('.benefits-text-1').innerHTML = 'Any<br>Device'
				document.querySelector('.benefits-text-3').innerHTML = 'Today\'s<br>Paper'
			} else {
				document.querySelector('.benefits-text-1').innerHTML = 'Any Device'
				document.querySelector('.benefits-text-3').innerHTML = 'Today\'s Paper'
			}
		}

		var desktopWidth = window.matchMedia("(max-width: 480px)")
		mediaQueryDesktop(desktopWidth)
		desktopWidth.addListener(mediaQueryDesktop)
	</script>

	<script>
	// var trackOtherSubscriptions = document.querySelectorAll('.other-subscriptions')
