	<script>
		function getParameterByName (name, url) {
			if (!url) url = window.location.href;
			name = name.replace(/[\[\]]/g, '\\$&');
			var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
					results = regex.exec(url);
			if (!results) return null;
			if (!results[2]) return '';
			return decodeURIComponent(results[2].replace(/\+/g, ' '));
		}
		var promote_channel = getParameterByName('promote_channel') || "none"
		var PROMOTE_CHANNEL
		if (promote_channel !== "none") {
			PROMOTE_CHANNEL = "&promote_channel=" + promote_channel
		} else {
			PROMOTE_CHANNEL = ""
		}
	</script>

	<script>
		var deviceType
		var md = new MobileDetect(window.navigator.userAgent)
		var isMobile = md.phone() || navigator.userAgent.match(/iPhone|iPad|iPod/i)
		var isTablet =  md.tablet()
		if (isMobile) {
			deviceType = 'mobile'
		} else if (isTablet) {
			deviceType = 'tablet'
		} else {
			deviceType = 'desktop'
		}
	</script>

