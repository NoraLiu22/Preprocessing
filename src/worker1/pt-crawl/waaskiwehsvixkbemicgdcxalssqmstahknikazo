var myNavBar1 = new NavBar(0);
var dhtmlMenu;

//define menu items (first parameter of NavBarMenu specifies main category width, second specifies sub category width in pixels)
//add more menus simply by adding more "blocks" of same code below

dhtmlMenu = new NavBarMenu(120, 0);
dhtmlMenu.addItem(new NavBarMenuItem("Home", "../index.html"));
dhtmlMenu.addItem(new NavBarMenuItem("About the Site", "../about.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Frequently Asked Questions", "../faqs.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Search the Site", "../search.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Site Map", "../sitemap.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Message Board", "http://b4.boards2go.com/boards/board.cgi?user=kiefer"));
dhtmlMenu.addItem(new NavBarMenuItem("How To Navigate the Site", "../navigation.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Recent Acquisitions", "../acquisitions.html"));
dhtmlMenu.addItem(new NavBarMenuItem("'What is Past is Prologue'", "../past_prologue.html"));
myNavBar1.addMenu(dhtmlMenu);

dhtmlMenu = new NavBarMenu(240, 244);
dhtmlMenu.addItem(new NavBarMenuItem("Protest on the Homefront", "../protest/index.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Martin Luther King, Jr.", "../protest/homepage_king.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Muhammad Ali", "../protest/homepage_ali.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Julian Bond", "../protest/homepage_bond.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Stokely Carmichael and SNCC", "../protest/homepage_carmichael.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Malcolm X and the Nation of Islam", "../protest/homepage_malcolmx.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Eldridge Cleaver and the Black Panthers", "../protest/homepage_cleaver.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Civil Rights Organizations", "../protest/homepage_civilrights.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Early and International Protest", "../protest/homepage_early.html"));
dhtmlMenu.addItem(new NavBarMenuItem("'Subversive Influences'", "../protest/homepage_subversive.html"));
dhtmlMenu.addItem(new NavBarMenuItem("African-Americans in Politics", "../protest/homepage_politics.html"));
dhtmlMenu.addItem(new NavBarMenuItem("The Draft", "../protest/homepage_draft.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Public Opinion", "../protest/homepage_publicopinion.html"));
myNavBar1.addMenu(dhtmlMenu);

dhtmlMenu = new NavBarMenu(200, 204);
dhtmlMenu.addItem(new NavBarMenuItem("Those That Served", "../served/index.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Congressional Medal of Honor", "../served/homepage_cmoh.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Racial Tensions in the Military", "../served/homepage_tensions.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Prisoners of War (POWs)", "../served/homepage_pows.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Deserters and Collaborators", "../served/homepage_deserters.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Casualty Rates", "../served/homepage_casualty.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Veterans' Issues", "../served/homepage_vetsissues.html"));
dhtmlMenu.addItem(new NavBarMenuItem("'We, Too, Are Americans'", "../served/homepage_wetoo.html"));
dhtmlMenu.addItem(new NavBarMenuItem("G.I. Publications", "../served/homepage_gipubs.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Military Racial Policy", "../served/homepage_policy.html"));
dhtmlMenu.addItem(new NavBarMenuItem("'Our Men in Vietnam'", "../served/homepage_ourmen.html"));
myNavBar1.addMenu(dhtmlMenu);

dhtmlMenu = new NavBarMenu(180, 186);
dhtmlMenu.addItem(new NavBarMenuItem("Featured Highlights", "../special_features/index.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Speeches and Sounds", "../special_features/homepage_speeches.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Poetry, Fiction, and Drama", "../special_features/homepage_pofidr.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Political Cartoons", "../special_features/homepage_cartoons.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Personal Accounts", "../special_features/homepage_accounts.html"));
dhtmlMenu.addItem(new NavBarMenuItem("CIA, FBI, and Other Government Documents", "../special_features/homepage_govdocs.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Films, TV, and Music", "../special_features/homepage_ftm.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Letters", "../special_features/homepage_letters.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Posters, Buttons, Dolls, Videogames, etc.", "../special_features/homepage_pobudo.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Overviews, Historiography, and Reference Works", "../special_features/homepage_overviews.html"));
dhtmlMenu.addItem(new NavBarMenuItem("African-American Women", "../special_features/homepage_women.html"));
dhtmlMenu.addItem(new NavBarMenuItem("'An International Struggle'", "../special_features/homepage_international_struggle.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Famous African-Americans", "../special_features/homepage_famous.html"));
dhtmlMenu.addItem(new NavBarMenuItem("Reporting the War", "../special_features/homepage_reporting.html"));
myNavBar1.addMenu(dhtmlMenu);



//set menu colors
myNavBar1.setColors("#000000", "#000000", "#808080", "#ffffff", "#000000", "#000000", "#808080", "#ffffff", "#336699")

//uncomment below line to center the menu (valid values are "left", "center", and "right"
//myNavBar1.setAlign("center")

var fullWidth;

function init() {

  // Get width of window, need to account for scrollbar width in Netscape.

  fullWidth = getWindowWidth() 
    - (isMinNS4 && getWindowHeight() < getPageHeight() ? 16 : 0);

  myNavBar1.resize(750);
  myNavBar1.create();
  myNavBar1.setzIndex(2);
  myNavBar1.moveTo(10,120);
}
</script>
