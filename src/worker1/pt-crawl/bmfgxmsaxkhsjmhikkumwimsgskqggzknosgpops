		<script>
			/*截取url参数。转化成obj形式*/
			function getParams() {
				var url = decodeURI(window.location.href.split('#')[0]);
				var theRequest = {};
				var start = url.indexOf("?");
				// var start = url.lastIndexOf("?");// ios现在是两个问号
				if (start !== -1) {
					var str = url.substr(start + 1);
					var strs = str.split("&");
					for (var i = 0; i < strs.length; i++) {
						theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
					}
				}
				return theRequest;
			}
			
			var params = getParams();
			var articleId = params['articleId'];   
			if(articleId=='28f84e45ece54b40a21c87f298d10731'){
				location.href="../lianghui2022/index.html";
			}
			var isPc = false;
			if (window.innerWidth > 750) {
				isPc = true;
			}
			function initCss(){
				var cssArr=[];           
				if(articleId=="39d381f1a37844ffbaae3048f1d2bb2c" || articleId=='9019ec0f52c54591b72c2338085c35c4' || articleId=="cd8187659512495f98e2656678a8a0e0"||
				articleId=='00298fe947e144288ab71606d3c9877b'||articleId=='1d66f9883ca042a489fc82979a2f2e75'||articleId=='d675d28c6542434a81a8d1ebea4b7679'||
				articleId=='c8e2abcaf67546fd951fa56f78b9d7ad'||articleId=='8597ad23a2b24caea5aaffaf445e67cb'||articleId=='d17d9be492eb4ded9c24b12a7f0d17d4'){
					cssArr.push('./styles/ssgs2022.css?04');
				}
				for(var i=0;i<cssArr.length;i++){
					var link = document.createElement("link");
					link.rel = "stylesheet";
					link.type = "text/css";
					link.href = cssArr[i];
					document.getElementsByTagName("head")[0].appendChild(link);
				}
			}
			initCss();
		</script>
