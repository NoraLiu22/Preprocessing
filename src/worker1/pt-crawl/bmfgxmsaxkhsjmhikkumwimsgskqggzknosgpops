			function initJs(){
				var scriptArr=[];
				var params = getParams();
				var articleId = params['articleId'];
				if(articleId=="39d381f1a37844ffbaae3048f1d2bb2c"){
					scriptArr.push('./js/index_ssgs2022.js?03');
				}else if(articleId=='9019ec0f52c54591b72c2338085c35c4'||articleId=='00298fe947e144288ab71606d3c9877b'||articleId=='1d66f9883ca042a489fc82979a2f2e75'||
