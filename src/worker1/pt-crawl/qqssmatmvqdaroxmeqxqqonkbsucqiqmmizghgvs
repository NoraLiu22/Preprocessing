            <h1> 2024 Innovation Academy: Exhibitor resources</h1>
            <hr class="land-page-hr">

            <!--BeginNoIndex-->
    <!-- Secondary Nav -->
    <div class="sec-nav-container">
        <div class="nav sec-nav">
                    <div class="sec-nav-item">
                        <a class="sec-nav-item-link" href="https://www.aad.org/member/meetings-education/ia24/registration">Registration</a>
