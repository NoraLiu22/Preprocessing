<script type="text/javascript">
    $(document).ready(function () {
        ShowRenewal();
    });


    function ShowRenewal() {
        var url = '/api/ShowRenewal';
        $.ajax(url,
            {
                method: 'POST'
            })
            .done(function (data) {
                $('#renewalContainer').html(data);
            })
            .fail(function () {
                console.log('ERROR!');
            });
    }


    function openWin(url, wName, para) {
        var values = para;
        window.open(url, wName, values);
    }

</script>


        
<script type="text/javascript">
    function ClosePopup() {
        $('.popup-container').hide();
    }
</script>

