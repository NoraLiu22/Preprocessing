    <script>
        function searchKeyPress(e,id) {
            // look for window.event in case event isn't passed in
            e = e || window.event;
            if (e.keyCode == 13) {
                window.location.href = '/search-weather.aspx?q=' + document.getElementById(id).value;
                return false;
            }
            return true;
        }
    </script>
