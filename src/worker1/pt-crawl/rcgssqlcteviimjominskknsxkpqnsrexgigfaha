    <script>
            var topSwiper = new Swiper(".mySwiper2", {
                loop: true,
                effect: 'fade', //设置Slide的切换效果，默认为"slide"（普通位移切换），还可设置为"fade"（淡入）、"cube"（方块）、"coverflow"（3d流）、"flip"（3d翻转）、"cards"(卡片式)、"creative"（创意性）                
                pauseOnMouseEnter:true,
                navigation: {
                    nextEl: ".swiper-button-next",
                    prevEl: ".swiper-button-prev",
                },
                autoplay: {
                    delay: 5000,
                    stopOnLastSlide: false,
                    disableOnInteraction: false,
                },
                thumbs: {
                    swiper: {
                    el: "#topSwiper",
                    spaceBetween: 10,
                    slidesPerView: 6,
                    freeMode:true,
                    watchSlidesVisibility: true
                    },
                    slideThumbActiveClass: "my-slide-thumb-active"
                }
                });
            $(".top-swiper-items").hover(function(e) {
                console.log(e.currentTarget.dataset.index);
                let indexTo = e.currentTarget.dataset.index;
                // 调用slideToLoop(index, speed, runCallbacks)
                topSwiper.slideToLoop(indexTo, 1000, false); //切换到指定slide，速度为1秒
                },function(){
                // 恢复自动轮播
                topSwiper.autoplay.start();
            });
        </script>
