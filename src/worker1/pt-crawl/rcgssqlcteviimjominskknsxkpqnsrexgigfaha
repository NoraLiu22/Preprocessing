<script>
    $(document).ready(function () {
        //cosplay
        var cosplayTag = $("#cosplayTag").find("span");
        var cosplayPic = $("#cosplayPic").find(".box_44");
        for (var i = 0; i < cosplayTag.length; i++) {
            cosplayTag[i].id = "cosplay" + i;
            cosplayPic[i].id = "coser" + i;
            cosplayTag[i].onmouseover = function () {
                for (var j = 0; j < cosplayTag.length; j++) {
                    cosplayPic[j].style.display = "none";
                    cosplayTag[j].style.color = "rgba(38, 38, 38, 1)";
                }
                cosplayPic[this.id.substr(this.id.length - 1)].style.display = "flex";
                cosplayTag[this.id.substr(this.id.length - 1)].style.color = "rgba(169, 27, 51, 1)";
                if ((this.id.substr(this.id.length - 1)) % 2 === 0) {
                    $(".image_6")[0].style.margin = "0.214rem 0 0 23.887rem";
                } else {
                    $(".image_6")[0].style.margin = "0.214rem 0 0 26.987rem";
                }
            }
        }
        //漫画世界
        var comics = $(".comics");
        for (var x = 0; x < comics.length; x++) {
            comics[x].id = "comics" + x;
            comics[x].onmouseover = function () {
                for (var y = 0; y < comics.length; y++) {
                    comics[y].style.width = "2.934rem";
                }
                comics[this.id.substr(this.id.length - 1)].style.width = "17.707rem";
            }
        }
        //漫说漫画
        var comic = $(".comic");
        var comicPic = $(".comicPic");
        for (var m = 0; m < comic.length; m++) {
            comic[m].id = "comic" + m;
            //添加鼠标移入事件
            comic[m].onmouseover = function () {
                //先将所有图片的宽度设为0
                for (var n = 0; n < comic.length; n++) {
                    comicPic[n].style.width = "0";
                }
                //将对应的元素宽度设置
                comicPic[this.id.substr(this.id.length - 1)].style.width = "18.56rem";
                //判断是否是最后一个元素，更改边框半径
                if (Number(this.id.substr(this.id.length - 1)) !== (comic.length - 1)) {
                    comic[comic.length - 1].style.borderRadius = "0px 8px 8px 0px";
                    comicPic[comicPic.length - 1].style.borderRadius = "0";
                } else {
                    comic[comic.length - 1].style.borderRadius = "0";
                    comicPic[comicPic.length - 1].style.borderRadius = "0px 8px 8px 0px";
                }
            }
        }
    })
</script>
</body>
</html>
 contentType 9 text/html url 33 http://animation.china.com.cn:80/ responseCode 3 200 