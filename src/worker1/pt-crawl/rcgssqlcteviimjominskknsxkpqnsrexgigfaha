        <script>
            var length = $(".swiper-slide.box_6.box_6_mnxh.flex-row").length;
            var mySwiper = new Swiper(".mySwiper3", {
                // loop: true,
                effect: 'fade', //设置Slide的切换效果，默认为"slide"（普通位移切换），还可设置为"fade"（淡入）、"cube"（方块）、"coverflow"（3d流）、"flip"（3d翻转）、"cards"(卡片式)、"creative"（创意性）
                pauseOnMouseEnter: true,
                autoplay: {
                    delay: 5000,
                    stopOnLastSlide: false,
                    disableOnInteraction: false,
                },
                on: {
                    slideChangeTransitionStart: function(){
                        var height = $(".swiper-slide.box_6.box_6_mnxh.flex-row").eq(0).innerHeight();
                        var index = this.activeIndex;
                        var scrollTop = 0;
                        if (index <= (length-5)) {
                            scrollTop = index * height;
                            $(".mySwiper4").animate({scrollTop: scrollTop},500);
                        } else if (index === 0) {
                            $(".mySwiper4").animate({scrollTop: 0},1000);
                        }
                    },
                },
                thumbs: {
                    swiper: {
                        el: ".mySwiper4",
                        spaceBetween: 0,
                        slidesPerView: 5,
                        watchSlidesVisibility: true,
                        direction: 'vertical',
        
                    },
                }
            });
            $(".box_6_mnxh").hover(function (e) {
                // 取得绑定的缩略图data-index
                console.log(e.currentTarget.dataset.index);
                let indexTo = e.currentTarget.dataset.index;
                // 调用slideToLoop(index, speed, runCallbacks)
                mySwiper.slideToLoop(indexTo, 1000, false); //切换到指定slide，速度为1秒
            }, function () {
                // 恢复自动轮播
                mySwiper.autoplay.start();
            });
        </script>

