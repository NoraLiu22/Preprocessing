    <tr>
      <td style="text-align: left">600K</td>
      <td style="text-align: left">$330</td>
    </tr>
    <tr>
      <td style="text-align: left">1200K</td>
      <td style="text-align: left">$660</td>
    </tr>
    <tr>
      <td style="text-align: left">2400K</td>
      <td style="text-align: left">$1,320</td>
    </tr>
