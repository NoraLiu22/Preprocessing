<script type="text/javascript">
$( document ).ready(function() {
  var articleUrl = encodeURIComponent("https://web.archive.org/web/20170926041333/http://www.nacion.com/archivo/extranjeros-viven-Costa-Rica_0_1273072910.html"),
  articleTitle = encodeURIComponent("385.899 extranjeros viven en Costa Rica"),
  articleTitleEncode = encodeURIComponent("385.899 extranjeros viven en Costa Rica");


  $('.nav_header').css("display","none");
  $('.ver-mas-noticias').css("display","none");
  $('#sharefacebook').attr('href','https://web.archive.org/web/20170926041333/http://www.facebook.com/sharer/sharer.php?u=' + articleUrl);
  $('#sharetwitter').attr('href', 'https://web.archive.org/web/20170926041333/http://twitter.com/intent/tweet?text=' + articleTitleEncode + "%20-%20" + articleUrl);
  $('#sharegoogle').attr('href', 'https://web.archive.org/web/20170926041333/https://plus.google.com/share?url=' + articleUrl);
  $('#sharemail').attr('href', 'mailto:?subject=' + articleTitleEncode + '&body=' + articleUrl);

  $(".whatsapp").click(function() {

    if (typeof ga == 'function') {
      ga("send", "event", "Botón Whatsapp", "Compartir en Whatsapp", "/archivo/extranjeros-viven-Costa-Rica_0_1273072910.html");
    }
    window.location = "whatsapp://send?text=" + articleTitleEncode + "%20-%20" + articleUrl;
  });

  if( $("#banner34").length > 0 ) {
      $("#banner34").append("<span>Publicidad</span>");
    } else {
      $("#banner34").hide();
    }

    if( $("#banner01").length > 0 ) {
      $("#banner01").append("<span>Publicidad</span>");
    } else {
      $("#banner01").hide();
    }

    if( $("#banner-rigth").length > 0 ) {
      $("#banner-rigth").prepend("<p>Publicidad</p>");
    } else {
      $("#banner-rigth").hide();
    }

});

</script>

