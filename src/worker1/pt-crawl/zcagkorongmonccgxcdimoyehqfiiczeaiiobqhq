<script type="text/javascript">
var GNANewsletterSuscriptionMODataLayer = GNANewsletterSuscriptionMODataLayer || [];
	GNANewsletterSuscriptionMODataLayer["sectionId"] = 1300,
	fnNewsletterOffer83647 = tmpl("newsletterOffer83647");

function updateNewsletterOffer83647() {
	// Se arma la oferta que se mostrara al usuario
	html = fnNewsletterOffer83647(GNANewsletterSuscriptionMO.getInstance().getTplData());
	$("#container83647").html(html);

	// Se asigna la funcion de click al boton que se acaba de crear por medio del templamte
	$("#suscribe83647").unbind("click").click(function(event) {
		sendNewsletterOffer83647();
	});

	// Se asigna la funcionalidad del close de la notificacion
	$("#close83647").unbind("click").click(function(event) {
		$("#container83647").html("").hide();
	});
}

function sendNewsletterOffer83647() {
	GNANewsletterSuscriptionMO.getInstance().sendNewsletterOffer($("#useremail83647").val(), $("#terminos83647"), 83647);
}

// Listener para el evento 'GNANewsletterSuscriptionMO' que sera llamado
// una vez que se hay obtenido informacion de los boletines de la
// seccion indicada por el contenido
$(document).on("GNANewsletterSuscriptionMO::dataUpdate", function(evt, responseData) {
var randomPercentaje = Math.floor((Math.random() * 100) + 1);

	// En este caso no aplica nada porque el porcentaje aleatorio
	// no es mayor al indicado por configuracion
	if (responseData.Percentage  <= randomPercentaje) {
			return false;
	   } 

	// Si tenemos oferta de boletines para este usuario, entonces
	// se muestra uno al azar
	if (responseData.offerNewsletters.length > 0) {
		// Se actualiza el contenedor para mostrar la nueva oferta de boletin
		updateNewsletterOffer83647();

		// Si tenemos contenido que mostrar, entonces asignamos los eventos del caso
		// para que el widget aparezca cuando corresponde
		$("#content-social-bottom").appear({ force_process: true });

		$("#content-social-bottom").on("appear", function(e, $affected) {
			if ($('#container83647').html() !== "") {
				$('#container83647').stop().animate({ bottom: '0px' });
			}
		});

		$("#content-social-bottom").on("disappear", function(e, $affected) {
			// Solo se oculta si un elemento no esta visible en pagina
			if (! ($("#mas-contenido").is(":appeared"))) {
				 // $('#container83647').hide();
				$('#container83647').stop().animate({ bottom: '-350px' });
			}
		});
	}
});

$(document).on("GNANewsletterSuscriptionMO::offerUpdate", function(evt, responseData) {
	if ($('#container83647').html() !== "") {
		// Se actualiza el contenedor para mostrar la nueva oferta de boletin
		updateNewsletterOffer83647();
	}
});
</script>


