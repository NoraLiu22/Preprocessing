<script type="text/javascript">
var keepAliveRunCookieName = "ac6e67e19558680298f72d41d5794967",
    keepAliveRunCookie = jQuery.cookie(keepAliveRunCookieName);

Date.prototype.addHours = function(h) {
    this.setHours(this.getHours() + h);
    return this;
};

if (keepAliveRunCookie === null) {
    var imgObj = document.createElement("img"),
        expiresDate = new Date().addHours(12);

    imgObj.src = "https://web.archive.org/web/20170926041333/https://id.gruponacion.biz/public/usuario/onepixel";
    document.body.appendChild(imgObj);

    // Asignamos el cookie flag para no volver a pedir los cookies
    // hasta que este flag expire
    jQuery.cookie(keepAliveRunCookieName, "Checked", { path: "/", expires: expiresDate });
}
</script>

