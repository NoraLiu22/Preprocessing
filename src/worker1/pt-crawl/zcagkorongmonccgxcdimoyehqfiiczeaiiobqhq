<script type="text/javascript">
	var userData = jQuery.cookie('5e004dead877b9e6e19a61613a3139e7');
	if (userData != null) {
		var userObj = jQuery.parseJSON(userData);

		jQuery("span.usr-profile").html(userObj.Name.replace(/\+/g, ' '));
		if( userObj.DataRequired !== undefined ) {
				jQuery("div.dot_count").html(userObj.DataRequired.length);
				//llenamos la data con las notificaciones, en caso de tener notificaciones en cola
				if( userObj.DataRequired.length > 0 ) {
					data = userObj.DataRequired;
					notificaciones = "";
					for( x in data ) {
						notificaciones += '<li><a href="/run-user/callback/pre-editprofile"><span class="danger"></span><p><span class="title-notification">Información requerida</span><br/>'+data[x].replace(/\+/g, ' ')+'</p></a></li>';
					}
					$("#message-list").html(notificaciones);
				}
		} else {
				jQuery("div.dot_count").html(0);
		}

		if (userObj.hasOwnProperty('SubscriptionType') && userObj.SubscriptionType === "subscribed") {
			jQuery(".login-imagen").removeClass("user-registered").addClass("user-subscribed");
		}

		jQuery("div.lnc-login").show();
		jQuery("div.lnc-logout").remove();
	} else {
		jQuery("div.lnc-logout").show();
		jQuery("div.lnc-login").remove();
	}

	//eventos para las notificaicones
	$(".alert-ico").click(function(){
		$(".messages").slideToggle(400);
	});

	OneSignal.push(["isPushNotificationsEnabled", function(enabled) {
		if (OneSignal.isPushNotificationsSupported() && enabled) {
			$(".web-push").show();
			enabledSuscriptionPush();
		}
	}]);

	function enabledSuscriptionPush() {
		//activamos las notificaciones que ya se marcaron previamente
		var sections = jQuery.parseJSON('{"portada":"portada","ultima_hora":"\u00daltimas noticias","nacional":"nacional","deportes":"deportes","sucesos":"sucesos","tecnologia":"tecnologia","economia":"economia","entretenimiento":"entretenimiento","vivir":"vivir","mundo":"mundo","opinion":"opinion"}');
		if( localStorage.getItem("channelsEnabled") ) {
			for(x in sections) {
				var value = localStorage.getItem(x);
				if( value && value == 1 ) {
					document.getElementById(x).checked = true;
				}
			}
		} else {
			//obtenemos los canales por OneSignal
			OneSignal.push(["getTags", function(tags) {
				//validamos que no existan las secciones de portada y ultima hora, en caso de no existir las seteamos
				if( !inArray(tags, "portada") ) { enableChannel("portada"); }
				if( !inArray(tags, "ultima_hora") ) { enableChannel("ultima_hora"); }
				for(x in sections) {
					if( tags[x] && tags[x] == "true" ) {
						document.getElementById(x).checked = true;
					}
				}
			}]);
		}

		$(".alert-ico-suscrip").click(function(){
			$(".channels").slideToggle(400);
		});
		$("#notification-suscrip .cmn-toggle").change(function() {
			var channel = this.id;

			if( !localStorage.getItem("channelsEnabled") ) {
				localStorage.setItem("channelsEnabled", 1);
			}
			if(this.checked) {
				OneSignal.push(["sendTag", channel, true]);
				//almacenamos la data en el localStorage
				localStorage.setItem(channel, 1);
			} else {
				OneSignal.push(["sendTag", channel, false]);
				localStorage.setItem(channel, 0);
			}
		});

		function enableChannel(channel) {
			OneSignal.push(["sendTag", channel, true]);
			localStorage.setItem(channel, 1);
			document.getElementById(channel).checked = true;
		}

		function inArray(array, value) {
			for( x in array) {
				if( x == value ) { return true; }
			}
			return false;
		}
	}

</script>
