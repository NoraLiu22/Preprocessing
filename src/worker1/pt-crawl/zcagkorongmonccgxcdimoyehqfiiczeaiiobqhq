    $(document).ready(function(){
        if(  jQuery().slick ){
            $('.slick-sub-menu-footer').slick({
                infinite: true,
                slidesToShow: 4,
                variableWidth: true,
                slidesToScroll: 1,
                arrows: false,
                responsive: [
                    { breakpoint: 690, settings: { slidesToShow: 3 } },
                    { breakpoint: 320, settings: { slidesToShow: 2 } }
                ]

            });
        }

    $('#hamburger_2').change(function() {   

        $('#item-list-menu').toggle(function(){
          if(!$('#item-list-menu').hasClass('m_mostrar') )
            $('#item-list-menu').addClass('m_mostrar');
          else
            $('#item-list-menu').removeClass('m_mostrar');
        });
      });
    

        var resizeTimer_2;
        $(window).on('resize', function (e) {
            clearTimeout(resizeTimer_2);
            resizeTimer_2 = setTimeout(function () {
                if ($(window).width() > 960) {
                    $('#item-list-menu').show();
                } else {
                    $('#item-list-menu').hide();
                }
            }, 1);
        });

    });
  </script>



