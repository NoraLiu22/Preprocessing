<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js"></script>

<script>
   document.querySelector('#PHistory').classList.remove('render-lightbox');
   window.addEventListener('load', (event) => {

      $('[data-behavior="open-video"]').on('click', function(evt) {
         evt.preventDefault();
         target = $(this).attr('data-target');
         $(target).addClass('opened');
      });

      $('.oyster-video-modal').on('click', function(evt) {
         if($(evt.target).hasClass('.modal__inner') || $(evt.target).closest('.modal__inner').length > 0) {
            return;
         }

         $(this).removeClass('opened');
      });

      $('.link-see-recipes').on('click', function(evt) {
         evt.preventDefault();
         var target = $(this).attr('href');
         var headerHeight = $('#header').outerHeight();

         $('html, body').animate({
            scrollTop: $(target).offset().top - headerHeight
         }, 
         1000);
      });
   });

   var $jQuery = jQuery.noConflict();
   $jQuery(document).ready(function() {
      $jQuery('.sauce-main-carousel').slick({
         fade: true,
         dots: true,
         arrows: false,
         responsive: [
            {
               breakpoint: 992,
               settings: {
                  dots: false,
                  adaptiveHeight: true
               }
            }
         ]
      }).on('beforeChange', function(event, slick, currentSlide, nextSlide) {
         $jQuery('.sauce-bottle-carousel .bottle-slide[data-index="' + nextSlide + '"]').addClass('active').siblings().removeClass('active');
      });

      $jQuery('.sauce-bottle-carousel .bottle-slide').on('click', function() {
         let slideIndex = $(this).attr('data-index');
         $jQuery('.sauce-main-carousel').slick('slickGoTo', slideIndex);
      });

      $jQuery('.sauce-slide--prev').on('click', function() {
         $jQuery('.sauce-main-carousel').slick('slickPrev');
      });

      $jQuery('.sauce-slide--next').on('click', function() {
         $jQuery('.sauce-main-carousel').slick('slickNext');
      });
   });
   
</script>

