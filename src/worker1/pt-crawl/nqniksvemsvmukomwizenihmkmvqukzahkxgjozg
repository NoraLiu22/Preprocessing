<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript">




// ROLLOVERS SCRIPT



// Preload images



if (document.images) {



var imageUrls = new Array("/history/scottishhistory/images/europe/history_europe_trails_on.gif",
"/history/scottishhistory/images/europe/history_europe_media_on.gif",
"/history/scottishhistory/images/europe/history_europe_games_on.gif",
"/history/scottishhistory/images/europe/history_europe_oddities_on.gif",
"/history/scottishhistory/images/europe/history_europe_web_guide_on.gif");



        var pic = new Array(imageUrls.length);



        for (i=0; i<imageUrls.length; i++) {

                pic[i] = new Image();

                pic[i].src = imageUrls[i];

        }

}







// Rolling over

function hiLite(imageName) {

        if (document.images) {

                top.document.images[imageName].src = "/history/scottishhistory/images/europe/" + imageName + "on.gif";

        }

}



// Rolling out

function backNormal(imageName) {

        if (document.images) {

                top.document.images[imageName].src = "/history/scottishhistory/images/europe/" + imageName + "off.gif";

        }

}

//-->



</SCRIPT>

