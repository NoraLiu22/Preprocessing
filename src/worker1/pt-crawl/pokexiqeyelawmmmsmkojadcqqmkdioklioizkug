<div class="flex-inner" data-toggle-tripbuilder>
	<div class="control-wrapper">
        <div class="inner"></div>
        <div class="dropdown-support"></div>
	    <div class="suitcase">
    		<div class="tripbuilder" id="tripbuilder_counter_31312dde-f863-4958-bc77-dd7b07884080"></div>
    	</div>
    	<i class="fa fa-angle-down" aria-hidden="true"></i>
	</div>
</div>

<script>
	require(['jquery', 'site_tripbuilder'], function($, tripbuilder) {
		if (!tripbuilder.enabled) { return; }
		
		var root = $('#tripbuilder_counter_31312dde-f863-4958-bc77-dd7b07884080');
		var container = $('[data-sv-tripbuilderCounter]');
		
		var renderContainer = root.closest('.contentRender_name_plugins_tripbuilder_tripbuilder_counter');
		
		container.append(renderContainer);
		
		var onChangeCB = function() {
			root.html("<i class='fa fa-suitcase' aria-hidden='true'></i><div class='tally'>" + tripbuilder.getTotalCount() + " </div>");
		}
		
		onChangeCB();
		tripbuilder.events.on('change', onChangeCB);
		
		$('[data-sv-trip-mobile-one] .trip-bldr').append($('[data-toggle-tripbuilder]').clone());
		$('[data-sv-trip-mobile-dd]').append($('[data-tripbuilder-list]').clone());
	});
</script>
