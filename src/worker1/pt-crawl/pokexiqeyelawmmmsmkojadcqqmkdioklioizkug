<script>
	require(["jquery"], function($) {
		// Sticky nav
		var dist = 450;
		var scrollTop;
		var stickyNav;

		$(window).on("scroll", function(){
			scrollTop = $(window).scrollTop();
			if(scrollTop > dist){
				stickyNav = $("body").find("[data-stickynavigation]");
				stickyNav.addClass("in-view");
			}else{
				stickyNav = $("body").find("[data-stickynavigation]");
				stickyNav.removeClass("in-view");
			}
		});

		var hideWeather = function() {
			$('[data-sv-weatherwidget]').removeClass('active');
			$('[data-weathermobile]').removeClass('active');
            $('.header-section .mobile-weather').removeClass('active');
            $(".header-section .trip-bldr-panel").removeClass("active");
		};

		var hideTrip = function() {
			$('[data-tripbuilder-container]').removeClass('active');
			$('[data-toggle-tripbuilder]').removeClass('active');
			$('[data-sv-trip-mobile-dd]').removeClass('active');
			$('[data-tripmobile]').removeClass('active');
            $('.header-section .mobile-tripbldr').removeClass('active');
            $(".header-section .trip-bldr-panel.active").removeClass("active");
		};

		var hideLanguage = function() {
			$('[data-language-selector] .translate-cont').removeClass('active');
			$('[data-language-panel]').removeClass('active');
			$('[data-sv-language-mobile-one] .translate-cont').removeClass('active');
            $('[data-sv-language-mobile-dd] .translate-panel').removeClass('active');
		};

		var hideSearch = function() {
			$('[data-primarysearch]').removeClass('active');
			$('[data-mobilesearch]').removeClass('active');
		}

		// Mobile Nav
		$("body").on("click", "[data-toggle-mobilenav]", function(){
			var mobilePrompt = $("[data-toggle-mobilesearch]");
			if($(mobilePrompt).hasClass("active")){
				$(mobilePrompt).removeClass("active");
				var mobileSearch = $("[data-mobilesearch]");
				$(mobileSearch).removeClass("active");
			}
			$("body").toggleClass("search-open");
			$("[data-mobilenav]").toggleClass("active");
		});

		$("body").on("click", "[data-menu-toggleparent]", function(){
			$(this).parent().parent().toggleClass("active");
		});

		//search
		$("body").on("click", "[data-toggle-search]", function () {
			hideWeather();
			hideTrip();
			hideLanguage();

			$(this).parent().toggleClass("active");
		});

		$("body").on("click", "[data-toggle-mobilesearch]", function () {
			var mobileMenu = $("[data-mobilenav]");
			if($(mobileMenu).hasClass("active")){
				$(mobileMenu).removeClass("active");
			}
			$(this).toggleClass("active");
			$("body").toggleClass("search-open");
			$("[data-mobilesearch]").toggleClass("active");
		});

		$("body").on("click", "[data-toggle-stickysearch]", function () {
			$(this).parent().parent().toggleClass("sticky-active");
			$("[data-stickysearch]").toggleClass("sticky-active");
		});

		//Weather
		$('body').on('click', '[data-toggle-weather]', function () {
			hideSearch();
			hideTrip();
			hideLanguage();

			$(this).parent().toggleClass("active");
		});

		$("body").on("click", "[data-toggle-weathermobile]", function () {
			hideSearch();
			hideTrip();
			hideLanguage();

			$(this).parent().toggleClass("active");
			$("[data-weathermobile]").toggleClass("active");
		});

		//Trip Builder
		$("body").on("click", "[data-toggle-tripbuilder]", function () {
            $(this).toggleClass("active");
            $("[data-tripbuilder-container]").toggleClass("active");
            $("[data-sv-trip-mobile-dd]").toggleClass("active");
			hideSearch();
			hideWeather();
			hideLanguage();
		});

		// language selector
		$('[data-sv-language-mobile-one]').append($('[data-toggle-language]').parent().clone());
		$('[data-sv-language-mobile-dd]').append($('[data-language-panel]').clone());

		$('.language-selector [data-toggle-language]').on('click', function() {
			hideSearch();
			hideWeather();
			hideTrip();

			$(this).parent().toggleClass('active');
			$('[data-language-panel]').toggleClass('active');
		});

		$('[data-sv-language-mobile-one]').on('click', function() {
			hideSearch();
			hideWeather();
			hideTrip();

			$(this).find('.translate-cont').toggleClass('active');
			$('[data-sv-language-mobile-dd] .translate-panel').toggleClass('active');
        });
        
        $("body").on("click", "[data-sv-close]", function(){
            $("[data-toggle-tripbuilder]").removeClass("active");
            $("[data-sv-trip-mobile-dd]").removeClass("active");
        });
	});
</script>
</div>
	</div>
