	require(['jquery', 'sv_clientLib'], function ($, clientLib) {
		var root = $('[data-guid="553075ae-dbd6-466a-ba77-b837c5be9c2a"]');
		var cookieName = 'gdpr_banner_dismissed';
		var cookieFound = clientLib.getCookie(cookieName);

		if (cookieFound !== 'true') {
			root.removeClass('hide');
			setTimeout(function() {
				root.removeClass('dismissed'); 
			}, 500);
		}

		root.find('[data-sv-close-banner]').on('click', function(e) {
			root.addClass('dismissed');
			setTimeout(function() {
				root.addClass('hide'); 
			}, 500);
			
			clientLib.setCookie({ 
				name : cookieName,
				value : 'true',
				path : '/',
				'max-age' : 3600 * 72 // 72 hours
			});
		});
	});
</script>
