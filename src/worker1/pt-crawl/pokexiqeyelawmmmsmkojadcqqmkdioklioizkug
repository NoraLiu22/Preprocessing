<script>
	require(['jquery'], function($) {
		var leftDesktopNav = $('[data-sv-nav-left]').html();
		$('[data-sv-nav-left]').append(leftDesktopNav);
		var rightDesktopNav = $('[data-sv-nav-right]').html();
		$('[data-sv-nav-right]').append(rightDesktopNav);

		var mobileNavMenu = $('[data-mobile-nav-main]');
	    $('[data-sv-main-mobile]').append(mobileNavMenu);

	    var stickyNavMenuLeft = $('[data-sticky-nav-main-left]').html();
	    $('[data-sv-nav-left-sticky]').append(stickyNavMenuLeft);
	    var stickyNavMenuRight = $('[data-sticky-nav-main-right]').html();
	    $('[data-sv-nav-right-sticky]').append(stickyNavMenuRight);

        // main nav dropdown js
        
        var navEl = $(".primary-item .dd-menu .item.has-dd");
        var menu;
        var mh; //current height of nav menu
        var uh; //height of submenu
        navEl.on("mouseenter", function(){
            menu = $(this).closest("ul");
            mh = menu.outerHeight();
            uh = $(this).find("ul").first().outerHeight();
            if(mh < uh){
                menu.css("height", uh);
            }
        });
        navEl.on("mouseleave", function(){
            menu.css("height",  mh);
        });

        var navEl = $(".sticky-nav .primary-item .dd-menu .item.has-dd");
        var menu;
        var mh; //current height of nav menu
        var uh; //height of submenu
        navEl.on("mouseenter", function(){
            menu = $(this).closest("ul");
            mh = menu.outerHeight();
            uh = $(this).find("ul").first().outerHeight();
            if(mh < uh){
                menu.css("height", uh);
            }
        });
        navEl.on("mouseleave", function(){
            menu.css("height",  mh);
        });

	});
</script>

