<script>
	require(["jquery", "sv_goatee!plugins=stringLib,clientMoment,sv_site", "sv_urlLib", "sv_site", "sv_clientMoment"], function($, goatee, urlLib, sv_site, clientMoment) {
		var root = $("[data-sv-weatherWidget]");
		var template = $("[data-weatherwidget-template]").html();
		var mobileOne = $("[data-sv-weatherWidget-mobile-one]");
		var templateMobile = $("[data-weather-mobile-one-template]").html();
		var mobileDD = $("[data-sv-weatherWidget-mobile-dd]");
		var templateDD = $("[data-weather-mobile-dd-template]").html();

		var settings = {"unit_of_measure":"f"};
		var site = sv_site.site;

		if(site == "primary") {
			var url = urlLib.build({
				path : "/plugins/core/svapi/",
				get : {
					service : "weatherv2",
					endpoint : "latlng",
					serviceArgs : JSON.stringify({
						lat : sv_site.siteConfig.latitude,
						lng : sv_site.siteConfig.longitude,
						unit : settings.unit_of_measure
					})
				}
			});
		} else {
			var url = urlLib.build({
				path : "/plugins/core/svapi/",
				get : {
					service : "weatherv2",
					endpoint : "latlng",
					serviceArgs : JSON.stringify({
						lat : sv_site.siteConfig.latitude,
						lng : sv_site.siteConfig.longitude,
						unit : "c"
					})
				}
			});
		}


		$.get(url, function(res) {
			if (res.success !== true) { return; }
			res.data.forecast.splice(5); // trim forecast to first 5 days
			var html = goatee.fill(template, res.data);
			var htmlMobile = goatee.fill(templateMobile, res.data);
			var htmlDD = goatee.fill(templateDD, res.data);
			root.html(html);
			mobileOne.html(htmlMobile);
			mobileDD.html(htmlDD);

			var link = $('.weather-link');
			var linkMobile = $('.weather-link').clone(true);
			root.find("[data-sv-weatherlink]").append(link);
			mobileDD.find ("[data-sv-weatherlink-mobile]").append(linkMobile);
			$('[data-sv-weatherlink] .weather-link').removeClass('hide');
			$('[data-sv-weatherlink-mobile] .weather-link').removeClass('hide');

			$('[data-sv-weather-mobile-one]').append(mobileOne);
			$('[data-sv-weather-mobile-dd]').append(mobileDD);
		});


	});
</script>
