<script>
	require(["jquery", "site_tripbuilder", "plugins_listings/main", "goatee"], function($, tripbuilder, Listings, goatee) {
		var root = $('[data-tripbuilder-list="a816fb6a-a2d0-4365-843f-421d760d8b94"]');
		var itemTemplate = root.find('[data-sv-itemTemplate]').html();
		var infoWindowTemplate = root.find('[data-sv-infoWindowTemplate]').html();
		var placemarks = [];

		var updateLayout = function() {
			tripbuilder.getData(function(err, data) {

				if (err) { throw new Error(err); }

				var html = goatee.fill(itemTemplate, { types : data.types });
				root.find('[data-sv-tripbuilder-items]').html(html);

				var validItemCount = 0;
				var tripbuilderItems = data.typeIndex;
				var listHTML = '';
				root.addClass("empty");
				for(var key in tripbuilderItems) {
					if(tripbuilderItems[key].items.length > 0) {
						listHTML += "<li class='li-title'>" + tripbuilderItems[key].type.label + "</li>";
						for(var i = 0; i < tripbuilderItems[key].items.length; i++) {
							var liClass = i % 2 == 0 ? 'odd' : 'even';
							var item = tripbuilderItems[key].items[i];
							listHTML += "<li class=" +  liClass + " data-tb-recid=" + item.recid + " data-tb-type='" + tripbuilderItems[key].type.name + "'><span class='tb-remove' data-tb-remove></span>" + item.title + "</li>"
						}
						root.removeClass("empty");
					}
				}

				tripbuilder.initButtons({
					root : root,
					removeButtonTemplate : '<button class="remove-item text-button" data-tb-remove>(<span data-i18n="tripbuilder.remove">remove</span>)</button>'
				});
			});
		}

		updateLayout();
		tripbuilder.events.on('change', updateLayout);

		tripbuilder.events.on('removeFromTrip', function(e, item) {
			var itemElem = root.find('[data-sv-tripbuilder-item][data-type="' + item.type + '"][data-recid="' + item.recid + '"]');
			var sectionElem = root.find('[data-sv-tripbuilder-section][data-type="' + item.type + '"]');

			itemElem.remove();

			if (sectionElem.find('[data-sv-tripbuilder-item]').length === 0) {
				sectionElem.remove();
			}
		});
		root.find('[data-sv-close]').on('click', function(e){
			e.preventDefault();
			$('body').find('[data-tripbuilder-container]').removeClass('active');
			$('body').find('.trip-bldr-panel').removeClass('active');
			$('body').find('.trip-bldr').removeClass('active');
		})
		root.find('[data-view-trip]').attr('href', tripbuilder.path);
	});
</script>
</div>
