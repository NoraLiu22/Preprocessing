<script type="text/javascript">
function inspect(){
		var osww=$(window).width();
		var oshh=$(window).height();
		var oscolw=(osww-1270)/2;
		var num=0;
		if(oscolw>60){
			num=(osww-1270)/2-60;
		}else{
			num=0;
		}
		
		if($(window).scrollTop() > oshh/2){
			$(".zhongtaizhisheng19232_ind04").show();
			$(".zhongtaizhisheng19232_ind04").css({"right":num+"px"})
		}else{
			$(".zhongtaizhisheng19232_ind04").hide();
			$(".zhongtaizhisheng19232_ind04").css({"right":num+"px"})
		}
	}
	inspect();
	$(".zhongtaizhisheng19232_ind04").click(function(){
		$("body,html").animate({scrollTop:0},300)
		return false;
	});
	var throldHold = 100;
	window.onresize = window.onscroll = function () {
		clearTimeout(throldHold);
		window.timer = setTimeout(function(){
			inspect();
		}, throldHold);
	}

	$(".zhongtaizhisheng19232_ind04").hover(function(){
		$(".zhongtaizhisheng19232_ind04 img").attr("src","//p3.img.cctvpic.com/photoAlbum/templet/common/DEPA1587635080178743/zongtaizhisheng19232_topCur_20200428.png");
	},function(){
		$(".zhongtaizhisheng19232_ind04 img").attr("src","//p3.img.cctvpic.com/photoAlbum/templet/common/DEPA1587635080178743/zongtaizhisheng19232_top_20200428.png");
	})
</script>
	
				
