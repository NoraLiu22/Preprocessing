				<div class="stvp-competition-open--single__overlay"></div>
				<img class="stvp-competition-open--single__spinner" src="{{ data.spinnerUrl }}">
				<# _.each( data.votingOptions, function( item, index ) { #>
					<# voted_class = ( item.ID == data.votedOptions[ data.categoryId ] ) ? ' stvp-voted' : '' #>
					<li data-option-id="{{ item.ID }}" class="stvp-competition-open--single__voting-option-item{{ voted_class }}">
						<div class="stvp-competition-open--single__item-container">
							<div class="stvp-competition-open--single__item-bg" style="background: url( {{ item.image_url }} )"></div>
							<h3 class="stvp-competition-open--single__item-title">{{ item.title }}</h3>
