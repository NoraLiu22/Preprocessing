		#stvp-competition-open--single__voting-option-list li * {
			pointer-events: none;
		}
	</style>

	<script type="text/html" id="tmpl-competition-open--multiple">
		<h1 class="stvp-competition-open--multiple__conpetition-title">1st, 2nd and 3rd place nominations for:<br />{{data.categoryName}}</h1>
		<# if ( 'Team of the Year Award' === data.categoryName ) { #>
			<div class="stvp-competition-open--multiple__video-link">Find out about the nominees for the <a target="_blank" href="https://www.sportswomenoftheyear.co.uk/team-of-the-year-award-nominees/">TEAM OF THE YEAR AWARD</a></div>
		<# } #>
		<# if ( 'Grassroots Sportswoman of the Year Award' === data.categoryName ) { #>
			<div class="stvp-competition-open--multiple__video-link">Find out about the nominees, by watching our <a target="_blank" href="https://www.sportswomenoftheyear.co.uk/grassroots-sportswoman-of-the-year-award-nominees/">GRASSROOTS SPORTSWOMEN OF THE YEAR AWARDS VIDEO</a></div>
		<# } #>
		<# if ( 'Changemaker Award' === data.categoryName ) { #>
			<div class="stvp-competition-open--multiple__video-link">Find out about the nominees, by watching our <a target="_blank" href="https://www.sportswomenoftheyear.co.uk/changemaker-award-nominees/">CHANGEMAKER AWARDS VIDEO</a></div>
		<# } #>
		<div class="stvp-competition-open--multiple__container">
			<div class="stvp-competition-open--multiple__container__header">
				<# if ( 1 < data.pno ) { #>
					<# var disableButton='' #>
				<# } else { #>
					<# var disableButton='disabled' #>
				<# } #>
				<button {{ disableButton }} class="stvp-competition-open--multiple__nav-button" type="button" id="stvp-competition-open__previous">Previous</button>
				<#
					var textTitle = 'First';

					switch( Math.abs( data.selectedCount ) ) {
						case 1:
							textTitle = 'second';
							break;

						case 2:
							textTitle = 'third';
							break;

						case 3:
							textTitle = 'third';
							break;

						default:
							textTitle = 'first';
							break;
					}
				#>
				<span class="stvp-competition-open--multiple__page">Choose your {{ textTitle }} place nominee ({{ data.pno }} of {{ data.cl }})</span>
			</div>
			<ul id="stvp-competition-open--multiple__voting-option-list" class="stvp-competition-open--multiple__voting-option-list">
				<div class="stvp-competition-open--multiple__overlay"></div>
				<img class="stvp-competition-open--multiple__spinner" src="{{ data.spinnerUrl }}">
				<# _.each( data.votingOptions, function( item, index ) { #>
					<# var sequenceId = Array.isArray( data.votedOptions[ data.categoryId ] ) && data.votedOptions[ data.categoryId ].indexOf( item.ID.toString() ); #>
					<# voted_class = ( Array.isArray( data.votedOptions[ data.categoryId ] ) && -1 != data.votedOptions[ data.categoryId ].indexOf( item.ID.toString() ) ) ? ' stvp-voted' : '' #>
					<li data-option-id="{{ item.ID }}" class="stvp-competition-open--multiple__voting-option-item{{ voted_class }} {{ ( false === sequenceId || -1 === sequenceId ) ? '' : 'stvp-sequence-' + ( sequenceId + 1 ) }}">
						<div class="stvp-competition-open--multiple__item-container">
							<div class="stvp-competition-open--multiple__item-bg" style="background: url( {{ item.image_url }} )"></div>
							<h3 class="stvp-competition-open--multiple__item-title">{{ item.title }}</h3>
							<p class="stvp-competition-open--multiple__item-description">{{ item.description }}</p>
						</div>
					</li>
				<# }) #>
			</ul>
			<div class="stvp-competition-open--multiple__nav-controls">
				<#
					var disableClearButton = 'disabled';

					if ( Math.abs( data.selectedCount ) > 0 ) {
						disableClearButton = '';
					} else {
						disableClearButton = 'disabled';
					}

					var disableNavButton = 'disabled';

					if ( Math.abs( data.selectedCount ) > 2 ) {
						disableNavButton = '';
						data.nextText = 'Next';
					} else {
						data.nextText = 'Next';
						disableNavButton = 'disabled';
					}
				#>
				<button {{ disableClearButton }} class="stvp-competition-open--multiple__clear-all" type="button" id="stvp-competition-open__clear-all">Clear Selection</button>

				<# if ( data.pno < data.cl ) { #>
				<button {{ disableNavButton }} class="stvp-competition-open--multiple__nav-button" type="button" id="stvp-competition-open__next">{{ data.nextText }}</button>
				<# } else { #>
				<button {{ disableNavButton }} class="stvp-competition-open--multiple__nav-button" type="button" id="stvp-competition-open__finish">Finish</button>
				<# } #>
			</div>
		</div>
	</script>

