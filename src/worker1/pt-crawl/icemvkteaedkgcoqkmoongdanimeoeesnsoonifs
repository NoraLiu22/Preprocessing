  <div class="accordion__details">
    <div class="accordion__images">
      <div class="accordion__image">    <picture>
                  <source srcset="/sites/default/files/styles/unscaled/public/denominations/notes/100-face-web_0.webp?itok=qVDYmHez 1x" type="image/webp" width="640" height="271"/>
