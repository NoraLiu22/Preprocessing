                    <div>
                        <label class="whiteText" for="CompanyName">Company Name</label>
                        <input Name="Company" data-val="true" data-val-length="The field Company Name must be a string with a maximum length of 50." data-val-length-max="50" data-val-required="The Company Name field is required." id="completeProfileModalCompanyName" name="CompanyName" type="text" value="" />
                    </div>
                    <div>
                        <label class="whiteText" for="JobTitle">Job Title</label>
                        <input Name="JobTitle" data-val="true" data-val-length="The field Job Title must be a string with a maximum length of 255." data-val-length-max="255" data-val-required="The Job Title field is required." id="completeProfileModalJobTitle" name="JobTitle" type="text" value="" />
                    </div>
                    <div>
                        <label class="whiteText" for="IndustryCode">Industry</label>
                        <select data-val="true" data-val-number="The field Industry must be a number." data-val-required="The Industry field is required." id="completeProfileModalIndustry" name="IndustryCode"><option value=""></option>
<option value="101">Agency</option>
<option value="102">Betting</option>
<option value="103">Broadcast, Media, Production</option>
<option value="104">Charity, Non for Profit</option>
<option value="105">College, University</option>
<option value="106">Facility, Venue &amp; Stadium</option>
<option value="107">Financial Services</option>
<option value="108">Gaming, Esports</option>
<option value="109">Health, Wellness &amp; Fitness</option>
<option value="110">Leisure, Entertainment, Travel &amp; Tourism</option>
<option value="111">Manufacturing</option>
<option value="112">Professional Services</option>
<option value="113">Retail, Food &amp; Bev, FMCG</option>
<option value="114">Rights Holder</option>
<option value="115">Technology</option>
<option value="99">Other</option>
</select>
                    </div>

                    <p class="completeProfileModalError" id="completeProfileModalError"></p>
                    <div class="completeProfileModalError" id="waitForcompleteProfileModal">Please wait...</div>
                    <div class="completeProfileModalError" id="completeProfileModalErrorMissingField">Please Fill Out Missing Fields</div>

                    <button type="button" class="complete-profile-modal-btn" id="completeProfileModalSubmit">SUBMIT</button>

                    <div style="color: white;">If you already have an account, <a href="/Member/Login.aspx" style="color: white; text-decoration: underline;">click here</a> to log in.</div>
                    <div class="complete-profile-modal-image">
                        <img src="/-/media/SBJ-Logo-White.ashx" />
                    </div>
                </form>
            </div>
        </div>

    </div>
</div>

<script>
    var completeProfileIndustryCode = 0;

    function completeProfileModalSubmit() {
        var profileInformation = {
            companyName: $('#completeProfileModalCompanyName').val(),
            jobTitle: $('#completeProfileModalJobTitle').val(),
            industryCode: $('#completeProfileModalIndustry').val()
        };

        $('#completeProfileModalSubmit').addClass('disabledCursor').prop('disabled', true);
                
        if (!completeProfileModalValidation(profileInformation)) {
            $('#completeProfileModalSubmit').removeClass('disabledCursor').prop('disabled', false);
            $('#waitForcompleteProfileModal').hide();
            return false;
        }

        console.log(profileInformation);
        $.ajax({
            url: '/api/completeProfile',
            data: profileInformation,
            method: 'put',
            success: function (data) {
                $('#waitForcompleteProfileModal').hide();
                $('#completeProfileModalError').text(data.message).show();
                if (data.status == 201 || data.status == 200) {
                    $('#completeProfileModalError').removeClass('completeProfileModalError').addClass('completeProfileModalSuccess');
                    //Set hidden form input for use with pardot form handler, used when submitting form
                    $('#completeProfileOrg').val($('#completeProfileModalIndustry option:selected').text());
                    location.reload();
                } else {                                        
                    $('#completeProfileModalSubmit').removeClass('disabledCursor').prop('disabled', false);
                }
            },
            error: function (data) {
                $('#completeProfileModalError').text('An error occurred while updating user information.').show();
                $('#waitForcompleteProfileModal').hide();
            }
        });        
    }

    function completeProfileModalValidation(profileInformation) {
        var valid = true;

        $('#completeProfileModalError').text('').hide();
        $('.completeProfileModalError').hide();
        $('#waitForcompleteProfileModal').show();

        if (!$('#completeProfileModalForm').valid())
            valid = false;

        if (!profileInformation.companyName || !profileInformation.jobTitle || !profileInformation.industryCode)
            $('#completeProfileModalErrorMissingField').show();

        return valid;
    }

    function showCompleteProfilePopup() {
        $("#complete-profile-modal-pop-up").show();
        $("#complete-profile-modal-container").show();
    }

    function completeProfileModalInit() {
        $('.completeProfileModalError').hide();
    }

    $(document).ready(function () {
        $('#completeProfileCloseBtn').click(function () {
            $("#complete-profile-modal-pop-up").hide();
            $("#complete-profile-modal-container").hide();
        });

        $('#completeProfileModalSubmit').click(function () {
            completeProfileModalSubmit();
        });

        completeProfileModalInit();
    });
</script>
        <!-- END Footer for metered paywall -->

