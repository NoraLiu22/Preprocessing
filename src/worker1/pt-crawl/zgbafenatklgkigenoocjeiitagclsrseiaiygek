   function simpleTemplating(data) {
    let html = '<div class="podcast-episode-body">';
       $.each(data, function (index, item) {
           if (item.IsAcast) {
               html += '<div class="podcast-episode"><h3 class="sub-heading">' + item.Title + '</h3><p>' + item.Description + '</p><iframe height="" width="100%" frameborder="no" scrolling="no" seamless src="' + item.EmbedUrl + '"></iframe></div>';
           }
           else {
               html += '<div class="podcast-episode"><h3 class="sub-heading">' + item.Title + '</h3><div>' + item.Description + '</div><iframe width="100%" height="188" src="' + item.EmbedUrl + '" scrolling="no" frameborder="0" style="border:none;overflow:hidden;"></iframe></div>';
           }
    });
    html += '</div>';
    return html;
}

$('#pagination-container').pagination({
    dataSource: allPodcasts,
    pageSize: 10,
	showPageNumbers: true,
	showPrevious: true,
	showNext: true,
    showNavigator: true,
    callback: function (data, pagination) {
        pagination = 1;
        var html = simpleTemplating(data);
        $('#data-container').html(html);
    }
})

</script>


        </main>
