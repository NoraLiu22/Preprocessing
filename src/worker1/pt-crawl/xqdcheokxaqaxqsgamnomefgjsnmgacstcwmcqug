	if ($opt.hasClass('open')) {
		$opt.removeClass('open');
		$optbd.hide();
		$opt.find('span.expandInd').html('&#9660;');
	} else {
		$opt.addClass('open')
		$optbd.show();
		$opt.find('span.expandInd').html('&#9650;');
	}
	return false
