<script type="text/javascript">
function adjust_treenodes() 
{ 
    var i,j;
    var tree = document.getElementById('ctl00_baseContent_tvwResults');
    if (tree != null)
    {
        //first search for all the <div> for treeview nodes 
        var divs = tree.getElementsByTagName("div");
        for (i=0;i<divs.length;i++)
        {
            if(divs[i].id.indexOf("tvwResults") != -1) 
            { 
                //then set the vertical alignment for each <td> nested inside the treenode div 
                var imgs = divs[i].getElementsByTagName("img"); 
                for(j=0;j<imgs.length;++j)
                {
                    if (imgs[j].src.indexOf("dot_grey") != -1)
                    {
                        imgs[j].parentNode.style.verticalAlign="top";
                        imgs[j].parentNode.style.paddingTop = 5;
                    }
                }
            }
        }
    } 
} 
adjust_treenodes();
</script>       

