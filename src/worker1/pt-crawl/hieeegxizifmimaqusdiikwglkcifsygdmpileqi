<script type="text/javascript">
 function CheckAllSectionCheckBoxes(parentCheckBox)
 {
    parentId = parentCheckBox.id;
    parentId = parentId.substring(parentId.indexOf('ctlg_')).split('_')[1];
    inputControls = document.body.getElementsByTagName('input');
    for(i = 0; i < inputControls.length; i++)
    {
        element = inputControls[i];
        if (element.type == 'checkbox' && element.id.indexOf('ctlg_')!=-1)
        {
            checkboxId = element.id.substring(element.id.indexOf('ctlg_'));
            idFields = checkboxId.split('_');
            if (idFields.length == 3 && idFields[2] == parentId)
                element.checked = parentCheckBox.checked;
        }
    }
 }
</script>
