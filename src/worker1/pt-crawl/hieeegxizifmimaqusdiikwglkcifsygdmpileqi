<script type="text/javascript">
function showFilterPanel(panelControl, hiddenControl, titleControl, dropdownControl)
{
    if (panelControl.style.display == "none")
    {
        panelControl.style.display = document.all?"":"table-row";
        titleControl.style.backgroundColor = "#f7f6f4";
        dropdownControl.style.backgroundColor  = "#f7f6f4";
    }
    else
    {
        panelControl.style.display = "none";
        titleControl.style.backgroundColor  = "#ffffff";
        dropdownControl.style.backgroundColor  = "#ffffff";
    }
    hiddenControl.value = panelControl.style.display; 
}
</script>
