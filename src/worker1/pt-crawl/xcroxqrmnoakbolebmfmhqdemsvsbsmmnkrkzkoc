    <h1 itemprop="headline" class="headline">
    <span>CH. 7'S BRENON ON THE MARK AS RINK-SIDE REPORTER</span>
</h1>
    <div class="meta">
        <span>
            <ul class="list-inline">
        <li><span id="author--asset-5dec5712-eac2-5f37-96c9-2587a3fc8390" class="tnt-byline asset-byline" rel="popover" itemprop="author">
            By Alan Pergament
        </span></li>
        
        
            <li class="visible-print">
        <time datetime="1994-04-02T04:01:00-05:00" class="tnt-date asset-date text-muted">
            Apr 2, 1994
        </time></li>
            <li class="hidden-print">
        <time datetime="1994-04-02T04:01:00-05:00" class="tnt-date asset-date text-muted">Apr 2, 1994</time></li>
        
        
            <li class="hidden-print"><a href="https://buffalonews.com/news/ch-7s-brenon-on-the-mark-as-rink-side-reporter/article_5dec5712-eac2-5f37-96c9-2587a3fc8390.html?mode=comments" class="cm">
            <span class="fas tnt-comments"></span> 
            <span id="openWeb-count-5dec5712-eac2-5f37-96c9-2587a3fc8390-default" twt-comment-count="https://buffalonews.com/news/ch-7s-brenon-on-the-mark-as-rink-side-reporter/article_5dec5712-eac2-5f37-96c9-2587a3fc8390.html" twt-article-id="5dec5712-eac2-5f37-96c9-2587a3fc8390">0</span> 
        </a>
        <script type="text/javascript">
            if(window.Osano?.cm?.getConsent().MARKETING != 'ACCEPT' || window.Osano?.cm?.getConsent().ANALYTICS != 'ACCEPT'){
                document.getElementById("openWeb-count-5dec5712-eac2-5f37-96c9-2587a3fc8390-default").parentNode.style.display = 'none';
            } else {
            fetch("https://open-api.spot.im/v1/messages-count?spot_id=sp_CDo5xdS1&posts_ids=5dec5712-eac2-5f37-96c9-2587a3fc8390")
                .then(function(response) {
                    return response.json();
                })
                .then(function(openWeb) {
                    if (openWeb && openWeb.messages_count) {
                        document.getElementById("openWeb-count-5dec5712-eac2-5f37-96c9-2587a3fc8390-default").innerHTML = openWeb.messages_count['5dec5712-eac2-5f37-96c9-2587a3fc8390'];
