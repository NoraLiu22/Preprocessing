<script src="/scripts/Chart.min.js"></script>
<script>
function createChart(chartElement, jsonData){
	var chartData = { chartLabels: [], datasets: [] };
	// remove empty datasets
	var notEmpty = _.forEach(jsonData, function (value, key) {
		if (value.length < 1 ){
			delete jsonData[key];
		}
	});
	var chartType = chartElement.data("type") || "line";
	var chartType1 = chartElement.data("tip");
	var startElementValue = chartElement.data("start");
	var chartStartValue = parseFloat(startElementValue)
		
	
	var availableDatasets = Object.keys(notEmpty);
	var datasetSource = "";
	
	// parse returned dataset into format that is acceptable to chartJs
	for (var i = 0; i< availableDatasets.length; i++){
		var crtDatasetName = availableDatasets[i];
		var crtDatasetValueArray = jsonData[crtDatasetName];
		var crtDatasetData = [];

		chartData.datasets.push({}); // add new dataset empty object

		var crtDataset = chartData.datasets[i];
		crtDataset.fill = false;
		crtDataset.lineTension = 0;		
		crtDataset.data = [];
		if (availableDatasets.length === 2){
		 if (chartType1 === "mix"){
			if (i === 0 ){
				crtDataset.type = "line";
			}
				
		}
		}

										   
		for (var j = 0; j < crtDatasetValueArray.length; j++) {

			var crtDatasetXName = Object.keys(crtDatasetValueArray[j])[2];
			var crtDatasetXValue = crtDatasetValueArray[j][crtDatasetXName];

			var crtDatasetYName = Object.keys(crtDatasetValueArray[j])[3];
			var crtDatasetYValue = crtDatasetValueArray[j][crtDatasetYName];
			
                var crtDatasetColor = Object.keys(crtDatasetValueArray[j])[1];
				var crtDatasetLegendColor = crtDatasetValueArray[j][crtDatasetColor];
			
                var crtDatasetLegend = Object.keys(crtDatasetValueArray[j])[0];
				var crtDatasetLegendValue = crtDatasetValueArray[j][crtDatasetLegend];
			

			if (i === 0){
				if(chartType =="line" ||chartType =="bar" || chartType =="horizontalBar" || chartType =="mix") {
				chartData.chartLabels.push(crtDatasetXValue);
				}
				else
				{
					chartData.chartLabels.push(crtDatasetLegendValue);
				}
				
			}
			if (j === 0){
				
				var crtDatasetSource = Object.keys(crtDatasetValueArray[j])[4];
				var crtDatasetSourceText = crtDatasetValueArray[j][crtDatasetSource];
				if(chartType =="line" ||chartType =="bar" || chartType =="horizontalBar" || chartType =="mix") {
					
					
				crtDataset.label = crtDatasetLegendValue;
				crtDataset.borderColor = crtDatasetLegendColor;
					crtDataset.backgroundColor = crtDatasetLegendColor;
				}

				
				if (datasetSource.indexOf(crtDatasetSourceText) === -1 
						&& crtDatasetSourceText != null) {
					datasetSource += " " + crtDatasetSourceText;
				}
			}
			crtDatasetData.push(crtDatasetYValue);
			
		}
		chartData.datasets[i].data = crtDatasetData;

	}
	var datasetSourceElement = chartElement.parent().next();
	var currentText = datasetSourceElement.text();
	datasetSourceElement.text(currentText + datasetSource);
	
	var chartOptions = {
		cutoutPercentage: 30,
		responsive: true,
		maintainAspectRatio: false,
			point: {
				radius: 150,
			},
		legend:
		{
			display: chartData.datasets.length > 1,
			position: 'bottom',

		},
		
	};
		
		var indId = chartElement.attr("id").replace("chart-", "");  
    if (indId == '0902020102IND01'){
        chartOptions.scales= {
            yAxes: [{
                ticks: {
                    min: 0,
                    stepSize: 1, 
                    max: 5 
                }
            }]
        };
	}		
	
		
   if(chartType=="horizontalBar")
	{
		if (isNaN(chartStartValue) == false){
		chartOptions.scales = {
			xAxes: [{
				ticks: {
					min: parseFloat(chartStartValue),
				},
			}],
		};
		}
	}
	else{
	if (isNaN(chartStartValue) == false){
		chartOptions.scales = {
			yAxes: [{
				ticks: {
					min: parseFloat(chartStartValue) -10,
				},
			}],
		};
	}}
	if(chartType =="line" ||chartType =="bar" || chartType =="horizontalBar" || chartType =="mix") { 	
	return new Chart(chartElement, {
			type: chartType,
			data: {
				labels: chartData.chartLabels,
				datasets: chartData.datasets
			},
			options: chartOptions
		});
	}
	else
	{
	
	 var PieChartData =  
            {  
                labels:chartData.chartLabels,  
                datasets: [{  
                    label:  "Moja pita", 
                    backgroundColor: ["#00ad4d","#F3cd05","#006ab5","#AD2F3D","#2ba59f","#74a17d","#513b29","#d4a0ac", 
                         "#93c6ff","#ce9adf","#45c983","#982222","#e300ff","#cc5c5c","#348db8","#14a525","#ff0000","#00ff00",
                         "#0000ff","#000000","#00aedb","#a200ff","#f47835","#d41243",
									 ]
					,  
                    borderWidth: 2,  
                    data: crtDatasetData
                }]  
            };  
  
      	return new Chart(chartElement, {
                        type: 'pie',  
                        data: PieChartData,  
                        options:  
                            {  legend:        
		                             {  						
										position: 'right',
			
																	},  
                                title:  
                                {  
                                    display: true,  
                                    
                                },  
                                responsive: true,  
                                maintainAspectRatio: true  
                            }  
                    });  
            }  
	
};
$( document ).ready(function() {
	var chartElements = $('[id^=chart-]');
	var chartObjects = {};
	//var statPage = $(".indicator__carousel_list").data("stat-page");
	
	// create the carousel
	$('.indicator__carousel_list').slick({
		slidesToShow: 1,
		slidesToScroll: 1,
		autoplay: false,
		dots: true,
		autoplaySpeed: 2000,
	});
	
	// create the actual charts
	chartElements.each(function(index, element){
		var indicatorId = $(this).attr("id").replace("chart-", "");
		var selfElement = $(this);
		if (!indicatorId){
			return true;
		}
		$.ajax({
		  method: "GET",
		  url: "/umbraco/SorsApi/indicator/values",
		  data: { langName: "sr-Cyrl", indicatorId: indicatorId }
		})
		.done(function( msg ) {
			if (!msg){
				console.log('no values returned');
				return;
			}
			var datasets = $.parseJSON(msg);
			chartObjects[indicatorId] = createChart(selfElement, datasets);
		})
		.fail(function( jqXHR, textStatus ) {
		 	console.log('fail', 'jqXHR', jqXHR, 'textStatus', textStatus);
		});
	});
	// handle the clicking on chart events
	$('[data-stat-release-id]').each(function(){
		var element = $(this); 
		element.css('cursor','pointer');
		element.on('click', function() { 
			var elStatPage = $(this).closest('[data-stat-page]').data("stat-page");

			var elemetnId = $(this).data('stat-release-id');
			var backupDoc = $(this).data("stat-doc");
			$.ajax({
			  method: "GET",
			  url: "/umbraco/SorsApi/calendar/latestrelease",
			  data: { langName: "sr-Cyrl", subareaId: elemetnId }
			})
			.done(function( msg ) {
				if (msg == null || msg == 0){
					if (backupDoc.length <= 0) {
						return;
					}
					window.location = backupDoc;
					return;
				}
				window.location = backupDoc;  
			})
			.fail(function( jqXHR, textStatus ) {
				console.log('fail', 'jqXHR', jqXHR, 'textStatus', textStatus);
			});
		});
		
		
	});
});
</script>


	


