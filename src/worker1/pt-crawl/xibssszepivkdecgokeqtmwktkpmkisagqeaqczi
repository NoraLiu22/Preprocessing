</style> <script>
	window.onload = function () {

		//宽广告图动效

		var hadvShow = document.getElementsByClassName('hggtdx');
		for (var i = 0; i < hadvShow.length; i++) {
			hadvfx(i);
		}

		function hadvfx(k) {
			var imgShow = hadvShow[k].getElementsByTagName('a');
			if (imgShow.length == 0) {
				hadvShow[k].style.display = 'none';
			}
			else if (imgShow.length == 1) {
				imgShow[0].style.top = '0';
			} else {
				imgShow[0].style.top = '0';
				var kNum = 1;
				hadvShow[k].timer = setInterval(function () {
					for (var i = 0; i < imgShow.length; i++) {
						imgShow[i].style.top = '126px'
					}
					imgShow[kNum].style.top = '0';
					kNum++;
					if (kNum == imgShow.length) {
						kNum = 0;
					}
				}, 5000);
			}
		}



		//广告图动效

		var advShow = document.getElementsByClassName('ggtdx');
		for (var i = 0; i < advShow.length; i++) {
			advfx(i);
		}

		function advfx(k) {
			var imgShow = advShow[k].getElementsByTagName('a');
			if (imgShow.length == 0) {
				advShow[k].style.display = 'none';
			}
			else if (imgShow.length == 1) {
				imgShow[0].style.top = '0';
			} else {
				imgShow[0].style.top = '0';
				var kNum = 1;
				advShow[k].timer = setInterval(function () {
					for (var i = 0; i < imgShow.length; i++) {
						imgShow[i].style.top = '80px'
					}
					imgShow[kNum].style.top = '0';
					kNum++;
					if (kNum == imgShow.length) {
						kNum = 0;
					}
				}, 5000);
			}
		}


		//主导航动效
		var aNavli = document.getElementsByClassName('nav-con');
		aNavli[0].style.backgroundColor = '#5982c7';
		for (var i = 0; i < aNavli.length; i++) {
			aNavli[i].onmouseover = function () {
				aNavli[0].style.backgroundColor = '#194d98';
				this.style.backgroundColor = '#5982c7';
			}
			aNavli[i].onmouseout = function () {
				this.style.backgroundColor = '#194d98';
				aNavli[0].style.backgroundColor = '#5982c7';
			}
		}


		//地市导航动效
		var aDslist = document.getElementsByClassName('dishi-list');
		var aDishi = document.getElementsByClassName('nav-dishi');
		for (var i = 0; i < aDslist.length; i++) {
			dsShow(i);
		}

		//地市栏目展示
		function dsShow(k) {
			aDishi[k].onmouseover = function () {
				clearInterval(aDslist[k].timer);
				aDishi[k].firstElementChild.style.color = 'black';
				aDishi[k].firstElementChild.style.fontWeight = 'bold';
				aDslist[k].style.display = 'block';
				var opc = parseFloat(getStyle(aDslist[k], 'opacity'));
				aDslist[k].timer = setInterval(function () {
					opc += 0.10;
					aDslist[k].style.opacity = opc;
					if (opc >= 1) {
						clearInterval(aDslist[k].timer);
					}
				}, 40);
			}
			aDishi[k].onmouseout = function () {
				clearInterval(aDslist[k].timer);
				aDishi[k].firstElementChild.style.color = '#848484';
				var opc = parseFloat(getStyle(aDslist[k], 'opacity'));
				aDslist[k].timer = setInterval(function () {
					opc -= 0.10;
					aDslist[k].style.opacity = opc;
					if (opc <= 0) {
						aDslist[k].style.display = 'none';
						aDishi[k].firstElementChild.style.fontWeight = '';
						clearInterval(aDslist[k].timer);
					}
				}, 40);
			}
		}

		//头条


		var oFirst = document.getElementsByClassName('news_box')[0];
		var aLi = oFirst.getElementsByTagName('li');
		var aNewsFirst = oFirst.getElementsByClassName('first_news');
		var tNum = 0;
		for (var j = 0; j < aLi.length; j++) {
			turnShow(j);
			tNum = j;
		}
		function turnShow(k) {
			aLi[k].onmouseover = function () {
				for (var m = 0; m < aLi.length; m++) {
					aNewsFirst[m].id = '';
					aLi[m].id = '';
				}
				aNewsFirst[k].id = 'focus-list';
				aLi[k].id = 'focus';
				turnOpac(aNewsFirst[k], 1, 0.5, 1, 0.07);
			}
		}
		tShow();
		function tShow() {
			oFirst.rTimer = setInterval(function () {
				for (var m = 0; m < aLi.length; m++) {
					aNewsFirst[m].id = '';
					aLi[m].id = '';
				}
				if (tNum == 1) {
					tNum = 0;
				} else {
					tNum++;
				}
				aNewsFirst[tNum].id = 'focus-list';
				aLi[tNum].id = 'focus';
				turnOpac(aNewsFirst[tNum], 1, 0.5, 1, 0.07);
			}, 4000);
		}
		oFirst.onmouseover = function () {
			clearInterval(oFirst.rTimer);
		}
		oFirst.onmouseout = function () {
			tShow();
		}



		//三晋要闻切换
		var yws = document.getElementsByClassName('focleft')[0].getElementsByTagName('a');
		var ywt = document.getElementsByClassName('ywjd');
		yws[0].onmousemove = function () {
			yws[0].className = 'title-focus';
			yws[1].className = 'title-turn';
			ywt[0].style.display = 'block';
			ywt[1].style.display = 'none';
		}
		yws[1].onmousemove = function () {
			yws[1].className = 'title-focus';
			yws[0].className = 'title-turn';
			ywt[1].style.display = 'block';
			ywt[0].style.display = 'none';
		}

		//选项卡
		var aCard = document.getElementsByClassName('card');
		for (var i = 0; i < aCard.length; i++) {
			cordTurn(i);
		}
		function cordTurn(k) {
			var aCardli = aCard[k].getElementsByTagName('li');
			var aContBox = aCard[k].getElementsByClassName('cont1-box');
			for (var m = 0; m < aCardli.length; m++) {
				cardMove(m, aCardli, aContBox);
			}
		}

		function cardMove(j, obj, cont) {
			obj[j].onmouseover = function () {

				for (var num = 0; num < obj.length; num++) {
					obj[num].id = '';
					cont[num].id = '';
				}
				obj[j].id = 'focus-btn';
				cont[j].id = 'cont1-box';
			}
		}

		//title_1选项卡
		var oTitleTurn = document.getElementsByClassName('title_1_turn');

		var oTurnList = document.getElementsByClassName('tit-list');

		for (var i = 0; i < oTurnList.length; i++) {
			titleTurn(i);
		}
		function titleTurn(k) {
			var aTitle = oTitleTurn[k].getElementsByTagName('a');
			var aTitBox = oTurnList[k].getElementsByClassName('tit-box');
			for (var m = 0; m < aTitle.length; m++) {
				titTurn(m, aTitle, aTitBox);
			}

			function titTurn(k, obj, objbox) {
				aTitle[k].onmouseover = function () {
					for (var l = 0; l < obj.length; l++) {
						aTitle[l].className = 'title-turn';
						aTitBox[l].id = '';
					}
					this.className = 'title-focus';
					aTitBox[k].id = 'tit';
				}
			}
		}

		//山西风光
		var oDot = document.getElementsByClassName('line_5')[0];
		var aDotturn = oDot.getElementsByTagName('span');
		var aFg = document.getElementsByClassName('sxfg');
		var aFnimg = oDot.getElementsByTagName('a');
		var aFnp = oDot.getElementsByTagName('p');
		var Dnum = 50;
		for (var i = 0; i < aDotturn.length; i++) {
			aDotturn[i].style.right = Dnum + 'px';
			Dnum -= 15;
			fnFg(i);
		}
		function fnFg(i) {
			aDotturn[i].onmouseover = function () {
				for (var k = 0; k < aDotturn.length; k++) {
					aDotturn[k].style.opacity = 0.5;
					aFg[k].style.display = 'none';
				}
				this.style.opacity = 1;
				aFg[i].style.display = 'block';
			}
		}
		aDotturn[0].style.opacity = 1;
		aFg[0].style.display = 'block';

		//显示标题
		var aAct = document.getElementsByClassName('move-act');
		var aShow = document.getElementsByClassName('move-show');
		for (var i = 0; i < aAct.length; i++) {
			show(i);
		}
		function show(k) {
			aAct[k].onmouseover = function () {
				var len = parseFloat(getStyle(aShow[k], 'bottom'));
				stepMove(len, 40, 'bottom', aShow[k]);
			}
			aAct[k].onmouseout = function () {
				var len = parseFloat(getStyle(aShow[k], 'bottom')) - 50;
				stepMove(-len, 40, 'bottom', aShow[k]);
			}
		}

		//视频动画
		var aSpimg = document.getElementsByClassName('sp-img');
		var aDhact = document.getElementsByClassName('dh-box');
		for (var i = 0; i < aSpimg.length; i++) {
			spAction(i);
		}
		function spAction(k) {
			aDhact[k].onmouseover = function () {
				var opa = parseFloat(getStyle(aSpimg[k], 'opacity'));
				if (opa > 0.6) {
					turnOpac(aSpimg[k], aDhact[k], opa, 0.6, -0.07);
				} else if (opa <= 0.6) {
					aSpimg[k].style.opacity = 0.6;
				}
			}
			aDhact[k].onmouseout = function () {
				var opa = parseFloat(getStyle(aSpimg[k], 'opacity'));
				turnOpac(aSpimg[k], aDhact[k], opa, 1, 0.07);
			}
		}

		//右边切换
		var aSwc = document.getElementsByClassName('switch');



		for (var i = 1; i < aSwc.length; i++) {
			allAct(i);
		}

		function allAct(n) {
			var aRol = aSwc[n].getElementsByClassName('rol-img');
			var arolP = aSwc[n].getElementsByTagName('p');
			var arolimg = aSwc[n].getElementsByTagName('img');
			aRol[0].style.height = '210px';
			arolP[0].style.backgroundImage = 'url(http://www.sx.news.cn/ztjn/2017/imgs/tb-bg.png)';
			arolP[0].style.fontWeight = 'bold';
			arolimg[0].style.display = 'block';
			var actNum = 0;
			var onOff = true;
			for (var m = 0; m < arolP.length; m++) {
				turnAct(m);
			}

			function turnAct(k) {
				arolP[k].onmousemove = function () {
					if (onOff == true && k != actNum) {
						onOff = false;
						clearInterval(aRol[k].timer);
						var hei1 = 50;
						var hei2 = 210;
						aRol[k].timer = setInterval(function () {
							arolP[actNum].style.fontWeight = 'normal';
							arolP[k].style.fontWeight = 'bold';
							arolP[actNum].style.backgroundImage = '';
							arolP[k].style.backgroundImage = 'url(http://www.sx.news.cn/ztjn/2017/imgs/tb-bg.png)';
							arolimg[actNum].style.display = 'none';
							arolimg[k].style.display = 'block';
							hei1 += 30;
							hei2 -= 30;
							if (hei1 >= 210) {
								hei1 = 210;
								hei2 = 50;
								clearInterval(aRol[k].timer);
							}
							aRol[actNum].style.height = hei2 + 'px';
							aRol[k].style.height = hei1 + 'px';
							if (aRol[k].offsetHeight == 210) {
								actNum = k;
								onOff = true;
							}
						}, 20);
					}
				}
			}
		}

		//焦点图-新
		var btnBox = document.getElementsByClassName('focus-btns')[0];
		var btns = btnBox.getElementsByTagName('span');
		var focusM = document.getElementsByClassName('cont-box')[0];
		var fnum = 0;
		for (var i = 0; i < btns.length; i++) {
			onMuse(i);
		}
		var timerF = setInterval(function () {
			if (fnum == btns.length - 1) {
				fnum = 0;
			} else {
				fnum++;
			}
			focus(fnum);
		}, 3000);
		function onMuse(k) {
			btns[k].onmouseover = function () {
				clearInterval(timerF);
				fnum = k;
				focus(fnum);
			}
			btns[k].onmouseout = function () {
				timerF = setInterval(function () {
					if (fnum == btns.length - 1) {
						fnum = 0;
					} else {
						fnum++;
					}
					focus(fnum);
				}, 3000);;
			}

		}
		function focus(k) {
			for (var i = 0; i < btns.length; i++) {
				btns[i].id = '';
			}
			btns[k].id = 'focus-btns';
			focusM.style.left = -k * 775 + 'px';
		}
	}
</script> <script src="http://www.sx.news.cn/ztjn/2017/js/moveModel.js"></script> <body><div class="nav_list"><div class="nav1"><ul><li class="nav-list1"><a href="http://203.192.6.89/xhs/">新华社简介</a></li> <li class="nav-list1"><a href="http://www.xinhuanet.com/">新华网首页</a></li> <li class="nav-list1"><a href="http://www.gov.cn/">中国政府网</a></li></ul></div> <div class="func"><div class="logo"><a href="#" class="logo-link"><img src="http://www.sx.news.cn/ztjn/2017/imgs/logo.jpg" width="100%" height="100%"></a></div> <div class="adver-logo"><div class="imgs_box ggtdx"><a href="http://sx.news.cn/2022zt/gzlfz.htm"><img src="http://sx.news.cn/gg/2023gg/imgs/gzl02.jpg" width="100%" height="100%"></a></div> <div class="imgs_box ggtdx"><a href="http://sx.news.cn/2022zt/xczx09.htm"><img src="http://sx.news.cn/gg/2023gg/imgs/xczx02.jpg" width="100%" height="100%"></a></div></div> <div class="icon-logo"><a href="http://www.sx.news.cn/2017zt/txcj.htm"><img src="http://www.sx.news.cn/ztjn/2017/imgs/icon1.jpg">大学生图像采集</a> <a href="http://www.sx.news.cn/shanxiold/sxfc/sxfx-n.htm"><img src="http://www.sx.news.cn/ztjn/2017/imgs/icon2.jpg">新华社山西分社</a></div></div> <div class="adver1 ggtdx"><a href="http://sx.news.cn/ztjn/2023zt/xfzxqx11/index.html"><img src="  http://sx.news.cn/gg/2023gg/xfzxqx11.jpg" width="100%" height="100%"></a> <a href="http://sx.news.cn/ztjn/2023zt/gbsx06/index.html"><img src="http://sx.news.cn/gg/2023gg/imgs/gb061.jpg" width="100%" height="100%"></a> <a href="http://sx.news.cn/ztjn/2023zt/nygm07/index.html"><img src="http://sx.news.cn/gg/2023gg/imgs/nygm071.jpg" width="100%" height="100%"></a> <a href="http://sx.news.cn/ztjn/2023zt/yhgzl12/index.html"><img src=" http://sx.news.cn/gg/2023gg/imgs/sxyhgzl12.jpg" width="100%" height="100%"></a> <a href="http://sx.news.cn/ztjn/ztjn2024/sjfy04/index.html"><img src="http://sx.news.cn/gg/2024gg/imgs/sjfygg04.jpg" width="100%" height="100%"></a> <a href="http://sx.news.cn/ztjn/ztjn2024/xhzsyjy04/index.html"><img src="http://s