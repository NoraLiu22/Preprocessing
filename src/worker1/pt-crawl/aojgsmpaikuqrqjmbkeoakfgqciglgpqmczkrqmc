jQuery.validator.addMethod("specialChars", function( value, element ) {
        var regex = new RegExp("^([a-zA-Z0-9_\r\n\t\f\v \s':;.@#!,\-]+)?$");
        var key = value;
        if (!regex.test(key)) {
           return false;
        }
        return true;
    }, "please use only alphanumeric or alphabetic characters");    
    
  var validator = $( "#PersonalisedGifts" ).validate(
      {
          rules:{
              Name:{
                  specialChars:true
              },
              Message1:{
                  specialChars:true
              },
              Message2:{
                  specialChars:true
              },
              Message3:{
                  specialChars:true
              },
              Message4:{
                  specialChars:true
              },
              Message5:{
                  specialChars:true
              }
              
          }
      }
);

 

function customProductValidate() {
 return validator.form();
}
