//<![CDATA[

  function sendEmail() {
    var args = [];
    var emailStr = "mailto:FDA508Coordinator@fda.hhs.gov";
    var body = "name: "+encodeURIComponent(document.getElementById('name').value);
    body+="%0D";
    body+="phone: "+encodeURIComponent(document.getElementById('phone').value);
    body+="%0D";
    body+="email: "+encodeURIComponent(document.getElementById('email').value);
    body+="%0D";
    body+="webaddr: "+encodeURIComponent(document.getElementById('webaddr').value);
    body+="%0D";
    body+="issuecmts: "+encodeURIComponent(document.getElementById('issuecmts').value);
    emailStr += '?' + "body="+body;
    window.location.href = emailStr;
  }

