			<main>
				
				<script>
				'use strict';
					(function() {
						document.addEventListener('pjax:success', function() {
	const hoverText = document.querySelector('.productLinkHoverText');
	const onMouseMove = function(event) {
		hoverText.style.top = `${event.pageY}px`;
		if(event.pageX + hoverText.clientWidth > window.innerWidth) {
			hoverText.classList.add('productLinkHoverText--reverse');
			hoverText.style.right = `${window.innerWidth - event.pageX}px`;
			hoverText.style.left = 'unset';
		} else {
			hoverText.classList.remove('productLinkHoverText--reverse');
			hoverText.style.left = `${event.pageX}px`;
			hoverText.style.right = 'unset';
		}
	};

	window.addEventListener('mousemove', onMouseMove);
	let hoverTransition;
	document.querySelectorAll('.productLink').forEach((el) => {
		el.addEventListener('mouseenter', function(event) {
			clearTimeout(hoverTransition);
			hoverText.style.opacity = 1;
			hoverText.style.display = 'flex';
		});
		el.addEventListener('mouseleave', function(event) {
			hoverText.style.opacity = 0;
			hoverTransition = setTimeout(function() {
				hoverText.style.display = 'none';
			}, 200);
		});
	});

	document.addEventListener('pjax:send', function() {
		window.removeEventListener('mousemove', onMouseMove);
	}, {once: true});
}, {once: true});
					})();
				</script>

				


