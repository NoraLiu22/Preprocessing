<script>
document.addEventListener('tracking:pageInit', function() {
	try {
		const product_ids = [];
		document.querySelectorAll('.productIndex__product').forEach(function(el) {
			product_ids.push({
				product: parseInt(el.dataset.productId),
				variant: parseInt(el.dataset.variantId),
			});
		});
		const impressions = product_ids.map(function(ids, i) {
			return track__buildImpression(ids.product.toString(), ids.variant.toString(), i+1, undefined, window.location.pathname);
		});
		track_collectionView(impressions);
	} catch(e) {
		console.error(e);
	}
}, {once: true});

document.addEventListener('pjax:success', function() {
	try {
		document.querySelectorAll('.productIndex__product').forEach(function(el, i) {
			const p_id = parseInt(el.dataset.productId);
			const v_id = parseInt(el.dataset.variantId);
			el.addEventListener('click', function(e) {
				const prod = track__buildProduct(p_id.toString(), v_id.toString(), undefined, i);
				track_productClick(prod);
			});
		});
	} catch(e) {
		console.error(e);
	}
}, {once: true});
