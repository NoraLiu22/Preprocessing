<div class="wp-site-blocks"><header class="wp-block-template-part"><script>
    function headerData() {
        return {
            open: false,
            transparentMenuModule: true,
            canBeTransparent: true,
            menuIsOpen: false,
            hideOnScrollModule: true,
            shouldHide: false,
            lastPos: window.scrollY + 0,
            isOnTop: true,
            scroll() {
                this.isOnTop = (window.pageYOffset > 150) ? false : true;
                this.shouldHide = this.hideOnScrollModule & ( window.scrollY > this.$refs.header.offsetHeight * .8 && window.scrollY >= this.lastPos );
                setTimeout(() => { this.lastPos = window.scrollY }, 10);
            }
        }
    }

    jQuery(function($) {

        // Turn parent menu item into accordion elements
        $(document).on('click', '.menu-center-burger-esturgeon .menu-item-has-children > a', function(ev) {
            ev.preventDefault();

            var $link = $(ev.currentTarget),
                $li = $link.parent(),
                $submenu = $link.next();

            // Reset accordion & submenus states
            $li.toggleClass('is-open');
            $submenu.toggleClass('hidden');
        });

        // Reset state of overlay scrollbars & block body scroll while menu open
        $(document).on('click', '.menu-center-burger-esturgeon  #burger', function(ev) {

            var $burger = $(ev.currentTarget),
                $header = $burger.parents('header'),
                $body   = $('body'),
                $link = $header.find('.menu-item-has-children > a'),
                $li = $link.parent(),
                $submenu = $link.next();

            // Lock / unlock body scroll
            $body.toggleClass('overflow-hidden');

            // Reset accordion & submenus states
            $li.removeClass('is-open');
            $submenu.addClass('hidden');
        });
    });

</script>
