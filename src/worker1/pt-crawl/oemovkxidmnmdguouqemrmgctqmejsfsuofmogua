<script type=text/javascript>
    jQuery(window).resize(function(){
		console.log(jQuery(window).height());
		resizeHeightBlocFloat();
	})
	
	function resizeHeightBlocFloat(){
		if(jQuery(window).width() >= 1024){
			var heightBlocFloat = jQuery('.testcyllene1').height();
			var heightBlocFloatPart2 = heightBlocFloat/2;
			console.log(heightBlocFloat);
			console.log(heightBlocFloatPart2);
			jQuery('.columns-contained-background-section-part2').css('height', heightBlocFloatPart2);
			
			if(heightBlocFloat > 310){
				 jQuery('.testcyllene1').css('transform', 'translate(-50%, -39%)');
			}
		}else{
			jQuery('.columns-contained-background-section-part2').css('height', 0);
			jQuery('.testcyllene1').css('transform', 'none');
		}	
	}
	
	console.log(jQuery(window).width());
	resizeHeightBlocFloat();
</script>



