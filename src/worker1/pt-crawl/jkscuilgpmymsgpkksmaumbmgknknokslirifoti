                                    renewalPaymentMethodType = "card";
                                    
                                if(outputEl){ outputEl.innerHTML = ""; }if(oData.items.length){ bFoundItems = true }oData.items.sort(function(itemA, itemB){
                                    if((itemA.year < itemB.year) || ((itemA.year == itemB.year) && (itemA.month < itemB.month))){ return 1 }
                                    
                                    return -1
                                });

                                filteredItems = oData.items.filter(function(item){
                                    if((item.year > dateCheck.getUTCFullYear()) || ((item.year == dateCheck.getUTCFullYear()) && (item.month >= dateCheck.getUTCMonth()))){ return true }

                                    return false;
                                });filteredItems = filteredItems.filter(function(item){
                                    if(uniqueItems.indexOf(item.last_four_digits) < 0){ uniqueItems.push(item.last_four_digits); return true; }
                                    
                                    return false;
                                });

                                limitedItems = filteredItems.slice(0,3);
                                

                                if(limitedItems.length){
                                    limitedItems.forEach(function(item){
                            
                                        var cardType = "credit card",
                                            cardIcon = null,
                                            cardIconEl = null,
                                            cardLabel = "Credit Card",
                                            expClass = "tn-cc-exp text-muted";

                                        if(item.card_type && cardData[item.card_type] && cardData[item.card_type].name){
                                           cardLabel = cardData[item.card_type].name;
                                           cardIconEl = iconsEl.querySelector("." + item.card_type);

                                           if(cardIconEl && cardIconEl.content){
                                               cardIcon = cardIconEl.content.cloneNode(true);
                                           }
                                        } else {
                                            cardIconEl = iconsEl.querySelector(".other");

                                            if(cardIconEl && cardIconEl.content){
                                                cardIcon = cardIconEl.content.cloneNode(true);
                                            }
                                        }

                                        if(item.year == dateCheck.getUTCFullYear() && item.month <= (dateCheck.getUTCMonth()+1)){
                                            expClass = "tn-cc-exp text-warning"
                                        }

                                        var elements = [
                                           {
                                               'element' : 'a.btn',
                                               'attr' : [
                                                   {
                                                       'attr': 'data-token',
                                                       'value': JSON.stringify(item)
                                                   }
                                               ]
                                           },{
                                               'element' : '.tn-cc-label',
                                               'html' : cardLabel
                                           }, {
                                               'element' : '.tn-cc-last-four',
                                               'html' : item.last_four_digits
                                           }, {
                                               'element' : '.tn-cc-exp',
                                               'attr' : [
                                                   {
                                                       'attr' : 'class',
                                                       'value' : expClass
                                                   }
                                               ],
                                               'html' : " Exp " + item.month + "/" + item.year
                                           }
                                        ];

                                        __tnt.template(
                                            templateEl, // template element
                                            outputEl,   // output element
                                            false,           // add items from start of output
                                            elements,       // elements in template to modify
                                            function(t){    // callback to run after template is added

                                                var link = t.querySelector('a'),
                                                    cardIconSVG = null;

                                                if(cardIcon){ 
                                                    cardIconSVG = document.importNode(cardIcon, true);
                                                    t.querySelector('a .tn-cc-lg').appendChild(cardIconSVG);
                                                }t.querySelector('a').addEventListener('click',function(ev){
                                                    setWalletItemSpreedly(this);
                                                });
                                            });
                                        });

                                        if(expiredNotification && !bFoundRenewalToken && renewalPaymentMethodType == "card"){
                                            expiredNotification.hidden = false;
                                            expiredNotification.classList.remove('fade');
                                        }

                                        if(thirdPartyPaymentNotification && !bFoundRenewalToken && renewalPaymentMethodType != "card"){
                                            if(renewalPaymentMethodType == "sprel"){
                                                thirdPartyPaymentNotification.querySelector('.renewal-method').innerHTML = "Paypal";
                                            }
                                            
                                            thirdPartyPaymentNotification.hidden = false;
                                            thirdPartyPaymentNotification.classList.remove('fade');
                                        }
                                
                                        // TNT Payment
                                        resolve({ complete: true });

                                        oForm.dispatchEvent(tntPayment); 
                                    }else{
                                        resolve({ complete: true });
                                    }

                                });
                            }
                        }).catch(function(error){
                            console.log("ERROR: Problem fetching wallet data - ", error);

                            resolve({ complete: true });
                        });
                    }); // wrapping promise
                }else{
                    if(oForm){ 
                        oForm.dispatchEvent(tntPayment);
                    }
                }


                var oButton = document.getElementById('form-submit-super-purchase'),
                    oTOS = document.getElementById('tn-tos-super-purchase');

                oTOS.addEventListener('change',function(ev){
                    SpreedlyallowSubmit();
                });paymentPromisesSpreedly[paymentPromisesSpreedly.length] = new Promise(function(resolve, reject){ 
                        var serviceId = oForm['service_id'].value,
                            rateId = oForm['rate_id'].value,
                            fetchUrl = "/tncms/subscription/services/",
                            searchParams = new URLSearchParams();

                        if(serviceId){ searchParams.append('service_id', serviceId) }
                        if(rateId){ searchParams.append('rate_id', rateId) }

                        fetch(fetchUrl + "?" + searchParams.toString(),{
                            headers: {
                               'X-Requested-With': 'XMLHttpRequest'
                            }
                        }).then(function(oResp){
                            if(oResp.ok){
                                oResp.json().then(function(oData){

                                    /* Paypal Mode */
                                    try {
                                        var enablePaypal = false;
                                        oData[0].payment_methods.forEach(function(item){
                                            if(item.type == "paypal"){ enablePaypal = true; }
                                        });

                                        var outputElement = document.getElementById('tn-pay-method-alt-super-purchase'),
                                            paypalButton = outputElement.querySelector('.tn-pay-method-paypal'),
                                            newCardButton = document.getElementById('tn-pay-method-new-card-super-purchase');
                                        
                                        if(!paypalButton && enablePaypal){
                                            paypalButton = document.createElement('a');
                                            paypalButton.id = "paypal-payment-super-purchase";
                                            paypalButton.classList.add("btn", "btn-lg", "btn-block", "wallet-btn", "tn-pay-method-paypal", "text-center", "tn-pay-method-alt");
                                            paypalButton.style = "";
                                            paypalButton.innerHTML = '<i class="fab tnt-paypal"></i> Paypal';
                                            paypalButton.addEventListener('click',function(ev){
                                                var paymentForm = document.getElementById('form-super-purchase');
                                            
                                                // Set Form to Paypal Mode
                                                paymentForm.setAttribute('data-paypal-mode', true);

                                                // Fire events if this is not already the selected method
                                                if (!paypalButton.classList.contains('tn-pay-method-selected')) {
                                                    // Log checkout step events for PayPal
                                                    __tnt.subscription.offers.logCheckoutStepViewed({
                                                        step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                                                        label: 'Selected payment method',
                                                        paymentMethod: 'PayPal'
                                                    });
                                                    __tnt.subscription.offers.logCheckoutStepCompleted({
                                                        step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                                                        label: 'Selected payment method',
                                                        paymentMethod: 'PayPal'
                                                    });

                                                    // Log payment info entered event for PayPal
                                                    __tnt.subscription.offers.logPaymentInfoEntered({
                                                        step: __tnt.subscription.offers.getPurchaseStep('payment_info'),
                                                        paymentMethod: 'PayPal'
                                                    });
                                                }

                                                // do not require elements for paypal purchase
                                                if(paymentForm['tnpay-name'].required){
                                                    paymentForm['tnpay-name'].required = false;
                                                    paymentForm['tnpay-name'].setAttribute('data-required-for-cc', true);
                                                }
                                            });
                                            
                                            outputElement.appendChild(paypalButton);
                                        }
                                    } catch(e){ console.log(e) }

                                    resolve({ complete: true });
                                });
                            }
                        });
                    });/* All Payment Methods Ready */
                Promise.all(paymentPromisesSpreedly).then(function(aValues){
                    paymentPromisesSpreedly = [];

                    var payMethodSelect = document.getElementById('tn-pay-method-select-super-purchase'),
                        payMethodCard = document.getElementById('tn-pay-manual-entry-super-purchase'),
                        selectedPayment = null,
                        altPaymentContainer = document.getElementById('tn-pay-method-alt-super-purchase'),
                        altPayment = false,
                        purchaseMode = false,
                        paypalPaymentMethod = null;
  
                    if(oForm){
                        purchaseMode = oForm.dataset.purchaseType;
                    }

                    if(payMethodSelect){ 
                        payMethods = payMethodSelect.querySelectorAll('a');
                        selectedPayment = payMethodSelect.querySelector('a.tn-pay-method-selected');
                        altPayment = payMethodSelect.querySelector('a.tn-pay-method-alt');
                    }

                    if(altPaymentContainer && purchaseMode == 'gift'){
                        // disable paypal for gift purchase
                        paypalPaymentMethod = altPaymentContainer.querySelector('.tn-pay-method-paypal');

                        if(paypalPaymentMethod){ 
                            paypalPaymentMethod.remove();
                            payMethods = payMethodSelect.querySelectorAll('a');
                        }
                    }
                
                    if(payMethods.length > 1){

                        if(selectedPayment && selectedPayment.classList.contains('wallet-show-card')){
                            setWalletItemSpreedly(selectedPayment);
                        }

                        if(payMethodSelect && payMethodCard) {
                            payMethodSelect.hidden = false;
                            payMethodCard.hidden = true;
                        }

                        payMethods.forEach(function(payMethod){
                            payMethod.addEventListener('click',function(ev){
                                ev.preventDefault();
                                ev.stopPropagation();
                                
                                payMethods.forEach(function(item){
                                    item.classList.remove('tn-pay-method-selected');
                                });
                                
                                this.classList.add('tn-pay-method-selected');
                                if(this.classList.contains('wallet-show-card')){
                                    payMethodCard.hidden = false;
                                }else{
                                    payMethodCard.hidden = true;
                                }
                                
                                if(!this.classList.contains('tn-pay-method-paypal')){
                                    oForm.removeAttribute('data-paypal-mode');
                                }
                            });
                        });
                    }else if(payMethodCard && payMethodSelect){
                        payMethodCard.hidden = false;
                        payMethodSelect.hidden = true;
                    }
                });

            });

            function spreedlyInit_Spreedly() {
            var scriptPromises = [];
            scriptPromises.push(__tnt.loadScript('https://core.spreedly.com/iframe/iframe-v1.min.js'));
            
                scriptPromises.push(__tnt.loadScript('https://www.google.com/recaptcha/enterprise.js?render=6LdF3BEhAAAAAEQUmLciJe0QwaHESwQFc2vwCWqh'));
            

            Promise.all(scriptPromises).then(
                function() {
                    window.Spreedly = new SpreedlyPaymentFrame();

                    Spreedly.init("X7rJZ6QgeO39MdUWonvRd9m0BB7", {
                        "numberEl": "spreedly-number-super-purchase",
                        "cvvEl": "spreedly-cvv-super-purchase"
                    });

                    window.setWalletItemSpreedly = function(card) {
                        var token = JSON.parse(card.dataset.token),
                            payCardWrapper = document.getElementById('tn-pay-manual-entry-super-purchase'),
                            payMethodWrapper = document.getElementById('tn-pay-method-output-super-purchase'),
                            payMethodNewCard = document.getElementById('tn-pay-method-new-card-super-purchase'),
                            payMethodSelect = document.getElementById('tn-pay-method-select-super-purchase'),
                            payMethodCard = document.getElementById('tn-pay-manual-entry-super-purchase'),
                            oMonth = null,
                            oYear = null,
                            oName = null,
                            oZip = null;

                        if(oForm){
                            oForm.querySelector('input[name=tnpay-token]').value = token.id;

                            // Get required, non-sensitive, values from host page
                            oMonth = oForm.querySelector('select[name=tnpay-month]');
                            oYear = oForm.querySelector('select[name=tnpay-year]');

                            if(oMonth){ oMonth.value = token.month }
                            if(oYear){ oYear.value = token.year }

                            oForm.classList.add('use-saved-card');
                            
                            // Clear required when in wallet mode
                            payMethodCard.hidden = true;
                            payMethodSelect.hidden = false;
                            
                            oName = oForm.querySelector('input[name=tnpay-name]');
                            oZip = oForm.querySelector('input[name=postal_code]');
                            
                            oName.required = false;
                            oZip.required = false;
                        }

                        SpreedlyIdentifyPaymentCard(token.card_type);
                        payCardWrapper.hidden = false;

                        Spreedly.setRecache(token.id, {
                            'card_type': token.card_type,
                            'last_four_digits': token.last_four_digits
                        });

                        Spreedly.setParam('allow_blank_name', true);
                        Spreedly.setParam('allow_expired_date', true);

                        if (!card.classList.contains('tn-pay-method-selected')) {
                            __tnt.subscription.offers.logCheckoutStepViewed({
                                step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                                label: 'Selected payment method',
                                paymentMethod: 'Credit Card'
                            });
                            __tnt.subscription.offers.logCheckoutStepCompleted({
                                step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                                label: 'Selected payment method',
                                paymentMethod: 'Credit Card'
                            });
                        }
                    }

                    Spreedly.on('ready', function(){
                        Spreedly.setFieldType("number", "text");
                        Spreedly.setNumberFormat("prettyFormat");
                        Spreedly.setPlaceholder("number", "Card number"); // Card number
                        Spreedly.setPlaceholder("cvv", "CVV"); // CVV
                        Spreedly.setStyle("number", 'font-family: "Helvetica Neue",Helvetica,Arial,sans-serif; font-size: 14px;padding:2px 0 0');
                        Spreedly.setStyle("cvv", 'font-family: "Helvetica Neue",Helvetica,Arial,sans-serif; font-size: 14px;padding: 2px 0 0');
                        
                        var oFormEl = document.getElementById('form-super-purchase');
                            oFormEl.classList.remove('use-saved-card');
                            
                        SpreedlyIdentifyPaymentCard(null);
                    });

                    Spreedly.on('errors', function(aErrors) {
                        var errorAlert = document.getElementById('form-error-super-purchase'),
                            errorOutput = "",
                            modalTabPurchase = document.getElementById('user-modal-tab-purchase');

                        if(errorAlert){
                            for ( const oError of aErrors ) {
                                errorOutput += "Error saving card " + oError["key"] + ": " + oError["message"];
                            }

                            errorAlert.querySelector('.error-message').innerHTML = errorOutput;
                            errorAlert.hidden = false;
                            errorAlert.classList.remove('fade');

                            if(modalTabPurchase){
                                __tnt.subscription.access.showTab(modalTabPurchase, "access-offers-modal", "access-methods");
                            }
                        }
                    });

                    Spreedly.on('fieldEvent', function(name, type, activeEl, inputProperties) {
                        if (type == 'input') {
                            var oForm = document.getElementById('form-super-purchase'),
                                cardType = null;

                            if (!oForm.classList.contains('use-saved-card') && activeEl == 'number') {
                                if (inputProperties.cardType != null) {
                                    cardType = inputProperties.cardType;
                                }

                                SpreedlyIdentifyPaymentCard(cardType);
                            }

                            if (name == 'number' || name == 'cvv') {
                                __tnt.subscription.offers.ccValidNumber = inputProperties.validNumber || (inputProperties.maskedNumber && inputProperties.maskedNumber.length > 0);
                                __tnt.subscription.offers.ccValidCvv = inputProperties.validCvv;

                                if (__tnt.subscription.offers.ccInputsEntered()) {
                                    // Log payment info entered event
                                    __tnt.subscription.offers.logPaymentInfoEntered({
                                        step: __tnt.subscription.offers.getPurchaseStep('payment_info'),
                                        paymentMethod: 'Credit Card'
                                    });
                                }
                            }
                        }
                    });

                    Spreedly.on('paymentMethod', function(token, oMethod){
                        SpreedlyPayment(token, oMethod);
                    });
                    Spreedly.on("recache", function(token, oMethod){
                        SpreedlyPayment(token, oMethod);
                    });
                },
                function(error) {
                    console.error(error);
                }
            );
        }
    } /* end spreedly initialization check */
})(); /*]]>*/</script>
