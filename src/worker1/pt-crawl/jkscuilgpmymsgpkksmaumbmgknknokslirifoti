                                           }, {
                                               'element' : '.tn-cc-exp',
                                               'attr' : [
                                                   {
                                                       'attr' : 'class',
                                                       'value' : expClass
                                                   }
                                               ],
                                               'html' : " Exp " + item.month + "/" + item.year
                                           }
                                        ];

