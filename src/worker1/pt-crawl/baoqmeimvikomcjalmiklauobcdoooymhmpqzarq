    <script type="text/javascript">
        function change(year, month) {
            var dtStart = new Date(year, month - 1, 1);
            var dtEnd = new Date(year, month, 1);
            $("#pdfday").html("");
            for (var i = 1; i <= parseInt((dtEnd - dtStart) / 86400000); i++) {
                if (Number(year) >= 2022) {
                    var dtTempNow = new Date(year, month - 1, i);
                    if (dtTempNow.getDay() == 0)continue;
                }
                $("#pdfday").append("<option value='" + i + "'>" + i + "</option>");
            }
        }

        $(function () {
            $("#pdfyear").change(function () {
                change(Number($("#pdfyear").val()), Number($("#pdfmonth").val()));
            })

            $("#pdfmonth").change(function () {
                change(Number($("#pdfyear").val()), Number($("#pdfmonth").val()));
            });
        })
    
    </script>

            
