    function initAppPopover() {
        if ($.fn.popover) {
            $('.app').popover({
                trigger: 'hover',
                title: "<h4>Scan the QR code</h4>",
                html: true,
                content: "<img src='/images/QRcode5.png' style='width:240px;height:240px;'>"
            });
        } else {
            setTimeout(initAppPopover, 300);
        }
    }
    setTimeout(initAppPopover, 300);
</script>
