<script type="text/javascript">
    // This loops through the divs on a page, looking for the "articleoptions" divs.
    // We're rewriting those divs to add comment-number information and share-this info.
    // 
    // 
    // We don't want to run this on the home page or section fronts.
    if ( ArticleTitle != "null" )
    {
        var tags = document.getElementsByTagName("DIV");
        var y=0;
        //var dp_buzzrewrite = document.getElementById('buzzholder').innerHTML;
        for(x=0; x<tags.length; x++)
        {
            /*
            if( tags[x].className == "listingItemTitle" && tags[x].innerHTML.indexOf("Sorry, no items are currently available.") != -1)
            {
                var clb = tags[x].parentNode;
                //Traverse the DOM tree backwards until we hit a table with a class name of "complexListingBox"
                while(clb.className != "complexListingBox")
                {
                    clb = clb.parentNode;
                }
                clb.style.display="none";
            }
            else 
            */
            
            if ( tags[x].className == "articleOptions" )
            {
                var commenttitle = "View comments on this article";
                if ( commentnumber == 1 ) 
                {
                    var commentlabel = "Comment";
                }
                else 
                {
                    if ( commentnumber == 0 ) var commenttitle = "Add your comments to this article";
                    var commentlabel = "Comments";
                }
            

                //var defaultlinks = '<a onclick="return addthis_sendto()" onmouseout="addthis_close()" onmouseover="return addthis_open(this, \'\', \'[URL]\', \'[TITLE]\')" href="https://web.archive.org/web/20100121133535/http://www.addthis.com/bookmark.php" class="articleOptions"><img src="https://web.archive.org/web/20100121133535/http://extras.denverpost.com/images/icon-share.gif" alt="" border="0"></a><a onclick="return addthis_sendto()" onmouseout="addthis_close()" onmouseover="return addthis_open(this, \'\', \'[URL]\', \'[TITLE]\')" href="https://web.archive.org/web/20100121133535/http://www.addthis.com/bookmark.php" class="articleOptions">Share</a> '
                
                var defaultlinks = '';
				var facebook_link = '<br><a name="fb_share" type="button_count" href="https://web.archive.org/web/20100121133535/http://www.facebook.com/sharer.php" share_url="https://web.archive.org/web/20100121133535/http://www.denverpost.com/ci_' + article_id + '?source=sb-facebook" style="display:block; margin-top:2px;">Share</a>';
				//facebook_link = '';
                var t = tags[x].innerHTML;
        
                //Link list at the top of the article, next to the headline:
                if ( y == 0 ) 
                {
                    tags[x].innerHTML = t + defaultlinks + '<br><a href="#comments" title="View comments on this article" class="articleOptions"><img src="https://web.archive.org/web/20100121133535/http://extras.mnginteractive.com/live/media/site36/2008/0409/20080409_060550_comment_article.gif" alt="Comments" width="16" height="16" border="0"></a><a href="#comments" title="' + commenttitle + '" class="articleOptions">' + commentnumber + ' ' + commentlabel + '</a>' + facebook_link;
                    
                    if ( typeof(article_flag) != "undefined" )
                    {
                        tags[x].innerHTML = tags[x].innerHTML + article_flag;
                    }
                }
        
                //Link list at the bottom of the article, below the last graf:
                else if ( y == 1 ) 
                {
                    tags[x].innerHTML = t + defaultlinks + '&nbsp;&nbsp;&nbsp;<strong>&raquo;</strong>&nbsp;<a href="https://web.archive.org/web/20100121133535/https://www.denverpostcustomercare.com/ZipCheck.asp?PageName=newstartstep1.asp&Pid=3&PageDisplay=P" title="Subscribe" class="articleOptions">Get Home Delivery</a><div id="share_this_bottom" class="social_bookmarks" style="display:none;">&nbsp;</div>';
                }

                y++;
            }
            else if ( tags[x].className == "social_bookmarks" && y == 0 )
            {
                tags[x].id = 'share_this';
            }
        }
    }
</script>



