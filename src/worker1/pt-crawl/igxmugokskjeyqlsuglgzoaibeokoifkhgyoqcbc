    function load_js(file_name)
    {
        var article_votes = new Number(document.getElementById('article_votes').innerHTML);
        document.getElementById('article_votes').innerHTML = article_votes + 1;
    }
    function refresh_votes()
    {
        setTimeout("load_js();",3000);
    }
</script>
