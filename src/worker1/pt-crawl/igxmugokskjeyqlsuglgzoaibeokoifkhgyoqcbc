function comment_order(action, forumtext, forumtext_reverse)
{
    var linktext = document.getElementById('commentreverse');
    var commentwrapper = document.getElementById('commentwrapper');
    if ( action == 'reverse' )
    {
        linktext.innerHTML = '<a href="#" onClick="comment_order(\'forward\', forumtext, forumtext_reverse); return false;">List the oldest comments at the top</a>';
        commentwrapper.innerHTML = forumtext_reverse;
        var comment_ul = document.getElementById('forumtext_reverse');
        comment_ul.style.display = 'block';
    }
    else if ( action == 'forward' )
    {
        linktext.innerHTML = '<a href="#" onClick="comment_order(\'reverse\', forumtext, forumtext_reverse); return false;">List the newest comments at the top</a>';
        commentwrapper.innerHTML = forumtext;
    }
}
if ( commentlast > 0 )
{
    //Oldest-to-newest (now commented out)
    //document.write('<p style="clear:both;">&raquo; <strong id="commentreverse"><a href="#" onClick="comment_order(\'reverse\', forumtext, forumtext_reverse); return false;">Switch comment order to newest-to-oldest</a></strong></p>');
    document.write('<p style="clear:both;">&raquo; <strong id="commentreverse"><a href="#" onClick="comment_order(\'forward\', forumtext, forumtext_reverse); return false;">Show oldest comments first</a> (currently newest comments are first)</strong></p>');
}
</script>
<div id="commentwrapper">
<script type="text/javascript">
if ( commentnumber > 0 ) 
{
    //Oldest-to-newest (now commented out)
    //if ( commentnumber > 15 && document.location.hash != '' ) document.write(forumtext_reverse);
    //else document.write(forumtext);
    document.write(forumtext_reverse);
}
</script>
</div><!-- close div#commentwrapper -->

