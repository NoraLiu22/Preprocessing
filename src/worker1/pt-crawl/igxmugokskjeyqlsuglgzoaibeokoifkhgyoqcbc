    var tmp = section[section.length-1].split('ci_');
    article_id = tmp[1].substr(0,8);
    var article_id_tmp = redirect_lookup(article_id);
    if ( article_id_tmp != article_id && article_id_tmp != '' ) article_id = article_id_tmp;

    //The section comes from the URL -- we split it at the slashes to get each part.
    //DEFINED ABOVE: var section = document.location.pathname.split('/');
    //If there is more than one part then we know we've got a section value
