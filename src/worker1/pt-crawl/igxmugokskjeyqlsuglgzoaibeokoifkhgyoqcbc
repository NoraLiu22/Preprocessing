{

    //If we have a value for the fifteenth comment then we link to that instead
    var viewall_top = '&raquo; <a href="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/viewtopic.php?t=' + article_id + '" style="margin-top: 5px;">View all this article\'s ' + commentnumber + ' comments</a><br>';
    if ( isNaN(commentsixteen) == true ) var commentsixteen = 0;
    if ( commentsixteen > 0 )
    {
        viewall_top = '&raquo; <a href="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/viewtopic.php?p=' + commentsixteen + '#p' + commentsixteen + '" style="margin-top: 5px;">View all this article\'s ' + commentnumber + ' comments</a>';
    }

    var viewall = viewall_top;
    // The "commentlast" variable was added to the comment feed 20080528
    if ( isNaN(commentlast) == true ) var commentlast = 0;
    if ( commentlast > 0 )
    {
        viewall += '<br>&raquo;  <a href="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/viewtopic.php?p=' + commentlast + '#p' + commentlast + '" style="margin-top: 5px;">Jump to the latest comment</a><br>';
    }
    viewall += '&raquo; <a href="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/viewtopic.php?t=' + article_id + '&bookmark=1" style="margin-top: 5px;">Bookmark this article</a><br>';
    viewall += '&raquo; <a href="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/viewtopic.php?t=' + article_id + '&watch=topic&start=0" style="margin-top: 5px;">Subscribe to this article\'s comments</a><br>';
    var formtag = '<form action="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/posting_check.php?thread=' + article_id + '" method="post">';
}
else 
{
    var viewall = '';
    var viewall_top = '';
    var formtag = '<form action="https://web.archive.org/web/20100121133535/http://neighbors.denverpost.com/posting_check.php" method="post">';
}
    document.write(viewall_top);
    document.write(formtag);
</script>
