if (document.images) {

var imageUrls = new  Array("/history/scottishhistory/images/web_guide/history_webguide_birth_of_a_nation_on.gif","/history/scottishhistory/images/web_guide/history_webguide_impact_of_the_monk_on.gif","/history/scottishhistory/images/web_guide/history_webguide_mysterious_ancestors_on.gif","/history/scottishhistory/images/web_guide/history_webguide_wars_of_independence_on.gif","/history/scottishhistory/images/web_guide/history_webguide_renaissance_on.gif");        var pic = new Array(imageUrls.length);

        for (i=0; i<imageUrls.length; i++) {
                pic[i] = new Image();
                pic[i].src = imageUrls[i];
        }
}



// Rolling over
function hiLite(imageName) {
        if (document.images) {
                top.document.images[imageName].src = "/history/scottishhistory/images/web_guide/" + imageName + "on.gif";
        }
}

// Rolling out
function backNormal(imageName) {
        if (document.images) {
                top.document.images[imageName].src = "/history/scottishhistory/images/web_guide/" + imageName + "off.gif";
        }
}


