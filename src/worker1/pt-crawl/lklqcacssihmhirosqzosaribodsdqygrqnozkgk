    function createScript(url, callback) {
      var oScript = document.createElement('script')
      oScript.type = 'text/javascript'
      oScript.async = true
      oScript.defer = true
      oScript.charset = 'utf-8'
      oScript.src = url
      /*
       ** script标签的onload和onreadystatechange事件
       ** IE6/7/8支持onreadystatechange事件
       ** IE9/10支持onreadystatechange和onload事件
       ** Firefox/Chrome/Opera支持onload事件
       */

      // 判断IE8及以下浏览器
      var isIE = !-[1]
      if (isIE) {
        oScript.onreadystatechange = function () {
          if (this.readyState == 'loaded' || this.readyState == 'complete') {
            callback()
          }
        }
      } else {
        // IE9及以上浏览器，Firefox，Chrome，Opera
        oScript.onload = function () {
          callback()
        }
      }
      document.body.appendChild(oScript)
    }
  </script> <script>
    function setupBanner(selector) {
      $(function () {
        var index = 0;
        var adtimer;
        var _wrap = $(selector + " ol");
        var len = $(selector + " ol h").length;

        if (len > 1) {
          $(selector).hover(function () {
              clearInterval(adtimer);
            },
            function () {
              adtimer = setInterval(function () {

                var _field = _wrap.find('h:first');
                var _h = _field.height();
                _field.animate({
                  marginTop: -_h + 'px'
                }, 500, function () {
                  _field.css('marginTop', 0).appendTo(_wrap);
                })

              }, 5000);
            }).trigger("mouseleave");

          $(selector).mouseover(function () {
            $(selector + " .mouse_direction").css("display", "block");
          });
          $(selector).mouseout(function () {
            $(selector + " .mouse_direction").css("display", "none");
          });
        }

        $(selector).find(".mouse_top").click(function () {
          var _field = _wrap.find('h:first');
          var last = _wrap.find('h:last');
          var _h = last.height();
          $(selector + " ol").css('marginTop', -_h + "px");
          last.prependTo(_wrap);
          $(selector + " ol").animate({
            marginTop: 0
          }, 500);
        });

        $(selector).find(".mouse_bottom").click(function () {
          var _field = _wrap.find('h:first');
          var _h = _field.height();
          _field.animate({
            marginTop: -_h + 'px'
          }, 500, function () {
            _field.css('marginTop', 0).appendTo(_wrap);
          })
        });
      });
    }

    // 对于每个 banner，调用 setupBanner 函数并传入对应的选择器
