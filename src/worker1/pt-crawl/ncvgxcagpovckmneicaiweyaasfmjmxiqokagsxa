<div id="bulle" style="position: absolute; z-index: 2"></div>
<SCRIPT>
ns4 = (document.layers)? true:false;
ie4 = (document.all)? true:false;

decal_x = 10;
decal_y = 0;

var skn = (ns4) ? document.bulle : bulle.style;

if (ns4) document.captureEvents(Event.MOUSEMOVE);
document.onmousemove = suivre_souris;

function pop(message) {
	var contenu = "<table border=1 cellspacing='0' cellpadding='0'><tr><td bgcolor='#FFFFEE' bordercolor='black' class='text3'>Go to </font><font class='text'>" + message + "</td></tr></table>";
	if (ns4) {
		skn.document.write(contenu);
		skn.document.close();
		skn.visibility = "visible";
		}
	else if (ie4) {
		document.all("bulle").innerHTML = contenu;
		skn.visibility = "visible";
		}
	}

function suivre_souris(e) {
	var x = (ns4) ? e.pageX : event.x + document.body.scrollLeft;
	var y = (ns4) ? e.pageY : event.y + document.body.scrollTop;
	if ( (navigator.userAgent.indexOf('Mac') != -1) && (navigator.userAgent.indexOf('MSIE') != -1) ) {
		skn.left = x + decal_x - 135;
		skn.top  = y + decal_y - 155;
		}
	else {
		skn.left = x + decal_x;
		skn.top  = y + decal_y;
		}
	}

function off() {
	if (ns4) {
		skn.document.write('');
		skn.document.close();
		skn.visibility = "hidden";
		}
	else if (ie4) {
		document.all("bulle").innerHTML = '';
		skn.visibility = "hidden";
		}
	}



function verif_form(F)
{
  if (F.Pays.value=="")
	{alert("Please, you must select one country !");
	 return false;
	}

  F.submit();
}

</SCRIPT>



