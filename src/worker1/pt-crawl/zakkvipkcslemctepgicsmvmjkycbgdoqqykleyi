<!-- Google Tag Manager (noscript) --> <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-5V7T99" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript> <!-- End Google Tag Manager (noscript) -->
<script>
(function () {
  // Number to replace
  const webSalesNumber = "8886703148";

  // Map of query parameters to replacement number
  const queryParamPhoneMap = {
    advertising: {
      google: "8884151728",
      bing: "8885048166",
      facebook: "8882199412",
      any: "8884030249",
    },
    appmessage: {
      any: "8883989481",
    },
    email: {
      any: "8884133160",
    },
    organic: {
      // set manually when referrer is google, bing or duckduckgo
      any: "8883108092",
    },
    social: {
      any: "8883156477",
    },
  };

  function getQueryParams() {
    return new URL(document.location).searchParams;
  }

  function isProperGetUtmParamsSet(params) {
    return params.get("utm_medium") && params.get("utm_source");
  }

  function setPhoneCookieFromGetParams(params) {
    setPhoneCookie(params.get("utm_medium"), params.get("utm_source"));
  }

  function setPhoneCookie(medium, source) {
    if (medium) {
      // Simplify any version of *advertising* to just 'advertising'
      medium = medium.indexOf("advertising") >= 0 ? "advertising" : medium;
      document.cookie = `utm-medium=${medium}; SameSite=lax; Secure`;
    }
    source = source ? source : "any";
    document.cookie = `utm-source=${source}; SameSite=lax; Secure`;
  }

  function isPhoneCookieSet(cookieCrumbles) {
    if ("utm-medium" in cookieCrumbles && "utm-source" in cookieCrumbles) {
      return true;
    }
    return false;
  }

  function updateHrefTel(find, replace) {
    const phoneAnchors = [...document.querySelectorAll("a[href^=tel]")];
    phoneAnchors.forEach((el) => {
      const newHref = replacePhoneNumber(
        el.getAttribute("href"),
        find,
        replace
      );
      if (newHref != el.getAttribute("href")) {
        el.setAttribute("href", newHref);
      }
      updateBodyTextTel(find, replace, el);
    });
  }

  const updateBodyTextTel = (find, replace, target) => {
    [
      target,
      ...target.querySelectorAll("*:not(script):not(noscript):not(style)"),
    ].forEach(({ childNodes: [...nodes] }) =>
      nodes
        .filter(({ nodeType }) => nodeType === Node.TEXT_NODE)
        .forEach((textNode) => {
          const newContent = replacePhoneNumber(
            textNode.textContent,
            find,
            replace
          );
          if (newContent != textNode.textContent) {
            textNode.textContent = newContent;
          }
        })
    );
  };

  function replacePhoneNumber(searchStr, find, replace) {
    if (
      find.length !== 10 ||
      replace.length !== 10 ||
      isNaN(find) ||
      isNaN(replace)
    ) {
      throw new Error(
        `replacePhoneNumber requires two 10 digit number only values as input. Given find: ${find}, replace: ${replace}`
      );
    }
    findRegex = new RegExp(
      `(.*?)${find.slice(0, 3)}([^\d]*)${find.slice(3, 6)}([^\d]*)${find.slice(-4)}(.*)`,
      "gm"
    );
    return searchStr.replace(
      findRegex,
      `$1${replace.slice(0, 3)}$2${replace.slice(3, 6)}$3${replace.slice(-4)}$4`
    );
  }

  const updatePhoneNumbers = () => {
    if (cookieCrumbles["utm-medium"] in queryParamPhoneMap) {
      if (
        cookieCrumbles["utm-source"] in
        queryParamPhoneMap[cookieCrumbles["utm-medium"]]
      ) {
        updateHrefTel(
          webSalesNumber,
          queryParamPhoneMap[cookieCrumbles["utm-medium"]][
            cookieCrumbles["utm-source"]
          ]
        );
      } else if ("any" in queryParamPhoneMap[cookieCrumbles["utm-medium"]]) {
        updateHrefTel(
          webSalesNumber,
          queryParamPhoneMap[cookieCrumbles["utm-medium"]]["any"]
        );
      }
    }
  };

  const debounce = (func, delay) => {
    let debounceTimer;
    return function () {
      const context = this;
      const args = arguments;
      clearTimeout(debounceTimer);
      debounceTimer = setTimeout(() => func.apply(context, args), delay);
    };
  };

  // Debounce updatePhoneNumbers so that it won't execute
  // until it hasn't been called for 25ms to account
  // for rapid dom mutations
  const debouncedUpdatePhoneNumbers = debounce(updatePhoneNumbers, 25);

  // Main
  const urlSearchParams = getQueryParams();
  // Update cookie if proper query parameters set
  if (isProperGetUtmParamsSet(urlSearchParams)) {
    ``;
    setPhoneCookieFromGetParams(urlSearchParams);
  } else if (
    document.referrer.indexOf("google.") >= 0 ||
    document.referrer.indexOf("bing.") >= 0 ||
    document.referrer.indexOf("duckduckgo.") >= 0
  ) {
    setPhoneCookie("organic");
  }

  const cookieCrumbles = Object.fromEntries(
    document.cookie
      .split("; ")
      .map((v) => v.split(/=(.*)/s).map(decodeURIComponent))
  );

  // If utm_medium and utm_source are set in cookie
  if (isPhoneCookieSet(cookieCrumbles)) {
    document.addEventListener("DOMContentLoaded", updatePhoneNumbers);

    // Select the node that will be observed for mutations
    const targetNode = document.querySelector("body");

    // Options for the observer (which mutations to observe)
    const config = { attributes: true, childList: true, subtree: true };

    // Callback function to execute when mutations are observed
    const callback = () => {
      debouncedUpdatePhoneNumbers();
    };
    // Create an observer instance linked to the callback function
    const observer = new MutationObserver(callback);

    // Start observing the target node for configured mutations
    observer.observe(targetNode, config);
  }
})();

function openChat() {
    var button = document.querySelector("#helpButtonSpan > span.message");
    button.click();
}
</script>

