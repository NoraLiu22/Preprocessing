<script type="text/javascript">
  propublica.views.CartogramSetup11244 = propublica.View.extend({
    id : "cartogram-box-11244",

    render : function() {
      if (window.POSITIONS) return;

       $.get("https://web.archive.org/web/20160902164236/https://pp-projects-static.s3.amazonaws.com/congress/assets/senate_113_1_97.json", function(thisVote){
            var chamber = thisVote.chamber.toLowerCase();
            _.each(thisVote.votes, function(vote) {
              var district = vote.district;
              var state = new propublica.ApIzer(vote.display_state_abbrev).toPostal();
              var party = vote.party;
              var position = vote.position;
            $("#cartogram-box-11244-" + chamber + "_" + state + "_" + district).attr("class", "st0 " + party + "_" + position); // the ids are formatted like "#house_CA_1" or "senate_WV_2"
            })
          });
    },
  });
</script>

        </div>
      </div>
    </div>
    <div style="clear:both;"></div>
