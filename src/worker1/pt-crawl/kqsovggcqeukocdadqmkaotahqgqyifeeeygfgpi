<script type="text/javascript">
jQuery(document).ready(function() {
var subsiteURL  = "";
var jsonSrc     = "";
var siteCode    = "";
subsiteURL  = "/subjects/lwcf/";
siteCode = subsiteURL.split("/");
siteCode = siteCode[2];
if ( subsiteURL.includes("orgs") | subsiteURL.includes("im") | subsiteURL.includes("rlc") ) {
jsonSrc = subsiteURL+"structured_data_orgs_"+siteCode+".json?v="+Date.now();
}
if ( subsiteURL.includes("locations") ) {
jsonSrc = subsiteURL+"structured_data_locations_"+siteCode+".json?v="+Date.now();
}
if ( subsiteURL.includes("subjects") ) {
jsonSrc = subsiteURL+"structured_data_subjects_"+siteCode+".json?v="+Date.now();
}
// Location Site Check //
urlCheck = window.location.href;
if (urlCheck.includes("locations")) {
fileURL = '/locations/lwcf/structured_data_lwcf.json';
}
jQuery.getJSON(jsonSrc, function( data ) {
var hasSocialMedia = 0;
var outputSocialMedia = '<div class="col-sm-6 col-md-4 "><div class="ParkFooter-socialLinks"><h3>Stay Connected</h3><ul class="ParkFooter-socialLinks row">';
if (data.facebookURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.facebookURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#facebook"></use></svg>Facebook</a></li>';
hasSocialMedia = 1;
}
if (data.twitterURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.twitterURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#twitter"></use></svg>Twitter</a></li>';
hasSocialMedia = 1;
}
if (data.instagramURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.instagramURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#instagram-dark"></use></svg>Instagram</a></li>';
hasSocialMedia = 1;
}
if (data.youtubeURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.youtubeURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#youtube"></use></svg>YouTube</a></li>';
hasSocialMedia = 1;
}
if (data.flickrURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.flickrURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#flickr"></use></svg>Flickr</a></li>';
hasSocialMedia = 1;
}
if (data.tumblrURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.tumblrURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#tumblr"></use></svg>Tumblr</a></li>';
hasSocialMedia = 1;
}
if (data.foursquareURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.foursquareURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#foursquare"></use></svg>Foursquare</a></li>';
hasSocialMedia = 1;
}
if (data.pinterestURL) {
outputSocialMedia += '<li class="col-6 col-sm-12 col-md-6"><a href="'+data.pinterestURL+'"><svg class="icon" focusable="false" role="img"><use xlink:href="/common/commonspot/templates/assets/sprite.symbol.svg#pinterest"></use></svg>Pinterest</a></li>';
hasSocialMedia = 1;
}
outputSocialMedia += '</ul></div></div>';
if(hasSocialMedia == 1) {
jQuery("#sml_position").replaceWith(outputSocialMedia);
}
});
