<script>
(function(window){
    window.setupTabs = function(){
        let imageButton = document.querySelector("#viewer-image-tab");
        if(imageButton != null){
            imageButton.addEventListener("click", function(event){
                navigateTo('image');
                event.preventDefault();
            });
        }

        let textButton = document.querySelector("#viewer-text-tab");
        if (textButton != null){
            textButton.addEventListener("click", function(event){
                navigateTo('text');
                event.preventDefault();
            });
        }

        let pdfButton = document.querySelector("#viewer-pdf-tab");
        if (pdfButton != null){
            pdfButton.addEventListener("click", function(event){
                navigateTo('pdf');
                event.preventDefault();
            });
        }

        let epubButton = document.querySelector("#viewer-epub-tab");
        if (epubButton != null){
            epubButton.addEventListener("click", function(event){
                navigateTo('epub');
                event.preventDefault();
            });
        }
    };

    window.deselectTab = function(buttonId){
        let tabButton = document.querySelector("#" + buttonId);
        tabButton.classList.remove("active");
        tabButton.setAttribute("aria-selected", "false");
    };

    window.selectTab = function(buttonId){
        let tabButton = document.querySelector("#" + buttonId);
        tabButton.classList.add("active");
        tabButton.setAttribute("aria-selected", "true");
    };

    $(document).ready(setupTabs);
})(window);
</script>

