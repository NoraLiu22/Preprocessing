        class='iconic screen-dependent-image'
        
        data-image-tablet="https://tile.loc.gov/image-services/iiif/service:sgp:sgpbatches:batch_dlc_argonne_ver03:data:20001931:print:1919032801:0001/full/pct:12.5/0/default.jpg#h=843&amp;w=650"
        

        
            src="https://tile.loc.gov/image-services/iiif/service:sgp:sgpbatches:batch_dlc_argonne_ver03:data:20001931:print:1919032801:0001/full/pct:6.25/0/default.jpg"
        


    }
        onerror="this.onerror=null;this.src='/static/images/original-format/newspaper.svg'"
    

    
