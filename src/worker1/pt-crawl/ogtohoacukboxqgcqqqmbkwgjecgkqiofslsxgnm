window.onerror = function (msg, url, line) {
var f = function (str) {
return encodeURIComponent(str).replace(/[!'()*]/g, function (c) {
return '%' + c.charCodeAt(0).toString(16);
});
};
try {
var sl = 'https://web.archive.org/web/20190123010250/https://www.newspapers.com/report';
