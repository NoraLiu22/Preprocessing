<script>
    function addFreeRegistration(button) {        
        var email = $(button).closest('.register-container').children('.register-email').first().val();
        var source = $(button).closest('.register-container').children('.register-source').first().val();

        if (email) {
            $('.register-modal-log-in').hide();

            $.ajax({
                url: '/api/AddFreeRegistration',
                data: {
                    email,
                    source
                },
                method: 'post',
                success: function (data) {
                    var modalText = '';

                    if (data.success) {
                        modalText = 'You have been sent an email, please check your inbox to continue setting up your account.';
                    } else {
                        modalText = data.message;
                        $('.register-modal-log-in').show();
                    }

                    $('.register-modal-text').text(modalText);
                    console.log(data);

                    $("#register-modal-pop-up").show();
                    $("#register-modal-container").show();
                },
                error: function (data) {
                    console.log(data);
                }
            });
        } else {

        }        
    }
</script>                <div class="paywall-footer-subscriber-login">
                    <div class="flex-center"><a href="/Subscribe/Subscription-Landing.aspx">Subscribe</a> <div class="red-spacer">|</div> Already a subscriber? <a href="/Member/Login.aspx">Log In</a></div>
                </div>

        </div>
    </div>

