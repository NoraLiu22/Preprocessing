<script type="application/javascript" src="//www.google.com/recaptcha/api.js?onload=CaptchaCallback&render=explicit" async="" defer=""></script>
<script>
    function getSubscribeButtonUrl(button, subscribeButtonUrl, appendProductId) {
        var productId = getSubscribeButtonProductId(button);
        window.location.href = subscribeButtonUrl + ((appendProductId && productId) ? ("?productIds=" + productId) : "");
    }

    function getSubscribeButtonProductId(button) {
        return $(button).parent().parent().find('.sub-price.selected').data('product-id');
    };

    $(document).ready(function () {
        ResizeSubscriptionOptions();
        addEventListener("resize", ResizeSubscriptionOptions, true);

        $('.subscription-buttons-container, .feature-comparison-chart-container').addClass('loaded');
        $('.tablinks').click(function () {
            $('.tablinks').removeClass('active');
            $(this).addClass('active');
            $('.options-tab-content.active').removeClass('active');
            $('.options-tab-content[data-tab="' + $(this).data('tab') + '"').addClass('active');
            ResizeSubscriptionOptions(true);
        });

        $('.billing-period').click(function () {
            UpdateBillingPeriodSwitch(this);
            UpdateBillingPeriodInfo(this);
            UpdateBillingInfo(this);
            ResizeSubscriptionOptions();
        });

        $('.choose-region').change(function () {
            UpdateBillingInfo(this);
            ResizeSubscriptionOptions();
        });

        $('label[for=billing-option-print]').click(function (e) {
            let optionInput = $(this).siblings('input[name=billing-option-print]');
            if (optionInput.prop('checked')) {
                optionInput.removeProp('checked');
            } else {
                optionInput.prop('checked',true);
            }
            UpdateBillingInfo(optionInput);
        });

        function ResizeSubscriptionOptions(reset) {
            if ($(".subscription-option-container")) {
                var maxHeight = 0;

                if (reset) {
                    $(".subscription-option-container, .subscription-option-container").css('height', '');
                }

                maxHeight = Math.max.apply(null, $(".subscription-option-container").map(function () { return $(this)[0].scrollHeight; }).get());
                SetSubscriptionOptionHeight(maxHeight);
            }
        }

        function SetSubscriptionOptionHeight(height) {
            $(".subscription-option-container, .subscription-option-container").css('height', height + 'px');
        }

        function UpdateBillingInfo(input) {
            let tab = $(input).closest('.tab-wrapper');

            let selectedPeriod = tab.find('.billing-period-container .switch-label.switch-label--selected').data('billing-period');
            let selectedRegion = tab.find('.choose-region-container .choose-region option:selected').val();

            let subscriptionOptions = tab;
            if ($(tab).find('.subscription-option').length) {
                subscriptionOptions = $(tab).find('.subscription-option');
            }

            let billingPeriodContainer = $(tab).find('.billing-period-container');
            for (let currentSubscriptionOption of subscriptionOptions) {
                if ($(billingPeriodContainer).length) {
                    let billingOptions = $(currentSubscriptionOption).find('.billing-options');
                    if (billingOptions.length) {
                        billingOptions.each(function (i, e) {
                            billingInfoContainer = $(e).parents('.subscription-option');
                            let billingOption = $(e).find('.billing-option');
                            let splitMonthly = $(e).find('.split-monthly-billing-options').val() == "True";
                            let splitAnnual = $(e).find('.split-annual-billing-options').val() == "True";

                            switch (selectedPeriod) {
                                case 'MON':
                                    if (splitMonthly)
                                        $(e).show();
                                    else
                                        $(e).hide();
                                    break;
                                case 'ANN':
                                    if (splitAnnual)
                                        $(e).show();
                                    else
                                        $(e).hide();
                                    break;
                            }

                            let digitalOnly = billingOption.length && ((selectedPeriod == 'MON' && splitMonthly) || (selectedPeriod == 'ANN' && splitAnnual)) && !$(billingOption).prop('checked');
                            $(billingInfoContainer).find('.billing-info[data-region][data-billing-period]').removeClass("selected");
                            $(billingInfoContainer).find('.billing-info[data-region = "' + selectedRegion + '"][data-billing-period = "' + selectedPeriod + '"]' + (digitalOnly ? '[data-digital]' : ':not([data-digital])')).addClass("selected");
                        });
                    } else {
                        $(currentSubscriptionOption).find('.billing-info[data-region][data-billing-period]').removeClass("selected");
                        $(currentSubscriptionOption).find('.billing-info[data-region = "' + selectedRegion + '"][data-billing-period = "' + selectedPeriod + '"]:not([data-digital])').addClass("selected");
                    }
                }
            }
        }


        function UpdateBillingPeriodInfo(input) {
            let tab = $(input).closest('.tab-wrapper');
            $(tab).find('.billing-info:not([data-region])').toggleClass("selected");
        }

        function UpdateBillingPeriodSwitch(input) {
            let tab = $(input).closest('.tab-wrapper');

            let unselectedBillingPeriod = tab.find('.billing-period-container .switch-label:not(.switch-label--selected)');
            let selectedBillingPeriod = tab.find('.billing-period-container .switch-label.switch-label--selected');
            selectedBillingPeriod.removeClass('switch-label--selected');
            unselectedBillingPeriod.addClass('switch-label--selected');
        }
    });
</script>

        </main>
        <!-- contain main informative part of the site ends -->
