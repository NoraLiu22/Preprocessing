    const displayArea = document.getElementById('gloss-display');

    displayArea.innerHTML = ''; 

    const list = document.createElement('ul'); 
    list.className = 'gloss-list'; 

    if (glosses[character]) {
        glosses[character].forEach(gloss => {
            const listItem = document.createElement('li'); 
            listItem.className = 'gloss-item'; 

            const glossLink = document.createElement('a');
            glossLink.href = glossBaseURL + gloss;
            glossLink.textContent = gloss;
            glossLink.className = 'link-primary m-1';

            listItem.appendChild(glossLink);
            list.appendChild(listItem);
        });
    } else {
        list.innerHTML = '<li>No entries found for this letter.</li>'; 
    }

    displayArea.appendChild(list); 

    if (activeLink) {
        activeLink.classList.add('active'); 
    }
}
