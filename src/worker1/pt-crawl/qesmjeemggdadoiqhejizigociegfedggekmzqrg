<script>
(function(window, $) {
$(".nav_li li").mouseenter(function(){
	var index = $(this).index(".nav_li li");
	$(".nav_hover").css({"border-top":"solid 1px #3778b2"})
	$(".nav_li li").eq(index).addClass("hover").siblings(".nav_li li").removeClass("hover");
	$(".nav_hover li").eq(index).addClass("hover").siblings(".nav_hover li").removeClass("hover");
});
$(".nav_box").mouseleave(function(){
	$(".nav_hover").css({"border-top":"none"})
	$(".nav_li li").removeClass("hover");
	$(".nav_hover li").removeClass("hover");
});

$(".list2 .nav").mouseenter(function(){
	var index = $(this).index(".list2 .nav");
	$(".list2 li.nav").eq(index).addClass("hover").siblings(".list2 li.nav").removeClass("hover");
	$(".list2 li.hide").eq(index).addClass("hover").siblings(".list2 li.hide").removeClass("hover");
});

$(".list3 li").mouseenter(function(){
	var index = $(this).index(".list3 li");
	$(".list3 li").eq(index).addClass("hover").siblings(".list3 li").removeClass("hover");
});

var mySwiper3 = new Swiper('.swiper-container3',{
  autoplay : 3000,
  loop:true,
  slidesPerView : 1,
  paginationClickable :true,
  pagination:'.swiper-pagination.s3',
})	

$(".nav_b li").mouseover(function(){
	var index = $(this).index(".nav_b li");
	$(".nav_b li").eq(index).addClass("hover").siblings(".nav_b li").removeClass("hover");
	$(".flash").eq(index).addClass("hover").siblings(".flash").removeClass("hover");
});

playimg(".slides")
    playimg(".slides1")
	playimg(".slides2")
	playimg(".slides3")
    function playimg(cl){
    var box = $(cl);
    var width = $(".flash").width();
    var obj = box.find("li");
	var num = obj.length;
	var index = 0;
	var time = 0;
	var num_c = 0;

        box.siblings(".f_left").click(function(){
		if(index==0){
			index=num;
		};
		index--;
        box.stop().animate({left:"-" + width*index + "px"},300);
		box.siblings(".dian").find("li").eq(index).addClass("hover").siblings().removeClass();
	});
        box.siblings(".f_right").click(function(){
		if(index==num-1){
			index=-1;
		};
		index++;
        box.stop().animate({left:"-" + width*index + "px"},300);
		box.siblings(".dian").find("li").eq(index).addClass("hover").siblings().removeClass();
	});
	box.siblings(".dian").find("li").click(function(){
		index = $(this).index();
		box.stop().animate({left:"-" + width*index + "px"},300);
		$(this).addClass("hover").siblings().removeClass();

	});	
}
	
$(".two_btn").mouseenter(function(){
	$(".two").show();
});
$(".two_btn").mouseleave(function(){
	$(".two").hide();
});
$(".top_btn").click(function(){
	$('html,body').animate({scrollTop:1},300);
});


} (window, window.jQuery));
