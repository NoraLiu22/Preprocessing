function load6Sense() {
var interval_e = 0;
timer_e = setInterval(function () {
if( window._storagePopulated || interval_e==500){
clearInterval(timer_e);
fireAnalytics();
}
interval_e++;
}, 1);
}
function fireAnalytics() {
