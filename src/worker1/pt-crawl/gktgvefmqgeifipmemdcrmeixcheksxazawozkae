<script type="text/javascript" src="http://images.china.cn/images1/resource/scripts/jquery/jquery-1.12.4.min.js" ></script>
<script type="text/javascript" >
	var num = 0;
	var time;
	$('.bottom .left span').click( function(){
		
		// 下面的代码就是将判断之后的结果保存在了 bFlag 这个变量里
		var bFlag = $(this).hasClass('r');
		
		if( bFlag ){
			// 如果 bFlag 是真的那么就说明用户点击的是  右按钮
			num++;
			if( num == 5 ){ num = 0 }
		}else{
			num--;
			if( num == -1 ){ num = 4 }
		}
		
		// 无论点左还是点右 当num 发生改变了之后我们就是需要做动画。
		play();
		
	} );
	// 三、自动播放【 定时器 】
	time = setInterval( autoPlay,3000 );
	
	// 四、鼠标移上和离开
	$('.section2 .left').hover( function(){
		clearInterval( time );
	},function(){
		// 离开的时候还需要再次自动播放所以我们需要将定时器再次开启
		time = setInterval( autoPlay,3000 );
	} );
			
	// 将切换图片和改变数字样式的 代码定成一个函数
	function play(){
		$('.bottom .left .list').stop().animate( {'left':-560*num} );
	}
	// 将定时器时间到了之后需要做的事件拿出来定义成一个函数
	function autoPlay(){
		
		// 每过3秒钟我们都希望图片可以自动切一张，而图片之所以会换就是因为 num变了之后会影响到 play（）
		num++;
		if( num == 5 ){ num = 0 }
		play();	
	}
</script>
 <script type="text/javascript">
	
			/*
			Tab滑动门JS
			*/
			function scrollDoor(){
			}
			scrollDoor.prototype = {
			sd : function(menus,divs,openClass,closeClass){
			var _this = this;
			if(menus.length != divs.length)
			{
			alert("菜单层数量和内容层数量不一样!");
			return false;
			}
			for(var i = 0 ; i < menus.length ; i++)
			{
			_this.$(menus[i]).value = i;
			_this.$(menus[i]).onmouseover = function(){
			for(var j = 0 ; j < menus.length ; j++)
			{
			_this.$(menus[j]).className = closeClass;
			_this.$(divs[j]).style.display = "none";
			}
			_this.$(menus[this.value]).className = openClass;
			_this.$(divs[this.value]).style.display = "block";
			}
			}
			},
			$ : function(oid){
			if(typeof(oid) == "string")
			return document.getElementById(oid);
			return oid;
			}
			}
			window.onload = function(){
			var SDmodel = new scrollDoor();
			SDmodel.sd(["m01","m02","m03"],["c01","c02","c03"],"sd01","sd02");}
			</script>	 
