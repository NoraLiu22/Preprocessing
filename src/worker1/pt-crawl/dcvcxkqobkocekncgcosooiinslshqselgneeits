    $(function () {
      //大头条
      setFontSize(); //聚焦轮播图
    });
    /* 大头条字号颜色设置 */
    function setFontSize() {
      var fs = $.trim($('#headline').attr('data-fontsize'));
      var fc = $.trim($('#headline').attr('data-fontcolor'));

      if (fs != '') {
        $('#headline').find('h1').css({
          'font-size': fs + 'px'
        });
      }

      if (fc != '') {
        $('#headline').find('h1').find('a').css({
          color: fc
        });
        $('#headline').find('p').find('a').css({
          color: fc
        });
      }
    }
