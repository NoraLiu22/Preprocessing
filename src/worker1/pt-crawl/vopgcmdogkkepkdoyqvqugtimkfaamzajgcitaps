<script>

  jQuery(function() {

    jQuery('#numPlayPause.pause').click(function() {
      var $self = jQuery(this);
      if ($self.text() == 'Pause Slider') {
          jQuery(this).closest('#views-bootstrap-carousel-1.carousel').carousel('pause');
          $self.text('Play Slider');
      } else {
        jQuery(this).closest('#views-bootstrap-carousel-1.carousel').carousel('cycle');
        $self.text('Pause Slider');
      }
      jQuery("#numPlayPause").toggleClass('pause play');
    });

  });

  function gridCarousel() {
    jQuery(".carousel-col .item").each(function() {
      for (var itemToClone = jQuery(this), i = 1; 3 > i; i++) itemToClone = itemToClone.next(), itemToClone.length || (itemToClone = jQuery(this).siblings(":first")), itemToClone.children(":first-child").clone().addClass("cloneditem-" + i).appendTo(jQuery(this))
    }), jQuery(".carousel").swiperight(function() {
      jQuery(this).carousel("prev")
    }), jQuery(".carousel").swipeleft(function() {
      jQuery(this).carousel("next")
    }), jQuery(".carousel").carousel()
  }
  gridCarousel();

</script>

