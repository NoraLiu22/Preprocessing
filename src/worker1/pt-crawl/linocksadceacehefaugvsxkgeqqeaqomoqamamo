	<script>
	var trackOtherSubscriptions = document.querySelectorAll('.other-subscriptions')
	for (var i = 0; i < trackOtherSubscriptions.length; i++) {
		if (trackOtherSubscriptions[i].classList.contains('other-subscriptions')) {
			trackOtherSubscriptions[i].setAttribute("href", "https://subscribe.smh.com.au/other-packages" + "?promote_channel=" + promote_channel)
		}
	}

	var cardPosition = document.querySelector(".card-position")
	var trackButtonStarter = document.querySelector('.card-content .subscribe-unidays')
	if (trackButtonStarter.classList.contains('subscribe-unidays')) {
		trackButtonStarter.setAttribute("href", "https://smh.myfairfax.com.au/members/subscriptions/new?brand=&bundle_key=smh_webonly&callback_uri=http%3A%2F%2Fwww.smh.com.au&channel_key=zHE9EWDHf1XPuz3Phk0YIg&plain_token=true" + PROMOTE_CHANNEL)
	}
	</script>

	<script>
		var smhPackages = [
			{
				name: 'premium digital',
				brand: 'smh',
				id: 'smh_premium_digital_weekly',
				price: '1.0',
				list_position: '1',
				variant: 'weekly'
			}
		]

		// ----------------------------------- other subscriptions -----------------------------------
		var otherSubscriptionPackages = [
			{
				name: 'other subscriptions',
				brand: 'smh',
				id: 'smh_other_subscriptions',
				price: '',
				list_position: '',
				variant: ''
			}
		]
		function trackOtherSubscriptions (data) {
			data = otherSubscriptionPackages[0]
			console.log('other subscription packages', otherSubscriptionPackages[0])
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'Product Impression',
				'eventLabel': promote_channel,
				'eventValue': 0
			})
			dataLayer.push({
				'ecommerce': {
				'currencyCode': 'AUD',
					'impressions': [
					{
						'name': data.name,
						'id': data.id,
						'price': data.price,
						'brand': data.brand,
						'variant': data.variant,
						'list_position': data.list_position
					}]
				}
			});
		}

		// ----------------------------------------------------------------------

		function trackUniDays (data) {
			data = smhPackages[0]
			console.log('smh unidays weekly: ', smhPackages[0])
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'addToCart',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
				'event': 'addToCart',
				'currencyCode': 'AUD',
						'add': [
						{
							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
					}
				});
		}
	</script>

	<script>
		dataLayer.push({
			'ecommerce': {
				'promoView': {
					'promotions': [{
						'id': promote_channel,
						'name': '',
						'creative': '',
						'position': ''
					}]
				}
			}
		})
	</script>

