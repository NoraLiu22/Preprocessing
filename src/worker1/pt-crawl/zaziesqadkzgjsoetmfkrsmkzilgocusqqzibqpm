    function entersearch2(obj){
        var event = window.event || arguments.callee.caller.arguments[0];
        if (event.keyCode == 13)
        {
            if(document.getElementById('search2').value != ""){
                //$(obj).closest("form")[0].submit();
                $("#search2").blur();
                location.href = "/search/index.aspx?keywords=" + $("#search2").val();
            }
        }
    }

    function initAppPopover() {
        if ($.fn.popover) {
            $('.app').popover({
                trigger: 'hover',
                title: "<h4>Scan the QR code</h4>",
                html: true,
                content: "<img src='/images/QRcode5.png' style='width:240px;height:240px;'>"
            });
        } else {
            setTimeout(initAppPopover, 300);
        }
    }
    setTimeout(initAppPopover, 300);
