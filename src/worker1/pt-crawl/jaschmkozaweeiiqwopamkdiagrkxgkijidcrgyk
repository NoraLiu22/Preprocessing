<script type="text/javascript">

    function Entity(name, id) {
        this.name = name;
        this.id = id;
    }

    function pop(id) {
        const videoString = 'https://stub.sbjatlas.com/listing/preview?id=';
        var windowH = $(window.top).height();
        const allId = id.split('-');
        const actualId = allId[2];
        const entityName = allId[3];
        const modalId = 'entity-modal-' + actualId;
        let atlasModal = document.getElementById(modalId);
        const elem = document.getElementById(id);
        const rect = elem.getBoundingClientRect();

        const contentId = 'entity-content-' + actualId;
        let contentEl = document.getElementById(contentId);
        let position = parseInt(windowH) - parseFloat(rect['y'])

        atlasModal.setAttribute("src", videoString + actualId + "&tag=" + entityName);

        contentEl.classList.toggle('entity-content-hidden');

        if (position >= windowH / 2) {
            contentEl.style.removeProperty('bottom');
            contentEl.style.top = '1%';
        }
        else {
            contentEl.style.removeProperty('top');
            contentEl.style.bottom = '1%';
        }
    }


    $(document).ready(function () {
        let keys = [];
        let values = [];
        let entities = [];
        let usedEntities = [];
        var model = [{"Key":"65881370974c4c72b0d662bdb1ba3032","Value":"F1"},{"Key":"22928934339049cf9ff2df8fd9111568","Value":"Lewis Hamilton"},{"Key":"d172abfb04db4981b4ecee45709be039","Value":"Visa"},{"Key":"6d9bafc13e5349f3be4b745789cc3dee","Value":"Cadillac"},{"Key":"65881370974c4c72b0d662bdb1ba3032","Value":"F1"},{"Key":"68fe049363de4c99ab30268b60cd4407","Value":"Lynx"},{"Key":"391d1f7c91b54439831369a97f44ac61","Value":"Rays"},{"Key":"9ee4f0a844654ff39f3e996c8c11250b","Value":"MLB"},{"Key":"0fa06a6cffea4cf48a64c0007fc187b6","Value":"MLB Network"},{"Key":"f2c8ea7a11e3427081231def09d1b41d","Value":"UCLA"},{"Key":"9d32085e4b4e4e459e110456a0cabc95","Value":"Mets"},{"Key":"7265c7c48b8a48f8b2223e2327f47780","Value":"NFL"},{"Key":"596c28e578274ac7bc579cf365406e75","Value":"PGA Tour"},{"Key":"57070ddf3c714b21a337ac6ade733e73","Value":"Dodgers"},{"Key":"c774a89b2c8646c4bb4119e5c2056506","Value":"Boston Bruins"},{"Key":"0f84531c52be4861a69bbabce5088450","Value":"Maple Leafs"},{"Key":"168fd675f9c04c7688d9f4ea39fda4c3","Value":"Cincinnati Bengals"},{"Key":"3176d0d7cf1c4a0a81c33e2fbd0ba1ef","Value":"EA Sports"},{"Key":"65881370974c4c72b0d662bdb1ba3032","Value":"Formula One"},{"Key":"d24f40dd3c794be5b6bbc24c4be8fb74","Value":"Nike"},{"Key":"011365a207384ba2812806b0a8d73ed8","Value":"Paycor Stadium"},{"Key":"5866e7009e2945faa56f098d37eb4b3e","Value":"ESPN"},{"Key":"8a22ea95340744f4ba209ec3d7509713","Value":"Auburn"},{"Key":"d3cdcf4a06d747d7ad47978d379d61f4","Value":"Indiana University"},{"Key":"9ee4f0a844654ff39f3e996c8c11250b","Value":"MLB"},{"Key":"b96a87bde0134ff1b2640d9de16ee7c0","Value":"NBA"},{"Key":"c8d35d8fead5403c9516637cbd376e32","Value":"Tom Brady"},{"Key":"ef4d57e56ccc42adb98ee30d13c4239f","Value":"University of North Carolina"},{"Key":"7e86df0923fb46218bad118b6bcef1b8","Value":"Cam Newton"},{"Key":"9bf38a68279042688ebdb84a3a1bf265","Value":"Venus Williams"}];
        console.log(1)
        if (model != null && model.length > 0) {
            console.log(2)
            for (let i = 0; i < model.length; i++) {
                keys.push(model[i].Key);
                values.push(model[i].Value);
                entities.push(new Entity(model[i].Value, model[i].Key));
            }

            var para = document.querySelectorAll('div.text-frame > p');
            console.log("para", para)
            for (let j = 0; j < para.length; j++) {
                var text = para[j].innerHTML;

                for (let i = 0; i < entities.length; i++) {

                    entities[i].name = entities[i].name.replace(/_/, ' ');
                      console.log(3)
                    if (entities[i].name.length > 1 && !usedEntities.includes(entities[i])) {
                          console.log(4)
                        if (text.includes(entities[i].name)) {
                            usedEntities.push(entities[i]);
                        }

                        var exp = new RegExp('(?<!\/)(?<!\-)(?<!\<)(?<!\")(?<!\')\\b' + entities[i].name + '\\b(?!\/)(?!\-)(?!\<)(?!\")(?!\')');
                        var enContent = "<span id='entity-content-" + entities[i].id + "' class='tagged-content entity-content-hidden' ><iframe id='entity-modal-" + entities[i].id + "' ></iframe></span>";

                        text = text.replace(exp, "<span class=\"highlight tagged-item\" id=\"tagged-item-" + entities[i].id + "\" onmouseenter=pop(this.id) onmouseleave=pop(this.id) ><a> " + entities[i].name + "</a >" + enContent + "</span>");
                    }
                }

                para[j].innerHTML = text;

            }
        }
    });

    var entityModals = document.getElementsByClassName('entity-modal');
    var entityLink = document.getElementsByClassName('modal-link');

    entityLink.onclick = function () {
        entityModals.style.display = "block";
    }

