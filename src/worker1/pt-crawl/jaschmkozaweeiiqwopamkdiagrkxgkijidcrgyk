        <script>
    function UrlWithReturnPath(currentPath, returnParameter) {
        if (returnParameter == null) {
            returnParameter = "redirectPath";
        }
        window.location.href = window.location.origin + currentPath + "?" + returnParameter + "=" + (window.location.pathname.substring(0, window.location.pathname.lastIndexOf('.')) || window.location.pathname);
    }
</script>


