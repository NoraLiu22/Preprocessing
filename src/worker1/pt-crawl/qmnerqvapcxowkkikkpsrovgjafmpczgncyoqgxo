<script type="text/javascript">
const cookieSFName = "Stat-Survey"
const cookieSFExpiredate = new Date("2023-07-14")
const cookieStatus = getSurveyStatus()

window.addEventListener('DOMContentLoaded', (e) => {

    const linkSurvey = document.getElementById("linkSurvey")
	//let pattern = /\/stat\/[(en|fr|de)\/]*(web\/)?euro-indicators/;
    //let matchPage = pattern.test(window.location.pathname);
    //if (matchPage) {

    if (cookieStatus != "stopped") {
        if (cookieStatus === undefined) {
            registerSurvey()
        }
        displayPopupSurvey()
        linkSurvey.value = 'Cookie status: ' + cookieStatus + ' => Popup should open!'
    }
    else {
        linkSurvey.value = 'Cookie status: ' + cookieStatus + ' => No pop-up!'
    }

    $('#popupSurveyInner').on('show.bs.modal', function (e) {
        document.getElementById('popupSurveyInner').style.display = "block"
    })
    $('#popupSurveyInner').on('hidden.bs.modal', function (e) {
        $('#popupSurveyInner').modal('dispose')
        $('body').removeClass('modal-open')
        $(".modal-backdrop").remove()
        $("#popupSurveyInner").remove()
    })
	//}
})
	
function reactivateSurvey() {
    registerSurvey()
    $('#popupSurveyInner').modal('show')
}
function registerSurvey() {
	if (getCookie("mojkolac") != "" && getCookie("mojkolac") != null){
    	persistSurvey("viewed")
	}
}
function displayPopupSurvey() {
	
	
	
	
	if (getCookie("mojkolac") === "Stat-Survey")
    {
       $('#popupSurveyInner').modal('hide')
    }
	else{
		$('#popupSurveyInner').modal('show')
	}
	
}
function stopSurvey() {
    
	if (document.getElementById("inputDeclineSurvey").checked) {
        persistSurvey("stopped")
	}
    $('#popupSurveyInner').modal('hide')
		persistSurvey("stopped");
		
			
}
function gotoSurvey() {
    persistSurvey("stopped")
    $('#popupSurveyInner').modal('hide')
	window.open("http://webrzs.stat.gov.rs/zk2023")
}

function getCookie(cname) {
  let name = cname + "=";
  let decodedCookie = decodeURIComponent(document.cookie);
  let ca = decodedCookie.split(';');
  for(let i = 0; i < ca.length; i++) {
    let c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

function getSurveyStatus() {
    if (document.cookie.length > 0) {
       var start_position = document.cookie.indexOf(" "+cookieSFName+"=")
        if (start_position!= -1) {
            start_position = document.cookie.indexOf("=",start_position) + 1
          var end_position = document.cookie.indexOf(";", start_position)
            if (end_position == -1)
                end_position = document.cookie.length
            return (decodeURIComponent(" " + document.cookie.substring(start_position, end_position)))
        }
    }
}


function persistSurvey(status) {
	
    document.cookie = "mojkolac=" + cookieSFName +  ";" + "expires=" + cookieSFExpiredate.toUTCString() + ";path=/";
	
}		
</script>		
	
		

<script type="text/javascript">
if (typeof jQuery != 'undefined') {
	jQuery(document).ready(function($) {
		
		var filetypes = /\.(zip|txt|pdf|doc*|xls*|ppt*)$/i;
		var baseHref = '';
		if (jQuery('base').attr('href') != undefined)
			baseHref = jQuery('base').attr('href');
		jQuery('a').each(function() {
			var href = jQuery(this).attr('href');
			if (href && (href.match(/^https?\:/i)) && (!href.match(document.domain))) {
				jQuery(this).click(function() {
					var extLink = href.replace(/^https?\:\/\//i, '');
					ga('send', 'event', 'External', 'Click', extLink);
					if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
						setTimeout(function() { location.href = href; }, 200);
						return false;
					}
				});
			}
			else if (href && href.match(/^mailto\:/i)) {
				jQuery(this).click(function() {
					var mailLink = href.replace(/^mailto\:/i, '');
					ga('send', 'event', 'Email', 'Click', mailLink);
				});
			}
			else if (href && href.match(filetypes)) {
				jQuery(this).click(function() {
					var extension = (/[.]/.exec(href)) ? /[^.]+$/.exec(href) : undefined;
					var filePath = href;
					ga('send', 'event', 'Download', 'Click-' + extension, filePath);
					if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
						setTimeout(function() { location.href = baseHref + href; }, 200);
						return false;
					}
				});
			}
		});
	});
}
</script>
