(function() {
    /* Check if Spreedly has already been initialized on this form */
    if (!document.getElementById('form-super-purchase').classList.contains('spreedly-initialized')) {
        document.getElementById('form-super-purchase').classList.add('spreedly-initialized');

        window.tntPayment = document.createEvent('Event');
        tntPayment.initEvent('tnt-payment',true,true,{});

        window.tntShowOfferModalTab = document.createEvent('Event');
        tntShowOfferModalTab.initEvent('tnt-show-offer-modal-tab',true,true,{});

        window.paymentPromisesSpreedly = [];

        var copyToggle = document.getElementById('same-info-super-purchase');
        if (copyToggle) {
            copyToggle.addEventListener("change", function(event){
                if(event.target.checked){
                    var billingForm = document.getElementById('form-billing-location-super-purchase'),
                        deliveryForm = document.getElementById('form-location-super-purchase');
                        
                    if(billingForm && deliveryForm){
                        try {
                            deliveryForm.querySelector('input[name=address]').value = billingForm.querySelector('input[name="postal-address"]').value;
                            deliveryForm.querySelector('input[name=municipality]').value = billingForm.querySelector('input[name="postal-city"]').value;
                        } catch (e) { /* Address, city not collected */ }
                        
                        deliveryForm.querySelector('select[name=region]').value = billingForm.querySelector('select[name="postal-state"]').value;
                        deliveryForm.querySelector('select[name=country]').value = billingForm.querySelector('select[name="postal-country"]').value;
                        deliveryForm.querySelector('input[name=postal_code]').value = billingForm.querySelector('input[name="postal-postcode"]').value;

                        /** Run tax calculation */
                        __tnt.subscription.offers.calculateTax(deliveryForm.closest('form'));
                    }
                }
            });
        }

        var oForm = document.getElementById('form-super-purchase');window.SpreedlyIdentifyPaymentCard = function(cardType) {
            /* Credit Card Icon Handler */
            var cardsEl = document.getElementById('tn-pay-cards-super-purchase'),
                cardActive = cardsEl.querySelector('.card-active'),
                cardsAllowed = ["visa","master","american_express","discover","jcb","diners_club","dankort"];

            if(cardType != null){
                if(!cardsAllowed.includes(cardType)){ cardType = "other" } 
                
                selectedCard = cardsEl.querySelector('[data-cc="' + cardType + '"]');
                
                if(cardActive){ cardActive.classList.remove('card-active') }
                if(selectedCard){
                    selectedCard.classList.add('card-active');
                    cardsEl.classList.add('card-identified');
                }

            }else if(cardType === null && cardActive){
                cardActive.classList.remove('card-active');
                cardsEl.classList.remove('card-identified');
            }
        }

        // Enable submit
        window.SpreedlyallowSubmit = function() {
            var form = document.getElementById('form-super-purchase'),
                tos = document.getElementById('tn-tos-super-purchase'),
                submit = document.getElementById('form-submit-super-purchase');
            
            if (tos.checked) {
                submit.disabled = false;
                
                __tnt.subscription.offers.logCheckoutStepViewed({
                    step: __tnt.subscription.offers.getPurchaseStep('tos'),
                    label: 'Agreed to terms of service'
                });
                __tnt.subscription.offers.logCheckoutStepCompleted({
                    step: __tnt.subscription.offers.getPurchaseStep('tos'),
                    label: 'Agreed to terms of service'
                });
            } else {
                submit.disabled = true;
            }
        }

        // Payment Function
        window.SpreedlyPayment = function(token, oMethod) {
            window.__tnt || (window.__tnt = {});
            __tnt.user || (__tnt.user = {});

            // Set the token in the hidden form field
            var tokenField = document.getElementById('tnpay-token-super-purchase');
            tokenField.setAttribute("value", token);

            // Submit the form
            const oForm = document.getElementById('form-super-purchase');
            
            // Endpoint
            var purchaseEndpoint = oForm.action;
            
            const oData = new URLSearchParams();
            for (const aPair of new FormData(oForm)) {
                oData.append(aPair[0], aPair[1]);
            }

            oData.append('tnpay-year', document.getElementById('tnpay-year-super-purchase').value);
            oData.append('tnpay-month', document.getElementById('tnpay-month-super-purchase').value);

            if (oMethod.last_four_digits) {
                oData.append('tnpay-last4', oMethod.last_four_digits);
            }
            
            fetch(purchaseEndpoint, {
                method: 'post',
                credentials: 'include',
                headers: {
                    'X-Requested-With': 'XMLHttpRequest'
                },
                body: oData
            }).then(function(oResp){
                    oPayload = oResp.json().then(function(oPayload){

                    var errorAlert = document.getElementById('form-error-super-purchase'),
                        messageAlert = document.getElementById('form-message-super-purchase'),
                        transactionIdEl = oForm.querySelector('[name="transaction_id"]'),
                        transactionId = oPayload.transaction_id || transactionIdEl && transactionIdEl.value;

                    if (oPayload.message && !oPayload.success) {
                    // is an error, found a message

                        if(errorAlert){
                            var errorMessage = errorAlert.querySelector('.error-message'),
                                errorButton = document.createElement('a');

                            errorMessage.innerHTML = oPayload.message;
                            if(oPayload.message == "Invalid password or account does not exist"){
                                errorButton.innerHTML = "Forgot your password?";
                                errorButton.classList.add("alert-link", "btn-block");
                                errorButton.href = "javascript:void(0)";
                                errorButton.addEventListener('click',function(ev){
                                    var modal = document.getElementById('access-offers-modal'),
                                        modalTabForgot = document.getElementById('user-modal-tab-forgot');

                                    __tnt.subscription.access.initForgotModal();
                                    __tnt.subscription.access.showTab(modalTabForgot, "access-offers-modal", "access-methods");
                                    $(modal).find('.modal').modal();
                                });

                                errorMessage.appendChild(errorButton);
                            }

                            errorAlert.hidden = false;
                            errorAlert.classList.remove('fade');

                            var modalTabPurchase = document.getElementById('user-modal-tab-purchase');

                            if(modalTabPurchase && __tnt.subscription.access.showTab){
                                __tnt.subscription.access.showTab(modalTabPurchase, "access-offers-modal", "access-methods");
                            }
                        }

                        return;
                    }else if(oPayload.message){
                        // not an error, found a message

                        if(messageAlert){
                            messageAlert.querySelector('.response-message').innerHTML = oPayload.message;
                            messageAlert.hidden = false;
                            messageAlert.classList.remove('fade');

                            var modalTabPurchase = document.getElementById('user-modal-tab-purchase');

                            if(modalTabPurchase && __tnt.subscription.access.showTab){
                                __tnt.subscription.access.showTab(modalTabPurchase, "access-offers-modal", "access-methods");
                            }
                        }
                    }

                    var modalTabPurchaseSuccess = document.getElementById('user-modal-tab-purchase-success'),
                        modalTabPurchaseInfo = document.getElementById('user-purchase-rate'),
                        modalReceipt = document.getElementById('modal-purchase-receipt'),
                        modalReceiptTemplate = null,
                        modalReceiptOutput = null,
                        selectedService = null,
                        selectedRate = null,
                        estimatedTax = null,
                        total = null,
                        tax = parseFloat( sessionStorage.getItem('__tnt.service.rate.tax') ).toFixed(2),
                        taxTotal = parseFloat( sessionStorage.getItem('__tnt.service.rate.total') ).toFixed(2),
                        startDate = new Date(),
                        serviceIdEl = oForm.elements.service_id,
                        serviceId = "",
                        siteNameEl = document.head.querySelector('meta[property="og:site_name"]'),
                        siteName = "",
                        serviceName = sessionStorage.getItem('__tnt.service.name'),
                        rate = sessionStorage.getItem('__tnt.service.rate'),
                        ratePriceEl = null,
                        rateNameEl = null,
                        ratePrice = parseFloat( sessionStorage.getItem('__tnt.service.rate.price') ).toFixed(2),
                        rateName = sessionStorage.getItem('__tnt.service.rate.name');
                        receiptElements = [
                           {
                              'element': '.transaction-id',
                              'html': transactionId
                           }
                        ];
                        
                    if(siteNameEl){ siteName = siteNameEl.content }
                    if(serviceIdEl){ serviceId = serviceIdEl.value }

                    if(modalTabPurchaseInfo){
                        rateNameEl = modalTabPurchaseInfo.querySelector('.rate-name');
                        ratePriceEl = modalTabPurchaseInfo.querySelector('.rate-price');
                        
                        if(rateNameEl){ selectedService = rateNameEl.innerHTML }
                        if(ratePriceEl){ selectedRate = ratePriceEl.innerHTML }
                    }else{
                        selectedService = oForm.querySelector('input[name=rate_name]').value;
                        selectedRate = oForm.querySelector('input[name=rate_price]').value;
                    }
                    
                    // Prepare rate for display
                    selectedRate = '$' + ratePrice + '';
                    
                    // Get estimated tax value from storage
                    if (tax != 'NaN') {
                        estimatedTax = '$' + tax + '';
                    } else {
                        estimatedTax = 'N/A';
                    }
                    
                    // Get total from storage
                    if (taxTotal != 'NaN') {
                        total = '$' + taxTotal + '';
                    } else {
                        total = selectedRate + ' + tax';
                    }
                    
                    // Populate modal receipt elements
                    receiptElements.push({'element': '.service-name', 'html': selectedService });
                    receiptElements.push({'element': '.service-rate', 'html': selectedRate });
                    receiptElements.push({'element': '.service-tax', 'html': estimatedTax });
                    receiptElements.push({'element': '.service-start', 'html': startDate.toLocaleDateString('en-US', {month: 'long', day: 'numeric', year: 'numeric' }) });
                    
                    if (total) receiptElements.push({'element': '.service-total', 'html': total });
                    if(modalReceipt){
                        modalReceiptTemplate = modalReceipt.querySelector('template');
                        modalReceiptOutput = modalReceipt.querySelector('.modal-receipt-output');
                
                        if(modalReceiptTemplate && modalReceiptOutput){
                            modalReceiptOutput.innerHTML = "";
                
                            __tnt.subscription.access.displayReceipt = true;
                            __tnt.template(modalReceiptTemplate, modalReceiptOutput, false, receiptElements, function(t){ });
                        }
                    }
                
                    __tnt.user.loginSuccess = true;
                    __tnt.user.purchasedAccess = true;
                    
                    if(__tnt.user && __tnt.user.updateLoggedIn){
                        __tnt.user.updateLoggedIn();
                    }else if(__tnt.subscription && __tnt.subscription.access && __tnt.subscription.access.status && userStatus){
                        __tnt.user.update();
                        __tnt.subscription.access.status.trigger(userStatus);
                    }

                    try {
                        modalTabPurchaseSuccess.querySelector('.username').innerHTML = " " + __tnt.user.screenName;
                    } catch(e){ }

                    
                    if(modalTabPurchaseSuccess && modalTabPurchaseSuccess.classList.contains('inline-purchase-stage')){
                        var blockEl = document.getElementById('access-offers-block-2568412'),
                            wrapperEl = document.getElementById(''),
                            purchaseStages = false;
                            
                        if(blockEl){
                            purchaseStages = blockEl.querySelectorAll('.inline-purchase-stage');
                        } else if(wrapperEl) { 
                            purchaseStages = wrapperEl.querySelectorAll('.inline-purchase-stage'); 
                        }

                        if(purchaseStages){
                            purchaseStages.forEach(function(stage){
                               stage.hidden = true;
                               stage.classList.remove('active');
                               stage.classList.add('hide');
                            });

                            modalTabPurchaseSuccess.hidden = false;
                        }
                    }
                    

                    if(modalTabPurchaseSuccess){
                    
                        __tnt.subscription.access.showTab(modalTabPurchaseSuccess, "access-offers-modal", "access-methods");
                    
                    
                        /* Reveal tax details */
                        if (oForm.dataset.taxable == 'true') {
                            document.querySelectorAll('.tnt-tax-detail').forEach(function(el) {
                                el.classList.remove('hidden');
                            });
                        }/* Reveal appropriate messaging for purchase type */
                        __tnt.subscription.offers.purchaseType(oForm, { 'purchase_type': oForm.dataset.purchaseType });}

                    /* LOG PURCHASE COMPLETE EVENT */
                    __tnt.subscription.offers.logOrderCompleted(transactionId); // this is going to need some parameters added to it.
                });
            });

            }

        window.submitPaymentFormSpreedly = function(token, oMethod) {
            var requiredFields = {},
                modalTabSpinner = document.getElementById('modal-spinner-tab'),
                oForm = document.getElementById('form-super-purchase'),
                oName = null,
                oMonth = null,
                oYear = null,
                bCheck = true,
                elError = document.getElementById('form-error-super-purchase'),
                sError = "",
                oCaptcha = null;

            // Gather location inputs
            var paymentZip = oForm.querySelector('input[name=postal_code]'),
                billLocState = oForm.querySelector('select[name=postal-state]'),
                billLocZip = oForm.querySelector('input[name=postal-postcode]'),
                billLocCountry = oForm.querySelector('select[name=postal-country]');

            try {
                if (typeof paymentZip === 'undefined') {
                    // Create hidden postal_code input
                    var paymentZip = document.createElement('input');
                    
                    paymentZip.setAttribute('type', 'hidden');
                    paymentZip.setAttribute('name', 'postal_code');
                    paymentZip.setAttribute('value', billLocZip.value);

                    oForm.appendChild(paymentZip);
                } else if (!paymentZip.value || paymentZip.value == null) {
                    // Populate empty postal_code input
                    paymentZip.value = billLocZip.value;
                } 
            } catch(e) {}

            try {
                // Ensure purchaser inputs are supplied for gift transactions
                if (oForm.dataset.purchaseType == 'gift') {

                    if (oForm.dataset.taxable == 'true') {

                        // Get purchaser inputs
                        let purchaserState = oForm.querySelector('input[name=purchaser_state]'),
                            purchaserZip = oForm.querySelector('input[name=purchaser_zip]'),
                            purchaserCountry = oForm.querySelector('input[name=purchaser_country]');

                        // Create hidden purchaser input fields if they don't exist
                        if (purchaserState === null && purchaserZip === null && purchaserCountry === null) {
                            purchaserState = document.createElement('input'),
                            purchaserZip = document.createElement('input'),
                            purchaserCountry = document.createElement('input');

                            purchaserState.setAttribute('type', 'hidden');
                            purchaserState.setAttribute('name', 'purchaser_state');

                            purchaserZip.setAttribute('type', 'hidden');
                            purchaserZip.setAttribute('name', 'purchaser_zip');

                            purchaserCountry.setAttribute('type', 'hidden');
                            purchaserCountry.setAttribute('name', 'purchaser_country');

                            // Append hidden inputs to form
                            oForm.appendChild(purchaserState);
                            oForm.appendChild(purchaserZip);
                            oForm.appendChild(purchaserCountry);
                        }

                        // Update purchaser input fields
                        purchaserState.value = billLocState.value;
                        purchaserZip.value = billLocZip.value;
                        purchaserCountry.value = billLocCountry.value;
                    }
                }
            } catch(e) {}

            if(oForm.getAttribute('data-paypal-mode') != "true"){grecaptcha.enterprise.ready(function(){
                        grecaptcha.enterprise.execute('6LdF3BEhAAAAAEQUmLciJe0QwaHESwQFc2vwCWqh', {action: 'subscribe'}).then(function(token){

                            var vk = document.getElementById("voight-kampf-super-purchase");
                                vk.value = token;

                            if(modalTabSpinner){
                                __tnt.subscription.access.showTab(modalTabSpinner, "access-offers-modal", "access-methods");
                            }

                            if(oForm){
                                // Get required, non-sensitive, values from host page
                                oName = oForm.querySelector('input[name=tnpay-name]');
                                oMonth = oForm.querySelector('select[name=tnpay-month]');
                                oYear = oForm.querySelector('select[name=tnpay-year]');
                                oZip = oForm.querySelector('input[name=postal_code]');

                                if(oName){ requiredFields["full_name"] = oName.value } else { bCheck = false; sError += " name not found,"; }
                                if(oMonth){ requiredFields["month"] = oMonth.value } else { bCheck = false; sError += " month not found,"; }
                                if(oYear){ requiredFields["year"] = oYear.value } else { bCheck = false; sError += " year not found,"; }
                                if(oZip){ requiredFields["zip"] = oZip.value } else { bCheck = false; sError += " zip code not found, "; }
                                
                                if(bCheck && oForm.classList.contains('use-saved-card')){
                                    Spreedly.recache();
                                }else if(bCheck){
                                    Spreedly.tokenizeCreditCard(requiredFields);
                                }
                            }
                        });
                    });}else{
                try{
                    var paypalForm = document.getElementById('tncms-paypal-super-purchase');
                    
                    sessionStorage.setItem('__tnt.subscription.offers.purchaseType',oForm.dataset.purchaseType);
                    
                    if(paypalForm){
                        paypalForm['service_id'].value = oForm['service_id'].value;
                        paypalForm['rate_id'].value = oForm['rate_id'].value;
                        paypalForm['email'].value = oForm['email'].value;
                        paypalForm['password'].value = oForm['password'].value;
                        paypalForm['password_verify'].value = oForm['password_verify'].value;;
                        paypalForm['postal-postcode'].value = oForm['postal-postcode'].value;
                        paypalForm['postal_code'].value = oForm['postal-postcode'].value;
                        paypalForm['postal-country'].value = oForm['postal-country'].value;
                        paypalForm['country'].value = oForm['postal-country'].value;

                        paypalForm.submit();
                    }
                } catch(error){ console.log(error) }
            }
            
            return false;
        }

        
            /* Federated Auth Toggle */
            var oFederatedAuth = document.getElementById('form-federated-auth-super-purchase'),
                oFederatedAuthEmailForm = document.getElementById('form-federated-auth-super-purchase-email'),
                oSPPLogin = document.getElementById('form-signup-super-purchase');

            // Identifiy form as accepting fed auth
            oForm.dataset.authFederated = 'true';

            window.enableFormForKnownUserSpreedly = function() {
                oForm.classList.remove("not-logged-in-form-disabled");
                if (oSPPLogin) oSPPLogin.classList.remove('hide');
                if (oFederatedAuth) oFederatedAuth.classList.add('hide');
                oForm.hidden = false;

                var modalEl = document.getElementById('access-offers-modal'),
                    modalDialogContainer = null,
                    activeTab = null;

                if(modalEl){
                    modalDialogContainer = modalEl.querySelector('.modal');

                    if(modalDialogContainer && modalDialogContainer.classList.contains('in')){
                        activeTab = modalDialogContainer.querySelector('.modal-content.active');

                        if(activeTab){
                            activeTab.classList.remove('modal-scrollable');
                            activeTab.removeAttribute('data-scrollable');
                        }
                    }

                    tntShowOfferModalTab.details = {};
                    tntShowOfferModalTab.details.tab = activeTab;
                    tntShowOfferModalTab.details.resizeTab = true;
                    window.dispatchEvent(tntShowOfferModalTab);
                }
            }

            window.addEventListener('tnt-user-status',function(ev){
                if(__tnt.user.authToken == null){
                    oForm.classList.add("not-logged-in-form-disabled");
                    if (oSPPLogin) oSPPLogin.classList.add('hide');
                    if (oFederatedAuth) oFederatedAuth.classList.remove('hide');
                    oForm.hidden = true;
                } else {
                    enableFormForKnownUserSpreedly();
                }
            });

            window.selectedContinueWithEmailSpreedly = function() {
                var emailAddressEl = oFederatedAuthEmailForm.querySelector('input[name=email]'),
                    emailAddress = "";
                
                if(emailAddressEl){ emailAddress = emailAddressEl.value }
                
                if(emailAddress != ""){
                    oForm.querySelector('input[name=email]').value = emailAddress;
                    oForm.dataset.continueWithEmail = 'true';
                    enableFormForKnownUserSpreedly();
                }
                
                return false;
            };
        var spreedlyInitialized_Spreedly = false;
        oForm.addEventListener('tnt-purchase', function(event) {
            if (spreedlyInitialized_Spreedly == false) {
                spreedlyInit_Spreedly();
                spreedlyInitialized_Spreedly = true;
            }

            var oForm = document.getElementById('form-super-purchase'),
                oButton = document.getElementById('form-submit-super-purchase'),
                oTOS = document.getElementById('tn-tos-super-purchase'),
                oEmailFields = document.querySelectorAll('#email-super-purchase, #email-federated-option-super-purchase'),
                oFormAddress = oForm.querySelector('input[name="address"]'),
                oFormMunicipality = oForm.querySelector('input[name="municipality"]'),
                oFormRegion = oForm.querySelector('input[name="region"]'),
                oFormPostalCode = oForm.querySelector('input[name="postal_code"]'),
                oFormCountry = oForm.querySelector('input[name="country"]'),
                oFormPhone = oForm.querySelector('input[name="phone"]'),
                oFormCardName = document.getElementById('tnpay-name-super-purchase'),
                oFormLocation = document.getElementById('form-location-super-purchase'),
                oAltPaymentSelect = document.getElementById('tn-pay-method-select-super-purchase'),
                oAltPaymentSelected = null,
                oAltPayment = document.getElementById('tn-pay-method-alt-super-purchase'),
                purchaseType = null;

                /* Make init aware of gift subscription */
                if(oForm.dataset.purchaseType){ purchaseType = oForm.dataset.purchaseType }

                /* Set up Alt Payment */
                window.paymentPromisesSpreedly = [];
                oAltPaymentSelect.hidden = true;
                oAltPayment.innerHTML = "";

                oForm['tnpay-name'].required = true;try {
                    if(purchaseType != "gift" && !__tnt.user.authToken){
                        if(oForm['email']){
                            oForm['email'].disabled = false;
                            oForm['email'].required = true;
                        }

                        if(oForm['password']){
                            oForm['password'].disabled = false;
                            oForm['password'].required = true;
                        }
                    }
                } catch(error){ console.log(error) }if(oAltPaymentSelect){
                    oAltPaymentSelected = oAltPaymentSelect.querySelector('.tn-pay-method-selected');
                    if(oAltPaymentSelected){ oAltPaymentSelected.classList.remove('tn-pay-method-selected') }
                }
                
                window.paymentPromisesSpreedly = [];
                oAltPayment.innerHTML = "";

                /* Checkout Step - entering the email */
                oEmailFields.forEach(function(oEmailField) {
                    oEmailField.addEventListener('focus', function(ev) {
                        __tnt.subscription.offers.logCheckoutStepViewed({
                            step: __tnt.subscription.offers.getPurchaseStep('email'),
                            label: 'Entering email address'
                        });
                    });

                    oEmailField.addEventListener('blur', function(ev) {
                        if (oEmailField.value) {
                            __tnt.subscription.offers.logCheckoutStepCompleted({
                                step: __tnt.subscription.offers.getPurchaseStep('email'),
                                label: 'Email address defined'
                            });
                        }
                    });
                });

                /* Checkout Step - entering the location */
                if (oFormAddress) {
                    var locationFields = oFormLocation.querySelectorAll('input[type=text], input[type=tel], select');
                    locationFields.forEach(function(el) {
                        // Set lengths for inputs that are pre-filled
                        if (el.value.length > 0) {
                            __tnt.subscription.offers.locationInputLengths[el.name] = el.value.length;
                        }
                    
                        el.addEventListener('focus', function(ev) {
                            __tnt.subscription.offers.logCheckoutStepViewed({
                                step: __tnt.subscription.offers.getPurchaseStep('location'),
                                label: 'Entering location information'
                            });
                        });
                        
                        el.addEventListener('blur', function(ev) {
                            __tnt.subscription.offers.locationInputLengths[ev.target.name] = ev.target.value.length;
                            
                            if (__tnt.subscription.offers.locationInputsEntered()) {
                                __tnt.subscription.offers.logCheckoutStepCompleted({
                                    step: __tnt.subscription.offers.getPurchaseStep('location'),
                                    label: 'Location information defined'
                                });
                            }
                        });
                    });
                }
                
                /* Checkout Step - entering the cardholder name */
                if (oFormCardName) {
                    oFormCardName.addEventListener('focus', function(ev) {
                        __tnt.subscription.offers.logCheckoutStepViewed({
                            step: __tnt.subscription.offers.getPurchaseStep('cardholder_name'),
                            label: 'Entering cardholder name',
                            paymentMethod: 'Credit Card'
                        });

                    });

                    oFormCardName.addEventListener('blur', function(ev) {
                        if (oFormCardName.value) {
                            __tnt.subscription.offers.logCheckoutStepCompleted({
                                step: __tnt.subscription.offers.getPurchaseStep('cardholder_name'),
                                label: 'Cardholder name defined',
                                paymentMethod: 'Credit Card'
                            });

                            __tnt.subscription.offers.ccNameLength = oFormCardName.value.length;

                            if (__tnt.subscription.offers.ccInputsEntered()) {
                                __tnt.subscription.offers.logPaymentInfoEntered({
                                    step: __tnt.subscription.offers.getPurchaseStep('payment_info'),
                                    paymentMethod: 'Credit Card'
                                });
                            }
                        }
                    });
                }

                /* Checkout Step - agreeing to terms of service */
                oTOS.addEventListener('change', function(ev) {
                    SpreedlyallowSubmit();
                });


                /* Reset Form When in Saved Card State */
                document.getElementById('tn-pay-method-new-card-super-purchase').addEventListener('click',function(ev){
                    ev.preventDefault();

                    var paymentForm = document.getElementById('form-super-purchase'), 
                        payMethodWrapper = document.getElementById('tn-pay-method-output-super-purchase'),
                        payMethodNewCard = document.getElementById('tn-pay-method-new-card-super-purchase');

                    document.getElementById('tn-pay-manual-entry-super-purchase').hidden = false;

                    if(paymentForm['tnpay-name'].required){
                        paymentForm['tnpay-name'].required = true;
                    }
                    if(paymentForm['postal_code'].required){
                        paymentForm['postal_code'].required = true;
                    }
                    
                    if(oForm.classList.contains('use-saved-card')){
                        Spreedly.reload();
                    }

                    var newCardBtn = document.getElementById('tn-pay-method-new-card-super-purchase');
                    if (!newCardBtn.classList.contains('tn-pay-method-selected')) {
                        /* Checkout Step - Selected payment method */
                        __tnt.subscription.offers.logCheckoutStepViewed({
                            step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                            label: 'Selected payment method',
                            paymentMethod: 'Credit Card'
                        });
                        __tnt.subscription.offers.logCheckoutStepCompleted({
                            step: __tnt.subscription.offers.getPurchaseStep('selected_payment_method'),
                            label: 'Selected payment method',
                            paymentMethod: 'Credit Card'
                        });
                    }
                });

                if(__tnt.user.authToken){
                    

                        var oEmail = null,
                            oPass = null,
                            oName = null,
                            oMonth = null,
                            oYear = null,
                            oZip = null;

                        if(oForm){
                            oEmail = oForm.querySelector('input[name=email]');
                            oPass = oForm.querySelector('input[name=password]');
                            oName = oForm.querySelector('input[name=tnpay-name]');
                            oMonth = oForm.querySelector('select[name=tnpay-month]');
                            oYear = oForm.querySelector('select[name=tnpay-year]');
                            oZip = oForm.querySelector('input[name=postal_code]');

                            try {
                                if(oEmail){
                                    oEmail.required = false;
                                    oEmail.disabled = true;
                                }

                                if(oForm['password']){
                                    oPass.required = false;
                                    oPass.disabled = true;
                                }
                            } catch(error){ console.log(error) }
                        }
                    

                    paymentPromisesSpreedly[paymentPromisesSpreedly.length] = new Promise(function(resolve, reject){ 
                        fetch('/tncms/pay/wallet/list/', {
                            method: 'get',
                            headers: {
                                'X-Requested-With': 'XMLHttpRequest'
                            }
