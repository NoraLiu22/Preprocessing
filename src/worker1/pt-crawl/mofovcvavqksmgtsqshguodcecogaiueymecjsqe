                    price: "0.00", // Static value
                  },
                ],
              },
            },
          },
        ];

        $(".sub-btn, .other").click(function (event) {
          var dataLayer = window.dataLayer || [];
          dataLayer.push({
            event: "addToCart", // This value must not change. Required
            ecommerce: {
              currencyCode: "AUD", // Required, static
              add: {
                products: [
                  {
