          });
        });

        function copyParamToAnchorUrl(anchor, paramName) {
          var url = anchor.attr("href");
          var paramValue = getUrlParam(document.URL, paramName);
          if (paramValue) {
            anchor.attr("href", (url += "&" + paramName + "=" + paramValue));
          }
        }

        function copyParamToAnchorUrl1(anchor, paramName) {
          var url = anchor.attr("href");
          var paramValue = getUrlParam(document.URL, paramName);
          if (paramValue) {
            anchor.attr("href", (url += "?" + paramName + "=" + paramValue));
          }
        }

        function getUrlParam(url, name) {
          var a = document.createElement("a");
          a.href = url;
          var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
          var r = a.search.substr(1).match(reg);
          if (r != null) return unescape(r[2]);
          return null;
        }
        var promo_channel = getUrlParam(document.referrer, "promote_channel");
        window.dataLayer.push({
          event: "event-Ecommerce",
          eventCategory: "Ecommerce",
          eventAction: "Product Impression",
          eventLabel: promo_channel,
          eventValue: "",
        });

        $(".sub-btn").mousedown(function (event) {
          // var anchor = $(this).closest("a");
          var anchor = $(this)
          copyParamToAnchorUrl(anchor, "promote_channel");
          copyParamToAnchorUrl(anchor, "campaign_code");
        });

        $(".other").mousedown(function (event) {
          // var anchor = $(this).closest("a");
          var anchor = $(this)
          copyParamToAnchorUrl1(anchor, "promote_channel");
          copyParamToAnchorUrl1(anchor, "campaign_code");
        });
      });
    </script>

