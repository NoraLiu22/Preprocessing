        });

        $(".annual-sub").click(function (event) {
          var dataLayer = window.dataLayer || [];
          dataLayer.push({
            event: "addToCart", // This value must not change. Required
            ecommerce: {
              currencyCode: "AUD", // Required, static
              add: {
                products: [
                  {
                    name: "WAtoday - Save 50% for 6 months", // Insert relevant name, Required (e.g. All Digital Access + Weekend Papers [Free Trial where applicable])
