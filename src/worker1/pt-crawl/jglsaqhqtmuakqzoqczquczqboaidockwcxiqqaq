<script language="javascript" type="text/javascript">
function showmen(n){
	for(var i=1;i<=2;i++){
		var curCon=document.getElementById("p1Aa_"+i);
		var curBtn=document.getElementById("p1Ab_"+i);
		if(n==i){
			curBtn.style.display="block";
			curCon.className="red"
		}else{
			curBtn.style.display="none";
			curCon.className="gray";			
		}
	}
}
function showmen2(n){
	for(var i=1;i<=5;i++){
		var curCon=document.getElementById("p2Aa_"+i);
		var curBtn=document.getElementById("p2Ab_"+i);
		if(n==i){
			curBtn.style.display="block";
			curCon.className="withe"
		}else{
			curBtn.style.display="none";
			curCon.className="";			
		}
	}
}
</script>
