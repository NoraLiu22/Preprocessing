				<script>
				var speed=40
				var index_notice =document.getElementById("index_notice_div");
				var index_notice2=document.getElementById("index_notice2_ul");
				var index_notice1=document.getElementById("index_notice1_ul");
				index_notice2.innerHTML=index_notice1.innerHTML
				function Marquee()
				{
					if(index_notice2.offsetTop-index_notice.scrollTop<=0)
						index_notice.scrollTop-=index_notice1.offsetHeight
					else
					{
						index_notice.scrollTop++
					}
				}
				var MyMar=setInterval(Marquee,speed)
				index_notice.onmouseover=function() {clearInterval(MyMar)}
				index_notice.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
				</script> 
