      var fontSize = $.trim($('.Box1_Top .none').text());


      if (/\d{2}/.test(fontSize)) {
        $('.Box1_Top h1 a').css('fontSize', fontSize + 'px');
      }


    }(window, jQuery);

    (function ($, window) {
      $('.Box1_Right li:last').css('border-bottom', 'none');

      var npSum = $('.Box1_Left li').length,
        npCur = 0,
        npAuto = null;

      $('.Box1_Left li').first().show();
      $('.Box1_Left p').fadeTo(0, 0.7);
      $('.plr img').fadeTo(0, 0.4).on('mouseenter', function () {
        $(this).fadeTo(100, 1);
      }).on('mouseleave', function () {
        $(this).fadeTo(100, 0.4);
      });
      $('.pmenu').html('<span>' + (npCur + 1) + '</span>' + ' / ' + npSum);
      $('#npl').on('click', function () {
        npCur -= 1;
        if (npCur < 0) {
          npCur = npSum - 1;
        }

        newpicAdd();
      });
      $('#npr').on('click', function () {
        npCur = (npCur + 1) % npSum;
        newpicAdd();
      });

      function newpicAuto() {
        npCur = (npCur + 1) % npSum;
        newpicAdd();
      }

      function newpicAdd() {
        $('.Box1_Left li').hide();
        $('.Box1_Left li').eq(npCur).fadeTo(200, 1);
        $('.pmenu span').text(npCur + 1);
      }

      npAuto = window.setInterval(newpicAuto, 5000);
      $('.Box1_Left').on('mouseenter', function () {
        window.clearInterval(npAuto);
      }).on('mouseleave', function () {
        npAuto = window.setInterval(newpicAuto, 5000);
      });

    })(window.jQuery, window);
  </script>


  <script type="text/javascript">
    (function ($, window) {

      $('.Tabs').each(function (idx, el) {
        var el_li = $(el).find('>ul>li'),
          el_cont = $(el).siblings('.choose').find('.Content'),
          el_cur = 0;

        el_cont.eq(0).show();
        el_li.on('mouseenter', function () {
          el_cur = $(this).index();
          el_li.removeClass('Cur');
          $(this).addClass('Cur');
          el_cont.hide();
          el_cont.eq(el_cur).show();
        });
      });

    })(window.jQuery, window);
  </script>

  <script type="text/javascript">
    (function ($, window) {

      rollpic('#roll1', 262);
      rollpic('#roll2', 210);
      rollpic('#roll3', 210);
      rollpic('#roll4', 210);


      function rollpic(id, wid) {
        var $id = $(id),
          pic_ul = $id.find('ul'),
          li_wid = wid,
          pic_left = $id.find('.left'),
          pic_right = $id.find('.right'),
          pic_cur = 0,
          pic_sum = $id.find('li').length,
          pic_auto = null;

        pic_ul.css('width', li_wid * pic_sum * 2);
        pic_ul.append(pic_ul.find('li').clone());

        pic_left.on('click', function () {
          if (pic_cur == 0) {
            pic_cur = pic_sum;
            pic_ul.css('margin-left', -li_wid * pic_cur);
          }
          pic_cur -= 1;
          pic_ul.stop(true, true).animate({
            'margin-left': -li_wid * pic_cur
          });
        });

        pic_right.on('click', function () {
          pic_cur += 1;
          pic_ul.stop(true, true).animate({
            'margin-left': -li_wid * pic_cur
          }, function () {
            if (pic_cur >= pic_sum) {
              pic_ul.css('margin-left', '0px');
              pic_cur = 0;
            }
          });

        });

        function AutoRoll() {
          pic_cur += 1;
          pic_ul.stop(true, true).animate({
            'margin-left': -li_wid * pic_cur
          }, function () {
            if (pic_cur >= pic_sum) {
              pic_ul.css('margin-left', '0px');
              pic_cur = 0;
            }
          });
        }

        pic_auto = window.setInterval(AutoRoll, 5000);
        $id.on('mouseenter', function () {
          window.clearInterval(pic_auto);
        }).on('mouseleave', function () {
          pic_auto = window.setInterval(AutoRoll, 5000);
        });
      }

    })(window.jQuery, window);
  </script>

</body>

