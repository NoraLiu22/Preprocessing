		$opt.addClass('open')
		$optbd.show();
		$opt.find('span.expandInd').html('&#9650;');
	}
	return false
});
		IT.logTime( "Initial reflow" );
		IT.reflow();
		IT.logTimeEnd( "Initial reflow" );
		IT.logGroupTimeEnd( "toolset onready" );
	});
//]]>
