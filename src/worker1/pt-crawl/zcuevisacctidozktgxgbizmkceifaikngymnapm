    (function () {
        try {
          window.onload = function(){
            var frameName = new ds07o6pcmkorn({
                openElementId: "#shielded-logo",
                modalID: "modal",
            });
            frameName.init();
            frameName.frameOpenerEl.addEventListener("keypress", function(event){
              var code = event.charCode || event.keyCode;
              if((code === 32)|| (code === 13)){
                frameName.openFrame(event);
              }
            })  
          }
        }catch(err) {}
    })();
