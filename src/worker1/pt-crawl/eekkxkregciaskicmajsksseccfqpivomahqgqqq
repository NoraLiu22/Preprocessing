<script type="text/template" class="deferred-script">


	var nav = $('#site-search');
	var search = {
		'form': $('#searchform'),
		'top': 0,
		'open': $('#search-icon a'),
		'isOpen': false,
		'tweenLength': 300
	};

	search.open.click(function() {



		if (search.isOpen == false) {

			search.form.animate(
				{'left': "43px", 'opacity': 1, 'z-index':999},
				300,
				function() {
					search.isOpen = true;
				}
			);

		} else {

		search.form.animate(
				{	'left': "268", 'display': 'none', 'opacity': 0, 'z-index':1 },
				300,
				function() {
					search.isOpen = false;
				}
			);


		}
	});
</script>


