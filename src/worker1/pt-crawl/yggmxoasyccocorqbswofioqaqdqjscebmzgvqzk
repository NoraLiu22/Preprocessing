<section class="article">

<div class="relative clear ">
            <div class="panel-hero panel-hero--short">
            <div class="panel-hero__image" style="background-image: url(https://bcu.imgix.net/open-day-hero-from-prospectus-131364784995180160.jpg?auto=format&amp;fm=jpg)"></div>
            <div class="panel-hero__content">
                <div class="panel-hero__content-inner">
                <h1 class="panel-hero__title">Manage Your Privacy</h1>
                                </div>
            </div>
        </div>
</div><div class=" relative clear">
            <div class="bcu-editable">
            <section class="privacy-content mtl ptl">
            <article>
                <div class="privacy-content__areas">
                    <section class="privacy-content__text">
                        <h2>It's Your Choice</h2>
                        <p>
                            You can manage your privacy settings at any time using the sections below and by visiting our
                            privacy policy which includes a link back to these settings.
                        </p>
                    </section>

                    <section class="privacy-content__text">
                        <h2>Cookies (Required &amp; Optional)</h2>
                        <p>
                            This website uses cookies (small files place on your device) and similar technologies to store
                            information about your preferences and settings, provide customisation and, analyse how our
                            service is performing.
                        </p>
                    </section>
                </div>
            </article>
</section>
        </div>
</div>
<div class="relative clear ">

        <section class="privacy-content mtl ptl">
            <article>
                <form method="post" action="/manage-privacy">
                    <input name="__RequestVerificationToken" type="hidden" value="hpw1T94sbDJ3T6M5KTkyJ2FMmBaF6Ggqr6rskQT2p7bXUEvq4cyrNwc8W2ZrNWPkNQcHJT0J02TSAhFr354aF2c_LSmgFAeCWxC4e1kvQ8xqSZrvfAVmzBCjn00oygow0" />

                    <div class="privacy-content__setting-sections">
                        <div class="privacy-content__setting-section js-privacy-setting-section-cookies">

                            <noscript>
                                <input type="submit" value="Save Settings" class="privacy-content__submit raised-button raised-button--primary" />
                            </noscript>

                            <section class="privacy-content__setting">
                                <div class="privacy-control">
                                    <label class="switch js-privacy-cookies-functional">
