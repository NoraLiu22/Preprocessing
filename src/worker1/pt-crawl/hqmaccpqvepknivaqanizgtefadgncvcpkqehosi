        var n,dq=0,wid
                    function autoPlay(){
                        dq++
                        if(dq<n-1) 
                        {
                            $($("#number li")[dq]).addClass("current")
                            $($("#number li")[dq]).siblings().removeClass("current")
                            $($(".txt dl")[dq]).fadeTo(200,0.7)
                            $($(".txt dl")[dq]).siblings().hide()
                            $("#scrollArea").stop(true,true).animate({scrollLeft:650*dq})
                        }
                        else
                        {
                            $($("#number li")[0]).addClass("current")
                            $($("#number li")[0]).siblings().removeClass("current")
                            $($(".txt dl")[0]).fadeTo(200,0.7)
                            $($(".txt dl")[0]).siblings().hide()
                            $("#scrollArea").stop(true,true).animate({scrollLeft:650*dq}).animate({scrollLeft:0},0)
                            dq = 0
                        }
                    }
                    
                     $(function(){
                     $(".turnscroll").fadeTo(0,0.7);
                     $(".txt dl:first").show();
                        n = $("#scrollArea li").size()
                            $("#focus").append("<ul id=\"number\" class=\"number\"><\/ul>")
                        for(i=1;i<=n;i++)
                        {
                            $("#number").append("<li>" + i + "<\/li>")
                        }
                        $("#scrollArea ul").append($("#scrollArea li:first").clone())
                        n = n + 1
                        $("#scrollArea ul").css("width",650*n)
                        $("#number li").css("left",function(i){return i * 23 + 260})
                        $("#number li:first").addClass("current")
                        
                        $("#number li").hover(
                           function(){
                              window.clearInterval(wid)
                              dq = $(this).index()
                              $(this).addClass("current")
                              $(this).siblings().removeClass("current")
                              $("#scrollArea").stop(true,true).animate({scrollLeft:650*dq}) 
                              $(".txt dl").hide()
                              $($(".txt dl")[dq]).fadeTo(200,1)
                           },
                           function(){ wid = window.setInterval("autoPlay()",3000) }       
                        )
                        
                        $(".turnscroll").hover(
                           function(){ window.clearInterval(wid) },
                           function(){ wid = window.setInterval("autoPlay()",3000) }       
                        )
                        
                        $('#turnleft').on('click', function() {
                            dq = (dq - 2 + n) % (n - 1);
                            $("#scrollArea").stop(true,true).animate({scrollLeft:650*dq}) 
                            $("#number li:eq(" + dq + ")").addClass("current")
                            $("#number li:eq(" + dq + ")").siblings().removeClass("current")
                            $(".txt dl:eq(" + dq + ")").fadeTo(200,1)
                            $(".txt dl:eq(" + dq + ")").siblings().hide();
                            })
                            
                        $('#turnright').on('click', function() {
                            dq = (dq + n) % (n - 1);
                            $("#scrollArea").stop(true,true).animate({scrollLeft:650*dq}) 
                            $("#number li:eq(" + dq + ")").addClass("current")
                            $("#number li:eq(" + dq + ")").siblings().removeClass("current")
                            $(".txt dl:eq(" + dq + ")").fadeTo(200,1)
                            $(".txt dl:eq(" + dq + ")").siblings().hide();
                            })
                        
                        wid = window.setInterval(autoPlay,3000);
                     })
