            $('.picLeft').on('click', function () {
                picCur -= 1;
                picCur += picSum;
                picCur %= picSum;
                scrPic(picCur);
            });

            function scrPic(i) {
                $('.pic ul').animate({
                    'margin-left':
                        -610 * i
                });
                $('.picMenu p').removeClass('picCur');
                $('.picMenu p').eq(i).addClass('picCur');
                $('.text dl').hide();
                $('.text dl').eq(i).fadeTo(200, 1);
            }

            function autoPic() {
                picCur += 1;
                picCur %= picSum;
                scrPic(picCur);
            }
            var ImgPlay = window.setInterval(autoPic, 5000);
            $('.bigpic').on('mouseenter', function () {
                window.clearInterval(ImgPlay);
            }).on('mouseleave', function () {
                ImgPlay = window.setInterval(autoPic, 5000);
            });

            $('.basicLine dl').each(function (idx) {
                $(this).find('span').text(idx + 1);
            })

            // time line
            var timeSum = $('.time p').size();
            var timeLast = 6;
            $('.time div').width(139 * timeSum);
            $('.time p').first().addClass('timeCur');
            $('.bookAll dl').first().show();
            var timeCur = 0;

            $('.bookRight').on('click', function () {
                timeCur += 1;
                if (timeCur > timeSum - 1) {
                    timeCur = timeSum - 1
                };
                nextTime(timeCur);
            })
            $('.bookLeft').on('click', function () {
                timeCur -= 1;
                if (timeCur <= 0) {
                    timeCur = 0
                };
                preTime(timeCur);

            });

            $('.time p').on('click', function () {
                timeCur = $(this).index();
                changeTime(timeCur);
            });



            function nextTime(i) {
                if (timeCur <= timeLast) {
                    changeTime(timeCur);
                }
                if (timeLast < timeSum && timeCur > timeLast) {
                    timeLast += 1;
                    $('.time div').stop(true, true).animate({
                        'margin-left':
                            -130 * (i - 6)
                    });
                    changeTime(timeCur);
                }


            }

            function preTime(i) {
                if (timeCur >= timeLast - 6) {
                    changeTime(timeCur);
                }
                if (timeCur < timeLast - 6 && timeLast > 6) {
                    timeLast -= 1;
                    $('.time div').stop(true, true).animate({
                        'margin-left':
                            -130 * i
                    });
                    changeTime(timeCur);
                }
            }

            function changeTime(i) {
                $('.time p').removeClass('timeCur');
                $('.time p').eq(i).addClass('timeCur');
                $('.bookAll dl').hide();
                $('.bookAll dl').eq(i).fadeTo(200, 1);
            }


            // change maps
            $('.mapAll dl').first().show();
            var mapSum = $('.mapLine li').size();
