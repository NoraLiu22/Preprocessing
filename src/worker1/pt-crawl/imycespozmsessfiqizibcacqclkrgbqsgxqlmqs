\":\"Content\",\"description\":\"Just in\",\"displayName\":\"Just in\",\"id\":\"1qk\",\"name\":\"Just in\",\"slug\":\"\",\"urls\":{}}],\"primary\":{\"context\":\"Topic\",\"displayName\":\"Pharmaceuticals\",\"id\":\"hnn\",\"name\":\"Pharmaceuticals\",\"slug\":\"pharmaceuticals\",\"urls\":{\"published\":{\"afr\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"brisbanetimes\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"canberratimes\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"nine\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"smh\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"theage\":{\"path\":\"/topic/pharmaceuticals-hnn\"},\"watoday\":{\"path\":\"/topic/pharmaceuticals-hnn\"}}}},\"secondary\":[{\"context\":\"Topic\",\"displayName\":\"Weight loss\",\"id\":\"1msw\",\"name\":\"Weight loss\",\"slug\":\"weight-loss\",\"urls\":{\"published\":{\"afr\":{\"path\":\"/topic/weight-loss-1msw\"},\"brisbanetimes\":{\"path\":\"/topic/weight-loss-1msw\"},\"canberratimes\":{\"path\":\"/topic/weight-loss-1msw\"},\"nine\":{\"path\":\"/topic/weight-loss-1msw\"},\"smh\":{\"path\":\"/topic/weight-loss-1msw\"},\"theage\":{\"path\":\"/topic/weight-loss-1msw\"},\"watoday\":{\"path\":\"/topic/weight-loss-1msw\"}}}},{\"context\":\"Topic\",\"displayName\":\"Biotech\",\"id\":\"5v0\",\"name\":\"Biotechnology\",\"slug\":\"biotechnology\",\"urls\":{\"published\":{\"afr\":{\"path\":\"/topic/biotechnology-5v0\"},\"brisbanetimes\":{\"path\":\"/topic/biotechnology-5v0\"},\"canberratimes\":{\"path\":\"/topic/biotechnology-5v0\"},\"nine\":{\"path\":\"/topic/biotechnology-5v0\"},\"smh\":{\"path\":\"/topic/biotechnology-5v0\"},\"theage\":{\"path\":\"/topic/biotechnology-5v0\"},\"watoday\":{\"path\":\"/topic/biotechnology-5v0\"}}}}]},\"urls\":{\"canonical\":{\"brand\":\"smh\",\"path\":\"/business/companies/it-introduced-ozempic-to-the-world-now-it-must-remake-itself-20240423-p5flv7.html\"},\"published\":{\"brisbanetimes\":{\"path\":\"/business/companies/it-introduced-ozempic-to-the-world-now-it-must-remake-it