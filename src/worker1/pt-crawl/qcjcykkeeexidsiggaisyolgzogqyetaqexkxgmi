  <script type="text/javascript">
      (function ($, window) {

        var pics = $('.b1la li').size(),
          pcur = 0,
          autop = null,
          jlis = $('.b4b li').size(),
          jcur = 0,
          pm = '';

        $('.b1la p').fadeTo(0, 0.7);
        $('.b1la ul').css('width', 435 * (pics + 1));
        $('.b1la li').each(function (idx, el) {
          pm += '<span>·<\/span>';
        });
        $('.b1la div').html(pm);
        $('.b1la div span').first().addClass('cur2');
        $('.b1la ul').append($('.b1la li:first').clone());
        $('.b1la div span').on('click', function () {
          pcur = $(this).index();
          $('.b1la div span').removeClass('cur2');
          $('.b1la div span').eq(pcur).addClass('cur2');
          $('.b1la ul').animate({
            'margin-left': pcur * -435
          });
        });
        function roll() {
          pcur += 1;
          $('.b1la div span').removeClass('cur2');
          $('.b1la div span').eq(pcur).addClass('cur2');
          $('.b1la ul').animate({
            'margin-left': pcur * -435
          }, function () {
            if (pcur == pics) {
              $('.b1la ul').css('margin-left', '0px');
              pcur = 0;
              $('.b1la div span').removeClass('cur2');
              $('.b1la div span').eq(pcur).addClass('cur2');
            }
          });
        }

        autop = window.setInterval(roll, 5000);
        $('.b1la').on('mouseenter', function () {
          window.clearInterval(autop);
        }).on('mouseleave', function () {
          autop = window.setInterval(roll, 5000);
        });

      })(window.jQuery, window);

  </script>

  <script type="text/javascript">

    $(function () {
      menu('#menu1');
      menu('#menu2');
      menu('#menu3');

      function menu(id) {
        var c = 0,
          menu = $(id);
        menu.find('.Trade_Content div:first').show();
        menu.find('.Trade_Tabs li').mouseover(function () {
          c = $(this).index();
          menu.find('.Trade_Tabs li').removeClass("Cur");
          menu.find('.Trade_Tabs li').eq(c).addClass("Cur");
          menu.find('.Trade_Content div').hide();
          menu.find('.Trade_Content div').eq(c).fadeTo(200, 1);
        });
      }
    })

  </script>

  <script type="text/javascript">
      (function ($) {
        var slideW = document.body.clientWidth;
        var itemW = $('#slideshow .list li').outerWidth(true);
        var len = $('#slideshow .list li').length;
        var cur = 0;
        var temp = '';
        $('#slideshow, #slideshow .img').width(slideW);
        $('#slideshow .previousPic').css({ 'left': - (itemW - (slideW - itemW) / 2) });
        $('#slideshow .nextPic').css({ 'right': - (itemW - (slideW - itemW) / 2) });
        $('#slideshow .btns .prev').css({ 'left': (slideW - itemW) / 2 + 9 });
        $('#slideshow .btns .next').css({ 'right': (slideW - itemW) / 2 + 9 });


        $('#slideshow .imgData').append($('#slideshow .list:first').clone());
        $('#slideshow .imgData').append($('#slideshow .list:first').clone());
        $('#slideshow .imgData').width(itemW * (3 * len));
        $('#slideshow .imgData').css({ 'margin-left': (slideW - itemW) / 2 - itemW * len });


        for (var i = 0; i < len; i++) {
          temp += '<span ' + (i == 0 ? 'class="cur"' : '') + '><\/span>'
        }
        $('#slideshow .btns').append('<div class="ico">' + temp + '<\/div>');

        $('#slideshow .btns .prev').on('click', function () {
          cur = (cur - 1 + len) % len;
          picAnimate(cur, 0);
        });
        $('#slideshow .btns .next').on('click', function () {
          cur = (cur + 1) % len;
          picAnimate(cur, 1);
        });
        $('#slideshow .btns span').on('click', function () {
          //cur = $(this).index();
          //picAnimate(cur);
        });

        function picAnimate(cur, str) {
          var OffsetLeft = parseInt($('#slideshow .imgData').css('margin-left'));
          if (str == 0) {
            $('#slideshow .imgData').animate({ 'margin-left': OffsetLeft + itemW }, function () {
              if (cur == len - 1) {
                OffsetLeft = parseInt($('#slideshow .imgData').css('margin-left'));
                $('#slideshow .imgData').css({ 'margin-left': OffsetLeft - itemW * len });
              }
            });
          } else if (str == 1) {
            $('#slideshow .imgData').animate({ 'margin-left': OffsetLeft - itemW }, function () {
              if (cur == 0) {
                OffsetLeft = parseInt($('#slideshow .imgData').css('margin-left'));
                $('#slideshow .imgData').css({ 'margin-left': OffsetLeft + itemW * len });
              }
            });
          } else {
            /*
            $('#slideshow .imgData').animate({'margin-left': OffsetLeft - cur * itemW},function(){
              if(cur == 0) {
                OffsetLeft = parseInt($('#slideshow .imgData').css('margin-left'));
                $('#slideshow .imgData').css({'margin-left': OffsetLeft + itemW * len});
              }
            });
            */
          }
          $('#slideshow .btns span').removeClass('cur').eq(cur).addClass('cur');
        }
      })(jQuery);

  </script>

  <script type="text/javascript">
    $(function () {
      var r = $('.Travel');
      var lis = r.find('div');
      lis.hover(function (el) {
        el = $(this);
        el.find('span').stop().animate({ 'bottom': 0 }, 400);
      }, function (el) {
        el = $(this);
        el.find('span').stop().animate({ 'bottom': -(el[0].offsetHeight) }, 300);
      });
    });
  </script>

