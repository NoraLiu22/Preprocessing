	 var $sidebar = $(".xzbk"),
	 $sidebam = $(".pxfbt"),
      $window = $(window),
      offset1 = $sidebar.offset(),
	  offset2 = $sidebam.offset();
      $window.scroll(function() {
        if ($window.scrollTop() > (offset1.top-40)) {
        $sidebar.css({"position":"fixed","top":"80px"})
      } else {
        $sidebar.css({"position":"","top":"0px"})
      }
	  
