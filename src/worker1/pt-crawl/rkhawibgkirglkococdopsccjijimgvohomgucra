function newImage(arg) {
	if (document.images) {
	       rslt = new Image(); rslt.src = arg; return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
	       for (var i=0; i<changeImages.arguments.length; i+=2) {
	               document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
	       }
	}
}

//Highlight a subheading under another section
function changeSectionImg(section, newSectionImg, img, newImg) {
  if (document.all || document.getElementById) //IE or Netscape 6
     changeImages(img, newImg);
  else if (document.layers) //Netscape 4.X
  {
     var imgObj = eval('document.submenu' + section + '.document.' + img);
	 imgObj.src = newImg;
  }

  changeImages(section, newSectionImg);
}

