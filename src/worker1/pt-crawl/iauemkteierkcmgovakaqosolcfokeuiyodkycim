  <script type="text/javascript">
    (function ($, window) {
      var num = 0;

      function change() {
        $('.Tabs li').removeClass('cur1');
        $('.Tabs li:eq(' + num + ')').addClass('cur1');
        $('.b1 .a7w').hide();
        $('.b1 .a7w:eq(' + num + ')').show();
      }
      change();
      $('.Tabs li').on('mouseenter', function () {
        num = $(this).index();
        console.log(num);
        change();
      })

      roll('#y2020');
      // roll('#y2021');

      function roll(id) {
        var $id = $(id),
          roll_ul = $id.find('ul'),
          roll_li = $id.find('li'),
          roll_left = $id.find('.left'),
          roll_right = $id.find('.right'),
          a7s = $id.find('li').size(),
          a7c = 0,
          a7w = 333,
          auto = null;

        roll_ul.css('width', a7w * a7s * 2);
        roll_ul.append(roll_li.clone());




        roll_right.on('click', function () {
          a7c = a7c + 1;
          roll_ul.stop(true, true).animate({
            'margin-left': a7c * -a7w
          }, function () {
            if (a7c == a7s) {
              roll_ul.css('margin-left', '0px');
              a7c = 0;
            }
          });
        });
        roll_left.on('click', function () {
          a7c = a7c - 1;
          if (a7c < 0) {
            roll_ul.css('margin-left', a7s * -a7w);
            a7c = a7s - 1;
          }
          roll_ul.stop(true, true).animate({
            'margin-left': a7c * -a7w
          });
        });

        function proll() {
          a7c = a7c + 1;
          roll_ul.stop(true, true).animate({
            'margin-left': a7c * -a7w
          }, function () {
            if (a7c == a7s) {
              roll_ul.css('margin-left', '0px');
              a7c = 0;
            }
          });
        };

        auto = window.setInterval(proll, 5000);
        $id.on('mouseenter', function () {
          window.clearInterval(auto);
        }).on('mouseleave', function () {
          auto = window.setInterval(proll, 5000);
        });
      }

    })(window.jQuery, window);
  </script>


