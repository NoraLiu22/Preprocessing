                      <div class="col">
                        <label for="tip_location">Tip Location</label>
                        <input type="text" id="tip_location" class="form-control form-data" name="crime_address" placeholder="Tip Location">
                      </div>
                    </div>
                    <div class="row form-group">
                      <div class="col">
                        <label for="tip_description">Tip</label>
                        <textarea name="crime_details" id="tip_description" cols="30" rows="5" class="form-control form-data" placeholder="Tip" required></textarea>
                        <div class="invalid-feedback">
                          This field is required.
                        </div>
                      </div>
                    </div>
                    <button class="btn btn-secondary" id="tipSubmit">Submit Tip</button>
                  </form>

