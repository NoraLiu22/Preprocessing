function init() {
    ready = true;

    var toggleButton = document.getElementById('ge2019-ticker-controls__button');
    var parentDOM = document.getElementById("ge2019-ticker__list");
    var tickerItems = parentDOM.getElementsByClassName('ge2019-ticker__item');
