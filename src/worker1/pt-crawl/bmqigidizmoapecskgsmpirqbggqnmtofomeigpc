                },
                getSite: function() {
                    return Promise.resolve(page.site);
                },
                getSection: function() {
                    return Promise.resolve(page.section);
                },
                getContentType: function() {
                    return Promise.resolve(page.contentType);
                },
                getContentId: function() {
                    return Promise.resolve(page.contentId);
                },
