    }

    toggleButton.addEventListener("click", function(e){
        var pausedText = getPausedText();

        if (isPaused) {
            this.className = this.className.replace('ge2019-ticker-controls__button--play','ge2019-ticker-controls__button--pause');
        } else {
            this.className = this.className.replace('ge2019-ticker-controls__button--pause','ge2019-ticker-controls__button--play');
