            <mtr>
              <mtd>
                <msub>
                  <mi>C</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mtext>H</mtext>
                  </mrow>
                </msub>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <mo stretchy="false">[</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>H</mtext>
                </mrow>
                <mo stretchy="false">]</mo>
                <mo>+</mo>
                <mi mathvariant="normal">&#x03A3;<!-- Σ --></mi>
                <mi>r</mi>
                <msub>
                  <mi>&#x03B2;<!-- β --></mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>p</mi>
                    <mi>q</mi>
                    <mi>r</mi>
                  </mrow>
                </msub>
                <mo stretchy="false">[</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>A</mtext>
                </mrow>
                <msup>
                  <mo stretchy="false">]</mo>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>p</mi>
                  </mrow>
                </msup>
                <mo stretchy="false">[</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>B</mtext>
                </mrow>
