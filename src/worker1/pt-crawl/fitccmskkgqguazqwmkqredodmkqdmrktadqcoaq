                    <mi>r</mi>
                  </mrow>
                </msup>
                <mo>&#x2212;<!-- − --></mo>
                <msub>
                  <mi>K</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>w</mi>
                  </mrow>
                </msub>
                <mo stretchy="false">[</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>H</mtext>
                </mrow>
                <msup>
                  <mo stretchy="false">]</mo>
