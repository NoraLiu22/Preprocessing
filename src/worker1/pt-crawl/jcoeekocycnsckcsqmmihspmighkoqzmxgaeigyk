    /**
     * 文字横向轮播 JS
     */
    function ScrollImgLeft() {
        var speed = 50;
        var MyMar = null;
        var scroll_begin = document.getElementById("scroll_begin");
        var scroll_end = document.getElementById("scroll_end");
        var scroll_div = document.getElementById("scroll_div");
        scroll_end.innerHTML = scroll_begin.innerHTML;

        function Marquee() {
            if (scroll_end.offsetWidth - scroll_div.scrollLeft <= 0)
                scroll_div.scrollLeft -= scroll_begin.offsetWidth;
            else
                scroll_div.scrollLeft++;
        }
        MyMar = setInterval(Marquee, speed);
        scroll_div.onmouseover = function () {
            clearInterval(MyMar);
        }
        scroll_div.onmouseout = function () {
            MyMar = setInterval(Marquee, speed);
        }
    }
    ScrollImgLeft();


    /**
     * 二级导航栏动态样式
     * */
    var navL1 = document.querySelector("#nav1LLink");
    var navL2 = document.querySelector("#nav2L");
    var navL1_2 = document.querySelector("#nav1LLink_2");
    var navL2_2 = document.querySelector("#nav2L_2");
    navL1.onmouseover = function () {
        navL2.style.display = 'block';
    }
    navL1.onmouseout = function () {
        navL2.style.display = 'none';
    }
    navL2.onmouseover = function () {
        navL2.style.display = 'block';
    }
    navL2.onmouseout = function () {
        navL2.style.display = 'none';
    }

    navL1_2.onmouseover = function () {
        navL2_2.style.display = 'block';
    }
    navL1_2.onmouseout = function () {
        navL2_2.style.display = 'none';
    }
    navL2_2.onmouseover = function () {
        navL2_2.style.display = 'block';
    }
    navL2_2.onmouseout = function () {
        navL2_2.style.display = 'none';
    }



    /**
     * 要闻顶部的核心价值观的动态JS效果
     * */
    var imgTitle1 = document.getElementById("imgTitle1");
    var imgTitle2 = document.getElementById("imgTitle2");
    var imgTitle = document.getElementById("imgTitle");

    var index1 = 1;
    var index2 = 0;

    // 定时器
    var playText = null;

    // 自动播放
    Play();

    function Play() {
        playText = window.setInterval(function () {
            // console.log("每隔一秒执行一次");
            if (index1 == 1) {
                imgTitle1.style.display = "none";
                imgTitle2.style.display = "block";
                index1 = 0;
                index2 = 1;
            } else if (index2 == 1) {
                imgTitle2.style.display = "none";
                imgTitle1.style.display = "block";
                index2 = 0;
                index1 = 1;
            }

        }, 1500)
    }

    //鼠标移入暂停轮播图
    imgTitle.onmouseenter = function () {
        clearInterval(playText);
        console.log("鼠标移入");
    }
    //鼠标移出轮播图自动轮播
    imgTitle.onmouseleave = function () {
        Play();
        console.log("鼠标移出");
    }
</script> <script>
    var imgShowBox = document.querySelector(".imgShowBox");
    var arrowIcon_left = document.querySelector(".arrowIcon_left");;
    var arrowIcon_right = document.querySelector(".arrowIcon_right");
    /**
     * 点击下一页
     * */
    arrowIcon_right.onclick = function () {
        //console.log("点击下一页");
        arrowIconNext();
    }

    /**
     * 点击上一页
     * */
    arrowIcon_left.onclick = function () {
        //console.log("点击上一页");
        var newLeft;
        if (imgShowBox.style.left === "0px") {
            newLeft = -500;
        } else {
            newLeft = parseInt(imgShowBox.style.left) + 500;
        }
        imgShowBox.style.left = newLeft + "px";
    }

    /**
     * 下一页
     * */
    function arrowIconNext() {
        var newLeft;
        if (imgShowBox.style.left === "-500px") {
            newLeft = 0;
            //console.log("没有下一页了");
        } else {
            newLeft = parseInt(imgShowBox.style.left) - 500;
            //console.log("还有下一页");
        }
        imgShowBox.style.left = newLeft + "px";
        //console.log("left==",imgShowBox.style.left);
    }

    /**
     * 定时器 让轮播图自己动起来
     * */
    var timer = null;

    function autoPlay() {
        timer = setInterval(function () {
            arrowIconNext();
        }, 3000);
    }
    autoPlay();

    //鼠标移入暂停轮播图
    imgShowBox.onmouseenter = function () {
        clearInterval(timer);
        //console.log("鼠标移入");
    }
    //鼠标移出轮播图自动轮播
    imgShowBox.onmouseleave = function () {
        autoPlay();
        //console.log("鼠标移出");
    }
