avičius\"] = 1,\n    [\"CITEREFMisiunasTaagepera1993\"] = 1,\n    [\"CITEREFMurauskaitė\"] = 1,\n    [\"CITEREFMüller2002\"] = 1,\n    [\"CITEREFO._Niglio\"] = 1,\n    [\"CITEREFPaknys\"] = 1,\n    [\"CITEREFPancerovas\"] = 1,\n    [\"CITEREFPocytė2019\"] = 1,\n    [\"CITEREFPrice1998\"] = 1,\n    [\"CITEREFRagauskas\"] = 1,\n    [\"CITEREFRobertas_Bernotas2020\"] = 2,\n    [\"CITEREFRoderich_von_Erckert1863\"] = 1,\n    [\"CITEREFRowell2003\"] = 1,\n    [\"CITEREFSchmemann1991\"] = 1,\n    [\"CITEREFSiaurusevicius1991\"] = 1,\n    [\"CITEREFSlocombe1970\"] = 1,\n    [\"CITEREFSmithPabriksPursLane2002\"] = 1,\n    [\"CITEREFSnyder2003\"] = 3,\n    [\"CITEREFSrebrakowski2000\"] = 1,\n    [\"CITEREFSrebrakowski2020\"] = 1,\n    [\"CITEREFStatkuvienė\"] = 1,\n    [\"CITEREFSteele2008\"] = 1,\n    [\"CITEREFStone2001\"] = 1,\n    [\"CITEREFSub-national_HDI\"] = 1,\n    [\"CITEREFTautavičius\"] = 1,\n    [\"CITEREFTiukšienėSisaitė2015\"] = 1,\n    [\"CITEREFTourism2011\"] = 1,\n    [\"CITEREFTyla2011\"] = 1,\n    [\"CITEREFTylińska\"] = 1,\n    [\"CITEREFVaišvilienė\"] = 1,\n    [\"CITEREFVanagas\"] = 1,\n    [\"CITEREFVardysSedaitis1997\"] = 1,\n    [\"CITEREFWeeks,_T._R.2015\"] = 1,\n    [\"CITEREFZalanskaite2018\"] = 1,\n    [\"CITEREFZimmerman2004\"] = 1,\n    [\"CITEREFZinkevičius\"] = 1,\n    [\"CITEREFpirmininkas\"] = 1,\n    [\"CITEREFČerniauskas\"] = 1,\n    [\"CITEREFŁossowski1995\"] = 1,\n    [\"CITEREFŠepetytė2022\"] = 1,\n    [\"CITEREFŽilionis\"] = 1,\n    [\"CITEREFВасютинскийДживелеговМельгунов1912\"] = 1,\n    [\"CITEREFЛавринец2004\"] = 1,\n}\ntemplate_list = table#1 {\n    [\"!\"] = 2,\n    [\"As of\"] = 3,\n    [\"Authority control\"] = 1,\n    [\"Better source needed\"] = 2,\n    [\"Big\"] = 2,\n    [\"Capital cities of the European Union\"] = 1,\n    [\"Cbignore\"] = 1,\n    [\"Circa\"] = 4,\n    [\"Citation\"] = 1,\n    [\"Citation needed\"] = 5,\n    [\"Cite book\"] = 62,\n    [\"Cite conference\"] = 1,\n    [\"Cite journal\"] = 10,\n    [\"Cite m