        $(function () {
            var cpOverlay = $("#_cookiePolicyOverlay");

            var dismissLink = $(".cp-dismiss", cpOverlay).click(function () {
                cpOverlay.removeClass("cp-visible");

                var aYearFromNow = new Date();
                aYearFromNow.setFullYear(aYearFromNow.getFullYear() + 1);

