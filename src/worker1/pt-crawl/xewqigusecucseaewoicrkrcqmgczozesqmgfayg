    <Script language="JavaScript">
        function getPage() {
            var cUrl = document.location.href;
            var p = cUrl.lastIndexOf("_");
            if (p <= 0) return cUrl;
            if (cUrl.charAt(p - 1) != 't') cUrl = cUrl.substring(0, p) + ".htm";
            return cUrl;
        }

        var g_PageUrl = getPage();

        function TalkFunc() {
            document.all("talkform").url.value = g_PageUrl;
            return true;
        }

        document.all("talkurl").href = "http://comments.xinhuanet.com/comment?url=" + g_PageUrl;

        document.all("talkurl2").href = "http://comments.xinhuanet.com/comment?url=" + g_PageUrl;
    </Script>
