                    <script>
                    (function(){
                        var $tag = $('#subTag_coop span');
                        var $cont = $('#subCont_coop .coopUnit');
                        var n = 0;
                        $tag.each(function(){
                            this.setAttribute('data-index', n);
                            n ++;
                        });
                        $tag.bind('mouseover', function(){
                            $tag.each(function(){
                                this.className = '';
                            });
                            $cont.each(function(){
                                this.style.display = 'none';
                            });
                            this.className = 'current';
                            $cont[parseInt(this.getAttribute('data-index'))].style.display = 'block';
                        });
                    })();
                    </script>
