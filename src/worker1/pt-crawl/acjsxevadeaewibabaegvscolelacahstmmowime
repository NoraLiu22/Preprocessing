								<script>
									function time() {
										dt = new Date();
										var y = dt.getFullYear();
										var mt = dt.getMonth() + 1;
										var day = dt.getDate();
										var h = dt.getHours(); //获取时
										var m = dt.getMinutes(); //获取分
										var s = dt.getSeconds(); //获取秒
										document.getElementById("showTime").innerHTML = y + "年" + mt + "月" + day +  "日";
									}
									var myDate = new Date;
									var week = myDate.getDay();
						    		var weeks = ["星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六"];	
						    		document.getElementById("week").innerHTML = (weeks[week]);
						    		time();
								</script>
