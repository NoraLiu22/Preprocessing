<input id="item" name="item" type="hidden" value="057ceaa7-7a04-4983-85c9-6967837f1e3e" />
<input id="renderingPath" name="renderingPath" type="hidden" value="{03732271-DAAA-4E2B-B6CD-B2C8CD945491}" />
<script>
    $(document).ready(function (e) {
        reloadSectionCollection({
            oldURL: "",
            newURL: window.location.href
        });
    });

    addEventListener('hashchange', (event) => {
        reloadSectionCollection(event);
    });

    jQuery.escapeSelector = function (str) {
        return str.replace(/[!"#$%&'()*+,./:;<=>?@\[\\\]^`{|}~]/g, "\\$&");
    };

    function reloadSectionCollection(event) {
        var regex = /(#page=)(\D*)(\d+)/g;
        var oldMatch = Array.from(event.oldURL.matchAll(regex));
        var newMatch = Array.from(event.newURL.matchAll(regex));
        var oldPageNumber = oldMatch.length ? oldMatch[0].length ? oldMatch[0][3] : 1 : 1;
        var newPageNumber = newMatch.length ? newMatch[0].length ? newMatch[0][3] : 1 : 1;

        var item = $('#item').val();
        var renderingPath = $('#renderingPath').val();
        if (!isNaN(newPageNumber)) {
            if (!oldPageNumber || oldPageNumber != newPageNumber) {
                $.ajax({
                    type: "GET",
                    url: "/api/sitecore/Collection/Index",
                    data: {
                        page: newPageNumber,
                        sitecoreItemId: item,
                        sitecoreRenderingPath: renderingPath,
                        type: "Section"
                    },
                    contentType: "application/json; charset=utf-8",
                    success: function (result) {
                        var archiveCollectionSelector = "#" + $.escapeSelector("ArchiveCollection-EventsandAttractions");
                        $(archiveCollectionSelector).html(result);
                        window.scrollTo(0, $(archiveCollectionSelector).siblings('.section-title').offset().top - 15);
                    },
                    error: function (a,b,c,d) {
                        console.log("Error loading section collection.");
                    }
                });
            }
        }
    }
</script>
        </div>
