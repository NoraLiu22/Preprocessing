	<script src="http://images.china.cn/images1/ch/20230911/js/script.js" type="text/javascript" charset="utf-8"></script>
    <script src="http://images.china.cn/images1/ch/2018dzd/js/share.js"></script>
	<script type="text/javascript">
		function show() {
			var show = document.getElementById('q');
			show.style.height = '180px';
		}

		function yc() {
			var show = document.getElementById('q');
			show.style.height = '0';
		}

		document.getElementById("search-icon").addEventListener("click", function() {
			var input = document.getElementById("search-input");
			input.classList.toggle("active");
			if (input.classList.contains("active")) {
				input.focus();
			} else {
				input.blur();
			}
		});

		let carouselIndex = 0; // 当前显示的图片索引
		const carouselContainer = document.querySelector('.carousel-container');
		const carouselList = document.querySelector('.carousel-list');
		const carouselItems = document.querySelectorAll('.carousel-item');
		const carouselDotsContainer = document.querySelector('.carousel-dots-container');

		// 生成小点点
		for (let i = 0; i < carouselItems.length; i++) {
			const dot = document.createElement('span');
			dot.classList.add('carousel-dot');
			if (i === 0) {
				dot.classList.add('active');
			}
			dot.addEventListener('click', () => {
				carouselIndex = i;
				updateCarouselPosition();
				updateActiveDot();
			});
			carouselDotsContainer.appendChild(dot);
		}

		function showNextImage() {
			carouselIndex = (carouselIndex + 1) % carouselItems.length;
			updateCarouselPosition();
			updateActiveDot();
		}

		function updateCarouselPosition() {
			const itemWidth = carouselItems[0].offsetWidth;
			const translation = -carouselIndex * itemWidth;
			carouselList.style.transform = `translateX(${translation}px)`;
		}

		function updateActiveDot() {
			const dots = carouselDotsContainer.querySelectorAll('.carousel-dot');
			dots.forEach((dot, index) => {
				if (index === carouselIndex) {
					dot.classList.add('active');
				} else {
					dot.classList.remove('active');
				}
			});
		}

		// 自动播放轮播图
		let autoplayIntervalId;

		function startAutoplay() {
			autoplayIntervalId = setInterval(() => {
				showNextImage();
			}, 3000);
		}

		function stopAutoplay() {
			clearInterval(autoplayIntervalId);
		}

		// 鼠标悬停时停止自动播放
		carouselContainer.addEventListener('mouseover', stopAutoplay);
		carouselContainer.addEventListener('mouseout', startAutoplay);

		// 启动自动播放
		startAutoplay();
	</script>
