  <script>
    require(["jquery", "jquery-ui", "headroom", "jquery-headroom"], function (
      $
    ) {
      window.toggleCalendar = function () {
        $(".main-header__calendar-toggle").toggleClass("show");
        $("body").toggleClass("calendar-open");
        $(".main-header__calendar-overlay").toggleClass("is-open");
      };

      window.closeCalendar = function () {
        $(".main-header__calendar-toggle").removeClass("show");
        $("body").removeClass("calendar-open");
        $(".main-header__calendar-overlay").removeClass("is-open");
      };

      $("html").headroom({ offset: 100 });

      $(".main-header").on(
        "mouseenter",
        ".c-navigation--main .c-navigation-desktop .c-navigation__parent .c-navigation__url--level-1",
        function () {
          $("body").addClass("hovered");
        }
      );
      $(".main-header").on(
        "mouseleave",
        ".c-navigation--main .c-navigation-desktop .c-navigation__parent .c-navigation__url--level-1",
        function () {
          $("body").removeClass("hovered");
        }
      );
      $(".main-header").on(
        "mouseenter",
        ".c-navigation--main .c-navigation-desktop .c-navigation__level-2",
        function () {
          $("body").addClass("hovered");
        }
      );
      $(".main-header").on(
        "mouseleave",
        ".c-navigation--main .c-navigation-desktop .c-navigation__level-2",
        function () {
          $("body").removeClass("hovered");
        }
      );
    });
  </script>
</html>
