                          if (sport.title.indexOf('Men') === -1 && sport.title.indexOf('Women') === -1) {
                            return sport.title
                          }
                          if (sport.title.indexOf('Men') !== -1) {
                            return sport.title.replace('Men\'s', 'M')
                          }
                          if (sport.title.indexOf('Women') !== -1) {
                            return sport.title.replace('Women\'s', 'W')
                          }
                        })"
