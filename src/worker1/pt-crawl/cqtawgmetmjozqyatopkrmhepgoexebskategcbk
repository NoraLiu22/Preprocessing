<?php
	include("geoip.inc");
	$gi = geoip_open("GeoIP.dat",GEOIP_STANDARD);
	$ip = $_SERVER['REMOTE_ADDR'];
	$ip_code4 = geoip_country_code4_by_addr($gi, $ip);
	$ip_country = geoip_country_name_by_addr($gi, $ip);
	if ($ip_code4 == "") $ip_code4 = "****";
	geoip_close($gi);
?>	







