        <div class="filter filter--time" ng-class="{'open': essaysCtrl.timeFilter == true}">
            <div class="filter-button" ng-click="essaysCtrl.timeFilter = !essaysCtrl.timeFilter; essaysCtrl.geoFilter = false; essaysCtrl.themeFilter = false;">
                <span ng-if="essaysCtrl.timeV == null">Time period</span>
                <span ng-if="essaysCtrl.timeV != null" ng-bind-html="essaysCtrl.tPs[essaysCtrl.timeV][1]"></span>
                <span class="filter-chevron__wrapper icon-arrow-down"><img class="filter-chevron" src="/Rodan/dist/svg/chevron-expanded.svg"></span>
            </div>
            <ul ng-if="essaysCtrl.timeFilter" class="filter-list filter-list--time">
                <li class="themes all" ng-click="essaysCtrl.themeV = null; essaysCtrl.checker.themeID(null);essaysCtrl.expanded = null;essaysCtrl.clearAlternateFilter();essaysCtrl.timeV = null;essaysCtrl.checker.timeID(null); essaysCtrl.filterTime = ''; essaysCtrl.timeFilter = false;essaysCtrl.pushURL();"><span>All Periods</span></li>
                <li class="themes" ng-repeat="tp in essaysCtrl.tPs" ng-class="{'active': essaysCtrl.timeV == tp[0]}" ng-click="essaysCtrl.themeV = null; essaysCtrl.checker.themeID(null);essaysCtrl.expanded = null;essaysCtrl.clearAlternateFilter();essaysCtrl.timeV = tp[0];essaysCtrl.timeFilter = false;essaysCtrl.pushURL();"><span ng-bind-html="tp[1]"></span></li>
            </ul>
        </div>
        <div class="filter filter--geo" ng-class="{'open': essaysCtrl.geoFilter == true}">
            <div class="filter-button" ng-click="essaysCtrl.geoFilter = !essaysCtrl.geoFilter; essaysCtrl.themeFilter = false; essaysCtrl.timeFilter = false;">
                <span ng-if="essaysCtrl.geoV == null">Region</span>
