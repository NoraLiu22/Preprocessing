                <li class="themes all" ng-click="essaysCtrl.themeV = null; essaysCtrl.checker.themeID(null); essaysCtrl.expanded = null;essaysCtrl.clearAlternateFilter();essaysCtrl.geoV = null;essaysCtrl.checker.geoID(null); essaysCtrl.filterGeo = ''; essaysCtrl.geoFilter = false;essaysCtrl.pushURL();"><span>All Regions</span></li>
                <li class="themes" ng-repeat="geoS in essaysCtrl.geoListOrder" ng-class="{'active': essaysCtrl.geoV == geoS[0]}" ng-click="essaysCtrl.themeV = null; essaysCtrl.checker.themeID(null); essaysCtrl.expanded = null;essaysCtrl.clearAlternateFilter();essaysCtrl.geoV = geoS[0];essaysCtrl.geoFilter = false;essaysCtrl.pushURL();"><span ng-bind-html="geoS[1]"></span></li>
