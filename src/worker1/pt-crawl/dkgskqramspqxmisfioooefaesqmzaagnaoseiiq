                },
                keyup: function(data, event) {
                  if(event.key == 'Enter') {
                    event.preventDefault();
                    closeSearch();
                          setTimeout(function() {
                        $('.c-search__toggle').focus();                        
                    }, 300);
                  }
 
                  if(event.key == 'Escape') {
                   closeSearch();
                   setTimeout(function() {
                        $('.c-search__toggle').focus();                        
                    }, 300);
                  }
                }
              }">
            <span aria-hidden="true" class="sf-close sidearm-font"></span>
        </button>
    </div>
