            <!-- /ko -->
            <!-- ko if: ad.location.effect === 'none' -->
                <div class="ads-container" data-bind="foreach: ad.campaigns">
                    <div class="ad multi-ad" data-bind="css: css_class">
                        <!-- ko if: link === '' || link === '#' -->
