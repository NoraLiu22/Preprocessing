<script>
    require(['jquery', 'slick'], function ($) {
        $('#ctl00_cplhMainContent_ctl02_ddl_all_hof').on("change", function () {
            var url_path = "/honors/manhattan-college-athletic-hall-of-fame?hof=" + $(this).val();
            window.location = url_path;
        });

        $('.sidearm-hof-member-bio-list').slick({
            slidesToShow: 6,
            slidesToScroll: 6,
            responsive: [
                {
                    breakpoint: 1417,
                    settings: {
                        slidesToShow: 5,
                        slidesToScroll: 5
                    }
                },
                {
                    breakpoint: 1281,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 4
                    }
                },
                {
                    breakpoint: 1025,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 3
                    }
                },
                {
                    breakpoint: 541,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                },
                {
                    breakpoint: 400,
                    settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1
                    }
                }
            ]
        });
    });    
</script>
            </article>
        
	
    <script>
		require(['jquery'], function ($) {
            // Make iframes responsive
			$('article iframe[width="100%"]:not(".showcase-video-player, .dfp-ad iframe")').wrap('<div class="sidearm-flex-video"></div>');

            switch ('alpha') {
                
                            case "alpha":
                                $(".sidearm-link-dropdown-groupings .sidearm-link-dropdown-selected").text("By Name");
                                break;
                        
                            case "sport":
                                $(".sidearm-link-dropdown-groupings .sidearm-link-dropdown-selected").text("By Sport");
                                break;
                        
                            case "class":
                                $(".sidearm-link-dropdown-groupings .sidearm-link-dropdown-selected").text("By Class");
                                break;
                        
                            case "class_induction":
                                $(".sidearm-link-dropdown-groupings .sidearm-link-dropdown-selected").text("By Induction Year");
                                break;
                        
            }

            $(".sidearm-link-dropdown-groupings a.alpha").addClass("selected");

            function resizeDropdown (selector) {
                var widthSport = $(selector + ":visible .sidearm-link-dropdown").innerWidth() + 2;

                $(selector + ":visible ul").css({
                    'width': widthSport + 'px'
                });
            }

            $(window).resize(function() {
                resizeDropdown(".sidearm-hof-select-filter");
            });

            $(document).on("focusin focusout", ".sidearm-link-dropdown a", function (e) {
                if (e.type === "focusin") {
                    $(this).closest(".sidearm-link-dropdown").addClass("open");
                } else if (e.type === "focusout") {
                    $(this).closest(".sidearm-link-dropdown").removeClass("open");
                }
            });

            $(document).on("click", function (e) {
                if (e.target.className.indexOf("sidearm-link-dropdown-link") > -1) {
                    $(e.target).closest("ul").find("a").removeClass("selected");
                    $(e.target).addClass("selected");
                }

                if ($(e.target).closest(".sidearm-link-dropdown").length) {
                    var $dropdown = $(e.target).closest(".sidearm-link-dropdown");

                    if ($dropdown.hasClass("open")) {
                        $dropdown.removeClass("open");
                    } else {
                        $dropdown.addClass("open");
                        resizeDropdown(".sidearm-hof-select-filter");
                    }
                } else {
                    $(".sidearm-link-dropdown").removeClass("open");
                }
            });
        });
    </script>


            </main>
            
