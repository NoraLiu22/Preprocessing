                        click: function(data, event) {
                            let currentTarget = event.currentTarget;
                            let menuItems = document.querySelectorAll('.c-navigation__url--level-1');
                            menuItems.forEach((element) => {
                                if(!currentTarget.isSameNode(element)) {
