            //    } else {
            //        $('#BackToTopButton').fadeOut();
            //    }
            //});
            //$("#BackToTopButton").on("click", function () {
            //    $('html').animate({ scrollTop: 0 }, 'fast');//IE, FF
            //    $('body').animate({ scrollTop: 0 }, 'fast');//chrome, don't know if Safari works
            //})
        });



  </script>
 
