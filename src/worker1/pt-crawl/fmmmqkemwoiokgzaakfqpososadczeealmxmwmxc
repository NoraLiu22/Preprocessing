ams2015\"] = 1,\n    [\"CITEREFYoung2023\"] = 1,\n    [\"CITEREFdi_Perna2013\"] = 1,\n}\ntemplate_list = table#1 {\n    [\"1996 Rock and Roll Hall of Fame\"] = 1,\n    [\"2015 Rock and Roll Hall of Fame\"] = 1,\n    [\"AllMusic\"] = 1,\n    [\"Authority control\"] = 1,\n    [\"Birth date\"] = 1,\n    [\"Cbignore\"] = 2,\n    [\"Cite AV media notes\"] = 2,\n    [\"Cite book\"] = 19,\n    [\"Cite encyclopedia\"] = 1,\n    [\"Cite episode\"] = 1,\n    [\"Cite journal\"] = 1,\n    [\"Cite magazine\"] = 19,\n    [\"Cite news\"] = 46,\n    [\"Cite press release\"] = 1,\n    [\"Cite web\"] = 57,\n    [\"Commons category\"] = 1,\n    [\"DEFAULTSORT:Reed, Lou\"] = 1,\n    [\"Death date and age\"] = 1,\n    [\"Discogs artist\"] = 1,\n    [\"Div col\"] = 1,\n    [\"Div col end\"] = 1,\n    [\"For\"] = 1,\n    [\"Formatnum:{{Inflation\"] = 1,\n    [\"Grammy Award for Best Music Film\"] = 1,\n    [\"Hlist\"] = 5,\n    [\"IBDB name\"] = 1,\n    [\"IMDb name\"] = 1,\n    [\"Inflation\"] = 1,\n    [\"Inflation-fn\"] = 1,\n    [\"Inflation-year\"] = 1,\n    [\"Infobox musical artist\"] = 1,\n    [\"Lou Reed\"] = 1,\n    [\"Main\"] = 1,\n    [\"Marriage\"] = 3,\n    [\"MoMP\"] = 1,\n    [\"Navboxes\"] = 1,\n    [\"Plainlist\"] = 1,\n    [\"Portal bar\"] = 1,\n    [\"Quote box\"] = 2,\n    [\"Redirect\"] = 1,\n    [\"Refbegin\"] = 1,\n    [\"Refend\"] = 1,\n    [\"Reflist\"] = 2,\n    [\"Refn\"] = 5,\n    [\"Sfnp\"] = 26,\n    [\"Short description\"] = 1,\n    [\"Small\"] = 1,\n    [\"Spnd\"] = 1,\n    [\"Subscription required\"] = 1,\n    [\"The Velvet Underground\"] = 1,\n    [\"Use American English\"] = 1,\n    [\"Use mdy dates\"] = 1,\n    [\"Webarchive\"] = 1,\n    [\"Wikiquote\"] = 1,\n}\narticle_whitelist = table#1 {\n}\n","limitreport-profile":[["?","220","20.0"],["MediaWiki\\Extension\\Scribunto\\Engines\\LuaSandbox\\LuaSandboxCallback::callParserFunction","180","16.4"],["recursiveClone \u003CmwInit.lua:41\u003E","160","14.5"],["dataWrapper \u003Cmw.lua:672\u003E","60","5.5"]