              search_dropdown.options[1].text = 'æœ¬æ–‡ï¼ˆè¨˜äº‹ï¼‰';
              search_dropdown.options[2].text = 'ã‚¿ã‚¤ãƒˆãƒ«ãƒ»æœ¬æ–‡';
            }
          }
        </script>

