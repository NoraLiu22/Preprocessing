                f.parentNode.insertBefore(j, f);
            })
            (window, document, 'script', 'dataLayer', 'GTM-MSDMZ4');
        }, 2500);
        } catch (err) {}
    })();
    </script>
    <!-- end Google Tag Manager -->
    <script src="https://www.googleoptimize.com/optimize.js?id=GTM-5LN5G3J" defer></script>
</body>
</html>
