<script>
$( document ).ready(function() {
	var statPage = $("#indicator-block").data("stat-page");
	$('[data-release-id]').each(function(){
		var element = $(this); 
		element.on('click', function() { 
			var backupDoc = $(this).data("stat-doc");
			window.location = backupDoc;

		});
	});
	$('[data-toggle="tooltip"]').tooltip({
		placement: 'top',
	}); 
});
</script>

	

