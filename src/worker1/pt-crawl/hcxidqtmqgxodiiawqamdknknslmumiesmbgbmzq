var myCounter=0

function myEnlarge() {
	if (enlargeValue == 0) {
		document.images.myPicBox.width = 1350;
		document.images.myPicBox.height = 1800;
		enlargeValue = 1} 
	else if (enlargeValue == 1){
		document.images.myPicBox.width = 1600;
		document.images.myPicBox.height = 2133;
	}
}
	
function myReduce() {
	document.images.myPicBox.width = 600;
	document.images.myPicBox.height = 800;
	enlargeValue = 0;
	}

function jump2page(whichPage){
	if((whichPage >0) &  (whichPage<jumpList.length)){
		myCounter=jumpList[whichPage];
		showPage(myCounter);}
}
 
function movePrev(){
	if (myCounter>0){
		myCounter--
		showPage(myCounter);}
}

function moveNext(){
	if (myCounter<viewPageImage.length-1){
		myCounter++;
		showPage(myCounter);}
	}
	
function showPage(whichPage){
	  	document.images.myPicBox.src="images/loading.jpg" + "?"
     	document.images.myPicBox.src=viewPageImage[whichPage] + "?"
		document.drop_list.Page_list.selectedIndex = whichPage
		document.reload;	
}

function changePage() {
	myCounter = document.drop_list.Page_list.selectedIndex;
	showPage(myCounter);
}


