    <script>
        var ListModel = {
            RecordCount: 1852
           , Id: 61648
           , CurrentPage: 1
           , PageSize: 10
        };
        ListModel.path = function (index) {
            return index > 30 ? ("/model.aspx?pi=" + index + "&lan=common&sid=" + ListModel.Id) : ("list_" + index + ".shtml?_=" + (new Date()).getTime());
        };
        ListModel.maxPage = Math.ceil(ListModel.RecordCount / 10)
