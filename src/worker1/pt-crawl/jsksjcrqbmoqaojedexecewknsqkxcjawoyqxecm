<script  type="javascript/blocked" data-wpmeteor-type="text/javascript" >

	function fds_comments() {

	var commentWrap = jQuery('.comment-list');
	var commentChilderns = commentWrap.find('.comment');
	var commentNumber = jQuery('#commentsNumber').val() - 1;
		for (var i = 0; i < commentChilderns.length; i++) {
			if (i > commentNumber) {
				jQuery(commentChilderns[i]).hide();
			}
		}
		if (commentChilderns.length > commentNumber + 1) {
        	commentWrap.append("<button id='more-comments' class='more-comments' onclick='fds_all_comments();' type='button'><span>EXPAND COMMENTS</span></button>");
   		}
	}

	//This function reveal all comments (used on SHOW MORE btn)
	function fds_all_comments(){
	var commentWrap = jQuery('.comment-list');
	var commentChilderns = commentWrap.find('.comment');
	var commentChildernsVisible = commentWrap.find('.comment:visible');
	var commentNumber = jQuery('#commentsNumber').val() - 1;
	if (commentChildernsVisible.length == commentNumber + 1) {
		jQuery(commentChilderns).show();
		jQuery('#more-comments span').text("COLLAPSE COMMENTS");
	} else {
		for (var i = 0; i < commentChilderns.length; i++) {
			if (i > commentNumber) {
				jQuery(commentChilderns[i]).hide();
			}
		}
		jQuery('#more-comments span').text("EXPAND COMMENTS");
	}
	}

	fds_comments();

</script>

<script  type="javascript/blocked" data-wpmeteor-type="text/javascript" >

	jQuery(document).ready( function($){ 

		// OuterPage Segment Event
		let dataUrl = window.location;

		$(document).on("click", "a", function(e) {
			let currentUrl = $(this).prop("href");
			let buttonUrl = $(this).attr('href');
			let betPromoId = $(this).closest('.bet-promo').attr('id');
			let bonusType = $(this).closest('.bet-promo').attr('id');
    		let sBonusType = bonusType ? bonusType.replace(/-\d+$/, '') : "N/A";
			let promoTextElement = $(this).closest('.bet-shortcodes').find('.promo-value-amount');
			if (promoTextElement.length > 0) {
				offerText = promoTextElement.text().trim();
			}
			let promoNameElement = $(this).closest('.bet-shortcodes').find('.promo-sb-name');
				if (promoNameElement.length > 0) {
					offerName = promoNameElement.text().trim();
				}
			
			if (currentUrl.includes('link') ) { 
				if ($(this).parents('.bet-shortcodes').length > 0) {
					let parents = $(this).parents('.bet-shortcodes');
					let parent = parents[0];
					let names = $(parent).find('.promo-sb-name');
					if (names.length > 0) {
						let name = $(names)[0];
						segmentClickout($(this), "Brand Clickout", $(name).text());
					} else {
						parents = $(this).parents('li');
						if (parents.length > 0) {
							parent = $(parents)[0];
							let names = $(parent).find('.casino-name');
							if (names.length > 0) {
								let name = $(names)[0];
								segmentClickout($(this), "Brand Clickout", $(name).text());
							}
						} 
					}
				} else {
					segmentClickout($(this), "Brand Clickout");
				}
			} else {
				let currentUrl = $(this).prop("href");
				let buttonUrl = $(this).attr('href');
				let parentClass = $(this).closest('.rgbc-doe-shortcode');
				
				let betPromoIdAd = $(this).closest('.rgbc-doe-shortcode').attr('data-shortcode-id');

				let promoNameElement = parentClass.find('.sports-offer-card__brand-name a:not(:empty)');
				if (promoNameElement.length > 0) {
					offerName = $(promoNameElement[0]).text().trim();
				}

				let promoTextElement = $(this).closest('.rgbc-doe-shortcode').find('.sports-offer-card__offer-name');
				if (promoTextElement.length > 0) {
					offerText = promoTextElement.text().trim();
				}
				
				if ($(this).parents('.rgbc-doe-shortcode').length > 0) {
					let parents = $(this).parents('.rgbc-doe-shortcode');
					let parent = parents[0];
					let names = $(parent).find('.sports-offer-card__brand-name');
					if (names.length > 0) {
						let name = $(names)[0];
						segmentClickoutAd($(this), "Brand Clickout", $(name).text(), );
					}
				} else {
					segmentClickoutAd($(this), "Brand Clickout");
				}
			}
		});

		// Brand Impression Segment Event
		var observer = new IntersectionObserver(function(entries) {
			if(entries[0].isIntersecting === true) {
				brandImpression(entries[0].target);
			}
		}, { threshold: [0] });

		var observerAd = new IntersectionObserver(function(entries) {
			if(entries[0].isIntersecting === true) {
				brandImpressionAd(entries[0].target);
			}
		}, { threshold: [0] });

		$(".rgbc-doe-shortcode > .sports-offer-card").each(function(){ 
			let imgBrand = $(this).find("img");
			let promoText = $(this).find('.sports-offer-card__offer-name');
			let promoLink = $(this).find('.sports-offer-card__button');

			let namePromo = $(this).find('.sports-offer-card__brand-name a:not(:empty)');
			let elementPromo = $(".rgbc-doe-shortcode");
			let idPromo = elementPromo.attr('data-shortcode-id');

			
			if (imgBrand.length > 0) {
				let textContent = promoText.text();
				imgBrand.attr('class', namePromo.text());

				let promoHref = promoLink.attr('href');
				imgBrand.attr('href', promoHref);

				imgBrand.attr('data-text', textContent);
				imgBrand.attr('data-shortcode-id', idPromo)
				observerAd.observe(imgBrand[0]);
			}
		});

		$(".bet-shortcodes > .wrapper").each(function(){ 
			let imgBrand = $(this).find("img");
			let brandName = $(this).find('.promo-sb-name');
			let promoLink = $(this).find('.promo-link');
			let promoText = $(this).find('.promo-content .promo-value-amount:not(:empty)');

			let elementPromo = $(".bet-promo");
			let idPromo = elementPromo.attr('id');

			if (imgBrand.length > 0) {
				imgBrand.attr('alt', brandName.text());
				
				let promoHref = promoLink.attr('href');
				imgBrand.attr('href', promoHref);
				
				let textContent = promoText.text();
				imgBrand.attr('data-text', textContent);
				imgBrand.attr('id', idPromo);
				observer.observe(imgBrand[0]);
			}
		});

		$(".bet-shortcodes > .tab-content > .tab-content-wrap > .tab-list > li").each(function(){ 
			let imgBrand = $(this).find("img");
			let brandName = $(this).find('.casino-name');
			
			if (imgBrand.length > 0) {
				imgBrand.attr('alt', brandName.text());
				observer.observe(imgBrand[0]);
			}
		});

		// End Brand Impression Segment Event

		// Content Shared Segment Event
		$(".login-register").on("click", function(e) {
			commentsLogin(e);
		});
		
		$(".submit").on("click submit", function(e) {
			commentSubmitted(e);
		});

	});
</script>

