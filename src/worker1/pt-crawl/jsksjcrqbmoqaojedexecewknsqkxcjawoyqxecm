<script  type="javascript/blocked" data-wpmeteor-type="text/javascript" >
function eventActiveCampaign( actid, key, event, eventdata = 'N/A' ) {
    const email = '';
  if (email){
    const data = {
        actid: actid,
        key: key,
        event: event,
        eventdata: eventdata,
        visit: JSON.stringify({
        email: email,
        }),
    };
    fetch('https://trackcmp.net/event', {
        method: 'POST',
        headers: {
        'Content-Type': 'application/x-www-form-urlencoded',
        },
        body: Object.keys(data)
        .map((key) => `${encodeURIComponent(key)}=${encodeURIComponent(data[key])}`)
        .join('&'),
    })
        .then((response) => response.json())
        .then((result) => {
        if (result.success) {
            console.log('Success!');
        } else {
            console.log('Error!');
        }
        console.log(result.message);
        })
        .catch((error) => {
        console.log('Request failed:', error);
        });
  }
}
</script>

<script  type="javascript/blocked" data-wpmeteor-type="text/javascript" >
    // PATH url
    var completeUrl = window.location.href;
    var urlObject = new URL(completeUrl);
    var pathSegments = urlObject.pathname.split('/');
    var remainingPath = '/' + pathSegments.slice(1).join('/');

	function brandImpression(event) {
          
    var series_tag = "connor";
    var team_data = "sports"; 

    let promoType = event.getAttribute("id");
    let sBonusType = promoType ? promoType.replace(/-\d+.*$/, '') : "N/A";

    analytics.track('Brand Impressions', {
      bonus_type: event.getAttribute("data-text"),
      brand_id: event.getAttribute("id"),
      brand_name: event.alt,
      offer_text: event.getAttribute("data-text"),
      path: remainingPath,
      url: window.location.href,
      product_group: "N/A",
      series_tag: series_tag,
      site_url: window.location.origin,
      team_tag: team_data,
    });
	}

  function brandImpressionAd(event) {
          
    var series_tag = "connor";
    var team_data = "sports"; 

    let buttonUrl = event.getAttribute('href');
    let betPromoId = event.getAttribute("id");

    analytics.track('Brand Impressions', {
      bonus_type: event.getAttribute("class"),
      brand_id: event.getAttribute("data-shortcode-id"),
      brand_name: event.getAttribute("class"),
      offer_text: event.getAttribute("data-text"),
      path: remainingPath,
      url: window.location.href,
      product_group: "N/A",
      series_tag: series_tag,
      site_url: window.location.origin,
      team_tag: team_data,
    });
	}

	function segmentClickout(event, type, promoName='') {

          
    var series_tag = "connor";
    var team_data = "sports"; 

    let destinationUrl = event.prop("href");
    let urlData = destinationUrl.split('/link/');

    let brandName = destinationUrl;

    if (urlData.length > 0) {
      brandName = urlData[1];
    }
    
    if (promoName != '') {
      brandName = promoName;
    }
    
    let buttonUrl = event.attr('href');
    let betPromoId = event.closest('.bet-promo').attr('id');

    let bonusType = event.closest('.bet-promo').attr('id');
    let sBonusType = bonusType ? bonusType.replace(/-\d+.*$/, '') : "N/A";

    analytics.track(type, {
      bonus_type: offerText,
      brand_id: betPromoId,
      brand_name: offerName,
      offer_text: offerText,
      path: remainingPath,
      url: window.location.href,
      product_group: "N/A",
      series_tag: series_tag,
      site_url: window.location.origin,
      team_tag: team_data,
    });
	}

  function segmentClickoutAd(event, type, promoName='', offerName) {
      
  var series_tag = "connor";
  var team_data = "sports"; 

  let destinationUrl = event.prop("href");
  let urlData = destinationUrl.split('/link/');

  let brandName = destinationUrl;

  if (urlData.length > 0) {
    brandName = urlData[1];
  }

  if (promoName != '') {
    brandName = promoName;
  }

  let buttonUrl = event.attr('href');
  let betPromoId = event.closest('.rgbc-doe-shortcode').attr('data-shortcode-id');


  analytics.track(type, {
    bonus_type: offerText,
    brand_id: betPromoId,
    brand_name: offerName,
    offer_text: offerText,
    path: remainingPath,
    url: window.location.href,
    product_group: "N/A",
    series_tag: series_tag,
    site_url: window.location.origin,
    team_tag: team_data,
  });
}

  function pageViewedEvent(event) {
        var series_tag = "connor";
    var team_data = "sports";
    analytics.track('Page View', {
      path: remainingPath,
      url: window.location.href,
      series_tag: series_tag,
      site_url: window.location.origin,
      team_tag: team_data,
    });
  }

	window.addEventListener('load', pageViewedEvent);

  function commentsLogin() {
    analytics.track('Comment Login Register', {
      site_url: window.location.href
    });
  }

  function commentSubmitted(event) {  
        var series_tag = "connor";
    var team_data = "sports"; 
    var user_email = false;
    var user_name = false;
    var user_id = 0;

    analytics.identify(user_id, {
      name: user_name,
      email: user_email
    });

    analytics.track('Comment Submitted', {
      comment_type: '',
      path: remainingPath,
      url: window.location.href,
      series_tag: series_tag,
      site_url: window.location.origin,
      team_tag: team_data
    });
  }

