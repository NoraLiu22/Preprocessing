<script type="text/javascript">
// optionally, set maximum number of captcha validation on event:
const maxNumberOfTries = 5; 

// captcha initial setup
var myCaptcha = new jCaptcha({ 
    el: ".jCaptcha", 
    canvasClass: "jCaptchaCanvas", 
    canvasStyle: { 
          // properties for captcha stylings:
          width: 100, 
          height: 23, 
          textBaseline: "top", 
          font: "15px Arial", 
          textAlign: "left", 
	  fillStyle: "#828282"
    }, 
    requiredValue: '',
    // set callback function
    callback: function(response, $captchaInputElement, numberOfTries) { 
        console.log("Answer: " + response);
        console.log("Attempt: " + numberOfTries);
        console.log("Response: " + response);
        console.log("Value: " + $captchaInputElement.value);
        var savedValue = $captchaInputElement.value;
        if (maxNumberOfTries === numberOfTries) { 
          return false;
          // maximum attempts reached, so do something
          // e.g. disable the form:
          document.querySelector("form.sanwebe-saran-inspired-form-style").removeEventListener("submit", formSubmit); 
          $captchaInputElement.classList.add("disabled"); 
          $captchaInputElement.placeholder = "Maximum attempts reached!"; 
          $captchaInputElement.setAttribute("disabled", "true"); 
          document.querySelector("button").setAttribute("disabled", "true"); 
          return; 
        } 

        if (response == 'success') { 
          numberOfTries = 0;
          $captchaInputElement.placeholder = 'Success!';
          $captchaInputElement.classList.remove('error'); 

          // continue with form submit
          document.querySelector("form.sanwebe-saran-inspired-form-style").submit();
        } 

        if (response == 'error') { 
          $captchaInputElement.classList.add('error'); 
          $captchaInputElement.placeholder = 'Please try again!'; 
          document.querySelector("#jccon").insertAdjacentHTML('beforeend', '<span class="msg-error">Required field</span>');
          return false;
        } 
    } 

}); 

var config = {
  container: 'form.sanwebe-saran-inspired-form-style',
  validationBy: 'onSubmit',
  onFormSubmit: function(container){
    // myCaptcha validate
    myCaptcha.validate(); 
  }
};
var validator = new VanillaValidator(config);
</script>
