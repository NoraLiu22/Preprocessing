	<script>
	var trackOtherSubscriptions = document.querySelectorAll('.other-subscriptions')
	for (var i = 0; i < trackOtherSubscriptions.length; i++) {
		if (trackOtherSubscriptions[i].classList.contains('other-subscriptions')) {
			trackOtherSubscriptions[i].setAttribute("href", "https://subscribe.smh.com.au/" + "?promote_channel=" + promote_channel)
		}
	}
		var trackStarterDigital = document.querySelector('.starter-digital')
		if (trackStarterDigital.classList.contains('starter-digital')) {
			trackStarterDigital.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_webbasic" + PROMOTE_CHANNEL)
		}

		var cardPosition = document.querySelector(".card-position")
		if (cardPosition.classList.contains("weekly-view")) {
			var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
			if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
				trackButtonWeekday.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigmonfri" + PROMOTE_CHANNEL)
			}
			var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
			if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
				trackButtonWeekend.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigwkend" + PROMOTE_CHANNEL)
			}
			var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
			if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
				trackButtonUltimate.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldig7day" + PROMOTE_CHANNEL)
			}
		}

		document.querySelector('.visual-status').addEventListener('click', function () {
			var cardPosition = document.querySelector(".card-position")
			if (cardPosition.classList.contains('annual-view')) {
				cardPosition.classList.remove("annual-view")
				cardPosition.classList.add("weekly-view")

				var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
				if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
					trackButtonWeekday.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigmonfri" + PROMOTE_CHANNEL)
				}
				var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
				if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
					trackButtonWeekend.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigwkend" + PROMOTE_CHANNEL)
				}
				var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
				if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
					trackButtonUltimate.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldig7day" + PROMOTE_CHANNEL)
				}

				var subscribeCtaWeekly = document.querySelectorAll('.subscribe-now')
				for (var i = 0; i < subscribeCtaWeekly.length; i++) {
					if (subscribeCtaWeekly[i].classList.contains('subscribe-now')) {
						subscribeCtaWeekly[i].innerHTML = 'SUBSCRIBE NOW'
					}
				}

				var headlineWeekly = document.querySelector('.page-headline')
				headlineWeekly.innerHTML = 'Get the Herald delivered to your home.'

				var subheadlineWeekly = document.querySelector('.page-subheadline')
				subheadlineWeekly.innerHTML = 'Enjoy The Herald newspaper delivered, plus uninterrupted digital access.'

				var termsWeekly = document.querySelector('.terms')
				termsWeekly.innerHTML = '*Subscriptions automatically renew unless the subscriber changes or cancels their subscription in My Account or by calling customer service on 13 66 66. Home delivery is available in selected areas only. Please allow 10 days for delivery to commence. For full subscription terms and conditions visit <a href="https://www.smh.com.au/digital-subscription-terms" target="_blank">smh.com.au/conditions</a>.'

			} else {
				cardPosition.classList.add("annual-view")
				cardPosition.classList.remove("weekly-view")
				var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
				if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
					trackButtonWeekday.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigmonfri_12m" + PROMOTE_CHANNEL)
				}
				var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
				if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
					trackButtonWeekend.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldigwkend_12m" + PROMOTE_CHANNEL)
				}
				var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
				if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
					trackButtonUltimate.setAttribute("href", "https://www.smh.com.au/subscription/account/?bundle_key=smh_alldig7day_12m" + PROMOTE_CHANNEL)
				}

				var subscribeCtaAnnual = document.querySelectorAll('.subscribe-now')
				for (var i = 0; i < subscribeCtaAnnual.length; i++) {
					if (subscribeCtaAnnual[i].classList.contains('subscribe-now')) {
						subscribeCtaAnnual[i].innerHTML = 'SUBSCRIBE NOW'
					}
				}

				var headlineAnnual = document.querySelector('.page-headline')
				headlineAnnual.innerHTML = 'Support news you can trust.'

				var subheadlineAnnual = document.querySelector('.page-subheadline')
				subheadlineAnnual.innerHTML = 'Subscribe and save 10% on your annual package.*'

				var termsAnnual = document.querySelector('.terms')
				termsAnnual.innerHTML = '*Discount is 10% off the annual subscription price for the first 12 months on selected packages only. Offer is available to new subscribers only. At the end of the promotional period the subscription rate will increase to the regular package rate, billed annually. This offer is subject to change. Subscriptions automatically renew unless the subscriber changes or cancels their subscription in My Account or by calling customer service on 13 66 66. For full subscription terms and conditions visit <a href="https://www.smh.com.au/digital-subscription-terms" target="_blank">smh.com.au/conditions</a>.'

			}
		})

	</script>

