				variant: 'annual'
			}
		]

		// --------------------------------------------------------------------------------------------------------------
		var starterDigitalPackages = [
			{
				name: 'starter digital',
				brand: 'smh',
				id: 'smh_starter_digital',
				price: '4.5',
				list_position: '1',
				variant: 'weekly'
			}
		]
		function trackStarterDigital(data) {
			data = starterDigitalPackages[0]
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction': 'addToCart',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
					'event': 'addToCart',
					'currencyCode': 'AUD',
					'add': [
						{
							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
				}
			});
		}
		// --------------------------------------------------------------------------------------------------------------

		function trackUltimateArrow(data) {
			if (document.querySelector('.card-position').classList.contains('annual-view')) {
				data = smhPackages[1]
				console.log('> smh ultimate digital annual: ', smhPackages[1])
			} else {
				data = smhPackages[0]
				console.log('> smh ultimate digital weekly: ', smhPackages[0])
			}
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction': 'Product Impression',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
					'currencyCode': 'AUD',
					'impressions': [
						{
							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
				}
			});
		}

