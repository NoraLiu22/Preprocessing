                pic[i].src = imageUrls[i];



        }



}















// Rolling over



function hiLite(imageName) {



        if (document.images) {



                top.document.images[imageName].src = "/history/scottishhistory/images/darkages/" + imageName + "on.gif";



        }



}







// Rolling out



function backNormal(imageName) {



        if (document.images) {



                top.document.images[imageName].src = "/history/scottishhistory/images/darkages/" + imageName + "off.gif";



        }



}



