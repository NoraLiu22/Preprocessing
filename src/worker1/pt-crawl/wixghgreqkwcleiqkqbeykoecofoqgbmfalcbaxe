if (document.images) {







var imageUrls = new  Array("/history/scottishhistory/images/darkages/history_darkages_history_on.gif","/history/scottishhistory/images/darkages/history_darkages_history_on.gif","/history/scottishhistory/images/darkages/history_darkages_media_on.gif","/history/scottishhistory/images/darkages/history_darkages_oddities_on.gif","/history/scottishhistory/images/darkages/history_darkages_web_guide_on.gif");







