              <source srcset="https://csis-website-prod.s3.amazonaws.com/s3fs-public/styles/990x430/s3/publication/GettyImages-1204537256.jpg?VersionId=rGSd1bcAyShPo4VQqZTkh6dpj9N9Qzie&amp;h=e5565190&amp;itok=-06a2el2 1x" media="(min-width: 769px)" type="image/jpeg" width="990" height="430"/>
              <source srcset="https://csis-website-prod.s3.amazonaws.com/s3fs-public/styles/768x461/s3/publication/GettyImages-1204537256.jpg?VersionId=8p3MXgQ9sXGnnTXRuKSCawz0fkEezUqW&amp;h=e5565190&amp;itok=CZTOm2Iz 1x" media="(min-width: 501px)" type="image/jpeg" width="768" height="461"/>
              <source srcset="https://csis-website-prod.s3.amazonaws.com/s3fs-public/styles/500_x_300/s3/publication/GettyImages-1204537256.jpg?VersionId=Hpp_Ea0H6h57xOnSQZhHprnkV.y2gUhv&amp;h=e5565190&amp;itok=exRCyYsg 1x" media="(min-width: 0px)" type="image/jpeg" width="500" height="300"/>
                  <img loading="lazy" src="https://csis-website-prod.s3.amazonaws.com/s3fs-public/styles/500_x_300/s3/publication/GettyImages-1204537256.jpg?VersionId=Hpp_Ea0H6h57xOnSQZhHprnkV.y2gUhv&amp;h=e5565190&amp;itok=exRCyYsg" width="500" height="300" alt="Photo: LUIS ACOSTA/AFP via Getty Images" />

  </picture>

</div>
          </div>

    <figcaption class="credit">      <span class="field-attribution inline-block mt-sm">
      <p>Photo: LUIS ACOSTA/AFP via Getty Images</p>

    </span>
  </figcaption>
  </figure>

        <span class="credit"></span>
      </div>
    </div>
