<!-- Start JavaScript --
function scrollmsg(msgpos)
{
  var sleep=125;
  var blanks="               ";
  var msg1="Welcome to ITA WWW Design ... ";
  var msg2="This is where you can have your World Wide Web pages made, hosted and managed expertly and very inexpensively ... ";
  var msg3="Our prices range from absolutely free to absolutely modest ... ";
  var msg4="We are uncommonly good at publicizing new sites ... ";
  var msg5="If there is an audience there, we will make sure they find your pages ..."
  var msg=blanks+blanks+blanks+blanks+blanks+msg1+blanks+msg2+blanks+msg3+blanks+msg4+blanks+msg5;
  out=msg.substring(msgpos,msg.length);
  window.status=out;
  msgpos++;
  if (msgpos>msg.length)
    msgpos=0;
  var exec="scrollmsg("+msgpos+")";
  zzz=window.setTimeout(exec,sleep);
}
// -- End of JavaScript -->

</SCRIPT>

