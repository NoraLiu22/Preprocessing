  var share_url = 'https://web.archive.org/web/20200222194612/https://www.geni.com/people/Elias-Sander/6000000027756093071' || window.location.href;

  $j(document).ready(function(){
    //
    //        Toggle panel display on click
    // -------------------------------------------------------------------
    $j(window).on('click', function(evt) {
      if (evt.target.id == 'nav-share') {
        if ($j('.social-sharing-panel').is(':visible')) {
          $j('.social-sharing-panel').slideUp();
        } else {
          $j('.social-sharing-panel').slideDown();
        }
      } else if ($j(evt.target).parents('.social-sharing-panel').length == 0) {
        // clicked outside the sharing panel
        $j('.social-sharing-panel').slideUp();
      }
    });

    //
    //        Toggle FB sharing on click
    // -------------------------------------------------------------------
    $j('#sharing-button').on('click', function(evt){
      var $this = $j(this);
      var $spinner = $j('#fb-sharing-wrapper .spinner');
      $spinner.fadeIn();

      if (evt.target.id == 'sharing-status') {
        // undo the toggle, we will do it in handleFBSucess only if Ajax call succeeds
        $j(evt.target).prop('checked', !$j(evt.target).prop('checked'));
      }

              fbLinkAccountAndForward(window.location.href, window.location.href)
          });

    // Enable copy-link-to-clipboard
    new ClipboardJS('#sharing__copy-link', {
      text: function(trigger) { return(share_url); }
    }).on('success', function(e) {
      var poptip = $j(e.trigger).find('.pop-tip-msg')
      if (poptip.length > 0) {
        poptip.fadeIn();
        setTimeout(function() { poptip.fadeOut(); }, 1000);
      }
    });
  });

  function handleFBSuccess(){
    var $nav_icon        = $j('#nav-share');
    var $sharingStatus   = $j('#fb-sharing-wrapper #sharing-status');
    var $firstDescriptor = $j('#sharing-descriptor strong').first();
    var $spinner         = $j('#fb-sharing-wrapper .spinner');
    $spinner.fadeOut();

    if ( $sharingStatus.prop('checked') ) {
      $nav_icon.removeClass('fb-share-on').addClass('fb-share-off');
      $sharingStatus.prop('checked', false);
      $sharingStatus.text( trl('OFF') );
      $firstDescriptor.text( trl('are not') );
    } else {
      $nav_icon.removeClass('fb-share-off').addClass('fb-share-on');
      $sharingStatus.prop('checked', true);
      $sharingStatus.text( trl('ON') );
      $firstDescriptor.text( trl('are') );
    }
  }

  function handleFBError(){
    var $error          = $j('#fb-sharing-wrapper .error').show()
    var $spinner        = $j('#fb-sharing-wrapper .spinner');
    $spinner.fadeOut();
    $error.fadeIn();
  }

</script>

      
