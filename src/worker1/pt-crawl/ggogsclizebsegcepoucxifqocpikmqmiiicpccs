        function GetMessage(obj) {
            try {
                if ((message != undefined && subject != undefined) && (message != '' && subject != '')) {
                    $("#msgMessage").html(message);
                    $('#msgMessageTitle').html(subject);
                    $('#divMessages').show();
                }
                else if (message != '' || subject != '') {
