    <script type="text/javascript">
        (function ($, window) {

            var picCur = 0,
                picSum = $('.focus_l li').size(),
                wid,
                vcur = 0,
                fcur = 0,
                person = $('.pwrap div').size() + 1,
                percur = 0;

            function changePic() {
                $('.focus_l li').hide();
                $('.focus_l li').eq(picCur).fadeTo(300, 1);
                $('#menu span').removeClass('cur');
                $('#menu span').eq(picCur).addClass('cur');
            };

            function autoPic() {
                picCur += 1;
                picCur %= picSum;
                changePic();
            };

            $('.focus_l li').first().show();
            $('#menu').empty();
            for (var i = 1; i <= picSum; i++) {
                $('#menu').append('<span>&nbsp;</span>');
            };
            $('#menu span:first()').addClass('cur');

            $('#menu span').on('click', function () {
                picCur = $(this).index();
                changePic();
            });

            $('#rarr').on('click', function () {
                picCur += 1;
                picCur %= picSum;
                changePic();
            });
            $('#larr').on('click', function () {
                picCur -= 1;
                picCur += picSum;
                picCur %= picSum;
                changePic();
            });
            wid = window.setInterval(autoPic, 5000);
            $('.focus_l').on('mouseenter', function () {
                window.clearInterval(wid);
            }).on('mouseleave', function () {
                wid = window.setInterval(autoPic, 5000);
            });

            // $('.video li:first').find('span').hide();
            // $('.video li:first').find('a').show();

            // function videoshow() {
            //   $('.video li').removeClass('vcur');
            //   $('.video div').hide();
            //   $('.video li').eq(vcur).addClass('vcur');
            //   $('.video div').eq(2 * (vcur + 1)).fadeTo(200, 1);
            //   $('.video div').eq(2 * (vcur + 1) + 1).fadeTo(200, 1);
            // }
            // videoshow();
            // $('.video li:first').removeClass('vcur');
            // $('.video li').on('click', function () {
            //   vcur = $(this).index();
            //   videoshow();
            //   $('.video li a').hide();
            //   $('.video li span').show();
            //   $(this).find('span').hide();
            //   $(this).find('a').show();
            // });

            function forumshow() {
                $('.forum_r h2').removeAttr('style');
                $('.forum_r h2').eq(fcur).css('font-weight', 'bold');
                $('.forum_r h1,.forum_r .lmcont').hide();
                $('.forum_r h1').eq(fcur).fadeTo(200, 1);
                $('.forum_r .lmcont').eq(fcur).fadeTo(200, 1);
            }
            forumshow();
            $('.forum_r h2').on('mouseenter', function () {
                fcur = $(this).index();
                forumshow();
            });

            $('.seriescont .one:first').show();

            $('.seriesmenu li').on('mouseenter', function () {
                var slcur = $(this).index();
                $('.seriesmenu li').removeClass('scur');
                $(this).addClass('scur');
                $('.seriescont .one').hide();
                $('.seriescont .one').eq(slcur).fadeTo(200, 1);
            });

            $('#yt3 dl:first,#yt3 ul:first').show();

            $('#yt3 div p').on('click', function () {
                var ytcur = $(this).index();
                $('#yt3 div p').removeClass('sscur');
                $(this).addClass('sscur');
                $('#yt3').find('dl').hide();
                $('#yt3').find('dl').eq(ytcur).fadeTo(200, 1);
                $('#yt3').find('ul').hide();
                $('#yt3').find('ul').eq(ytcur).fadeTo(200, 1);
            });

            $('.joint_m ul:first').show();
            $('.joint_m h1 span').on('mouseenter', function () {
                var jcur = $(this).index();
                $('.joint_m h1 span').removeClass('jcur');
                $(this).addClass('jcur');
                $('.joint_m').find('ul').hide();
                $('.joint_m').find('ul').eq(jcur).fadeTo(200, 1);
            });

            $('.picshow .pone').on('mouseenter', function () {
                $(this).find('img').fadeTo(200, 0.4);
                $(this).find('p').fadeTo(200, 1);
            }).on('mouseleave', function () {
                $(this).find('img').fadeTo(200, 1);
                $(this).find('p').fadeTo(200, 0);
            });
            $('.picshow .pone p').each(function () {
                var pph = $(this).height();
                $(this).css('margin-top', pph / -2);
            });

            $('.pwrap').width(980 * person);
            $('.pwrap').append($('.pwrap div:first').clone());

            function perroll() {
                percur += 1;
                $('.pwrap').animate({
                    'margin-left': -980 * percur
                }, function () {
                    if (percur >= person - 1) {
                        percur = 0;
                        $('.pwrap').css('margin-left', '0px');
                    }
                });
            }
            window.setInterval(perroll, 6000);
        })(window.jQuery, window);
    </script>
