	// <![CDATA[
	function gmc_login_block(x) {
	  var b=document.getElementById('sidebar-right');
	  b.style.display=x?"block":"none";
	  if (x) {
	    var left = 0, top = 0;
	    while (x) {
	      left += x.offsetLeft;
	      top += x.offsetTop;
	      x = x.offsetParent;
	    }
	    b.style.left = left+'px';
	    b.style.top = (top-b.offsetHeight+10)+'px';
	    setTimeout('document.getElementById("edit-name").focus()', 100);
	  }
	}
