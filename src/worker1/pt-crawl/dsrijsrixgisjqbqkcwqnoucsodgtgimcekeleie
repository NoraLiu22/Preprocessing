            window.adUnits[br] = window.adUnits[br] || {};
            window.adUnits[br]['bottom-mobile'] = {
              pos: 'bottom',
              size: ["fluid", [300, 250]],
            };
          });
<!--          window.googletag.cmd.push(function() {-->
<!--            window.googletag.display(`div-gpt-ad-804752003747430782-bottom-mobile`);-->
<!--          });-->
        })();
    </script>
  </div>
</section>




