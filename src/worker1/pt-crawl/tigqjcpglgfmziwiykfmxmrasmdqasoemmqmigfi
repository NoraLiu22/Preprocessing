             <script>
                require(["knockout", "jquery",, "jquery-ui", "config", "binding-handlers", "slick", "headroom", "jquery-headroom"], function (ko, $) {
                    $(window).resize(function() {
                        var tracker = ($('.main-header').outerHeight() + $('.leaderboard_ad').outerHeight());
                        $('.main-navigation').headroom('destroy').removeData('headroom').headroom({ offset:tracker });
                        $('.sport-navigation').headroom('destroy').removeData('headroom').headroom({ offset:tracker });
                    }).trigger('resize');

                    //create mutationobservable to detect when the ad image is loaded, then change headroom
                    var targetNode = document.querySelector('.leaderboard_ad ads-component');
                    var config = { attributes: true, childList: true, subtree: true };
                    var callback = function(mutationsList, observer) {
                        for(var mutation of mutationsList) {
                            if (mutation.type == 'attributes') {
                                if (mutation.attributeName == 'height') {
                                    var tracker = $('.main-header').outerHeight() + $('.leaderboard_ad').outerHeight();
                                    console.log(tracker);
                                    $('.main-navigation').headroom('destroy').removeData('headroom').headroom({ offset:tracker });
                                    $('.sport-navigation').headroom('destroy').removeData('headroom').headroom({ offset:tracker });
                                    observer.disconnect();
                                }
                            }
                        }
                    };
                    var observer = new MutationObserver(callback);
                    observer.observe(targetNode, config);

                    $(function(){
                        $('.main-navigation').on("keypress", '#search .search-input input[type="text"]', function(e) {
                            if (e.keyCode == 13) {
                                e.preventDefault();
                                window.location='/searchresults.aspx?cx=016306950182310286156%3Az5atj_nax4i&q=' + $("#search .search-input input[type='text']").val()+  '&sa=Search&cof=FORID%3A11#1141';
                                return false;
                            }
                        });

                        $('.main-navigation').on("click", "#search .search-button input[type='submit']", function(e){
                            e.preventDefault();
                            window.location='/searchresults.aspx?cx=016306950182310286156%3Az5atj_nax4i&q=' + $("#search .search-input input[type='text']").val()+  '&sa=Search&cof=FORID%3A11#1141';
                                return false;
                        });

                        setTimeout(function(){
                            $('.stickyLeaderboard').headroom('destroy').removeData('headroom').headroom({ offset: 300});
                        }, 5000)

                        $('.main-navigation').on("click", "#search-btn >i", function(){
                            $(this).parent().toggleClass("active")
                        });
                    });
                });
