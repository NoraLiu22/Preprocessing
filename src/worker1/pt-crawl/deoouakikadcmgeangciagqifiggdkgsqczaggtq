                    $(function() { 
                        $(window).resize(function(){
                            //Initalize headroom with proper offsets that is triggered only when the viewport is resized rather than onscroll
                            if (window.matchMedia('(min-width:768px)').matches) { offset = 135; }
                            else { offset = 117; }
                            $('html').headroom('destroy').removeData('headroom').headroom({ offset: offset });
                            $('html.sport').headroom('destroy').removeData('headroom').headroom({
                                offset: offset
                            });
                        }).trigger('resize');  
                    });
                });
