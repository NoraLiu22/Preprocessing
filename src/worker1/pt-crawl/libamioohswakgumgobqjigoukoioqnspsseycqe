<script data-load-strategy="gdpr">
(function(){
  var cookies = document.cookie.split(';').reduce(function(acc, val) {
    var p = val.trim().split('=');
    acc[p[0]] = p[1];
    return acc;
  }, {});

  var identify = [], iId, aId;
  if (iId = cookies['insider_uid']) identify.push({"id": iId, "tag": "insider_uid", "priority":1});
  if (aId = cookies['anonymous_id']) identify.push({"id": aId, "tag": "anonymous_id", "priority":2});
  if (uId = cookies['universal_id']) identify.push({"id": uId, "tag": "universal_id", "priority":2});
  if (identify.length) window.permutive.identify(identify);
})()
</script>

