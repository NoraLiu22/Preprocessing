<script src="http://images.china.cn/images1/ch/2018dzd/js/swiper.min.js"></script>
<script>
(function(window, $) {
/*轮播图*/
	var len=$(".hot_img").length,
		num=0,
		timer=0;
	$(".dian li").mouseover(function(){
		num=$(this).index();
		$('.hot_img').eq(num).addClass("hot_img_hover").siblings().removeClass("hot_img_hover");
        $(this).addClass("hover").siblings().removeClass();
	});	
	function auto(){
        num = ( num + 1) % len;
        $('.hot_img').eq(num).addClass("hot_img_hover").siblings().removeClass("hot_img_hover");
        $('.dian li').eq(num).addClass("hover").siblings().removeClass();
    };
    var timer=setInterval(auto,3000);
    $(".hot_img,.dian").hover(function () {
        clearInterval(timer);
    }, function () {
        timer = setInterval(auto, 3000);
    })

} (window, window.jQuery));
</script>

