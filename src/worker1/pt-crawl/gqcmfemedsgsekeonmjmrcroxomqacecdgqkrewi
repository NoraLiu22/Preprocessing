// ROLLOVERS SCRIPT



// Preload images



if (document.images) {



var imageUrls = new Array

        var pic = new Array(imageUrls.length);



        for (i=0; i<imageUrls.length; i++) {

                pic[i] = new Image();

                pic[i].src = imageUrls[i];

        }

}







// Rolling over

function hiLite(imageName) {

        if (document.images) {

                top.document.images[imageName].src = "/history/scottishhistory/images/victorian/" + imageName + "on.gif";

        }

}



// Rolling out

function backNormal(imageName) {

        if (document.images) {

                top.document.images[imageName].src = "/history/scottishhistory/images/victorian/" + imageName + "off.gif";

        }

}

