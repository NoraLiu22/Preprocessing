		$(".wbk").children(".hdg").each(function() {
			$(this).children("a").css({backgroundImage:"none"});
		});
		// and fade in/out on hover/away
		$(".wbk").hover(function() {
			$(this).addClass("hover").prepend('<div class="hdg-js"></div>');
