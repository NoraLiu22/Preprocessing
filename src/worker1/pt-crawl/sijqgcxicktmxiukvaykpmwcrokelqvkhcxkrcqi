<script type="text/javascript">
(function ($) {
    // 头条
    var len = $('#storyBox li').length;
    var webWidth = 0;
    var cur = len;
    var lock = false;
    $('#storyBox li').clone().appendTo('#storyBox ul');
    init();

    $(window).resize(function () {
        init();
    });
    $('.storyBox .arrLeft').on('click', function () {
        if (!lock) {
            cur = cur - 1;
            movePic();
            lock = true;
        }
    });
    $('.storyBox .arrRight').on('click', function () {
        if (!lock) {
            cur = cur + 1;
            movePic();
            lock = true;
        }
    });

    function init() {
        webWidth = $(window).width();
        $('#storyBox ul').css({ 'width': 1200 * len * 2, 'margin-left': -cur * 1200 + (webWidth - 1200) / 2 });
        $('#storyBox .shadeLeft, #storyBox .shadeRight').css({ 'width': (webWidth - 1200) / 2 });
        $('#storyBox li').eq(cur).find('div').show();
    }

    function movePic() {
        $('#storyBox li').find('div').fadeOut(500);
        $('#storyBox ul').delay(500).animate({ 'margin-left': -cur * 1200 + (webWidth - 1200) / 2 }, function () {
            lock = false;
            $(this).find('li').eq(cur).find('div').fadeIn();
            if (cur <= 1) {
                cur = len + cur;
                init();
            }
            if (cur >= 2 * len - 2) {
                cur = len - 2;
                init();
            }
        });
    }
}(jQuery));
(function(){
var slideMenu=function(){
  var sp,st,t,m,sa,l,w,gw,ot;
  return{
    destruct:function(){
        if(m){
          clearInterval(m.htimer);
          clearInterval(m.timer);
        }
    },
    build:function(sm,sw,mt,s,sl,h){
      sp=s; 
      st=sw; 
      t=mt;
      m=document.getElementById(sm);
      sa=m.getElementsByTagName('li');
      l=sa.length; 
      w=m.offsetWidth; 
      gw=w/l;
      ot=Math.floor((w-st)/(l-1)); 
      var i=0;
      for(i;i<l;i++){
        s=sa[i]; 
        s.style.width=gw+'px'; 
        this.timer(s)
      }
      if(sl!=null){
        m.timer=setInterval(function(){
          slideMenu.slide(sa[sl-1])
        },t)}
    },
    timer:function(s){
      s.onmouseover=function(){
        clearInterval(m.htimer);
        clearInterval(m.timer);
        m.timer = setInterval(function(){
          slideMenu.slide(s)
        },t);
        //console.log($(this).find('.mask_b').html());
        //$(this).find('.mask_b').hide();
        //console.log($(this).find('.mask_b').attr("style"));
    }
      s.onmouseout=function(){
        clearInterval(m.timer);
        clearInterval(m.htimer);
        m.htimer=setInterval(function(){
          slideMenu.slide(s,true)
        },t);
        //console.log($(this).find('.mask_b').html());
       //$(this).find('.mask_b').show();
       //console.log($(this).find('.mask_b').attr("style"));
     }
    },
    slide:function(s,c){
      var cw=parseInt(s.style.width);
      if((cw<st && !c) || (cw>gw && c)){
        var owt=0; var i=0;
        for(i;i<l;i++){
          if(sa[i]!=s){
            var o,ow; var oi=0; o=sa[i]; ow=parseInt(o.style.width);
            if(ow<gw && c){
              oi=Math.floor((gw-ow)/sp); 
              oi=(oi>0)?oi:1; 
              o.style.width=(ow+oi)+'px';
              //console.log(o);
            //console.log(o.style.width);
            }else if(ow>ot && !c){
              oi=Math.floor((ow-ot)/sp); 
              oi=(oi>0)?oi:1; 
              o.style.width=(ow-oi)+'px';
              //console.log(o);
              //console.log(o.style.width);
            }
            if(c){
              owt=owt+(ow+oi)
            }else{
              owt=owt+(ow-oi)
            }
          }
        }
        s.style.width=(w-owt)+'px';
      }else{
        if(m.htimer)
          clearInterval(m.htimer)
        if(m.timer)
          clearInterval(m.timer);
      }
    }
  };
}();
slideMenu.build('sm',875,10,10,1);
})();
(function($) {
    var picCur = 0,
        picSum = $('.picpart_3').size();

    function changePic() {
        $('.picpart_3').hide();
        $('.picpart_3').eq(picCur).fadeTo(300, 1);
    };

    $('.picpart_3').first().show();
    $('.more').on('click', function() {
        picCur += 1;
        picCur %= picSum;
        changePic();
    });
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('.menuVideo ul').size();

    function changePic() {
        $('.menuVideo ul').hide();
        $('.menuVideo ul').eq(picCur).fadeTo(300, 1);
        $('.menuswich span').removeClass('cur');
        $('.menuswich span').eq(picCur).addClass('cur');
    };

    $('.menuVideo ul').first().show();
    $('.menuVideo ul').each(function() {
        $('.menuswich').append('<span>&nbsp;</span>')
    });
    $('.menuswich span:first()').addClass('cur');

    $('.menuswich span').on('click', function () {
        picCur = $(this).index();
        changePic();
    });
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('.bigVideo li').size();

    function changePic() {
        $('.bigVideo li').hide();
        $('.bigVideo li').eq(picCur).fadeTo(300, 1);
        $('.menuVideo li').removeClass('cur');
        $('.menuVideo li').eq(picCur).addClass('cur');
        $('.menuVideo li').stop(true, false).find('div').animate({'width': '0'}, 200)
        $('.menuVideo li').stop(true, false).animate({'margin-left': '0','width':'325px'}, 200)
        $('.menuVideo li').eq(picCur).stop(true, false).find('.lblock').animate({'width': '3px'}, 200)
        $('.menuVideo li').eq(picCur).stop(true, false).find('.rblock').animate({'width': '48px'}, 200)
        $('.menuVideo li').eq(picCur).stop(true, false).animate({'margin-left': '-20px','width':'345px'}, 200)
    };

    $('.bigVideo li').first().show();
    $('.menuVideo li:first()').addClass('cur');

    $('.menuVideo li').on('click', function () {
        picCur = $(this).index();
        changePic();
    });
})(jQuery);
(function($) {
    var picCur = 0,
        picSum = $('#slide_1 li').size();

    function changePic() {
        $('#slide_1 li').hide();
        $('#slide_1 li').eq(picCur).fadeTo(300, 1);
        $('#slide_m1 span').removeClass('cur');
        $('#slide_m1 span').eq(picCur).addClass('cur');
    };

    $('#slide_1 li').first().show();
    $('#slide_1 li').each(function(i) {
        $('#slide_m1').append('<span>'+(i+1)+'</span>')
    });
    $('#slide_m1 span:first()').addClass('cur');

    $('#slide_m1 span').on('click', function () {
        picCur = $(this).index();
        changePic();
    });
    
    $('#rarr_1').on('click', function() {
        picCur += 1;
        picCur %= picSum;
        changePic();
    });

    $('#larr_1').on('click', function() {
        picCur -= 1;
        picCur += picSum;
        picCur %= picSum;
        changePic();
    });
})(jQuery);
