<script type="text/javascript">
    $(document).ready(function () {
        ShowRenewal();
    });


    function ShowRenewal() {
        var url = '/api/ShowRenewal';
        $.ajax(url,
            {
                method: 'POST'
            })
            .done(function (data) {
                $('#renewalContainer').html(data);
            })
            .fail(function () {
                console.log('ERROR!');
            });
    }


    function openWin(url, wName, para) {
        var values = para;
        window.open(url, wName, values);
    }

</script>


<!-- BANNER CAROUSEL START -->

<!-- BANNER CAROUSEL END -->


<script>
    document.addEventListener('DOMContentLoaded', function (event) {
        initOpenCloses();
    });

    function initOpenCloses() {
        var activeClass = 'active';
        var openers = document.querySelectorAll('.nav-opener, .popup-opener, .search-opener');
        var body = document.body;
        var refreshDelay = 310;

        for (var index = 0; index < openers.length; index++) {
            var element = openers[index];

            action(element);
            function action(element) {
                element.addEventListener('click', function (e) {
                    e.preventDefault();
                    body.classList.toggle(element.dataset.openClass);
                    if (element.dataset.typeClass) {
                        body.classList.toggle(element.dataset.typeClass);
                    }
                })
            }
        }

        document.addEventListener('click', function (e) {
            if (!e.target.closest('.navbar') && !e.target.closest('.nav-opener')) {
                body.classList.remove('nav-active');
            }
            if (!e.target.closest('.search') && !e.target.closest('.search-opener')) {
                body.classList.remove('search-active');
            }
            if (!e.target.closest('.popup') && !e.target.closest('.popup-opener')) {
                body.classList.remove('popup-active');
                if (body.classList.contains('sign-in')) {
                    body.classList.remove('sign-in');
                } else if (body.classList.contains('account')) {
                    body.classList.remove('account');

                }
            }
        })

        var articles = document.querySelectorAll('.article');

        for (var index = 0; index < articles.length; index++) {
            var article = articles[index];
            action(article);
            function action(article) {
                var textSlide = article.querySelector('.text-container');
                var openerTimer;
                if (textSlide) {
                    var textContent = textSlide.querySelector('.text-frame');
                    var opener = article.querySelector('.more');
                    var paragraphs = article.querySelectorAll('.text-frame p');


                    if (paragraphs && paragraphs.length === 1 && opener) {
                        opener.remove();
                        return;
                    }

                    if (opener) {
                        opener.addEventListener('click', function (e) {
                            e.preventDefault();
                            article.classList.toggle(activeClass);
                            refreshState();
                        });
                    }
                    refreshState();

                    function refreshState() {
                        if (article.classList.contains(activeClass)) {
                            textSlide.style.height = textContent.offsetHeight + 'px';
                            clearTimeout(openerTimer);
                            openerTimer = setTimeout(function () {
                                textSlide.style.height = '';
                            }, refreshDelay);
                        } else {
                            textSlide.style.height = textContent.offsetHeight + 'px';
                            clearTimeout(openerTimer);
                            setTimeout(function () {
                                textSlide.style.height = paragraphs[0].offsetHeight + 'px';
                            }, 10);
                        }
                    }
                }
            }

        }

        var openCloseItems = document.querySelectorAll('.open-close-container');

        for (var index = 0; index < openCloseItems.length; index++) {
            var holder = openCloseItems[index];
            var opener = holder.querySelector('.opener');
            action(holder, opener);
            function action(holder, opener) {
                opener.addEventListener('click', function (e) {
                    e.preventDefault();
                    holder.classList.toggle(activeClass);
                })
            }
        }

        var subNavs = Array.prototype.slice.call(document.querySelectorAll('.navbar .nav-hold > .col')).concat(Array.prototype.slice.call(document.querySelectorAll('.navbar .nav-hold > div > .col')));
        var subSubNavs = document.querySelectorAll('.navbar .col .col');
        var subNavActiveClass = 'submenu-active';
        var subNavActiveClassAlt = 'subMenu-active';

        for (var index = 0; index < subNavs.length; index++) {
            var subNav = subNavs[index];
            var opener = subNav.querySelector('.sub-title > a');

            function clickHandler(opener, subNav) {
                if (opener) {
                    opener.addEventListener('click', function (e) {
                        e.preventDefault();
                        subNav.classList.toggle(subNavActiveClass);
                    });
                }
            }
            clickHandler(opener, subNav);
        }

        for (var index = 0; index < subSubNavs.length; index++) {
            var subNav = subSubNavs[index];
            var opener = subNav.querySelector('.sub-title > a');

            function clickHandler(opener, subNav) {
                if (opener) {
                    opener.addEventListener('click', function (e) {
                        e.preventDefault();
                        subNav.classList.toggle(subNavActiveClassAlt);
                    });
                }
            }
            clickHandler(opener, subNav);
        }
    }
</script>

    
<script type="text/javascript">
    function ClosePopup() {
        $('.popup-container').hide();
    }
</script>
