            // <![CDATA[
            $(document).ready(function(){
                var photo = $("#article-photo img");
                var width = photo.width();
                var height = photo.height();
        
                if(width > height){
                    photo.addClass("flex-photo");
                    $("#article-photo, #sidebar").css("width", "100%").css("padding", "0");
                }
            });
