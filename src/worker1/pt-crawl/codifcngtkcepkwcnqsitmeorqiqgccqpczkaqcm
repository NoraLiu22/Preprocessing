     <div id="dnn_ctr436_ContentPane" class="contentpane"><!-- Start_Module_436 --><script>
var slider1 = [], i =0, imgNumber=2;
var arr;	

for(var j = 1; j <= imgNumber; j++){
	slider1.push("url('/Portals/0/images/Homepage Banner/gsa-banner-" + j + ".png')");
}

function randomIntFromInterval(min, max) { // min and max included 
  return Math.floor(Math.random() * (max - min + 1) + min)
}

i = randomIntFromInterval(0,imgNumber - 1);

function hpslide(){
 $('.wrapper').animate({ opacity: 0 }, 500, function() {
     $('.wrapper').css('background-image', arr[i]);
     i++;
     $('.wrapper').animate({ opacity: 1 }, 500, function() {
       if(i === arr.length) i = 0;
     });
   });
setTimeout(hpslide, 6000);
}
 
 $(function(){
 arr = slider1;
hpslide();
});
</script><div id="dnn_ctr436_ModuleContent" class="DNNModuleContent ModDNNGoContentBuilderC">
	
