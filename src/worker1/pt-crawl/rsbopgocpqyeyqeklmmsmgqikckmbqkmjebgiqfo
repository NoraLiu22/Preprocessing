          x: -scowllWhit / 2,
          duration: 280,
          ease: 'none'
        })
        $('.infoList').on('mouseenter', function () {
          tl.pause()
        })
        $('.infoList').on('mouseleave', function () {
          tl.play()
        })
      }
      marquee()
    })



