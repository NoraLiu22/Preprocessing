loadMarketUpdate = function(iIndex, moduleId) {
	for (var i=0; i<5; i++) {
		var mktUpTab = document.getElementById("marketUpdateTab" + i + "_" + moduleId);
		if (mktUpTab != null && mktUpTab != undefined) {
			if (i != iIndex) {
				mktUpTab.className = '';
			} else {
				mktUpTab.className = 'current';
			}
		}
	}
	document.getElementById("marketUpdateIframe_" + moduleId).src = iframeTargets[iIndex];
}
