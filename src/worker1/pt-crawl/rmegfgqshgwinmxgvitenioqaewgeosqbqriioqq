<script>
    function toggleCollapse() {
        const footer = document.querySelector('.collapsible');
        const collapseButton = document.querySelector('.paywall-footer-collapse');
        const expandButton = document.querySelector('.paywall-footer-expand');
        const topBar = document.querySelector('.paywall-footer-top-bar-center');

        footer.classList.toggle('hidden');
        collapseButton.classList.toggle('hidden');
        expandButton.classList.toggle('hidden');
        topBar.classList.toggle('hidden');

    }

    setTimeout(toggleCollapse, 2500);
</script>

<script>
    document.addEventListener('mouseleave', function (event) {
        // Check if the mouse left the document body
        if (!sessionStorage.getItem('popupShown')) {
            if (event.clientY <= 0 || (event.clientY >= window.innerHeight)) {
                var url = '/api/LeaveSite';
                $.ajax(url,
                    {
                        method: 'POST'
                    })
                    .done(function (data) {
                        $('#leaveSiteContainer').html(data);
                    })
                    .fail(function () {
                        console.log('ERROR!');
                    });
            }
        }
        
    });
</script>

