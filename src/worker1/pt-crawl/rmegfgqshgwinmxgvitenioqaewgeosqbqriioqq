<style>
    .highlight {
        position: relative;
        font-weight: bold;
    }

        .highlight a {
            text-decoration: none;
            color: #425AA5;
        }

    .tagged-content {
        width: 100%;
        border-radius: 30px;
    }

        .tagged-content iframe {
            height: 500px;
            border-radius: 30px;
        }

    @media screen and (min-width: 768px) {

        .tagged-content {
            position: fixed;
            width: 450px;
            height: 768px;
            background: #eeeeee;
            color: #000000;
            border: 1px solid #1a1a1a;
            font-size: 90%;
            
            
            
            z-index: 100000;
            box-shadow: 10px 5px 5px lightgrey;
        }

            .tagged-content iframe {
                width: inherit;
                height: inherit;
            }

    }

    .entity-content-hidden {
        display: none;
        position: fixed;
    }

</style>
<script type="text/javascript">
    


    let keys = [];
    let values = [];
    let entities = [];
    let usedEntities = [];
    const videoString = 'https://stub.sbjatlas.com/listing/preview?id=';
    var windowH = $(window.top).height();

    function Entity(name, id) {
        this.name = name;
        this.id = id;
    }

    function pop(id) {

        const allId = id.split('-');
        const actualId = allId[2];
        const entityName = allId[3];
        const modalId = 'entity-modal-' + actualId;
        let atlasModal = document.getElementById(modalId);
        const elem = document.getElementById(id);
        const rect = elem.getBoundingClientRect();

        const contentId = 'entity-content-' + actualId;
        let contentEl = document.getElementById(contentId);
        let position = parseInt(windowH) - parseFloat(rect['y'])

        atlasModal.setAttribute("src", videoString + actualId + "&tag=" + entityName);

        contentEl.classList.toggle('entity-content-hidden');

        if (position >= windowH / 2) {
            contentEl.style.removeProperty('bottom');
            contentEl.style.top = '1%';
        }
        else {
            contentEl.style.removeProperty('top');
            contentEl.style.bottom = '1%';
        }
    }

    


    $(document).ready(function () {



        var model = [];

        if (model != null && model.length > 0) {

            for (let i = 0; i < model.length; i++) {
                keys.push(model[i].Key);
                values.push(model[i].Value);
                entities.push(new Entity(model[i].Value, model[i].Key));
            }

            var para = document.querySelectorAll('div.content-wrap > p');

            for (let j = 0; j < para.length; j++) {
                var text = para[j].innerHTML;

                for (let i = 0; i < entities.length; i++) {

                    entities[i].name = entities[i].name.replace(/_/, ' ');

                    if (entities[i].name.length > 1 && !usedEntities.includes(entities[i])) {

                        if (text.includes(entities[i].name)) {
                            usedEntities.push(entities[i]);
                        }

                        var exp = new RegExp('(?<!\/)(?<!\-)(?<!\<)(?<!\")(?<!\')\\b' + entities[i].name + '\\b(?!\/)(?!\-)(?!\<)(?!\")(?!\')');

                        

                        var enContent = "<span id='entity-content-" + entities[i].id + "' class='tagged-content entity-content-hidden' ><iframe id='entity-modal-" + entities[i].id + "' ></iframe></span>";

                        

                        text = text.replace(exp, "<span class=\"highlight tagged-item\" id=\"tagged-item-" + entities[i].id + "\" onmouseenter=pop(this.id) onmouseleave=pop(this.id) ><a> " + entities[i].name + "</a >" + enContent + "</span>");
                    }
                }

                para[j].innerHTML = text;

            }
        }



        
    });



    

    if (window.location.href.indexOf("Login+Failed") > -1) {
        var content = $(".article-title");
        content.before("<div class='error-message'>Error logging in. Please try again.</div>");
    }

    // tagging keywords & phrases in article

    // would probably need to create a new list property on the model and get tags from that. until then, just using a dummy variable
    

    

    
    




    

    var entityModals = document.getElementsByClassName('entity-modal');
    var entityLink = document.getElementsByClassName('modal-link');

    entityLink.onclick = function () {
        entityModals.style.display = "block";
    }






    

    // copying to clipboard
    function copyToClipboard() {
        var copypopup = document.getElementById("copytoclipboardpopup");
        var copysuccess = document.querySelector(".copysuccess");
        var copyfailed = document.querySelector(".copyfailed");
        var closecopypopup = document.querySelector(".close-btn");
        //default showings
        copypopup.style.display = "flex";
        copysuccess.style.display = "none";
        copyfailed.style.display = "none";

        //copy data
        var copiedtext = document.createElement("textarea");
        copiedtext.innerHTML = "https://www.sportsbusinessjournal.com/Daily/Issues/2019/01/02/Leagues-and-Governing-Bodies/NFL-attendance.aspx";
        document.body.appendChild(copiedtext);
        copiedtext.select();

        //checking if command in browser was a success
        var success = document.execCommand("copy");
        success ? copySuccessful() : copyFailed();

        //functions to run on success/fail
        function copySuccessful() {
            copysuccess.style.display = "block";

            setTimeout(function () {
                copypopup.style.display = "none";
            }, 2000)
        }
        function copyFailed() {
            copyfailed.style.display = "block";
        }

        closecopypopup.onclick = function() {
            copypopup.style.display = "none";
        }

        //getting rid of textbox once everything is complete
        document.body.removeChild(copiedtext);
    }
</script>
        </main>
