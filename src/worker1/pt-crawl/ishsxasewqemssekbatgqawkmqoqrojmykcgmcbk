ishsxasewqemssekbatgqawkmqoqrojmykcgmcbk length 5 10756 page 10756 <script language="javascript">
function showImg(num,name,w,h,caption) {
	imgwin=window.open("", num, "width=" + (w) + ",height=" + (h) + ",status=0,resizeable=1");
	d = imgwin.document.open();
	d.write('<HTML>\n<BODY BGCOLOR="#ffffff" LEFTMARGIN="0" TOPMARGIN="0" MARGINHEIGHT="0" MARGINWIDTH="0">\n<TITLE>' +caption+ '</TITLE>\n<IMG SRC="' +name+ '">\n</BODY>\n</HTML>');
	d.close();
	imgwin.moveTo(10,10);
}
</script>

