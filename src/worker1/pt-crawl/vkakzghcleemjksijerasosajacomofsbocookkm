    <script>
        document.addEventListener('DOMContentLoaded', (event) => {

            const loadingMessage = document.querySelector('.loading-message');
            const iframeContainer = document.querySelector('#iframe-container');

            iFrameResize({
                log: false,
                checkOrigin: false,
                messageCallback: function (data) {
                    console.log(data.message);
                    iframeContainer.classList.remove('not-visible');
                    loadingMessage.classList.add('hidden');
                    if (data.message === 'Loaded iframe from Thank You page.') {
                        document.querySelector('.form-loading-content').classList.add('hidden');
                        document.querySelector('#crm-iframe').classList.remove('hidden');
                        document.querySelector('.form-submitted-content').classList.remove('hidden');
                        window.scrollTo({
                            top: 0,
                            behavior: "smooth"
                        });
                    };

                    if(data.message === 'Form submitted successfully.') {
                        document.querySelector('.form-loading-content').classList.remove('hidden');
                        document.querySelector('#crm-iframe').classList.add('hidden');
                        window.location.href = 'https://www.bcu.ac.uk/thank-you';
                    }
                },
            }, '#crm-iframe');
        });
    </script>
</div>
