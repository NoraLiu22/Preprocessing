                })

                function addCloseFnmel() {
                    closrBtn.on("click", function () {
                        $(this).parent().hide();
                        lhr.iframeRemove();
                    });
                };
                addCloseFnmel();
            },

            scrollbox1: function () {
                (function ($) {
                    $(window).on("load", function () {
                        $(".jxhrb ul").mCustomScrollbar();
                    });
                })(jQuery);
            },
