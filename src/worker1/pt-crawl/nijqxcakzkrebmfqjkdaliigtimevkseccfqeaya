        $(function () {
            // setTimeout(function () {
            //     window.scrollTo(0, 0); //滚动到顶部
            // }, 100)
            //createSnow('', 100);
            
            lhr.gdsgSwiper(); //感动时光
            //lhr.jcspSwiper(); //精彩视频
            lhr.ttSwiper(); //精彩视频
            lhr.gdtjg(); //感动推荐官
            lhr.ztq(); //主题曲  
           //lhr.jxhrlSwiper(); //主题曲
