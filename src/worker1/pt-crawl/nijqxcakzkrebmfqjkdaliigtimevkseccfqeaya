        var lhr = {
            // 全局视频插入
            videoInsert: function (v_url, obj) {
                this.iframeRemove();
                var vid = '<video src="' + v_url +
                    '" loop="loop" autoplay="autoplay" controls="controls" class="video0225"></video>'
                obj.find(".video_box").show();
                obj.find(".video_box").prepend(vid)
            },
            //删除所有iframe视频
            iframeRemove: function () {
                vVideo = $("body .video_box")
                vVideo.each(function () {
                    $(this).find("video").attr('src', '').remove();
                    $(this).hide();
                });
            },
            
            gdsgSwiper: function () {
                var _gdsgSwiper = new Swiper('.newSwiper .swiper-container', {
                    nextButton: '.newSwiper .swiper-button-next',
                    prevButton: '.newSwiper .swiper-button-prev',
                    // autoplay: 5000,
                    slidesPerView: 1.5,
                    loop: true,
                    centeredSlides: true,
                     spaceBetween:50,
                    effect: 'coverflow',
                    // Enable 3D Flow
                    coverflow: {
                        rotate: 0,
                        stretch:0,
                        depth: 300,
                        modifier:3,
                        // slideShadows: true
                    }
                });
            },
            ttSwiper: function () {
                var viewSwiper = new Swiper('.tt_view .swiper-container', {
                    onSlideChangeStart: function () {
                        updateNavPosition()
                    }
                })

                $('.tt_view .arrow-left,.tt_preview .arrow-left').on('click', function (e) {
                    e.preventDefault()
                    if (viewSwiper.activeIndex == 0) {
                        viewSwiper.slideTo(viewSwiper.slides.length - 1, 1000);
                        return
                    }
                    viewSwiper.slidePrev()
                })
                $('.tt_view .arrow-right,.tt_preview .arrow-right').on('click', function (e) {
                    e.preventDefault()
                    if (viewSwiper.activeIndex == viewSwiper.slides.length - 1) {
                        viewSwiper.slideTo(0, 1000);
                        return
                    }
                    viewSwiper.slideNext()
                })

                var previewSwiper = new Swiper('.tt_preview .swiper-container', {
                    //visibilityFullFit: true,
                    slidesPerView: 'auto',
                    allowTouchMove: false,
                    direction: 'vertical',
                    
                    onTap: function () {
                        viewSwiper.slideTo(previewSwiper.clickedIndex)
                    }
                })

                function updateNavPosition() {
                    $('.tt_preview .active-nav').removeClass('active-nav')
                    var activeNav = $('.tt_preview .swiper-slide').eq(viewSwiper.activeIndex).addClass(
                        'active-nav')
                    if (!activeNav.hasClass('swiper-slide-visible')) {
                        if (activeNav.index() > previewSwiper.activeIndex) {
                            var thumbsPerNav = Math.floor(previewSwiper.width / activeNav.width()) - 1
                            previewSwiper.slideTo(activeNav.index() - thumbsPerNav)
                        } else {
                            previewSwiper.slideTo(activeNav.index())
                        }
                    }
                }
                var videoSlide = $(".tt_view .swiper-slide");
                var videoBoxmel = $(".tt_view .video0225-box"),
                    closrBtn = $(".tt_view .video0225-close"),
                    veoPlayer = $(".mul-play");
                videoSlide.each(function () {
                    var vsrc = $.trim($(this).attr("data-vsrc"));
                    if (vsrc.indexOf("mp4") !== -1) {
                        $(this).find(".mul-play").show();
                    }
                });
                $(".tt_view .mul-play").on("click", function () {
                    videoBoxmel.show();
                    //iframemel.show();
                    var vsrc = $.trim($(this).parents(".swiper-slide").attr("data-vsrc"));
                    console.log(vsrc);
                    //iframemel.attr("src", vsrc);
                    lhr.videoInsert(vsrc, $(".tt_view"));
                })

                function addCloseFnmel() {
                    closrBtn.on("click", function () {
                        $(this).parent().hide();
                        lhr.iframeRemove();
                    });
                };
                addCloseFnmel();
            },
            jcspSwiper: function () {
                var viewSwiper = new Swiper('.view .swiper-container', {
                    onSlideChangeStart: function () {
                        updateNavPosition()
                    }
                })

                $('.view .arrow-left,.preview .arrow-left').on('click', function (e) {
                    e.preventDefault()
                    if (viewSwiper.activeIndex == 0) {
                        viewSwiper.slideTo(viewSwiper.slides.length - 1, 1000);
                        return
                    }
                    viewSwiper.slidePrev()
                })
                $('.view .arrow-right,.preview .arrow-right').on('click', function (e) {
                    e.preventDefault()
                    if (viewSwiper.activeIndex == viewSwiper.slides.length - 1) {
                        viewSwiper.slideTo(0, 1000);
                        return
                    }
                    viewSwiper.slideNext()
                })

                var previewSwiper = new Swiper('.preview .swiper-container', {
                    //visibilityFullFit: true,
                    slidesPerView: 'auto',
                    allowTouchMove: false,
                     
                    onTap: function () {
                        viewSwiper.slideTo(previewSwiper.clickedIndex)
                    }
                })

                function updateNavPosition() {
                    $('.preview .active-nav').removeClass('active-nav')
                    var activeNav = $('.preview .swiper-slide').eq(viewSwiper.activeIndex).addClass(
                        'active-nav')
                    if (!activeNav.hasClass('swiper-slide-visible')) {
                        if (activeNav.index() > previewSwiper.activeIndex) {
                            var thumbsPerNav = Math.floor(previewSwiper.width / activeNav.width()) - 1
                            previewSwiper.slideTo(activeNav.index() - thumbsPerNav)
                        } else {
                            previewSwiper.slideTo(activeNav.index())
                        }
                    }
                }
                var videoSlide = $(".view .swiper-slide");
                var videoBoxmel = $(".view .video0225-box"),
                    closrBtn = $(".view .video0225-close"),
                    veoPlayer = $(".mul-play");
                videoSlide.each(function () {
                    var vsrc = $.trim($(this).attr("data-vsrc"));
                    if (vsrc.indexOf("mp4") !== -1) {
                        $(this).find(".mul-play").show();
                    }
                });
                $(".view .mul-play").on("click", function () {
                    videoBoxmel.show();
                    //iframemel.show();
                    var vsrc = $.trim($(this).parents(".swiper-slide").attr("data-vsrc"));
                    console.log(vsrc);
                    //iframemel.attr("src", vsrc);
                    lhr.videoInsert(vsrc, $(".view"));
                })

                function addCloseFnmel() {
                    closrBtn.on("click", function () {
                        $(this).parent().hide();
                        lhr.iframeRemove();
                    });
                };
                addCloseFnmel();
            },
            gdtjg: function () {
                var $tab_li = $('.tab1 ul li');
                $tab_li.hover(function () {
                    $(this).addClass('selected').siblings().removeClass('selected');
                    var index = $tab_li.index(this);
                    $('div.tab_box > div').eq(index).show().siblings().hide();
                });
                $(".tab_menu").slimScroll({
                    width: '435px',
                    height: '486px',
                    size: '3px',
                    alwaysVisible: true,
                    disableFadeOut: false,
                    railVisible: true,
                });
            },
            ztq: function () {
                var videoBoxmel = $(".part05_rightCon .video0225-box"),
                    closrBtn = $(".part05_rightCon .video0225-close"),
                    veoPlayer = $(".mul-play");
                $(".part05_rightCon .mul-play").on("click", function () {
                    videoBoxmel.show();
                    //iframemel.show();
                    var vsrc = $.trim($(this).parents(".part05_rightCon").attr("data-vsrc"));
                    console.log(vsrc);
                    //iframemel.attr("src", vsrc);
