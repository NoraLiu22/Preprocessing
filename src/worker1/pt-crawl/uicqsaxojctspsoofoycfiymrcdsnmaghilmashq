    <div class="grid-12 region region-footer-first" id="region-footer-first">
  <div class="region-inner region-footer-first-inner">
    <div class="block block-bean block-join-our-list block-bean-join-our-list odd block-without-title" id="block-bean-join-our-list">
  <div class="block-inner clearfix">
                
    <div class="content clearfix">
      <a href="#time-modal" class="time-modal-trigger" style="display: none;">Display Modal</a>
<div id="time-modal" class="entity entity-bean bean-modal clearfix" about="/block/join-our-list" class="entity entity-bean bean-modal" style="display: none;">
        <div class="content">
    
<div class="field field-name-field-content field-type-text-long field-label-hidden">
    <div class="field-items">
          <div class="field-item even"><h4 style="text-align: center;"><a href="/web/20230928003722/https://www.migrationpolicy.org/signup/source"><img alt="" src="/web/20230928003722im_/https://www.migrationpolicy.org/sites/default/files/resize/source_images/Source%20Modal%20Redesign-FINAL-1000x320.png" style="width: 1000px; height: 320px;" width="1000" height="320"/></a></h4>

<p> </p>
</div>
      </div>
</div>
    <script data-cfasync="false" src="/web/20230928003722js_/https://www.migrationpolicy.org/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
      setTimeout(function() {
        var $ = jQuery;
        $(document).ready(function() {

          // Helper function to create cookies
          function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
          }

          // Set cookie to remember that we've already seen and closed the modal
          function timerModalClosed() {
            setCookie("time_modal", 'closed', 30);
            ga('send', 'event', 'modal', 'closed', '', 1);
          }

          // Function that sets modal with delay
          function showModal() {
            // Check to see if modal window exists
            if ($('.time-modal-trigger').length) {
              // Initialize modaal
              $(".time-modal-trigger").modaal({
                after_close: timerModalClosed
              });

              // Fire modaal
              $('.time-modal-trigger').click();
              timerModalClosed();
            }
          };

          // Check to see if we have a time delay
          // Let's check cookies to see if we can show modal
          function checkCookie() {
            var gdpr = getCookie("cookieconsent_status");
            var timeModal = getCookie("time_modal");
            if (gdpr == "dismiss" && timeModal != "closed") {
                            setTimeout(showModal, 13000);
                          }
          };

          // Helper function to ready cookies
          function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
              var c = ca[i];
              while (c.charAt(0) == ' ') {
                c = c.substring(1);
              }
              if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
              }
            }
            return "";
          }

          checkCookie();
        });
      }, 1500)
    </script>
  </div>
</div>
    </div>
