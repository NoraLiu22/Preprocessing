<script type="text/javascript">
		if(typeof(initedToggleMenu) == 'undefined') {
			var initedToggleMenu = false;
		}
		require(['jquery',
						'Ves_Megamenu/js/megamenuGeneral'
			],function($){
				$('.price-box.price-final_price', $("#menu-top1714743332806849298")).each(function(){
					$(this).removeClass('price-box').addClass('price-box1');
					$(this).attr('data-role','priceBox1');
				})
				if(typeof(playMegamenuJs) == 'function') {
					//console.log("\nrun playMegamenuJs function...");
					jQuery(document).ready(function($) {
						playMegamenuJs($, 'menu-top1714743332806849298', 1, 'hover', 0);

					});
				} else {
					jQuery(document).ready(function($) {
													jQuery('#menu-top1714743332806849298-menu .ves-megamenu .level0').hover(function() {
								var mParentTop = jQuery(this).parents('.ves-megamenu').offset().top;
								var mParentHeight = $(this).parent().height();
								var mTop =  $(this).height();
								var mHeight = $(this).height();
								var mParent = $(this).parent();
								if (mHeight < mParentHeight) {
									mTop = $(this).offset().top - mParent.offset().top + mHeight;
								}
								$(this).children('.submenu').css({top:mTop});
							});
															jQuery('p').each(function() {
									var $this = $(this);
									if ($this.html().replace(/\s|&nbsp;/g, '').length == 0)
									$this.remove();
								});
							});
					var toggle_nav = $("#menu-top1714743332806849298").attr("data-toggle-mobile-nav");
					if(toggle_nav == true || toggle_nav == 'true' || toggle_nav==1){
						if(!initedToggleMenu) {
							var menuToogle = function () {
								if ($('html').hasClass('nav-open')) {
									$('html').removeClass('nav-open');
									setTimeout(function () {
										$('html').removeClass('nav-before-open');
									}, 300);
								} else {
									$('body').append('<div class="ves-overlay ves-overlaymenu-top1714743332806849298"></div>');
									$('html').addClass('nav-before-open');
									setTimeout(function () {
										$('html').addClass('nav-open');
									}, 42);
								}
							}
							$(document).on("click", ".action.nav-toggle", menuToogle);
							initedToggleMenu = true;
						}
						$(document).on("click", ".ves-overlaymenu-top1714743332806849298", function(){
							$('#menu-top1714743332806849298').css("left","");
							$('html').removeClass('ves-navopen');
							setTimeout(function () {
								$('html').removeClass('ves-nav-before-open');
							}, 300);
							$(this).remove();
							return false;
						});
					}

					$("#menu-top1714743332806849298 .dynamic-items li").hover(function(){
						$(this).parents(".dynamic-items").find("li").removeClass("dynamic-active");
						$(this).addClass("dynamic-active");
						var id = $(this).data("dynamic-id");
						$("#menu-top1714743332806849298 ."+id).parent().find(".dynamic-item").removeClass("dynamic-active");
						$("#menu-top1714743332806849298 ."+id).addClass("dynamic-active");
					});
					var mImg = '';
					$("#menu-top1714743332806849298 img").hover(function(){
						mImg = '';
						mImg = $(this).attr('src');
						if ($(this).data('hoverimg')){
							$(this).attr('src',$(this).data('hoverimg'));
						}
					},function(){
						$(this).attr('src',mImg);
					});
					$("#menu-top1714743332806849298 li a").hover(function(){
						$(this).css({
							"background-color": $(this).data("hover-bgcolor"),
							"color": $(this).data("hover-color")
						});
					}, function(){
						$(this).css({
							"background-color": $(this).data("bgcolor"),
							"color": $(this).data("color")
						});
					});
					$(window).on("resize load", function(){

						if($("#menu-top1714743332806849298").data("disable-bellow") && $("#menu-top1714743332806849298").data("disable-above")){

							if (($(window).width() <= $("#menu-top1714743332806849298").data("disable-bellow")) || ($(window).width() >= $("#menu-top1714743332806849298").data("disable-above"))){
								$("#menu-top1714743332806849298-menu").hide();
							}else{
								$("#menu-top1714743332806849298-menu").show();
							}

							$("#menu-top1714743332806849298").find("li").each(function(index, element){
								if (($(window).width() <= $(this).data("disable-bellow")) || ($(window).width() >= $(this).data("disable-above"))){
									$(this).addClass("hidden");
								} else if ($(this).hasClass("hidden")){
									$(this).removeClass("hidden");
								}
							});

						} else if($("#menu-top1714743332806849298").data("disable-bellow") && !$("#menu-top1714743332806849298").data("disable-above")) {
							if ($(window).width() <= $("#menu-top1714743332806849298").data("disable-bellow")){
								$("#menu-top1714743332806849298-menu").hide();
							}else{
								$("#menu-top1714743332806849298-menu").show();
							}

							$("#menu-top1714743332806849298").find("li").each(function(index, element){
								if ($(window).width() <= $(this).data("disable-bellow")){
									$(this).addClass("hidden");
								}else if ($(this).hasClass("hidden")){
									$(this).removeClass("hidden");
								}
							});
						} else if($("#menu-top1714743332806849298").data("disable-above") && !$("#menu-top1714743332806849298").data("disable-bellow")) {
							if ($(window).width() >= $("#menu-top1714743332806849298").data("disable-above")){
								$("#menu-top1714743332806849298-menu").hide();
							}else{
								$("#menu-top1714743332806849298-menu").show();
							}

							$("#menu-top1714743332806849298").find("li").each(function(index, element){
								if($(window).width() >= $(this).data("disable-above")) {
									$(this).addClass("hidden");
								} else if ($(this).hasClass("hidden")){
									$(this).removeClass("hidden");
								}
							});
						}

						if ($(window).width() >= 768 && $(window).width() <= 1024){
							$("#menu-top1714743332806849298 .nav-anchor").off().click(function(){
								var iParent = $(this).parent('.nav-item');
								iParent.addClass("clicked");
								if ($(iParent).children('.submenu').length == 1){
									iParent.trigger('hover');
									if (iParent.hasClass('submenu-alignleft') || iParent.hasClass('submenu-alignright')){
										if ((iParent.offset().left + iParent.find('.submenu').eq(0).width()) > $(window).width()){
											iParent.find('.submenu').eq(0).css('max-width','100%');
											iParent.css('position','static');
										}
									}
									return false;
								}
							});
						}else{
							$("#menu-top1714743332806849298").find('.submenu').css('max-width','');
							$("#menu-top1714743332806849298").find('.submenu-alignleft').css('position','relative');
						}
						if ($(window).width() <= 768){
							$('.sections.nav-sections').removeAttr( "style" )
							$("#menu-top1714743332806849298").addClass("nav-mobile");

							/*
							$("#menu-top1714743332806849298 .nav-anchor").off().click(function(){

								$("#menu-top1714743332806849298 .nav-item").removeClass("item-active");
								var parent = $(this).parents(".nav-item").eq(0);
								$(this).toggleClass('item-active');
								$(parent).find(".submenu").eq(0).slideToggle();
								return false;
							});
							*/

						}else{
							$("#menu-top1714743332806849298").find(".submenu").css({'display':''});
							$("#menu-top1714743332806849298").find("div").removeClass("mbactive");
							$("#menu-top1714743332806849298").removeClass("nav-mobile");

							/*
							$("#menu-top1714743332806849298 .nav-anchor").off().click(function(){
								if("#" != $(this).attr("href"))
									window.location.url = $(this).attr("href");
								return true;
							});
							*/

						}
					}).resize();
					//Toggle mobile menu
					$('.ves-megamenu-mobile #menu-top1714743332806849298 .opener').on('click', function(e) {
						e.preventDefault();
						$("#menu-top1714743332806849298 .nav-item").removeClass("item-active");
						var parent = $(this).parents(".nav-item").eq(0);
						$(this).toggleClass('item-active');
						$(parent).find(".submenu").eq(0).slideToggle();
						$(parent).find(".submenu").eq(0).toggleClass('submenu-active');
						return false;
					});


					
									}
			});
	</script>
