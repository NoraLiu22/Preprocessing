<script>
    function finishPrint() {
        let lines = jQuery('.panel.header .header-name-cont .title').eq(0).outerHeight();
        if(lines > 20) {
            jQuery('.panel.header #lib-list').addClass('big');
            
        }
        jQuery('.header-name-cont').addClass('active');
        if(jQuery('.library-account-dashboard').length > 0) {
            if(!jQuery('.page-title-wrapper').hasClass('add')){
                jQuery('span.library-name').html(jQuery('.title.one').html());
                jQuery('.page-title-wrapper').addClass('add');
            }
        }
    }
</script>
