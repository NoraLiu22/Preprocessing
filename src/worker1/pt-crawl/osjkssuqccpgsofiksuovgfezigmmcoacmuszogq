    <script type="text/javascript">
        // Get the modal
        var modal = document.getElementById("19-635Audio");

        // Get the button that opens the modal
        var btn = document.getElementById("audioBtn");

        // Get the button that closes the modal
        var closebtn = document.getElementById("closeBtn");

        // Get the <span> element that closes the modal
        var span = document.getElementById("closeit");

        // When the user clicks the button, open the modal
        btn.onclick = function () {
            autoplayAudio(modal);
        }

        // When the user clicks on <span> (x), close the modal
        span.onclick = function () {
            stopAudio(modal);
        }

        // When the user clicks on close button, close the modal
        closebtn.onclick = function () {
            stopAudio(modal);
        }

        // When the user clicks anywhere outside of the modal, close the modal
        window.onclick = function (event) {
            if (event.target == modal) {
                stopAudio(modal);
            }
        }

        /**
         * Autoplay a HTML5 audio
         */
        var autoplayAudio = function (modal) {
            var audio = modal.querySelector('audio');

            // Bail if the modal doesn't have an audio
            if (!audio) return;

            // If a HTML5 audio, play it
            if (audio.tagName.toLowerCase() === 'audio') {
                audio.play();
                return;
            }

            // Add autoplay to audio src
            // audio.src: the current audio 'src' attribute
            // (audio.src.indexOf('?') < 0 ? '?' : '&'): if the audio.src already has query string parameters, add an "&", Otherwise, add a "?"
            // 'autoplay=1': add the autoplay parameter
            audio.src = audio.src + (audio.src.indexOf('?') < 0 ? '?' : '&') + 'autoplay=1';
        };

        /**
         * Stop a HTML5 audio
         */
        var stopAudio = function (modal) {
            var audio = modal.querySelector('audio');

            // Bail if modal doesn't have an audio
            if (!audio) return;

            // If a HTML5 audio, stop it and reload to begining
            if (audio.tagName.toLowerCase() === 'audio') {
                // audio.pause();
                audio.load();
                return;
            }

            // Remove autopaly from audio src
            audio.src = audio.sr.replace('&autoplay=1', '').replace('?autoplay=1', '');
        };

    </script>

    </div> 
