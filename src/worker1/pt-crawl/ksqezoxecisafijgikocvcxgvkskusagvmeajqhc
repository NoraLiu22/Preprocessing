    <script type="text/javascript">
        (function(window, $) {
            window.CarouselOne = function(div, divimg, divli, time) {
                var count = 0;
                var sum = $(divimg).length;
                var interval = time;

                function run() {
                    count++;
                    count = count == sum ? 0 : count;
                    $(divimg).eq(count).fadeIn(300).siblings(divimg).fadeOut(300);
                    $(divli).eq(count).addClass("hover").siblings('li').removeClass("hover");
                }
                var timer = setInterval(run, interval);
                $(div).hover(function() {
                    clearInterval(timer);
                }, function() {
                    timer = setInterval(run, interval);
                })
                $("body").on("mouseenter", divli, function() {
                    count = $(this).index();
                    count = count == sum ? 0 : count;
                    $(divimg).eq(count).fadeIn(300).siblings(divimg).fadeOut(300);
                    $(divli).eq(count).addClass("hover").siblings('li').removeClass("hover");
                });
            }
            $('#formid div').click(function() {
                if ($(".inputText").val().replace(/(^\s*)|(\s*$)/g, "") != "") {
                    $("#formid").submit();
                }
            })
        })(window, jQuery);
        (function(window, $) {
            $(function() {
                if (/Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)) {
                    window.location.href = 'http://keywords.china.org.cn/m/index.htm';
                }
                $('.news dl').each(function() {
                    $(this).hover(function() {
                        $(this).css('box-shadow', '0px 0px 30px rgba(35,52,88,.3)');
                    }, function() {
                        $(this).css('box-shadow', 'none');
                    });
                });
                $('.list').each(function() {
                    $(this).click(function() {
                        $(".list").removeClass("btn");
                        $(this).addClass("btn");
                        $(".show").hide();
                        n = $(this).index();
                        $(".show").eq(n).show();
                    })
                });
                $('.hot0').each(function() {
                    $(this).hover(function() {
                        $(this).css({
                            'box-shadow': '0px 0px 30px rgba(35,52,88,.3)',
                            'background-color': '#dd2626'
                        });
                        $(this).children('a').css('color', '#fff');
                        i = $(this).index();
                        $('.dot0').eq(i).css('background-image', 'url(http://images.china.cn/images1/en/keywords2020/yuan2_red.png)');
                    }, function() {
                        $(this).css({
                            'box-shadow': 'none',
                            'background-color': '#fff'
                        });
                        $(this).children('a').css('color', '#000');
                        i = $(this).index();
                        $('.dot0').eq(i).css('background-image', 'url(http://images.china.cn/images1/en/keywords2020/yuan2.png)');
                    });
                });
                $(".lunbo1_img").each(function() {
                    $(".lunbo1 .img_index ul").append($("<li></li>"));
                });
                if ($(".lunbo1_img").length == 1) {
                    $('.img_index').hide();
                }
                $('.lunbo1_img').eq(0).show();
                $('.lunbo1 .img_index ul li').eq(0).addClass("hover").siblings('li').removeClass("hover");
            });
            window.CarouselOne(".lunbo1", ".lunbo1_img", ".lunbo1 .img_index ul li", 5000);
        })(window, jQuery);
    </script>
