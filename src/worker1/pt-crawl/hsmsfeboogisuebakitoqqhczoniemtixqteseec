<script id="staticAssetJavascript0" type="text/javascript" src="/web/20211027212426js_/https://www.ebu.ch/generated-resources/db92956f85328e78c0d32d5e524.min.js?1635228229532"></script>

    <script type="text/javascript">
        function reloadMembers(url){
            
            var queryString =''
            if( url !==undefined && url!='' && url.indexOf('?')>0){
                queryString = "?"+url.substring(url.indexOf('?') + 1);
            }
            var refreshUrl = "/home/about-us/members/content/member-list.hidden.results.html.ajax" + queryString;
            var pageUrl = "/about/members" + queryString;
            $.get(refreshUrl, function(data){
                $('#module-3c1e22f2-c80a-48af-a74d-26d81a3876f1').replaceWith(data);
                try{
                    window.history.replaceState({'html':data,'pageTitle':''},'', pageUrl);
                }
                catch(err){
                    // Do nothing if browser does not support feature, yes I'm talking to you IE
                }
            });
        }

        function fullTextSearch(filter){
        // Loop through the comment list
            $(".members-list > div").each(function(){

                // If the list item does not contain the text phrase fade it out
                //log.info($(this).text());
                if ($(this).text().search(new RegExp(filter, "i")) < 0) {
                    $(this).fadeOut();

                // Show the list item if the phrase matches and increase the count by 1
                } else {
                    $(this).show();
                }
            });
        }


        // Read a page's GET URL variables and return them as an associative array.
        function getUrlVars()
        {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for(var i = 0; i < hashes.length; i++)
            {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }


        $(document).on('click', '.filter-3c1e22f2-c80a-48af-a74d-26d81a3876f1 a', function (event) {
            event.preventDefault();
            reloadMembers($(this).attr('href'));
        });

        $(document).on('change', '.filter-3c1e22f2-c80a-48af-a74d-26d81a3876f1 #member-search-country', function (event) {
            event.preventDefault();

            var urlVars = getUrlVars();

            urlVars['country'] = $(this).val();

            var params = [];

            // Rebuild url with current params and new country param
            var paramsStr = 'type,country,keyword',
                separated = paramsStr.split(',');
            $.each(separated, function(index, chunk){
                if(urlVars[chunk]){
                    params.push({name: chunk, value:urlVars[chunk]});
                }
            });
            reloadMembers(window.location.pathname+(params.length > 0?'?':'')+$.param(params));

        });

        $(document).on('keyup', '.filter-3c1e22f2-c80a-48af-a74d-26d81a3876f1 #member-search-keyword', function (event) {
            // Retrieve the input field text and reset the count to zero
            var filter = $(this).val();
            fullTextSearch(filter);
        });

    </script>

