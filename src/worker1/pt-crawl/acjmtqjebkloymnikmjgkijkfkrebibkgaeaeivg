      <div class="block-content block__content">
      <div><div class="view view-homepage view-id-homepage view-display-id-block_top_story js-view-dom-id-a6e6fe7ce2e7bafa630c7d6b8288b0262aa1c81c2b5f96062f0bfbc903f2f276">
  
    
      
