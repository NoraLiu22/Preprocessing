ање и развој &quot;},{&quot;Id&quot;:&quot;1002&quot;,&quot;AreaId&quot;:&quot;10&quot;,&quot;Name&quot;:&quot;Иновације &quot;},{&quot;Id&quot;:&quot;090201&quot;,&quot;AreaId&quot;:&quot;09&quot;,&quot;Name&quot;:&quot;Годишњи национални рачуни &quot;},{&quot;Id&quot;:&quot;090202&quot;,&quot;AreaId&quot;:&quot;09&quot;,&quot;Name&quot;:&quot;Квартални национални рачуни&quot;},{&quot;Id&quot;:&quot;090203&quot;,&quot;AreaId&quot;:&quot;09&quot;,&quot;Name&quot;:&quot;Економски рачуни пољопривреде&quot;},{&quot;Id&quot;:&quot;090204&quot;,&quot;AreaId&quot;:&quot;09&quot;,&quot;Name&quot;:&quot;Регионални рачуни&quot;},{&quot;Id&quot;:&quot;1101&quot;,&quot;AreaId&quot;:&quot;11&quot;,&quot;Name&quot;:&quot;Предшколско васпитање и образовање &quot;},{&quot;Id&quot;:&quot;1102&quot;,&quot;AreaId&quot;:&quot;11&quot;,&quot;Name&quot;:&quot;Основно образовање &quot;},{&quot;Id&quot;:&quot;1103&quot;,&quot;AreaId&quot;:&quot;11&quot;,&quot;Name&quot;:&quot;Средње образовање &quot;},{&quot;Id&quot;:&quot;1104&quot;,&quot;AreaId&quot;:&quot;11&quot;,&quot;Name&quot;:&quot;Високо образовање&quot;},{&quot;Id&quot;:&quot;1105&quot;,&quot;AreaId&quot;:&quot;11&quot;,&quot;Name&quot;:&quot;Образовање одраслих и целоживотно учење &quot;},{&quot;Id&quot;:&quot;130003&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Попис пољопривреде, 2023&quot;},{&quot;Id&quot;:&quot;1301&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Годишња биљна производња&quot;},{&quot;Id&quot;:&quot;1302&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Сточарство&quot;},{&quot;Id&quot;:&quot;1303&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Рибарство &quot;},{&quot;Id&quot;:&quot;1304&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Шумарство&quot;},{&quot;Id&quot;:&quot;1305&quot;,&quot;AreaId&quot;:&quot;13&quot;,&quot;Name&quot;:&quot;Вишегодишњи засади&quot;},{&quot;Id&quot;:&quot;130002&quot;,&quot;AreaId&quot;:&quot;13&quot;