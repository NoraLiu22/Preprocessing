    <div class="pw-big-photo__image">
        
        <picture style="" class="image-wrapper"><source srcset="https://ocdn.eu/pulscms-transforms/1/H_bk9kpTURBXy9lOTE0ODcyMjRhZjQxY2QxZWM3MmE0ZTg1ZmU4YTQ3MS5qcGeSlQMAzQHszRJyzQpikwXNAW_M9N4AAaEwBQ" type="image/webp"><img width="367" height="244" alt="" data-cy="article-image" src="https://ocdn.eu/pulscms-transforms/1/B_HktkpTURBXy9lOTE0ODcyMjRhZjQxY2QxZWM3MmE0ZTg1ZmU4YTQ3MS5qcGeSlQMAzQHszRJyzQpikwXNAW_M9A" class="image" loading="lazy" decoding="async"></picture> 
