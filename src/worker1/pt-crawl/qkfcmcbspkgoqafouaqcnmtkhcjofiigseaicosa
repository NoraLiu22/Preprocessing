            googletag.cmd.push(function () {
              gptSlots.push(
                slot['6f8f1b41-6046-426a-9ce5-ad9550e362ef'] = googletag
                  .defineSlot(adUnit, gptconfig.adSizes, '6f8f1b41-6046-426a-9ce5-ad9550e362ef')
