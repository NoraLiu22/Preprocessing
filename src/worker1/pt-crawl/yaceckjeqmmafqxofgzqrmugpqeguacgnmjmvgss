      <script src="/static/js/lib/jquery-2.1.1.min.js"></script>
      <script src="/static/js/lib/bootstrap.min.js"></script>
    
  <script src="https://js.stripe.com/v3/"></script>
  <script>
    $(document).ready(function () {

      var form = $("#payment-form");

      var CURRENCY = {
        // These must match form definitions.
        US_Dollar: {
          code: "USD",
          paypal_id: 'paypal@metabrainz.org'
        },
        Euro: {
          code: "EUR",
          paypal_id: 'paypal@metabrainz.org'
        }
      };

      //////////////////
      // PAYMENT BUTTONS
      //////////////////

      var buttons = {
        stripe: $('#btn-stripe'),
        paypal: $('#btn-paypal')
      };

      function setButtonsState(enabled) {
        if (enabled === true) {
          $.each(buttons, function( index, button ) {
            button.removeAttr('disabled');
          });
          updateButtonState();

        } else { // Disabled
          $.each(buttons, function( index, button ) {
            button.attr('disabled', 'disabled');
          });
        }
      }

      function updateButtonState() {
        // This function disables or enables payment buttons depending on what
        // features they support.

        // We don't support payments in EUR via Stripe currently.
        if (selectedCurrency === CURRENCY.Euro) {
          buttons.stripe.attr('disabled', 'disabled');
        } else {
          buttons.stripe.removeAttr('disabled');
        }
      }


      ///////////////
      // AMOUNT INPUT
      ///////////////
      var MAX_AMOUNT = 900000;
      var amount = $("#input-amount");
      var bigAmountInfo = $("#big-amount-info");
      amount.on('input', function () {
        var value = amount.val();
        if (value > MAX_AMOUNT) {
          bigAmountInfo.show();
          setButtonsState(false);
        } else {
          bigAmountInfo.hide();
          setButtonsState(true);
        }
      });

      /////////////////
      // CURRENCY INPUT
      /////////////////
      var selectedCurrency = CURRENCY.US_Dollar;
      $('input:radio[name=currency]').change(function() {
        if (this.value.toUpperCase() === CURRENCY.US_Dollar.code.toUpperCase()) {
          selectedCurrency = CURRENCY.US_Dollar;
        } else if (this.value.toUpperCase() === CURRENCY.Euro.code.toUpperCase()) {
          selectedCurrency = CURRENCY.Euro;
        } else {
          console.error("Unknown currency:", this.value)
        }
        updateButtonState();
      });

      ///////////////
      // EDITOR INPUT
      ///////////////
      var editor = $("#input-editor");
      var editorFormGroup = $("#input-editor-fg");

      function setEditorTooltip(text, state) {
        editor.tooltip('destroy');
        editor.tooltip({title: text});
        editorFormGroup.removeClass(function (index, css) {
          return (css.match(/(^|\s)has-\S+/g) || []).join(' ');
        });
        if (editor !== null) {
          editorFormGroup.addClass('has-' + state);
        }
      }

      var defaultEditorTooltipText = "Enter your MusicBrainz username to have Picard/MusicBrainz stop nagging you";
      setEditorTooltip(defaultEditorTooltipText, null);

      editor.on('input', function () { setEditorTooltip(defaultEditorTooltipText, null); });
      editor.change(function () {
        var value = editor.val();
        if (value.length == 0) {
          setEditorTooltip(defaultEditorTooltipText, null);
        } else {
          $.ajax({
            url: "/donate/check-editor/",
            data: {q: value},
            dataType: "json",
            success: function (response) {
              if (response.error) {
                setEditorTooltip("MusicBrainz is currently unavailable. " +
                "Please, make sure that your username is correct.", 'warning');
              } else if (response.found == true && response.editor == value) {
                setEditorTooltip("Your donation will be associated with this account.", 'success');
              } else {
                setEditorTooltip("We can't find this account. Please, make sure it's correct", 'error');
              }
            }
          });
        }
      });


      ///////////
      // PAYMENTS
      ///////////

      // Stripe
      buttons.stripe.on('click', function(e) {
        setButtonsState(false);
        e.preventDefault();
        form.attr("action", "/donations/stripe/?donation=True");
        form.submit();
      });

      // PayPal
      buttons.paypal.on('click', function(e) {
        setButtonsState(false);
        
          form.attr("action", "https://www.paypal.com/cgi-bin/webscr");
        

        

        $('<input>').attr({type: 'hidden', name: 'business', value: selectedCurrency.paypal_id}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'no_shipping', value: '2'}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'return', value: 'https://metabrainz.org/payment/complete?is_donation=True'}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'cancel_return', value: 'https://metabrainz.org/payment/cancelled?is_donation=True'}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'notify_url', value: 'https://metabrainz.org/donations/paypal/ipn'}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'currency_code', value: selectedCurrency.code}).appendTo(form);

        if ($('#recurring-flag').is(":checked")) { // Recurring donations
          $('<input>').attr({type: 'hidden', name: 'cmd', value: '_xclick-subscriptions'}).appendTo(form);
          $('<input>').attr({type: 'hidden', name: 'item_name', value: 'Recurring donation to the MetaBrainz Foundation'}).appendTo(form);
          $('<input>').attr({type: 'hidden', name: 't3', value: 'M'}).appendTo(form); 
          $('<input>').attr({type: 'hidden', name: 'p3', value: '1'}).appendTo(form); 
          $('<input>').attr({type: 'hidden', name: 'src', value: '1'}).appendTo(form); 
          $('<input>').attr({type: 'hidden', name: 'sra', value: '1'}).appendTo(form); 

        } else {  // One-time donation
          $('<input>').attr({type: 'hidden', name: 'cmd', value: '_xclick'}).appendTo(form);
          $('<input>').attr({type: 'hidden', name: 'item_name', value: 'Donation to the MetaBrainz Foundation'}).appendTo(form);
        }

        
        if ($('#recurring-flag').is(":checked")) { // Recurring donations
          amount.attr('name', 'a3');
        } else {  // One-time donation
          amount.attr('name', 'amount');
        }

        editor.attr('name', 'custom');

        $('<input>').attr({type: 'hidden', name: 'on3', value: 'is_donation'}).appendTo(form);
        $('<input>').attr({type: 'hidden', name: 'os3', value: 'yes'}).appendTo(form);

        $('<input>').attr({type: 'hidden', name: 'on0', value: 'contact'}).appendTo(form);
        $("#can-contact-flag").attr('name', 'os0');
        $("#can-contact-flag").attr('value', 'yes'); 

        $('<input>').attr({type: 'hidden', name: 'on1', value: 'anonymous'}).appendTo(form);
        $("#anonymous-flag").attr('name', 'os1');
        $("#anonymous-flag").attr('value', 'yes'); 

        form.submit();
        return false;
      });

    });
  </script>


  </body>

</html> contentType 9 text/html url 33 https://metabrainz.org:443/donate responseCode 3 200 