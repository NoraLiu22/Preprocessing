<script>
  const playPauseBtn = document.querySelector('.playpause');
  const player = document.querySelector('.h-fvideo__video');
  player.removeAttribute('controls');
  playPauseBtn.onclick = function(changeState) {
    if (player.paused) {
      player.play();
      playPauseBtn.textContent = 'Pause';
    } else {
      player.pause();
      playPauseBtn.textContent = 'Play';
    }
  };
</script>

</section>
