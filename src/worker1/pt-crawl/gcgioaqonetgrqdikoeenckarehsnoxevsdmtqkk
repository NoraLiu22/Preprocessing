            if ($(window).width() > 991) {
                $("#sidenavbody").css("display", "block");
                $("#searchbox").css("display", "block");
            } else {
                $("#sidenavbody").css("display", "none");
                $("#searchbox").css("display", "none");
            };
        });

            function toggleMenushow() {
                var query = $("#sidenavbody");
                var isVisible = query.is(":visible");

                if (isVisible === true) {
                    $("#sidenavbody").css("display", "none");
                } else {
                    $("#sidenavbody").css("display", "block");
                }

            }
    </script>

