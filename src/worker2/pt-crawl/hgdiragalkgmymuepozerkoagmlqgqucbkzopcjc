	<script>
    var newImgSrc1 = "https://s3.amazonaws.com/brunicoextranet/Matrix/playback/WCAmlnyhTBS05zVLgAxQ_PB.34659.RockiesStreamingLive_300x600.gif";
    var newImgScr2 = "https://s3.amazonaws.com/brunicoextranet/Matrix/playback/kQtnnTYkSmDmrP7g03lI_PB.34659.RockiesStreamingLive_300x250.gif";
    if (window.screen.availWidth <= 767) {
      $('#halfpage_img').attr("src", newImgSrc1);
      $('#leaderboard_top_img_1').attr("src", newImgScr2);
