                        <h2>OUTtv takes Dragula to television</h2>
                        <div class="byline">
                          <div class="author">
                            <a href="/author/jp.pinto">
                            <!--<div class="author_avatar">
                                                                    </div>-->
                              <div class="author_name">
                              By Jordan&nbsp;Pinto                              </div>
                            </a>
                          </div>
                          <div class="date">
                          July 28, 2017                          </div>
                          <!--
                          <div id="shareButtons">
                                                    </div>
                          -->
                          <div id="editMarker">
                                                    </div>
                        </div>                      
                    </div>

                    <!--<div id="singleSocialMetaShare" class="theSinglePostSocialbox" >
