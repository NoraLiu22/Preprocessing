    <!-- scrolling/locking sitewrap -->
    <script>

      var x;
      var y;
      var fade;

      $(window).on('scroll', function() {
      x = $(window).scrollTop();
      y = 100 + parseInt(x / 10, 0);

      if (y > 125) {
      y = 125;

      }

      fade = "0." + ((y - 100) * 4);

      if ( fade == "0.100") {
      fade = "1.0";

      }

      //console.log(fade);

      $('#background').css('background-size', y + '% ');

      $('.banffad').css("opacity", fade);

      //console.log((x) + 'px');
      $('.banffad').css("top", (x) + 'px');



      //$("#background").css("background-position", "-" + (y/2) + "px", "0px");
      });

      $(window).load(function(){
      var sw = $("#advertisement_panel");
      var main = $("#headerFeature");
      if(main.offset() && main.offset().top){
        var contentBottom = main.offset().top + main.height();
      }
      var swpos = sw.offset();
      var stickermax = contentBottom - sw.outerHeight(true);

      if ($('#wpadminbar').length > 0) {
      var stickheight = "172";
      } else {
      var stickheight = "140";
      }

      $(window).scroll(function() {
      var windowpos = $(window).scrollTop();
      if (windowpos >= (swpos.top) && windowpos < stickermax) {
				    if ($('#wpadminbar').length > 0) { 
					    sw.addClass("stick2"); //stick it
				    } else {
					    sw.addClass("stick"); //stick it
				    }
			    } else if (windowpos >= stickermax) {
				    sw.removeClass("stick"); //un-stick
				    sw.css({position: "absolute", top: stickermax + "px"}); //set sticker right above the footer	
			    } else {
				    sw.css({position: "absolute", top: stickheight + "px"}); //set sticker right above the footer	
				    sw.removeClass(); //top of page
			    }
		    });
		    //alert(stickermax); //uncomment to show max sticker position value on doc.ready
	    });
    </script>
    
