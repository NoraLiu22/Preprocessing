<script>
var reader = new commonmark.Parser();
var writer = new commonmark.HtmlRenderer();
$(document).ready(function () {
  $('.cmark').each(function () {
    var text = $(this).html();
    var parsed = reader.parse(text);
    var result = writer.render(parsed); // result is a String
    var r = result.replace(/<p>(.*?)<\/p>/g,"$1");
    $(this).html("");
    $(this).html(r);
  });
});
</script>
