        var altura = window.innerHeight;
        var cargarMenuJson = function () {
            var jsonFile = getJsonFile();
            getJSON(jsonFile, function (err, json) {
                if (err !== null) {
                    console.log('Error al cargar el menú ' + err);
                } else {
                    var ulMenuLateral = document.createElement("ul");
                    ulMenuLateral.className = "lista-menu-completo";
                    for (var i in json) {
                        var menuItem = document.createElement("li");
                        menuItem.setAttribute("itemprop", "name");
                        menuItem.className = "menuItemPrincipal";
                        var enlace = document.createElement("a");
                        enlace.setAttribute("itemprop", "url");
                        switch (json[i].tipo) {
                            case "home":
                                menuItem.classList.add(json[i].cssAdicional);
                                menuItem.classList.add("europapressMenu");
                                enlace.href = location.protocol + "//" + window.location.hostname;
                                enlace.setAttribute("title", json[i].titulo);
                                var extraClass = "";
                                if (false) {
                                    extraClass = "movil";
                                }
                                enlace.innerHTML = json[i].ep1 + " <h4 class='rojo " + extraClass + "'>" + json[i].ep2 + "</h4>";
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                                break;
                            case "abonados":
                                menuItem.classList.add(json[i].cssAdicional);
                                enlace.href = json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                enlace.innerHTML = json[i].titulo;
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                                break;
                            case "buscadormovil":
                                if (false) {
                                    menuItem.classList.add(json[i].cssAdicional);
                                    var formBuscador = document.createElement("form");
                                    formBuscador.id = "formulario-buscador-estrechado";
                                    var inputBuscador = document.createElement("input");
                                    inputBuscador.id = "texto-buscador-estrechado";
                                    inputBuscador.setAttribute("type", "text");
                                    inputBuscador.setAttribute("placeholder", json[i].titulo);
                                    inputBuscador.setAttribute("required", "");
                                    inputBuscador.setAttribute("itemportal", json[i].portalId);
                                    inputBuscador.setAttribute("canalid", json[i].canalId);
                                    var iconoBuscador = document.createElement("span");
                                    iconoBuscador.id = "icono-buscador-estrechado";
                                    iconoBuscador.className = "icon-search icono-buscador-estrechado";
                                    iconoBuscador.setAttribute("onclick", "buscar()");
                                    formBuscador.appendChild(inputBuscador);
                                    formBuscador.appendChild(iconoBuscador);
                                    menuItem.appendChild(formBuscador);
                                    ulMenuLateral.appendChild(menuItem);
                                }
                                break;
                            case "dominioexterno":
                                if (json[i].hasOwnProperty('cssAdicional')) {
                                    menuItem.classList.add(json[i].cssAdicional);
                                }
                                var dominio = "www.";
                                enlace.href = location.protocol + "//" + dominio + json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                enlace.setAttribute("rel", "nofollow");
                                if (json[i].hasOwnProperty('bullet')) {
                                    enlace.innerHTML = "<span class='bullet'>" + json[i].titulo + "</span>";
                                } else {
                                    enlace.innerHTML = json[i].titulo;
                                }
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                                break;
                            case "webexterna":
                                enlace.href = json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                enlace.setAttribute("rel", "nofollow");
                                enlace.innerHTML = json[i].titulo;
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                                break;
                            case "desplegable":
                                menuItem.classList.add("acordeon");
                                var span = document.createElement("span");
                                span.innerHTML = json[i].titulo;
                                var ul = document.createElement("ul");
                                ul.className = "sub-secciones";
                                var jsonHijos = json[i].hijos;
                                for (var j in jsonHijos) {
                                    var menuItem2 = document.createElement("li");
                                    menuItem2.setAttribute("itemprop", "name");
                                    menuItem2.className = "menuItemSecundario";
                                    menuItem2.classList.add("noacordeon");
                                    var enlace2 = document.createElement("a");
                                    enlace2.setAttribute("itemprop", "url");
                                    enlace2.href = location.protocol + "//" + window.location.hostname + jsonHijos[j].url;
                                    enlace2.setAttribute("title", jsonHijos[j].titulo);
                                    enlace2.innerHTML = jsonHijos[j].titulo;
                                    menuItem2.appendChild(enlace2);
                                    ul.appendChild(menuItem2);
                                }
                                menuItem.appendChild(span);
                                ulMenuLateral.appendChild(menuItem);
                                ulMenuLateral.appendChild(ul);
                                break;
                            default:
                                if (json[i].hasOwnProperty('nomobile') && false) {
                                    break;
                                }
                                if (json[i].hasOwnProperty('cssAdicional')) {
                                    menuItem.classList.add(json[i].cssAdicional);
                                }
                                enlace.href = location.protocol + "//" + window.location.hostname + json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                if (json[i].hasOwnProperty('bullet')) {
                                    enlace.innerHTML = "<span class='bullet'>" + json[i].titulo + "</span>";
                                } else {
                                    enlace.innerHTML = json[i].titulo;
                                }
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                        }
                    }
                    var divMenuLateral = document.getElementById("menu-lateral");
                    var navMenuLateral = document.createElement("nav");
                    navMenuLateral.id = "menu";
                    navMenuLateral.className = "nav";
                    navMenuLateral.setAttribute("itemscope", "");
                    navMenuLateral.setAttribute("itemtype", "https://web.archive.org/web/20210412011406/http://schema.org/SiteNavigationElement");
                    navMenuLateral.style.maxHeight = parseInt(document.documentElement.clientHeight) - 40 + "px";
                    altura -= 60;
                    ulMenuLateral.style.height = altura + 'px';
                    navMenuLateral.appendChild(ulMenuLateral);
                    divMenuLateral.appendChild(navMenuLateral);
                    var itemsAcordeon = document.getElementsByClassName("acordeon");
                    for (var k = 0; k < itemsAcordeon.length; k++) {
                        itemsAcordeon[k].onclick = function () {
                            this.classList.toggle("active");
                            this.nextElementSibling.classList.toggle("show");
                        };
                    }
                    defer(function () {
                        $("#formulario-buscador-estrechado").submit(function () {
                            buscar();
                            return false;
                        });
                    });
                }
            });
        };





        var cargarMenuJsonEuropaPress = function () {
            var jsonFile = getJsonFile();
            getJSON(jsonFile, function (err, json) {
                if (err !== null) {
                    console.log('Error al cargar el menú ' + err);
                } else {


                    var container = document.createElement("div");
                    container.className = "container";

                    var navSecciones = document.createElement("nav");
                    navSecciones.className = "portales-menu-home"


                    var ulMenuLateral = document.createElement("ul");

                    var ulMenuLateralSecciones = document.createElement("ul");
                    ulMenuLateralSecciones.className = "lista-horizontal-menu";

                    var ulMenuLateralServicios = document.createElement("ul");
                    ulMenuLateralServicios.className = "lista-horizontal-menu";

                    var servicios = document.createElement("div");
                    servicios.className = "container caja-servicios-home";


                    var h3servicios = document.createElement("h3")
                    h3servicios.innerHTML = "Servicios";
                    servicios.appendChild(h3servicios);


                    for (var i in json) {
                        var menuItem = document.createElement("li");
                        menuItem.setAttribute("itemprop", "name");
                        //menuItem.className = "menuItem-home";
                        //var enlace = document.createElement("a");
                        //enlace.setAttribute("itemprop", "url");
                        switch (json[i].tipo) {
                            case "seccion":
                                menuItem.className = "menuItem-home";
                                var enlace = document.createElement("a");
                                enlace.setAttribute("itemprop", "url");
                                enlace.href = json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                enlace.innerHTML = json[i].titulo;
                                menuItem.appendChild(enlace);
                                ulMenuLateralSecciones.appendChild(menuItem);

                            case "autonomias":
                                var autonomias = document.createElement("div");
                                autonomias.className = "banderas-autonomias";

                                var h3autonomias = document.createElement("h3")
                                h3autonomias.innerHTML = "Autonomías";

                                var ulAutonomias = document.createElement("ul");
                                ulAutonomias.classList.add("tagged");



                                //menuItem.classList.add("tagged");
                                //var span = document.createElement("span");
                                //span.innerHTML = json[i].titulo;


                                var jsonHijos = json[i].hijos;
                                for (var j in jsonHijos) {
                                    var menuItem2 = document.createElement("li");
                                    menuItem2.setAttribute("itemprop", "name");

                                    var img = document.createElement("img");
                                    img.alt = jsonHijos[j].titulo;
                                    img.src = "https://web.archive.org/web/20210412011406/https://s01.europapress.net/imagenes/estaticos/banderas/" + jsonHijos[j].imagen;
                                    img.className = "bandera";

                                    var enlace2 = document.createElement("a");
                                    enlace2.setAttribute("itemprop", "url");
                                    enlace2.href = location.protocol + "//" + window.location.hostname + jsonHijos[j].url;
                                    enlace2.setAttribute("title", jsonHijos[j].titulo);
                                    enlace2.appendChild(img);

                                    var spantitulo = document.createElement("span");
                                    spantitulo.innerText = jsonHijos[j].titulo;

                                    enlace2.appendChild(spantitulo);


                                    menuItem2.appendChild(enlace2);
                                    ulAutonomias.appendChild(menuItem2);
                                }


                                //menuItem.appendChild(span);
                                //ulMenuLateral.appendChild(menuItem);


                                autonomias.appendChild(h3autonomias);
                                autonomias.appendChild(ulAutonomias);
                                break;

                            case "portales":
                                var portales = document.createElement("div");
                                portales.className = "canales-menu-home";

                                var h3portales = document.createElement("h3")
                                h3portales.innerHTML = "Portales temáticos";

                                var ulPortales = document.createElement("ul");
                                ulPortales.classList.add("lista-canales-menu");

                                var jsonHijos = json[i].hijos;
                                for (var j in jsonHijos) {
                                    var menuItem2 = document.createElement("li");
                                    menuItem2.setAttribute("itemprop", "name");
                                    menuItem2.className = "menu-home-" + jsonHijos[j].class;


                                    var enlace2 = document.createElement("a");
                                    enlace2.setAttribute("itemprop", "url");
                                    enlace2.href = jsonHijos[j].url;
                                    enlace2.setAttribute("title", jsonHijos[j].titulo);



                                    if (jsonHijos[j].imagen === undefined) {

                                        switch (jsonHijos[j].class) {

                                            case "epSocial":
                                                enlace2.innerHTML = "<span class='color-theme'>ep</span><span class='color-dark'>social</span>";
                                                break;
                                            case "latam":
                                                enlace2.innerHTML = "<span class='color-dark'>noti</span><span class='color-theme'>mérica</span>";
                                                break;
                                            case "turismo":
                                                enlace2.innerHTML = "<span class='color-dark'>ep</span><span class='color-theme'>turismo</span>";
                                                break;
                                            case "desconecta":
                                                enlace2.innerHTML = "<span class='color-dark'>Desconecta</span>";
                                                break;
                                            case "motor":
                                                enlace2.innerHTML = "<span class='color-dark'>ep</span><span class='color-theme'>motor</span>";
                                                break;
                                            case "epagrama":
                                                enlace2.innerHTML = "<span class='color-dark'>ep</span><span class='color-theme'>agro</span>";
                                                break;
                                            case "epdata":
                                                enlace2.innerHTML = "<span class='color-dark'>ep</span><span class='color-theme'>data</span>";
                                                break;
                                            case "tendencias":
                                                enlace2.innerHTML = "<span class='color-theme'>Tendencias</span>";
                                                break;
                                        }
                                    } else {
                                        var img = document.createElement("img");
                                        img.alt = jsonHijos[j].titulo;
                                        img.src = jsonHijos[j].imagen;

                                        enlace2.appendChild(img);


                                    }



                                    menuItem2.appendChild(enlace2);
                                    ulPortales.appendChild(menuItem2);
                                }


                                //menuItem.appendChild(span);
                                //ulMenuLateral.appendChild(menuItem);


                                portales.appendChild(h3portales);
                                portales.appendChild(ulPortales);

                                break;

                            case "Servicios":


                                menuItem.className = "menuItem-home";
                                var enlace = document.createElement("a");
                                enlace.setAttribute("itemprop", "url");
                                if (json[i].titulo == "Hemeroteca") {
                                    var fecha = new Date();
                                    var year = fecha.getFullYear();
                                    enlace.href = json[i].url + year + "/";

                                } else {
                                    enlace.href = json[i].url;

                                }
                                enlace.setAttribute("title", json[i].titulo);
                                enlace.innerHTML = json[i].titulo;
                                menuItem.appendChild(enlace);


                                ulMenuLateralServicios.appendChild(menuItem);
                                break;
                            default:
                                if (json[i].hasOwnProperty('nomobile') && false) {
                                    break;
                                }
                                if (json[i].hasOwnProperty('cssAdicional')) {
                                    menuItem.classList.add(json[i].cssAdicional);
                                }
                                enlace.href = location.protocol + "//" + window.location.hostname + json[i].url;
                                enlace.setAttribute("title", json[i].titulo);
                                if (json[i].hasOwnProperty('bullet')) {
                                    enlace.innerHTML = "<span class='bullet'>" + json[i].titulo + "</span>";
                                } else {
                                    enlace.innerHTML = json[i].titulo;
                                }
                                menuItem.appendChild(enlace);
                                ulMenuLateral.appendChild(menuItem);
                        }
                    }
                    var divMenuCompleto = document.getElementById("menu-completo");
                    divMenuCompleto.style.marginTop = "0";
                    //Secciones

                    navSecciones.appendChild(ulMenuLateralSecciones);
                    servicios.appendChild(ulMenuLateralServicios);

                    container.appendChild(navSecciones);
                    container.appendChild(autonomias);
                    container.appendChild(portales);
                    divMenuCompleto.appendChild(container);
                    divMenuCompleto.appendChild(servicios);




                    //var navMenuLateral = document.createElement("nav");
                    //navMenuLateral.id = "menu";
                    //navMenuLateral.className = "nav";
                    //navMenuLateral.setAttribute("itemscope", "");
                    //navMenuLateral.setAttribute("itemtype", "https://web.archive.org/web/20210412011406/http://schema.org/SiteNavigationElement");
                    //navMenuLateral.style.maxHeight = parseInt(document.documentElement.clientHeight) - 40 + "px";
                    //altura -= 60;
                    //ulMenuLateral.style.height = altura + 'px';
                    //navMenuLateral.appendChild(ulMenuLateral);
                    //divMenuLateral.appendChild(navMenuLateral);


                }
            });
        };


        function getJsonFile() {
            var jsonName = "/menus/MenuEP.json";
            var url = window.location.pathname;
            var nombreHost = window.location.host;
            var objUrl = new URL(document.URL);
            var canalid = objUrl.searchParams.get("ch");

            if (!false) {
                jsonName = "/menus/europapress.json";
            }



            if (nombreHost.includes("notimerica.com") || canalid === "527") {
                jsonName = "/menus/notimerica.json";
            } else if (nombreHost.includes("hacerfamilia.com") || canalid === "861") {
                jsonName = "/menus/hacerfamilia.json";
            } else if (nombreHost.includes("aldia.cat")) {
                jsonName = "/menus/aldia.json";
            } else if (nombreHost.includes("culturaocio.com") || canalid === "777") {
                jsonName = "/menus/culturaocio.json";
            } else if (nombreHost.includes("infosalus.com") || canalid === "74") {
                jsonName = "/menus/infosalus.json";
            } else if (nombreHost.includes("mercadofinanciero.com") || canalid === "1170") {
                jsonName = "/menus/mercadofinanciero.json";
            }




            return jsonName;
        }

        function buscar() {
            var inputTexto = document.getElementById('texto-buscador-estrechado');
            if (inputTexto.value.length > 0) {
                window.location.href = ['/buscador.aspx?buscar=', inputTexto.value, '&buscarCodificado=', encode64(inputTexto.value), '&portal=', inputTexto.getAttribute("itemportal"), "&ch=", inputTexto.getAttribute("canalid"), '&mostrar=false&btn=true'].join("");
            }
            return false;
        }





        function defer(method) {
            if (window.jQuery) {
                method();
            } else {
                setTimeout(function () {
                    defer(method);
                }, 50);
            }
        }
    </script>





