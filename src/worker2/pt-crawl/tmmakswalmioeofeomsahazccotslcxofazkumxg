                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>&#xA0;cm</mtext>
                </mrow>
              </mrow>
              <mrow>
                <mn>980</mn>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>&#xA0;cm</mtext>
                </mrow>
