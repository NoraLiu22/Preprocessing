                  <mo>&#x2212;<!-- − --></mo>
                  <mn>4</mn>
                </mrow>
              </msup>
            </mrow>
            <mrow>
              <mn>2</mn>
              <mo>&#x00D7;<!-- × --></mo>
              <msup>
                <mn>10</mn>
                <mrow class="MJX-TeXAtom-ORD">
                  <mo>&#x2212;<!-- − --></mo>
                  <mn>2</mn>
                </mrow>
              </msup>
            </mrow>
