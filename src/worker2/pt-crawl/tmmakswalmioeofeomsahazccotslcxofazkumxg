                <mrow class="MJX-TeXAtom-ORD">
                  <mi mathvariant="bold">u</mi>
                </mrow>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <mn>0</mn>
              </mtd>
            </mtr>
            <mtr>
              <mtd>
                <msub>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi mathvariant="bold">u</mi>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mtext>bd</mtext>
                  </mrow>
                </msub>
              </mtd>
              <mtd>
