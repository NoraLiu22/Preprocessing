ectedCourseOptions:{}},assessments:{assessmentId:b,assessmentType:"short"},articles:{bitesizeCareers:[],statementGuides:[]},apprenticeships:{apprenticeships:[]},accommodation:{accommodation:{},accommodationUnis:{},accommodationSlug:{},accommodationUrl:{}}},serverRendered:o,routePath:"\u002Fsubject-widget",config:{app:b,siteName:"UCAS Explore",siteDescription:"Not sure what to do? Explore subjects, locations, universities, and colleges.",apiUrl:eN,searchDomain:eO,slugRegex:/^(?=.{3,40}$)[a-z][a-z0-9]*(?:-[a-z0-9]+)*$/,publicPath:"https:\u002F\u002Fweb.archive.org\u002Fweb\u002F20230131091501\u002Fhttps:\u002F\u002Fexplore.ucas.com\u002F",_app:{basePath:eR,assetsPath:eR,cdnURL:"https:\u002F\u002Fweb.archive.org\u002Fweb\u002F20230131091501\u002Fhttps:\u002F\u002Fexplore.ucas.com\u002F_nuxt\u002F"},embedded:o}}}("",false,null,"city-guide","Finance","2023",0,"0","2023-2024","1","100107","Full-time","90","Main Site",true,"N\u002FA","N300","3 Years","BSc (Hons)","Bachelor of Science (with Honours) - BSc (Hons)","See entry requirements","4 Years","region-guide","London","95","23000","79","65","72","30000","75","Anthropology","Journalism","Nursing","country-guide","DMM","70","26500","80","DDM","92","32000","Belfast","DDM - DDD","25000","60","Accountancy","Business","Maths","Economics","Philosophy","Creative Writing","Criminology","Theology","Canterbury","71","87","Poole","50.742979092900","-1.897168118900","BH12 5BB","BA (Hons)","Bachelor of Arts (with Honours) - BA (Hons)","BBB - ABB","Durham","DDD","10007143","6916","N305","83","https:\u002F\u002Fweb.archive.org\u002Fweb\u002F20230131091501\u002Fhttps:\u002F\u002Fdiscoveruni.gov.uk\u002Fcourse-details\u002F10007143\u002FN305\u002FFull-time","77","91","Colchester","54.584594432500","-5.934874397100","BT7 1NN","Reading","ABB","Aberdeen","Oxford","10007793","3961","https:\u002F\u002Fweb.archive.org\u002Fweb\u002F20230131091501\u002Fhttps:\u002F\u002Fdiscoveruni.gov.uk\u002Fcourse-details\u002F10007793\u002F\u002FFull-time","