<script>
//append target blank to all external links
jQuery( document ).ready(function( $ ) {
	$('a').each(function() {
	   var a = new RegExp('/' + window.location.host + '/');
	   if (!a.test(this.href) && (this.href != 'javascript:;')) {
		  $(this).attr("target","_blank");
	   }
	});
});
</script>
