		//fixed sidebars
		var $sidebar   = $(".sideAds"),
		$window    = $(window),
		offset     = $sidebar.offset(),
		topPadding = 15;

		$window.scroll(function() {
			if ($window.scrollTop() > offset.top) {
				$sidebar.css("position", "fixed");
				$sidebar.css("top", 0);
			} else {
				$sidebar.css("position", "absolute");
				$sidebar.css("top", 267);
