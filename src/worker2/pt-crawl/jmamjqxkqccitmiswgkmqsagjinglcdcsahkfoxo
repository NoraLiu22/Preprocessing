		clearTimeout(RefreshID);
		ga('send', 'event', 'Video', 'home',  videoTitle);
	jQuery('.wrapper-video').html('<iframe type="text/html" width="720" height="405" src="https://web.archive.org/web/20190308152711/https://www.youtube.com/embed/' + videoId + '?rel=0&autoplay=1" frameborder="0" allowfullscreen></iframe>');
	jQuery('.bloco-destaque-videos .gradiente-top, .bloco-destaque-videos .gradiente-bottom').hide();
	jQuery('.bloco-destaque-videos .bloco-cabecalho header').hide();
}
</script>
