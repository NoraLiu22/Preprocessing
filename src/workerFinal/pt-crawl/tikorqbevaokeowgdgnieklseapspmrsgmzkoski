        var accessAttributes =  [{"id":638837,"availableforpurchase":true},{"id":638927,"availableforpurchase":true},{"id":637844,"availableforpurchase":true},{"id":638085,"availableforpurchase":true},{"id":638854,"availableforpurchase":true},{"id":637444,"availableforpurchase":true},{"id":637446,"availableforpurchase":true},{"id":638150,"availableforpurchase":true},{"id":636179,"availableforpurchase":true},{"id":638629,"availableforpurchase":true},{"id":638752,"availableforpurchase":true},{"id":638627,"availableforpurchase":true},{"id":638630,"availableforpurchase":true},{"id":638836,"availableforpurchase":true},{"id":636152,"availableforpurchase":true},{"id":638628,"availableforpurchase":true},{"id":636008,"availableforpurchase":true},{"id":637519,"availableforpurchase":true},{"id":638084,"availableforpurchase":true},{"id":638262,"availableforpurchase":true},{"id":638153,"availableforpurchase":true},{"id":638083,"availableforpurchase":true},{"id":638152,"availableforpurchase":true},{"id":636007,"availableforpurchase":true},{"id":637253,"availableforpurchase":false},{"id":638631,"availableforpurchase":true},{"id":637443,"availableforpurchase":true},{"id":637521,"availableforpurchase":true},{"id":637442,"availableforpurchase":false},{"id":637520,"availableforpurchase":true},{"id":637845,"availableforpurchase":true},{"id":637846,"availableforpurchase":true},{"id":636089,"availableforpurchase":true},{"id":636927,"availableforpurchase":false},{"id":637052,"availableforpurchase":true},{"id":637386,"availableforpurchase":false},{"id":636801,"availableforpurchase":true},{"id":636375,"availableforpurchase":false},{"id":638928,"availableforpurchase":true},{"id":636056,"availableforpurchase":true},{"id":636055,"availableforpurchase":true},{"id":636058,"availableforpurchase":true},{"id":636057,"availableforpurchase":true},{"id":635903,"availableforpurchase":true},{"id":637445,"availableforpurchase":true},{"id":638151,"availableforpurchase":true},{"id":636054,"availableforpurc