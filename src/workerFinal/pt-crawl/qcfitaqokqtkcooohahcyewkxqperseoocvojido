<script type="text/javascript">
    function maind() {
        startdate = new Date()
        now(startdate.getFullYear(), startdate.getMonth(), startdate.getDate(), startdate.getHours(), startdate.getMinutes(), startdate.getSeconds())
    }

    function ChangeValue(number, pv) {
        numberstring = ""
        var j = 0
        var i = 0
        while (number > 1) {

            numberstring = (Math.round(number - 0.5) % 10) + numberstring
            number = number / 10
            j++
            if (number > 1 && j == 3) {
                numberstring = "," + numberstring
                j = 0
            }
            i++
        }

        numberstring = numberstring

        if (pv == 1) { document.getElementById("clock").innerHTML = numberstring }
    }

    function now(year, month, date, hours, minutes, seconds) {
        startdate = new Date(year, month, date, hours, minutes, seconds)

        var birthsPerSecond = 0.308683688483004
        nu = new Date()
        refdate = new Date(nu.getFullYear(), month, date);
        estimatedval = (nu.getTime() - refdate.getTime()) / 1000 * birthsPerSecond
        ChangeValue(estimatedval, 1);

        timerID = setTimeout("now(startdate.getFullYear(),startdate.getMonth(),startdate.getDate(),startdate.getHours(),startdate.getMinutes(),startdate.getSeconds())", 200)
    }

    window.onload = maind
</script>
