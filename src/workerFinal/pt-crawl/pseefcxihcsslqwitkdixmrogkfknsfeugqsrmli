    content: "?";
    font-size: 0.7em;
    position: relative;
    top: -6px;
}

.definition-popup {
    display:none;
    position: absolute;
    width: 500px;
    padding: 25px 10px 0 20px;
    font-size: 18px;
    font-weight: bold;
    text-align: center;
    background: white;
    padding: 30px;
    border: 2px solid #a01b27;
    margin-top: 4px;
}

@media (max-width: 600px) {
  .definition-popup {
    width:300px;
  }
}

.definition-popup .close {
    font-size:22px;
    cursor: pointer;
    position: absolute;
    right:10px;
    top:5px;
}
.definition {
  display: none;
}</style>
