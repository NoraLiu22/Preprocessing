        });

        function get_y_min(num) {
            // 获取y轴上的最小显示数字
            var num1 = parseInt(num*0.8);
            var tmp1  = Math.pow(10,num1.toString().length-1);
            num1 = Math.floor(num1/tmp1)*tmp1;
            return num1;
        }
