<script type="text/javascript">
    $(document).ready(function () {
        $('#search-keyboard-toggler').click(function (e) {
            e.preventDefault();

            VirtualKeyboard.pokaz = VirtualKeyboard.isOpen();

            VirtualKeyboard.toggle('search-input', 'search-keyboard');
        });

        $(document).click(function (e) {
            if (($(e.target).closest("#search-keyboard").length === 0) && ($(e.target).closest(".search-wrapper").length === 0)) {
                if (VirtualKeyboard.isOpen()) {
                    VirtualKeyboard.hide();
                }
            }
        });

        $('.svg-image').each(function () {
            var src = $(this).attr('src');

            src = src.replace('.png', '.svg');

            $(this).attr('src', src);
        });

        var sense = function () {
            var count = 0;

            $('.banner').each(function () {
                if (!($(this).height() === 0 || $(this).filter(':visible') === 0)) {
                    count = 1;

                    return (false);
                }
            });

            if (count == 0) {
                $.get('/sense.php', {'is': 1});

                return (true);
            }

            $.get('/sense.php', {'is': 0});

            return (false);
        };

        var senseInterval = setTimeout(function () {
            /*sense();*/
        }, 3000);


    });
</script>
