	<script>
		$("#btnSubmitEmail").on("click", function(event){
			event.preventDefault();
			if(Page_ClientValidate("Email")) {
				$("#formPartOne").hide();
				$("#formPartTwo").show();
			}
		});
	
	
        $("#ddlSegment").on("change", function() {
			$('[name="SegmentRadioButtonList"]').val([$("#ddlSegment").val()])
		});	
	
        function toggleStudentFields() {
            if ($("input[name=SegmentRadioButtonList]:checked").val() == "Student") {
                $("#studentFields").show();
            }
            else {
                $("#studentFields").hide();
            }
        }
    </script>

