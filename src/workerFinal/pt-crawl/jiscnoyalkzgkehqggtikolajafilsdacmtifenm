		},
		function (data) {
			$("#show_kinenote_data").append(data);
			$("#show_kinenote_data").show();
		}
	);
	$("#show_kinenote_data").next().remove();
	//	});
</script>


<div id="info_list_like" class="reveal-modal medium">
	<div class="modal">
		<div class="modal__header">
			<h2 class="heading-bar primary fz14">フォロワー
			</h2>
			<a href="javascript:void(0)" class="modal__close"><span class="ui-icon-15 ui-icon-109"></span></a>
		</div>
		<div class="modal__content " style="padding: 10px 0px 10px 10px">
			<div class=" scroll-panel" id="divScroll" style="max-height: 200px">
				<ul class="member_list_info">
				</ul>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript">
	function UpdateJScrollPaneShowStekine() {
		$('#divScroll').jScrollPane({
			autoReinitialise: true,
			showArrows: true
		}).bind('jsp-scroll-y', function (event, scrollPositionY, isAtTop, isAtBottom) {
			$(this).trigger('scroll');
			$that = $(this);
			var api = $that.data('jsp');
			if (($that.outerHeight() + scrollPositionY) >= (api.getContentHeight() - base_height_auto_load)) {
				var obj = $(".member_list_info");
				obj.GetData(obj.data("stekine"),
					obj.data("type"),
					obj.data("page"),
					obj.data("member_to_cd"));
			}
		});;
	}
	function GetDataMember(obj, _stekine, _type, _page, _member_to_cd) {
		if ($(obj).text() * 1 <= 0)
			return;
		var $that = $(".member_list_info");
		$that.data("stekine", _stekine);
		$that.data("type", _type);
		$that.data("page", _page);
		$that.data("is_stop", false);
		$that.data("member_to_cd", _member_to_cd);
		$that.GetData(_stekine, _type, _page, _member_to_cd);
	}

	$.fn.extend({
		GetData: function (_stekine, _type, _page, _member_to_cd) {
			var $that = $(this);
			if ($that.data("ajax") == true || $that.data("is_stop") == true) {
				return;
			}
			if (_type == 1) { }
			var _dataPost = "{'stekine': '" + _stekine +
						"', 'type': " + _type +
						", 'page': " + _page +
						 ", 'disp_line_count': 20," +
						 " 'member_to_cd':'" + _member_to_cd + "'}";

			loading_ajax("../../public/cinema/onsearch.aspx/GetData", _dataPost,
				function () {
					if ($that.data("page") * 1 <= 1) {
						$that.empty();
					}
					$that.data("ajax", true); //2重送信回避のためdisableに
					$("#divScroll").createAjaxIndicatorForLoadStekine();
				},
				function (data) {
					var _index = 1;
					if (data.d != "") {

						try {
							$.each(data.d, function (i, val) {
								_index++;
								$that.append("<li><a href=" + '/main/public/profile/' + "?member_cd=" + val.member_cd + ">" + val.member_display_name + "</a></li>");
							});
							// Xử lý phần hiển thị nếu data it hơn 200px
							if ($that.height() >= 200) {
								$('#divScroll').addClass("block-Scroll");
								UpdateJScrollPaneShowStekine();
								if ($that.data("page") * 1 <= 1) {
									$('#divScroll').data('jsp').scrollToY(0);
								}
							} else {
								$('#divScroll').removeClass("block-Scroll");
								if ($('#divScroll').children().hasClass("jspContainer")) {
									$('#divScroll').children(".jspContainer").css({ height: $that.height() });
								}
							}
						} catch (e) {

						}

						if ($that.data("page") * 1 <= 1) {
							openBoxModal("#info_list_like");
						}
						$that.data("page", _page * 1 + 1);
					}
					$that.data("is_stop", (_index < 20));
					$("#divScroll").find(".indicator").remove();
					$that.data("ajax", false);
				},
				function () {
					$that.data("ajax", false);
					$("#divScroll").find(".indicator").remove();
				}
				);
		},
		createAjaxIndicatorForLoadStekine: function () {
			var img = $(this);
			img.css({ position: 'relative' });
			var indicator = $("<span>").addClass("indicator").css({
				width: img.width(),
				height: img.height(),
				backgroundColor: "#ffffff",
				opacity: 0.3,
				display: "inline",
			}).append($("<img>", {
				src: "../../images/indicator.white.gif"
			}).css({
				position: "absolute",
				top: img.height() / 2 - 8,
				left: img.width() / 2 - 8
			}));
			return $(this).append(indicator);
		},
	});

</script>


		</div>
		<!-- Right Content -->
		<div id="rightContent">
			
	<div class="common_frame">
		<h2 class="heading-bar primary fz14 taCenter">
			
			Q&amp;A ・ お問い合わせ
		</h2>
		<p class="text">
			お客様から多く寄せられるご質問、ご要望についてまとめております。<br />
			お問い合わせの前に、是非一度ご確認ください。
		</p>
		<h2 class="heading-bar second fz14 mT10">iPhoneアプリについて</h2>
<div class="unit">
<p class="sub"><a id="contact53" class="contact_trigger" href="#">iPhoneアプリの対象端末、OS</a></p>
<div id="contact53_accordion" class="contact_text">
動作確認済み端末：iPhone5～iPhone6s,6s Plus<br />
動作確認済みOS：iOS7～9.1<br />
（2016年9月現在）<br /></div>
<p class="sub"><a id="contact50" class="contact_trigger" href="#">検索結果の表示に時間が掛かることがあります。速くならないのでしょうか？</a></p>
<div id="contact50_accordion" class="contact_text">
指定した条件次第で、検索結果が表示されるまでに時間が掛かることがあります。例えば公開年だけ、製作国だけ、ある一単語のみを指定といったように検索条件が少なければ少ないほど検索に時間が掛かる場合がございます。<br/><br/>

極力わかる範囲で検索条件を複数指定し絞り込みを行ってください。</div>
<p class="sub"><a id="contact51" class="contact_trigger" href="#">マイページにて、称号アイコンが表示されていません</a></p>
<div id="contact51_accordion" class="contact_text">
アプリではよりミニマムに、よりシンプルな情報表示となるよう、称号アイコンは省略させて頂いております。<br/><br/>

現状、今後のアップデートでも対応対象外とさせていただいております。<br/>

何卒、ご了承くださいませ。</div>
</div>
<h2 class="heading-bar second fz14 mT10">サービス全般について</h2>
<div class="unit">
<p class="sub"><a id="contact3" class="contact_trigger" href="#">会員登録方法を教えて下さい</a></p>
<div id="contact3_accordion" class="contact_text">
<b>【iPhoneアプリ】</b><br/>
ログイン画面下部にある、「新規会員登録はこちら」からメールアドレスを登録し、KINENOTEから届いたメールに記載された本登録URLをタップし登録を進めてください。<br/><br/>

<b>【Webサイト】</b><br/>
<dl>
	<dt>(1)</dt>
	<dd>「KINENOTE利用規約」を確認の上、「<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員登録</a>」ボタンをクリックして会員登録してください。</dd>
	<dt>(2)</dt>
	<dd>
		メールアドレス登録ページで、任意のメールアドレスを入力し「送信」ボタンをクリックします。<br />
		登録いただいたメールアドレス宛に「本登録用ページのご案内」というタイトルのメールが届きます。
	</dd>
	<dt>(3)</dt>
	<dd>「本登録用ページのご案内」メールの中に記載の認証用URLをクリックし、本登録用ページへアクセスします。（72時間を超過しますと、セキュリティ保持のため有効期限切れとなってしまいます）</dd>
	<dt>(4)</dt>
	<dd>
		会員登録ページで必要な情報を入力し（必須入力項目は※表示）、「登録」ボタンをクリックすると、内容の確認ページへ移動します。
	</dd>
	<dt>(5)</dt>
	<dd>確認ページで登録情報を確認の上、「登録」ボタンをクリックします。</dd>
	<dt>(6)</dt>
	<dd>会員登録完了のお知らせが登録メールアドレスへ送信され、会員登録が完了します。</dd>
</dl></div>
<p class="sub"><a id="contact5" class="contact_trigger" href="#">「本登録用ページのご案内」メールが届きません</a></p>
<div id="contact5_accordion" class="contact_text">
登録時に入力したメールアドレスにお間違いはありませんか？ あるいは、お使いのメールソフトの「ゴミ箱」や「迷惑メールフォルダ」にメールは届いていませんでしょうか？ 間違って「迷惑メール」と判別されている可能性があります。今一度、ご確認ください。 また、「迷惑メールブロック」を設定されている方は、「entry@admin.kinenote.com」を“受信許可”にしていただけますようお願いいたします。</div>
<p class="sub"><a id="contact9" class="contact_trigger" href="#">KINENOTEの推奨環境を教えてください</a></p>
<div id="contact9_accordion" class="contact_text">
KINENOTEはHTML5で作成されております。<br />
対応している主なブラウザは下記の通りです。<br />
<br />
<b>【iPhoneアプリ】</b><br/>
・動作確認済み端末：iPhone5〜iPhone6s,6s Plus<br />
・動作確認済みOS：iOS7〜9.1<br />
<br />
<br />
<b>【Webサイト】</b><br/>
・ウィンドウズ<br />
　Internet Explorer（10.0以降）<br />
　Google Chrome（40.0以降）<br />
　Mozilla Firefox（30.0以降）<br />
<br />
・Macintosh（OSX）<br />
　Safari（5.0以降） <br />
　Google Chrome（40.0以降）<br />
　Mozilla Firefox（30.0以降）<br />

<br />
・iPhone／iPad（iOS）<br />
　Google Chrome<br />
　Safari<br />
<br />
・Android<br />
　Google Chrome<br />
<br />
※上記掲載のブラウザであっても、OSとの組み合わせにより、動作や表示が正常に行われない場合もございます。<br />
※各ブラウザの対応OS、ダウンロード、インストール方法等はブラウザ提供元である各社サイトをご確認ください。</div>
<p class="sub"><a id="contact36" class="contact_trigger" href="#">スマートフォンから利用できますか？</a></p>
<div id="contact36_accordion" class="contact_text">
WEBブラウザ（上記KINENOTEの推奨環境をご参照ください）からスマートフォンの操作性に特化した専用版をご利用いただけます。<br/>
<br/>
Safariをご利用の場合は、ホーム画面にKINENOTEアイコンを設置することが可能です。<br/>
<br/>
<img src="../../images/sp_home_add.jpg" alt="ホーム画面へのアイコン追加"  width="800" /><br />
<br/>
2016年1月現在、<a href="http://www.kinenote.com/main/feature/sp_app/" target="_blank">iPhoneアプリ</a>がリリースされております。<br/></div>
<p class="sub"><a id="contact42" class="contact_trigger" href="#">iPhoneでは毎回ログイン情報の入力をしなければなりません</a></p>
<div id="contact42_accordion" class="contact_text">

Safariをご利用の場合、履歴やログイン情報を残さないプライベートモードになっていると、頻繁にログアウトしてしまいます。<br/>
下記の画像を参考にプライベートモードを解除してお使いになることをお勧めいたします。<br/>
<br/>
<img src="../../images/sp_private.jpg" alt="プライベートモード" width="800" /><br/>
<br/>
※プライベートモードを解除すると、他の方がユーザー様のiPhoneをご利用になった場合、サイト上でご入力なさった情報などが漏れる可能性があります<br/><br/>
尚、<a href="http://www.kinenote.com/main/feature/sp_app/" target="_blank">iPhoneアプリ</a>がリリースされておりますので、あわせてご利用ください。<br/></div>
<p class="sub"><a id="contact10" class="contact_trigger" href="#">携帯電話（フィーチャーフォン）から利用できますか？</a></p>
<div id="contact10_accordion" class="contact_text">
申し訳ございませんが、携帯電話（フィーチャーフォン）からの動作確認はできておりません。<br/>機種によっては正常にサービスをご利用いただけない場合がございますので、パソコンかスマートフォンでのご利用を推奨いたします。</div>
<p class="sub"><a id="contact6" class="contact_trigger" href="#">パスワードを忘れてしまいました</a></p>
<div id="contact6_accordion" class="contact_text">
<a href="../../public/entry/remind.aspx" target="_blank" class="js__qa_box_remind_password">こちら</a>の「パスワードの再設定」機能で登録時のメールアドレスを入力すると、登録メールアドレスに「パスワード再設定のお願い」というタイトルのメールが送信されます。メールの中に記載の認証用URLをクリックしてパスワード再設定ページへアクセスし、パスワードの再設定を行うことができます（72 時間を超過しますと、セキュリティ保持のため有効期限切れとなってしまいます）。</div>
<p class="sub"><a id="contact7" class="contact_trigger" href="#">登録のメールアドレスを変更したい</a></p>
<div id="contact7_accordion" class="contact_text">
ログイン後、<a href="/main/member/setting/" target="_blank">会員設定画面</a>の「メールアドレス」の「変更する」ボタンをクリックし、「メールアドレス変更」画面で新しいメールアドレスを入力することで再設定が行えます（72 時間を超過しますと、セキュリティ保持のため有効期限切れとなってしまいます）。また、メールアドレスを変更することで、ログインIDも同様に変更されます。</div>
<p class="sub"><a id="contact39" class="contact_trigger" href="#">設定を登録しても反映されません／ユーザー情報が公開されてしまいっています</a></p>
<div id="contact39_accordion" class="contact_text">
KINENOTEは、ユーザーさんのページについて、ユーザーさんご自身がご覧になるマイページと、<br/>他のユーザーさんがご覧になるページの2種類が存在します。<br/>
例えば性別、お住まいの地域、年齢の公開設定のチェックを外していただいた場合、<br/>マイページではそのまま表示されますが、他のユーザーの方が閲覧した場合には表示されないようになっております。<br/>
また、リニューアルでの仕様変更に伴い、各種公開設定を付加しましたが、<br/>旧サイトで「プロフィールを公開」としていなかった方は、性別、お住まいの地域、年齢のすべてがデフォルトでは非公開になっております<br/>
　万が一、元々非公開としていたのに、各情報が「公開」となってしまっていた場合は、<br/>誠にお手数ですが<a href="https://fs221.xbit.jp/t337/form1/" target="_blank">こちら</a>よりお知らせください。</div>
<p class="sub"><a id="contact8" class="contact_trigger" href="#">退会方法を教えてください</a></p>
<div id="contact8_accordion" class="contact_text">
ログイン後、<a href="/main/member/setting/">会員設定画面</a>
の「退会」機能で、パスワードを入力して退会することができます。退会された場合マイページは抹消されますが、公開設定しているレビューは引き続き各作品の「みんなのレビュー」欄にて表示されます。表示を希望されないレビューは、退会前に「鑑賞録」より削除をお願いします。</div>
<p class="sub"><a id="contact35" class="contact_trigger" href="#">登録映画数とスタッフ＆キャスト登録数はどのくらいありますか？</a></p>
<div id="contact35_accordion" class="contact_text">
日本公開作を中心に、邦画・洋画約7万タイトル以上と、映画関係者の人名26万件以上を網羅しております。</div>
<p class="sub"><a id="contact4" class="contact_trigger" href="#">映画検定合格者の登録とは何ですか？</a></p>
<div id="contact4_accordion" class="contact_text">
キネマ旬報社主催の映画の歴史・作品・俳優・スタッフ業界などの知識をはかる、「映画検定」の合格級数をMY KINENOTEに表示できる機能です。<br />
<br />
<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員登録画面</a>にて<br />
・氏名（合格時のお名前を本名としてご登録ください）<br />
・合格証番号（「－」ハイフンを除いた数字のみ）<br />
を入力いただくと、MY KINENOTEの左上のプロフィール欄に「映画検定　〇級」という形で公開できます。<br />
既に会員登録をされている方は、会員設定画面から同様に登録が可能です。<br />
<br />
<img src="../../images/certificate.gif" alt="映画検定" /><br />
<br />
合格証を失くされた、番号がわからない、という方はご登録のデータを確認いたしますので、<a href="https://fs221.xbit.jp/t337/form1/" target="_blank">こちら</a>からお問い合わせ内容に「氏名」と「級数」（もし分からなければ無くても可です）を記載頂き、お問い合わせください。<br /><br />
※設定画面から登録が出来ない場合お名前（本名）の表記をお確かめ下さい。氏名の間にスペース等が入っている場合登録できません。また名字の変更等がある場合は受験当時の名字にて登録いただけますようよろしくお願いいたします。</div>
<p class="sub"><a id="contact41" class="contact_trigger" href="#">「ベストレビュアー」「ベストラッカー」とはなんですか？</a></p>
<div id="contact41_accordion" class="contact_text">
レビューとラックに対し、「ステKINE!」されいる数が多いほどランキングの順位が上がり、トップの方には「ベスト○○１」の、2～10位の方には「ベスト○○10」の、11～100位の方には「ベスト○○100」の称号が付与されます。<br>
それぞれの称号はプロフィール写真の左側に、アイコンで表示されます。</div>
<p class="sub"><a id="contact44" class="contact_trigger" href="#">［PC版］マイページを表示すると一部情報が表示されない</a></p>
<div id="contact44_accordion" class="contact_text">
マイページには2種類ございます。<br /><br />

1)KINENOTEログイン後のTOPページ<br />
　会員ログイン後すぐに表示されるTOPページ<br />
　URLは下記となります。<br />
　http://www.kinenote.com/main/member/mypage/<br />
2)他のユーザーから見たあなたのページ<br />
　「設定画面」の「公開設定」より、他のユーザーから見たあなたのページをプレビューすることが可能です。<br />
　URLは下記のようなイメージとなります。<br />
　http://www.kinenote.com/main/public/profile/?member_cd=（ここには数字が入ります）<br /><br />

他のユーザーから見たあなたのページは、他の方も閲覧するユーザーページとなるため、非公開設定をされている情報については閲覧できなくなっております。<br /><br />

「公開設定」は設定画面より設定頂けます。<br />
<a href="https://www.kinenote.com/main/member/setting/index.aspx">https://www.kinenote.com/main/member/setting/index.aspx</a></div>
</div>
<h2 class="heading-bar second fz14 mT10">鑑賞映画の登録について</h2>
<div class="unit">
<p class="sub"><a id="contact25" class="contact_trigger" href="#">登録したい作品が見つからないときは、どうすればよいですか？</a></p>
<div id="contact25_accordion" class="contact_text">
お手数ですが、作品名を明記の上、<a href="https://fs221.xbit.jp/t337/form2/" target="_blank">こちら</a>からお問い合わせください。<br><br>
<font color="red">※現在、多くのリクエストを頂いているため、
また、正確な作品情報をお調べし登録するため対応までにお時間をいただいたております。
返信が遅くなる場合がございますが順番に対応を行っておりますので、
大変申し訳ございませんが、対応まで暫しお待ち下さい。</font></div>
<p class="sub"><a id="contact26" class="contact_trigger" href="#">作品情報ページで、別の作品の画像が表示されています。どうすればよいでしょうか？</a></p>
<div id="contact26_accordion" class="contact_text">
お手数ですが、作品名を明記の上、<a href="https://fs221.xbit.jp/t337/form1/" target="_blank">こちら</a>からお問い合わせください。</div>
<p class="sub"><a id="contact38" class="contact_trigger" href="#">評点、鑑賞費用の入力ができません</a></p>
<div id="contact38_accordion" class="contact_text">
評点および鑑賞費用に入力できるのは、整数の半角数字のみとなっております。なお、評点は0～100の間でご入力ください。</div>
<p class="sub"><a id="contact13" class="contact_trigger" href="#">レビューの文字数に制限はありますか？</a></p>
<div id="contact13_accordion" class="contact_text">
3000文字（改行も1文字と計算）までの入力が可能です。<br>
※キネマ旬報本誌の読者評に投稿する場合は800字前後でお書きください</div>
<p class="sub"><a id="contact37" class="contact_trigger" href="#">鑑賞記録ページに記載の「キネマ旬報の読者評にも投稿する」とは何ですか？</a></p>
<div id="contact37_accordion" class="contact_text">
お書きいただいたレビューをキネマ旬報の「読者の映画評」コーナーに応募ができます。審査に通過された方には、本誌編集部担当から掲載確認の連絡をいたします。<br>
※キネマ旬報本誌の読者評に投稿する場合は800字前後でお書きください</div>
<p class="sub"><a id="contact2" class="contact_trigger" href="#">一度登録した内容を修正・削除したい</a></p>
<div id="contact2_accordion" class="contact_text">
マイページの「鑑賞録」にて、各鑑賞記録の「編集」ボタン・「削除」ボタンをクリックすることで、評点やレビューなどの登録内容の編集や削除を行うことができます。</div>
<p class="sub"><a id="contact27" class="contact_trigger" href="#">作品登録ページの、レビュー（公開・非公開）とメモの違いについて</a></p>
<div id="contact27_accordion" class="contact_text">
「レビュー」は「公開」に設定すると他のユーザーから閲覧可能となります。途中で下書きとして一度保存したい場合や非公開にしたい場合は、「非公開」に設定すると公開されずに文章が保存されます。<br/>
「メモ」は非公開なので、他のユーザーにより閲覧されることはありません。
</div>
<p class="sub"><a id="contact12" class="contact_trigger" href="#">鑑賞データを登録してもランキングが変わらないのはなぜですか？</a></p>
<div id="contact12_accordion" class="contact_text">
申し訳ございませんが、現在のKINENOTEではランキングの集計は1日1回となっております。毎日午前3時頃に集計を行っておりますので、それ以降は反映されたものが表示されます。</div>
</div>
<h2 class="heading-bar second fz14 mT10">他のサービスとの連携について</h2>
<div class="unit">
<p class="sub"><a id="contact21" class="contact_trigger" href="#">Facebook、Twitterでのログインや連携とは何ですか？</a></p>
<div id="contact21_accordion" class="contact_text">
Facebook、TwitterのIDを持っている方は、KINENOTEのアカウントを簡単に作成できます。各SNSとの連携をすることで、鑑賞データやレビューを各SNSアカウントにシェアすることができます。連携の登録は<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員登録／情報画面</a>で行えます。<br><br>

※正しいFacebookアカウントを入力してもログインが出来ない場合がございます。これはFacebook社の提供している認証システムの不具合によるものです。大変申し訳ございませんがその場合はKINENOTEアカウントにてログインしご利用ください。</div>
<p class="sub"><a id="contact31" class="contact_trigger" href="#">外部SNS（Facebook、Twitter、Evernote）に投稿されません</a></p>
<div id="contact31_accordion" class="contact_text">
2016年4月26日の改修によってSNSへの自動投稿機能を廃止し、任意に投稿をして頂くインターフェースへ変更いたしました。詳しくは<a href="http://www.kinenote.com/pages/2016/03/kinenote-sns/" target="_blank">こちら</a>。
<br><br>
---------<br>
SNSとの連携が行われない原因には主に下記の４つが考えられます。<br>
<br>
●アカウントの連携が正しく行われていない<br>
2014年3月20日行ったメンテナンスにおいてFacebook、Twitterとの連携用アカウントを変更いたしました。<br>
そのため、それ以前に各SNSに「接続」されていた方は<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員設定画面</a>で再度「接続」をやり直してください<br>
<br>
●Facebookの「有効期限」が切れている<br>
Facebook側が設定をしている有効期限が切れている可能性があります。<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員設定画面</a>で更新を行ってください。<br>
<br>
●連携の設定ができていない<br>
KINENOTEでは３種類の連携方法をご用意していますので<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員設定画面</a>でそれぞれ設定を行ってください。<br>
【１】観たを押したら自動投稿<br>
　「観た」→「詳細は登録しない」をしたら自動的に投稿されます<br>
【２】さらに編集したら投稿する<br>
　「観た」→「詳細も登録する」もしくは「鑑賞録」などから「編集」をした際、最下部の「外部サービスと連携」でそれぞれチェックをした場合に投稿されます<br>
　※<a href="../../public/member/member_entry.aspx" class="js__qa_goto_member_setting" target="_blank">会員設定画面</a>でのチェックがされていないと、「外部サービスと連携」でチェックをしても投稿されませんのでご注意ください<br>
【３】観たいを押したら自動投稿<br>
　「観たい」をしたら自動的に投稿されます<br>
<br>
●レビュー内容に不適当な文字が入っている<br>
半角のシングルクォート（'）が入っている場合、こちらを全角で入力いただくか、削除していただければ投稿が可能になる場合があります。また、Facebook・Twitter・Evernote側のシステムに問題が発生している可能性がございますので、しばらく時間をおいてから再度お試しください。<br>
<br>
以上に該当しない場合は、<a href="https://fs221.xbit.jp/t337/form1/" target="_blank">こちら</a>よりお問い合わせください。</div>
</div>

