function toggleclass(element,elemMore){
	$(element).toggle();
	if($(element).is(":visible")){
		$(elemMore).html('<i class="fa fa-minus"></i>');
	}else{
		$(elemMore).html('<i class="fa fa-plus"></i>');
	}
}</script>


