<script>
    $(document).ready(function() {
    resizeMediaGrid();
    $(window).resize(function() {
        setTimeout(function() {
            resizeMediaGrid();
        }, 100);
    });
});

function resizeMediaGrid() {
    var winWid = $(window).width();
    if (winWid <= 991) {
        $("div.owl-stage-outer.owl-height").removeAttr("style");
        if (winWid < 400)
            $("div.parbase.c05v1_multimedia.section").width($(window).width() - 30);
        else if (winWid < 700)
            $("div.parbase.c05v1_multimedia.section").width($(window).width() - 30);
        else if (winWid < 991)
            $("div.parbase.c05v1_multimedia.section").width($(window).width() - 60);
    } else
        $("div.parbase.c05v1_multimedia.section").removeAttr("style");

}
 </script>

<style>
    .tab-content > .tab-pane:not(.active){
        display: block;
        height: 0;

    }
    .tab-content > .active{
        height: auto;
    }
</style>
