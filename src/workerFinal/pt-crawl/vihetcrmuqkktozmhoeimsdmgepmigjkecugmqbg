<script src="//images.china.cn/images1/resource/scripts/jquery/jquery-1.12.4.min.js"></script>
<script>
    ! function(window, $) {
        var ERR_EMAIL_REQUIRED = 'Please enter valid email.',
            pattern_email = /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/,
            $el_error = $('.form__error');

        window.check_form = function(form) {
            var $el_email = $('#email'),
                email = $.trim($el_email.val()),
                is_email = pattern_email.test(email);

            $el_error.hide();

            if (!is_email) {
                $el_error.text(ERR_EMAIL_REQUIRED);
                $el_error.show();
                return false;
            }
        };
    }(window, window.jQuery);
</script>
