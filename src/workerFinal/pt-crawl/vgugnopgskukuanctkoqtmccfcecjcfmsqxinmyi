<script>
	require(['jquery', 'site_gtm'], function($, gtm) {
		var $root = $('[data-guid="f5c478ef-3e5d-4d3a-899d-924f58703f7d"]');
		var $items = $root.find('[data-item-link]');
		var $content = $root.find('[data-seo-content]');
		var $top = $root.find('[data-seo-top]');
		var $topButton = $root.find('[data-seo-top-action]');
		
		var offset = Number() || 0;
		var animationSpeed = Number() || 1000;
		var backToTop = Boolean("true") || true;
		var gtmDataLayer = window.gtmDataLayer || [];
		var isGTM = true; // use false for legacy
		
		
		// send analytics function
		var sendAnalytics = function(category, action, label) {
			if (isGTM) {
				gtm.sendEvents({
					tClient : {
						eventCategory : category,
						eventAction : action,
						eventLabel : label
					}
				});
			} else {
				gtmDataLayer.push({
					event : 'tEvent',
					eventCategory : category,
					eventAction : action,
					eventLabel : label
				});
			}
		}
		
		
		$items.each(function(index, item) {
			$(item).on('click', function(e) {
				
				sendAnalytics('Jump List', 'Click' + ' - ' + item.hash, $(item).text().trim() );
				
				$('html, body').animate({scrollTop: $(this.hash).offset().top - offset }, animationSpeed);
			});
		});
		
		// handle back to top
		if (backToTop === true) {
			// handle click action for back to top
			$($topButton).on('click', function(e, item) {
				sendAnalytics('Jump List', 'Click Back to Top');
				
				$('html, body').animate({scrollTop: $top.offset().top - offset}, animationSpeed);
			});
		}
	});
</script>


