            <ul class="list-unstyled list-inline nowrap">
                    <li>
                        <a class="mrgn-rght-md" href="https://www.facebook.com/TrentSevernNHS/"><span class="fa-brands fa-square-facebook follow-social fa-2x black" aria-hidden="true"></span><span class="wb-inv">Facebook</span></a>
                    </li>
                                    <li>
                        <a class="mrgn-rght-md" href="https://www.twitter.com/TrentSevernNHS/"><span class="fa-brands fa-square-x-twitter follow-social fa-2x black" aria-hidden="true"></span><span class="wb-inv">Twitter</span></a>
                    </li>
                                    <li>
                        <a class="mrgn-rght-md" href="https://www.youtube.com/ParksCanada"><span class="fa-brands fa-square-youtube follow-social fa-2x black" aria-hidden="true"></span><span class="wb-inv">YouTube</span></a>
                    </li>
                                    <li>
                        <a class="mrgn-rght-md" href="https://www.instagram.com/parks.canada"><span class="fa-brands fa-square-instagram follow-social fa-2x black" aria-hidden="true"></span><span class="wb-inv">Instagram</span></a>
                    </li>
            </ul>
    </div>
<!-- End Social Media-->
        
        
    </div>
    <!-- End Column Two -->
