        try {
            var el = $('.fotorama').data('fotorama').data[0].html;
            var url = $(el).find('div').css('background-image');
            imagePath = url.replace('url(','').replace(')','').replace(/\"/gi, "");
        } catch (e) {
        }
    } else {
        // If there's just one image (i.e. it's not a gallery), find its path.
