	function mm(mx) {
	mx = mx + '@';
	mx = 'lto:' + mx;
	mx = 'mai' + mx;
	mx = mx + 'em.uni-frankfurt.de';
	location.href = mx;
	}

function frcheck() {
	yy=navigator.appName;
	zz=navigator.appVersion.charAt(0);
	if (navigator.appName == 'Netscape')
		xx = navigator.language;
	else
		xx = navigator.browserLanguage;
	xx=xx.charAt(0)+xx.charAt(1);
//	alert (xx);
	if (xx == "de") {
		indextop = '/indexd.htm';
		frametop = '/framed.htm';
		}
	else {
		if (xx == "fr") {
			indextop = '/indexf.htm';
			frametop = '/framef.htm';
			}
		else {
			if (xx == "es") {
				indextop = '/indexs.htm';
				frametop = '/frames.htm';
				}
			else {
				indextop = '/indexe.htm';
				frametop = '/framee.htm';
			}
		}
	}
  bIsIframe = top.location.href.indexOf (indextop);
  bIsFframe = top.location.href.indexOf (frametop);
  if ((bIsIframe == -1) && (bIsFframe == -1))
	{
		test='/curric/uni-idg.htm';
		testref=test+self.location.hash;
		bIsAsp = self.location.href.indexOf ('fkidg');
		if (bIsAsp > 0) {
// alert (bIsAsp);
			top.location= frametop+'?'+testref;
		} else {
			top.location= indextop+'?'+testref;
		}
	}
	else
	{
		testref=parent.testref;
		bIsmitteo = testref.indexOf ('2.htm');
		if (bIsmitteo == -1) {
			xtestref = testref.substr (0, testref.indexOf ('.htm')) + '2.htm';
			bIschild = parent.mitteo.location.href.indexOf (testref);
			if ((bIschild == -1) && (testref.length > 0))
				{
				parent.mitteo.location.href = testref;
				}
			parent.testref='';
			bIschild = parent.mitteu.location.href.indexOf (xtestref);
			if (bIschild == -1) {
				parent.mitteu.location.href = xtestref;
			}
		}
		else {
			xtestref = testref.substr (0, testref.indexOf ('2.htm')) + '.htm';
			bIschild = parent.mitteu.location.href.indexOf (testref);
			if ((bIschild == -1) && (testref.length > 0))
				{
				parent.mitteu.location.href = testref;
				}
			parent.testref='';
			bIschild = parent.mitteo.location.href.indexOf (xtestref);
			if (bIschild == -1) {
				parent.mitteo.location.href = xtestref;
			}
		}
	}
}

