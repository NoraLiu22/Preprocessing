    });

    $('#endDateButton').click(function () {
        $("#datepickerend input[type=text]").datepicker("show");
    });
	*/

    $("#datepickerstart input[type=text]").change(function () {
        $("form").submit();
    });

    $("#datepickerend input[type=text]").change(function () {
        $("form").submit();
    });

    $("#datepickerstart input[type=text], #datepickerend input[type=text], .submit-on-enter").keyup(function (e) {
        if (e.which === 13) {
            $("form").submit();
        }  
    });

    $("#datepickerstart input[type=text], #datepickerend input[type=text]").mask('0000');

    /*
    $("#datepickerend input[type=text]").keyup(function (e) {
        if (e.which == 13) {
            $("form").submit();
        }          
    });*/
});
</script>
