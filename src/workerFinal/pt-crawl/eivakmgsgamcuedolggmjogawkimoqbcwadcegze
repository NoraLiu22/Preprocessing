        jQuery(document).ready(function() {
            var toggleAccordion = function(e) {
                e.preventDefault();
                $(e.target)
                    .closest('.js-footer-accordion-mobile')
                    .toggleClass('is-open-mobile');
            };
            $('.js-footer-accordion-mobile__trigger').on('click', toggleAccordion);
        });
