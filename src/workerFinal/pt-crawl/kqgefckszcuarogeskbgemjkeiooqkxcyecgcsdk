              <source srcset="/sites/default/files/styles/hero_l/public/2016-09/Large_WW170581.jpg?h=d5bf761c&amp;itok=wy3OE1uU 1x" media="all and (min-width: 850px) and (max-width: 1149px)" type="image/jpeg" width="1150" height="895"/>
              <source srcset="/sites/default/files/styles/xlarge/public/2016-09/Large_WW170581.jpg?h=d5bf761c&amp;itok=RjsAvBfI 1x" media="all and (min-width: 1150px)" type="image/jpeg" width="1900" height="895"/>
                  <img loading="eager" src="/sites/default/files/styles/hero_s/public/2016-09/Large_WW170581.jpg?h=d5bf761c&amp;itok=omECKjXf" width="712" height="650" alt="Panda asleep on a rock" title="Panda asleep on a rock - © Bernard De Wetter / WWF" />

  </picture>



  </article>


          </picture>
          <div class="copyright--image">© WWF-Canon / Bernard DE WETTER</div>
          <div class="content">
            <div class="callout_box">
              <h1 class="title  title--stylised ">
                   Top 10 facts about Pandas
               </h1>
                            
              

          <div class="btn-wrap">
                          
                   

     <div class="btn-wrap--item">
     <a href="https://support.wwf.org.uk/adopt-a-panda"  class="btn anchor_link  btn--white" ga-on="click" ga-event-category="CTAButton" ga-event-action="click - https://support.wwf.org.uk/adopt-a-panda" ga-event-label=" Top 10 facts about Pandas  Adopt a panda">
         Adopt a panda
     </a>
     </div>

     
     
               
                   

     <div class="btn-wrap--item">
     <a href="/learn/fascinating-facts"  class="btn anchor_link  btn--white" ga-on="click" ga-event-category="CTAButton" ga-event-action="click - /learn/fascinating-facts" ga-event-label=" Top 10 facts about Pandas  more animal facts">
         more animal facts
     </a>
     </div>

     
     
      
     </div>
            </div>
          </div>
        </section>
                  <nav id="js-page-nav" class="page_nav">
            <div class="content">
                              <a class="anchor_link " href="" ga-on="click" ga-event-category="CTAButton" ga-event-action="click - " ga-event-label=" Top 10 facts about Pandas  "></a>
