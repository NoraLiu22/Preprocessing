        <path id="shape_2-cd89cccc-5ea2-4647-ab61-63bc9d2985f8" d="M13.825 7.8h1.35v2.025H17.2v1.35h-2.025V13.2h-1.35v-2.025H11.8v-1.35h2.025V7.8z" fill="#fff"/>
    </g>
