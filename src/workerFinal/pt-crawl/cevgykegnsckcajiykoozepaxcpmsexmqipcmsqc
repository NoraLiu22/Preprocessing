<script type="text/javascript">
    function OnBegin(xhr) {
        var el = $(this).parents('.widget-content');
        App.blockUI(el);
    }
    function OnSuccess(result) {
        var el = $(this).parents('.widget-content');
        App.unblockUI(el);

        if (result.IsSuccess) {
            noty({
                text: result.Message,
                type: 'success',
                timeout: 5000
            });

            $("#validate-1")[0].reset();
            $("#id").val("0");

            $.ajax({
                url: '/Home/FormProjeDestek',
                type: 'GET',
                dataType: 'html',
                success: function (data) {
                    $("#dtCategories").html(data);
                }
            });
        } else {
            noty({
                text: result.Message,
                type: 'error',
                timeout: 5000
            });
        }
    }
    function OnFailure(xhr, status) {
        var el = $(this).parents('.widget-content');
        App.unblockUI(el);
        noty({
            text: xhr.statusText,
            type: 'error',
            timeout: 5000
        });
    }
</script>
