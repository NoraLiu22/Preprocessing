      <script type="text/javascript">    jQuery(function($) { // DOM ready
        if ($('div#dl').length) {
        $('div#shortcuts').addClass('dl'); $('#dl').addClass('init');
        $('#dl').html('<'+'div class="main"><'+'/div>'); 
        $('#dl>div.main').append('<'+'div id="pdf"><a id="pdfDownload" href="./pdf/4831" title="Download PDF file" class="pdf init"><span class="fileinfo">PDF </span></a></div>');
 $('#dl>div.main').append('<'+'div id="epub"><a id="epubDownload" href="./epub/4831" title="Download ePub file" class="epub init"><span>Download ePub file</span></a></div>');
var apdfDownloadObj = $('a#pdfDownload');
if ($('a#pdfDownload').hasClass('facsimile')) {
    var pdftype='facsimile';
} else {
    var pdftype='pdf';
}
        $('#pdf').prepend('<'+'div id="pdfDownload" class="'+pdftype+' init"></div>');
$('a#pdfDownload > span.fileinfo').clone().appendTo('div#pdfDownload');
        $('a#pdfDownload').remove();
var aepubDownloadObj = $('a#epubDownload');
        $('#epub').prepend('<'+'div id="epubDownload" class="init"></div>');
$('a#epubDownload > span.fileinfo').clone().appendTo('div#epubDownload');
        $('a#epubDownload').remove();
////////////////////
        var pdfepubStatus = function() {
    var checkformats='';
            if($('#pdfDownload').hasClass('init') || $('#pdfDownload').hasClass('waiting')) {
checkformats='pdf';
    }
            if($('#epubDownload').hasClass('init') || $('#epubDownload').hasClass('waiting')) {
checkformats += 'epub';
    }
    if (checkformats == '') {
                $('#dl').removeClass('waiting init');
    }
            var waitingElements = $('#dl.waiting,#dl.init');
            if(!waitingElements.length) {
        if(interval) interval = clearInterval(interval);
                return;
            }
                    var locationport="";
                    if (window.location.port != ''){
                        locationport=":" + window.location.port;
                    } 
            waitingElements.each(function() {
                $.ajax({
                    url: window.location.protocol + '//' + window.location.hostname + locationport + window.location.pathname + '?format=pdfepubstatus&f='+checkformats+'&norecordurl=1',
                    cache: false,
                    error:function(jqXHR, textStatus, errorThrown) {
                        $('#dl').removeClass('waiting init');
                        $('#dl').addClass('error');
                        $('#dl').text("Error generating PDF and ePub files");
                        return;
                    },
                    success:function(data, textStatus, jqXHR) {
                        if(!data) {
                            $('#dl').removeClass('waiting init');
                            $('#dl').addClass('error');
                            $('#dl').text("Error generating PDF and ePub files");
                            return;
                        } else {
                            var response=$.parseJSON(data);
                            if (response.pdfstatus == 'available') {
                                $('#pdf').prepend(apdfDownloadObj);
                                $('a#pdfDownload').removeClass('waiting init');
                                $('a#pdfDownload').addClass('available');
                                $('div#pdfDownload').remove();
                            } else if (response.pdfstatus == 'waiting') {
                                $('#pdfDownload').removeClass('waiting init');
                                $('#pdfDownload').addClass('waiting');
                            } else if (response.pdfstatus == 'unavailable') {
                                $('#pdfDownload').removeClass('waiting init');
                                $('#pdfDownload').addClass('unavailable');
                            } else if (response.pdfstatus == 'error') { $('#pdfDownload').removeClass('waiting init');
                                $('#pdfDownload').addClass('error');
                                $('#pdfDownload > span.fileinfo').text('File not available');
                            }
                           if (response.epubstatus == 'available') {
                                $('#epub').prepend(aepubDownloadObj);
                                $('a#epubDownload').removeClass('waiting init');
                                $('a#epubDownload').addClass('available');
                                $('div#epubDownload').remove();
                            } else if (response.epubstatus == 'waiting') {
                                $('#epubDownload').removeClass('waiting init');
                                $('#epubDownload').addClass('waiting');
                            } else if (response.epubstatus == 'unavailable') {
                                $('#epubDownload').removeClass('waiting init');
                                $('#epubDownload').addClass('unavailable');
                            } else if (response.pdfstatus == 'error') { $('#epubDownload').removeClass('waiting init');
                                $('#epubDownload').addClass('error');
                                $('#epubDownload > span.fileinfo').text('File not available');
                            }
                            if(!$('#pdfDownload').hasClass('init') && !$('#pdfDownload').hasClass('waiting') && !$('#epubDownload').hasClass('init') && !$('#epubDownload').hasClass('waiting')) {
                               $('#dl').removeClass('waiting init');
                    }
                            if (response.accesstype == 'freemium') {
        if (!$('div.accesstype.freemium').length) {
$('#dl>div.main').append('<'+'div class="accesstype freemium"></div><'+'div class="info"><span>Info</span></div>');
                                    $('#dl>div.main>div.info').click(function(){
                                        $('#dl>div.accessdetails').toggle(100, function() {
                                            $('#dl').toggleClass('border-bottom');
});
                                    });
                                    $('#dl div.main div.accesstype.freemium').append('<'+'span>Open Access Freemium</span>');
                                    $('#dl').append('<'+'div class="accessdetails"></div>');
                                    $('#dl div.accessdetails').hide();
}
if ((response.pdfstatus == 'available' || response.epubstatus == 'available') && response.longname.length!=0) {
                                    var format_msg = "<strong>" + response.longname + "</strong>";
                                    format_msg += " has subscribed to <"+"a href=\"https://www.openedition.org/13053\">OpenEdition Freemium for Journals</a> and gives you access ";
    if (response.pdfstatus == 'available' && response.epubstatus == 'available') {
        format_msg += "to this PDF and this ePub.";
    } else if (response.pdfstatus != 'available' && response.epubstatus == 'available') {
        format_msg += "to this ePub.";
    } else if (response.pdfstatus == 'available' && response.epubstatus != 'available') {
        format_msg += "to this PDF.";
    } 
} else {
                                    var format_msg = "PDF and ePub files are available to readers via institutions subscribing to OpenEdition Freemium for Journals. ";
                                    format_msg += " <a class=\"auth\" href=\"https://auth.openedition.org\">Has your institution subscribed to the Freemium programme for Journals?</a>";
}
                                $('#dl div.accessdetails').html(format_msg);
    }
return
                        }
                    }
                });
            });
        };
        pdfepubStatus();
        var interval = setInterval(pdfepubStatus, 5000);
} });</script>                 
