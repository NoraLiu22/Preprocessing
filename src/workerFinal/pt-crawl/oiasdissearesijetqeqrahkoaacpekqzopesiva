    <input id="MathInput" onkeyup="Preview.Update()" class="query form-control" type="text" name="f" value="m &lt; \infty" aria-label="Search for mathematical formulae" placeholder="Search for mathematical formulæ" autocomplete="off">

                        
                    </div>
                    <div class="search-buttons input-group-btn">
