                        throw new Error('Unable to get the value status from the language set'); // Use Error, not FRError
                    }
                }
            };

        })();

        var FRJournalDetails = (function() {
            return {
                JournalType: 'section',
                JournalId: '606',
                SectionId: '161'
            };
    })();

    var FRArticleDetails = (function () {

            return {
