<script src="//ocdn.eu/ucs/static/UTheme/83fbd69d804d8951cff86bdf413dab3e/build/js/menu.min.js" defer></script>


<script>
    function gtmClickTracking(widgetName, widgetPosition, targetUrl) {
        if (!widgetName || !targetUrl) {
            return false;
        }

        

        dataLayer.push({
            event: 'select_content',
            content_type: 'widget',
            content_id: widgetPosition,
            link_id: targetUrl,
            placement: widgetName,
        });

        if (widgetPosition === 'google_news' || widgetPosition === 'googleNews') {
            dataLayer.push({
                event: 'follow',
                method: 'google news',
            });
        }

        return true;
    }

    function followViaGTM(widgetName, widgetPosition, targetUrl) {
        gtmClickTracking(widgetName, widgetPosition, targetUrl);

        

        dataLayer.push({
            event: 'follow',
            method: 'google news',
        });

        return true;
    }
</script>


