<script>
    (() => {
        document.addEventListener("DOMContentLoaded", () => {
            if (!document.body.classList.contains('flag-no_banner')) {
                
                    window.googletag = window.googletag || {};
                    window.vmpbjs = window.vmpbjs || {};
                    window.vpb = window.vpb || {};
                    vpb.cmd = vpb.cmd || [];
                    vpb.st = Date.now();
                    vpb.fastLoad = true;
                    googletag.cmd = googletag.cmd || [];
                    vmpbjs.cmd = vmpbjs.cmd || [];
                    var cmds = [];
                    try {
                        cmds = googletag.cmd.slice(0);
                        googletag.cmd.length = 0;
                    } catch (e) {}
                    var ready = false;

                    function exec(cb) {
                        return cb.call(googletag);
                    }

                    var overriden = false;
                    googletag.cmd.push(function () {
                        overriden = true;
                        googletag.cmd.unshift = function (cb) {
                            if (ready) {
                                return exec(cb);
                            }
                            cmds.unshift(cb);
                            if (cb._startgpt) {
                                ready = true;
                                for (var k = 0; k < cmds.length; k++) {
                                    exec(cmds[k]);
                                }
                            }
                        };
                        googletag.cmd.push = function (cb) {
                            if (ready) {
                                return exec(cb);
                            }
                            cmds.push(cb);
                        };
                    });

                    if (!overriden) {
                        googletag.cmd.push = function (cb) {
                            cmds.push(cb);
                        };
                        googletag.cmd.unshift = function (cb) {
                            cmds.unshift(cb);
                            if (cb._startgpt) {
                                ready = true;
                                if (googletag.apiReady) {
                                    cmds.forEach(function (cb) {
                                        googletag.cmd.push(cb);
                                    });
                                } else {
                                    googletag.cmd = cmds;
                                }
                            }
                        };
                    }

                    var dayMs = 36e5,
                        cb = parseInt(Date.now() / dayMs),
                        vpbSrc = '//player.bidmatic.io/prebidlink/' + cb + `/wrapper_hb_750537_18760.js`,
                        pbSrc = vpbSrc.replace('wrapper_hb', 'hb'),
                        gptSrc = '//securepubads.g.doubleclick.net/tag/js/gpt.js',
                        c = document.head || document.body || document.documentElement;

                    function loadScript(src, cb) {
                        var s = document.createElement('script');
                        s.src = src;
                        s.async = true;
                        c.appendChild(s);
                        s.onload = cb;
                        s.onerror = function () {
                            var fn = function () {};
                            fn._startgpt = true;
                            googletag.cmd.unshift(fn);
                        };
                        return s;
                    }

                    loadScript(pbSrc);
                    loadScript(gptSrc);
                    loadScript(vpbSrc);
                
            };
        })
    })()
</script>

    



















<script>
    function extractSecondParameter(str) {
        var parts = str.split(',');
        if (parts.length >= 2) {
            return parts[1].trim();
        } else {
            return null;
        }
    }

    function addIoAnalyticsScrollDepthDataAttribute() {
        const articleBox = document.querySelector('body.articleDetail div.articleBox');
        if (articleBox) {
            articleBox.setAttribute('data-io-article-url', "https://www.pulse.ng/lifestyle/food-travel/benin-the-hallowed-culture-of-one-of-the-worlds-most-ancient-people/fyg3mz1");
        }
    }

    function addVrZoneAttribute(selector, value) {
        let item = document.querySelector(selector);
        if (item) {
            item.setAttribute('data-vr-zone', value);
        }
    }

    function addVrContentAttribute(selector) {
        let parentItems = document.querySelectorAll(selector);
        parentItems.forEach((item, index) => {
            if ((item.getAttribute('onclick') || '').includes('gtmClickTracking')) {
                const position = extractSecondParameter(item.getAttribute('onclick')) || '';
                let child = item.querySelector('.layout-item');
                child.setAttribute('data-vr-contentbox', position);
                child.setAttribute('data-vr-contentbox-url', child.querySelector('.gradient-overlay a').getAttribute('href'));
            } else {
                // Some sections like `Editors Pick` do not have gtmClickTracking hence widgetPosition cannot be obtained.
                let child = item.querySelector('.layout-item');
                child.setAttribute('data-vr-contentbox', '');
                child.setAttribute('data-vr-contentbox-url', child.querySelector('.gradient-overlay a').getAttribute('href'));
            }
        });
    }

    function addVrHeadlineAttribute(selector) {
        let items = document.querySelectorAll(selector);
        if (items && items.length > 0) {
            items.forEach(item => {
                item.setAttribute('data-vr-headline', '');
            });
        }
    }

    document.addEventListener('DOMContentLoaded', () => {
        addIoAnalyticsScrollDepthDataAttribute();

        addVrZoneAttribute('.widget-trending-items', 'Trending section');
        addVrZoneAttribute('.basicWidget .items-wrapper', 'Articles sections');
        addVrZoneAttribute('.widget-editors-pick-items', 'Editor pick section');

        addVrContentAttribute('.basicWidget .layout-wrapper');
        addVrContentAttribute('.widget-trending-items .layout-wrapper');
        addVrContentAttribute('.widget-editors-pick-items .items-wrapper');

        addVrHeadlineAttribute('.widget-trending-items .layout-item .title-wrapper .title');
        addVrHeadlineAttribute('.widget-editors-pick-items .layout-item .bw-title .title');
        addVrHeadlineAttribute('.basicWidget .items-wrapper .layout-item .bw-title .title');
    });
</script>

