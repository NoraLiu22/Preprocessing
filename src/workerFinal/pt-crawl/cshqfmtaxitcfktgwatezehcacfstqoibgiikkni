        document.querySelectorAll(".ring-detail-social-share").forEach((socialShareWidget) => {
            socialShareWidget.addEventListener('click', (event) => {
                if (event.target.tagName === 'A') {
                   window.dataLayer.push({
                        event: "share",
                        method: event.target.getAttribute("title").toLowerCase(),
                        link_url: window.location.href
                   })
                }
            })
        })
