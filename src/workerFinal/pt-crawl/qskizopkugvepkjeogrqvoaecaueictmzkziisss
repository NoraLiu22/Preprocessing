    /*if(jQuery(this).prop('title') == ''){
       jQuery(this).prop('title','Visit Website');
    } */
    if(jQuery(this).text() == ''){
       jQuery(this).text('Visit Website');
    }
  });
  
  if('' === 'Yes'){
    jQuery('section.hero.container-fluid').css('margin-top','0');
  }
        
});//end document.ready

