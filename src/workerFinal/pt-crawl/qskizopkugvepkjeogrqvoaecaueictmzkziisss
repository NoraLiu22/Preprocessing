$(document).ready(function(){
  
  //START script to truncate characters in h1
  $('.banner_title_char_count').each(function (f) {
      var newstr = $(this).text().substring(0,50);
      $(this).text(newstr);
  });  
  $('.banner_subtitle p').each(function (f) {
      var newstr = $(this).text().substring(0,40);
      $(this).text(newstr);
  });
  //END script to truncate characters in h1
  
  //fix empty button labels
  $('a.hero_slider_button button.uf-partial-border-button').each(function(){
    					console.log( jQuery(this).html() );
