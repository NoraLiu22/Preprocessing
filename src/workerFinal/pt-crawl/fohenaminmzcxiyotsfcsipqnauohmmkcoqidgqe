  <script>
    var dataLayerOb = {
    		"event": "pageView",
    		"page": {"path":"/business-services/filming.html","site":"main site","siteSearchResultPage":false,"errorPage":false,"articlePage":false,"domain":"www.nhm.ac.uk","transactionPage":false,"title":"Filming | Natural History Museum","url":"https://www.nhm.ac.uk/business-services/filming.html"},
    		"user": {"id2":"","id1":""}
    };

    const pollForAdobeAnalytics = () => {
      let interval;
      let timer = 0;
      const TIME_LIMIT = 60000;
      interval = setInterval(() => {
        timer += 10;
        if (timer > TIME_LIMIT) {
          clearInterval(interval);
          console.log("adobe analytics failed to load within one minute")
          return;
        }
        if (window.__satelliteLoaded) {
          const dataLayerEvent = new CustomEvent("dataLayerEvent", { detail: dataLayerOb });
          document.querySelector("body").dispatchEvent(dataLayerEvent);
          
          clearInterval(interval);
        } 
      }, 10)
    }

    window.addEventListener("DOMContentLoaded", () => {
      pollForAdobeAnalytics();
    } );
  </script>


    

	
	
