                let i=2;
                let setPos = false;
                do {
                    i++;
                    if (paraElements[i].getBoundingClientRect().top > firstEnhancePos.bottom) { setPos = true }
                } while (!setPos && (i < paraElements.length - 3))
                if (setPos) { paraElements[i].insertAdjacentElement("beforebegin", newMidAdDiv);}
            }
        }
    }
