    (function () {
        var name = "Quality Hotel™ Ramsalt";
        var pagePath = window.location.pathname;
        var links = document.querySelectorAll(".js-explorer-link");
        if (links) {
            for (var i = 0; i < links.length; i++) {
                links[i].setAttribute('data-track-label', pagePath + '+' + name);
            }
        }
    })();
</script>
 
