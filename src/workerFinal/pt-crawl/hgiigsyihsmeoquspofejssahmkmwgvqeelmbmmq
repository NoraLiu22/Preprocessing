<img srcset="https://cdn.vox-cdn.com/thumbor/XsyYoHaemyJiCL192dSrgD-CvqM=/0x0:3000x2000/320x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 320w, https://cdn.vox-cdn.com/thumbor/B1mSw3m6McEN0ehf-iHUp-Bsp_Y=/0x0:3000x2000/520x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 520w, https://cdn.vox-cdn.com/thumbor/uBA8c42zzPYUkaHGH7wvQ8dh9cE=/0x0:3000x2000/720x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 720w, https://cdn.vox-cdn.com/thumbor/hBzS4wjeGYo8jnnP8iW6MOUFLes=/0x0:3000x2000/920x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 920w, https://cdn.vox-cdn.com/thumbor/QNnmD0PwK-IlEBikuZosbtQPMzY=/0x0:3000x2000/1120x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 1120w, https://cdn.vox-cdn.com/thumbor/-XQvv7Y6VUTQLQZxiSkvLEsiQAs=/0x0:3000x2000/1320x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 1320w, https://cdn.vox-cdn.com/thumbor/RbnIPjUnPmZ3MYrQBIvkgaR__kQ=/0x0:3000x2000/1520x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 1520w, https://cdn.vox-cdn.com/thumbor/9slgoEAOY2mIy72kt7XAcxkyCfY=/0x0:3000x2000/1720x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 1720w, https://cdn.vox-cdn.com/thumbor/-eBAPo_YleCKwoac7J4l-9YkUWE=/0x0:3000x2000/1920x0/filters:focal(0x0:3000x2000):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/13769045/GettyImages_496526892_t.jpg 1920w" sizes="90vw" alt="" loading="lazy" data-upload-width="3000" width="3000" height="2000" src="https://cdn.v