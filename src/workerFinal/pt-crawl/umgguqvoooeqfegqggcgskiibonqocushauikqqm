<script type="text/javascript">
function MoreInfo(LinkData)
{
	window.open(LinkData,'Pubsearch','width=400,height=350,screenX=0,screenY=0,top=0,left=0,scrollbars=yes,resizable=no');
}

function IsSearching()
{
	document.PubSearch.HasSearched.value = '1';
	document.PubSearch.pubspagenum.value = '1';
}	

function SortOrder(SortField)
{
    if (SortField == window.document.PubSearch.sort.value)
    {
        if (window.document.PubSearch.order.value == 1) 
        {
            window.document.PubSearch.order.value = 2;
        }
        else
        {
            window.document.PubSearch.order.value = 1;
        }
    }
    else
    {
        window.document.PubSearch.order.value = 1;
    }
    var centerName = 'NCES';

    if (centerName != 'NCES'){
        window.document.PubSearch.centername.value = window.document.PubSearch.center.options[document.PubSearch.center.selectedIndex].text;
    }

    window.document.PubSearch.sort.value=SortField;
    document.PubSearch.submit();
}

function SubmitIt()
{
    document.PubSearch.pubspagenum.value = '1';
    document.PubSearch.submit();
}    
</script>

