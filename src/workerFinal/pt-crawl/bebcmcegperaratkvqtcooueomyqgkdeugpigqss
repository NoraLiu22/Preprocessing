<script type="text/javascript">
jQuery(function($) {

	var now = new Date().getTime();
	var SECOND = 1000;
	var MINUTE = 60 * SECOND;
	var HOUR = 60 * MINUTE;
	var DAY = 24 * HOUR;

	$( "#latestNews .timestamp" ).each(function (i) {
		if ( $(this).attr("time") ) {
			var time = $(this).attr("time");
			var diff = now - time;
			if ( diff > 0 ) {
				var hours = Math.floor( diff / HOUR );
				var minutes = Math.floor( diff % HOUR / MINUTE );
				var seconds = Math.floor( diff % HOUR % MINUTE / SECOND );
				if ( diff < MINUTE ) {
					$(this).html( " " + seconds + " secs ago" );
				} else if ( diff < HOUR ) {
					if (minutes > 1)
						$(this).html( " " + minutes + " mins ago" );
					else
						$(this).html( " " + minutes + " min ago" );
				} else if ( diff < DAY ) {
					if (hours > 1)
						$(this).html( " " + hours + " hrs ago" );
					else
						$(this).html( " " + hours + " hr ago" );
				}
			}
		}
	});
});
</script>
