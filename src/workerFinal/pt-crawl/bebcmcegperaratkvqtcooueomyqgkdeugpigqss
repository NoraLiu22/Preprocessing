			MPlinkSet0[i].attachEvent('onclick', MPsetTabPrefReturn0);
		}
	}
	for (i=0;i<MPlinkSet1.length;i++) {
		if (document.addEventListener) {
			MPlinkSet1[i].addEventListener('click', MPsetTabPrefReturn1, false);
		} else if (document.attachEvent) {
			MPlinkSet1[i].attachEvent('onclick', MPsetTabPrefReturn1);
		}
	}
	for (i=0;i<MPlinkSet2.length;i++) {
		if (document.addEventListener) {
			MPlinkSet2[i].addEventListener('click', MPsetTabPrefReturn2, false);
		} else if (document.attachEvent) {
			MPlinkSet2[i].attachEvent('onclick', MPsetTabPrefReturn2);
		}
	}
function MPsetTabPrefReturn0() {
	document.cookie="mostPopTabs=0";
}
function MPsetTabPrefReturn1() {
	document.cookie="mostPopTabs=1";
}
function MPsetTabPrefReturn2() {
	document.cookie="mostPopTabs=2";
}
function MPfortGetCookie(cookieName) {
	var searchStr = cookieName + "=";
	var returnValue = "";
	if (document.cookie.length > 0) {
		offsetStr = document.cookie.indexOf(searchStr)
		if (offsetStr != -1) { 
			offsetStr += searchStr.length
			endStr = document.cookie.indexOf(";", offsetStr);
		if (endStr == -1) endStr = document.cookie.length;
			returnValue=unescape(document.cookie.substring(offsetStr, endStr))
		}
	}
	return returnValue;
}
var MPSelectMod = document.getElementById("fortuneTabsMostPop");
if(document.cookie.match("mostPopTabs=1")) {
	MPSelectMod.className = "Tab1Visible";
} else if(document.cookie.match("mostPopTabs=2")) {
	MPSelectMod.className = "Tab2Visible";
} else {
	MPSelectMod.className = "Tab0Visible";
}
/*if (MPfortGetCookie("mostPopTabs")!="") {
	var MPSelectMod = document.getElementById("fortuneTabsMostPop");
	var MPSelectTab = MPfortGetCookie("mostPopTabs");
	MPSelectMod.className = "Tab"+MPSelectTab+"Visible";
}*/
//alert(document.cookie);
</script></div>
