	<script type="text/javascript">

	// Uses both the shortcut for $(document).ready()
	// and the argument to write failsafe jQuery code
	// using the $ alias, without relying on the global alias.

jQuery(function($) {
	// Your code using failsafe $ alias here...

	var now = new Date().getTime();
	var SECOND = 1000;
	var MINUTE = 60 * SECOND;
	var HOUR = 60 * MINUTE;
	var DAY = 24 * HOUR;

	$( ".fortuneEyebrowTimestamp .timestamp" ).each(function (i) {
		if ( $(this).attr("time") ) {
			var time = $(this).attr("time");
			var diff = now - time;
			if ( diff > 0 ) {
				var hours = Math.floor( diff / HOUR );
				var minutes = Math.floor( diff % HOUR / MINUTE );
				var seconds = Math.floor( diff % HOUR % MINUTE / SECOND );
				if ( diff < MINUTE ) {
					$(this).html( seconds + " " + ( seconds == 1 ? "second" : "seconds" ) + " ago" );
				} else if ( diff < HOUR ) {
					$(this).html( minutes + " " + ( minutes == 1 ? "minute" : "minutes" ) + " ago" );
				} else if ( diff < DAY ) {
					$(this).html( hours + " " + ( hours == 1 ? "hour" : "hours" ) + " ago" );
				}
			}
		}
	});

});

	</script>
