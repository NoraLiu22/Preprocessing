<script type="text/javascript" src="https://i.cdn.turner.com/money/.element/script/jquery/plugins/scrollTo/1.2.3/compressed/jquery.scrollTo-min.js"></script>
	<script type="text/javascript">
		jQuery(function( $ ){
			$('.scrollArrows a')
				.filter('.next').bind('click', 'next', moveR )
				.end()
				.filter('.prev').bind('click', 'prev', moveL );
				
			function moveR( e ){
				$('#featuredContent').scrollTo( '+=194px', {
					speed:800,
					axis:'x'
				});
				return false;
			};
			function moveL( e ){
				$('#featuredContent').scrollTo( '-=194px', {
					speed:800,
					axis:'x'
				});
				return false;
			};
		});
	</script>
