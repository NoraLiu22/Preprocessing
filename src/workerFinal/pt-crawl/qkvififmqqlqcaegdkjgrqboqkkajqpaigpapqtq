    <script type="text/javascript">
       
        function OpenViewerCVWindows(id) {
            var w = screen.width;
            var h = screen.height;
            var popW = 1000, popH = 760;

            var leftPos = (w - popW) / 2;
            var topPos = (h - popH) / 2;
            var myWin = window.open('../../NewTextViewer.aspx?TextID=AR_' + id, 'TxtViewer', 'scrollbars=1,toolbar=0,location=0,statusbar=0,menubar=0,resizable=1,width=' + popW + ',height=' + popH + ',left=' + leftPos + ',top=' + topPos);
            if (myWin && !myWin.closed)
                myWin.focus();
        }
        function ShowDetails()
        {
            var divDetails = document.getElementById('ctl00_ContentPlaceHolder1_divDetails');
            divDetails.style.display = 'block';
            var ancrShowDetails = document.getElementById('ancrShowDetails');
            ancrShowDetails.className = 'current';
            
            var divPeriods = document.getElementById('ctl00_ContentPlaceHolder1_divPeriods');
            divPeriods.style.display = 'none';
            var ancrShowPeriods = document.getElementById('ancrShowPeriods');
            ancrShowPeriods.className = 'dimmed2';
        }
    
        function ShowPeriods()
        {
            var divDetails = document.getElementById('ctl00_ContentPlaceHolder1_divDetails');
            divDetails.style.display = 'none';
            var ancrShowDetails = document.getElementById('ancrShowDetails');
            ancrShowDetails.className = 'dimmed2';
            
            var divPeriods = document.getElementById('ctl00_ContentPlaceHolder1_divPeriods');
            divPeriods.style.display = 'block';
            var ancrShowPeriods = document.getElementById('ancrShowPeriods');
            ancrShowPeriods.className = 'current';
        }
        
        function ShowMembers(periodIndex)
        {
            var hdnSelectedPeriodIndex = document.getElementById('ctl00_ContentPlaceHolder1_hdnSelectedPeriodIndex');
            hdnSelectedPeriodIndex.value = periodIndex;
            
            //call the server side code to show ministry members of the selected period
            document.getElementById('ctl00_ContentPlaceHolder1_btnShowMembers').click();
        }
    </script>  
     
                                
