  (function () {
      window.dataLayer = window.dataLayer || [];

      let configCounter = 0;

      function gtmLoad() {
        const started = window.dataLayer.filter(function(item) {
          return Boolean(item['gtm.start']);
        });

        return configCounter && started.length === configCounter;
      }

