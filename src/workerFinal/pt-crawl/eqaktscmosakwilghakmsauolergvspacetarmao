              <span class="button outline indicator"></span>
            </button>
            <button class="carousel_indicator" aria-label="Scroll to item 8" data-index="7">
              <span class="button outline indicator"></span>
            </button>
      </nav>


    <script>{
        const target = document.currentScript.closest('.carousel');
        window.addEventListener('mainScripts', () => new BrockmanCarousel({ target }));
    }</script>
  </div>
  </section>



