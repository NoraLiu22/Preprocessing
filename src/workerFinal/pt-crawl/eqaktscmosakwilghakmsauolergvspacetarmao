  <section class="shelf" data-count="4" >
        <div class="section_title_wrapper">
            <div class="section_title">Bestest Bests</div>
        </div>







    <ul class="summary_items">

        <li>



<div
  class="summary"
  data-type="article"
  data-article-type="features"
  data-premium="false"
  data-external="false"
  data-size="small"
>


<div class="thumbnail">
  <img
    src="https://assetsio.gnwcdn.com/best-board-games-on-pc-2024.jpg?width=275&amp;height=155&amp;quality=80&amp;format=jpg&amp;auto=webp"
    srcset="https://assetsio.gnwcdn.com/best-board-games-on-pc-2024.jpg?width=275&amp;height=155&amp;quality=80&amp;format=jpg&amp;auto=webp 1x, https://assetsio.gnwcdn.com/best-board-games-on-pc-2024.jpg?width=275&amp;height=155&amp;quality=80&amp;format=jpg&amp;dpr=2&amp;auto=webp 2x"
