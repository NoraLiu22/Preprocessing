  <div class="advert_container halfpage_container">
    <div class="advert mpu"
      data-dfp-id="RPS_HP"
      data-dfp-sizes="300x600, 300x250, 300x1050"
      data-dfp-targeting="site=rockpapershotgun.com"
      data-dfp-above-the-fold="true"
      id="RPS_HP_1"
        data-dfp-collapse="true"
    ></div>
  </div>

