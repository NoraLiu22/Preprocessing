    <link rel="stylesheet" media="all" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" />
  <link href="css/nhdes.css?v03112021" rel="Stylesheet" media="screen" type="text/css" />
  <link href="css/formatting.css?v10262022" rel="Stylesheet" media="screen" type="text/css" />
  <link rel="stylesheet" href="css/leaflet/v0.7.7/leaflet.css" />
  <link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/dojo/1.10.4/dijit/themes/claro/claro.css" />
  <link rel="stylesheet" href="css/leaflet.label.css" />
  <link rel="stylesheet" href="css/AirQuality.css" />
  <style type="text/css">
    
  </style>
  <script type="text/javascript">
    // JS classes in ./js/jscl
    var dojoConfig = {
      baseUrl: "./js/",
      tlmSiblingOfDojo: false
    };
  </script>
  <script src="js/leaflet/v0.7.7/leaflet.js" type="text/javascript"></script>
  <script src="js/leaflet.label.js" type="text/javascript"></script>
  <script src="//code.highcharts.com/adapters/standalone-framework.js" type="text/javascript"></script>
  <script src="js/moment.js" type="text/javascript"></script>
  <script src="//ajax.googleapis.com/ajax/libs/dojo/1.10.4/dojo/dojo.js" data-dojo-config="async: true" type="text/javascript"></script>
  <script type="text/javascript">
    app = new Object();
    require(['dojo/_base/declare', 'dojo/_base/lang', 'dojo/dom', 'dojo/on', 'dojo/dom-attr', 'dojo/dom-style', 'dojo/query', 'dojo/_base/array', 'dojo/request/xhr', 'dojo/json', 'dojo/dom-construct', 'dojo/dom-geometry', 'dojo/window', 'dojo/_base/window', 'dijit/Dialog', 'dojo/_base/fx', 'dojo/dnd/Moveable', 'dojo/date', 'dojo/domReady!'], function (declare, lang, dom, on, domAttr, domStyle, query, array, xhr, JSON, domConstruct, domGeom, win, winbase, Dialog, fx, Moveable, date) {
      var url = 'json/forecast_data.json',
          url2 = 'GeoJSON/MapJSON.jgz';
      xhr(url, {
        handleAs: 'json'
      }).then(function (data) {
        data.d0 = moment(Date.parse(data.date));
        data.d1 = moment(data.d0).add(1, 'days');
        data.d2 = moment(data.d0).add(2, 'days');
        data.d3 = moment(data.d0).add(3, 'days');
        app.forecast_data = data;
        app.buildPage();
      }, function (err) {
        alert('Error');
      });
      xhr(url2, {
        handleAs: 'json'
      }).then(function (data) {
        countyBoundaryJSON = data['countyboundaries'];
        monareasJSON = data['monareas'];
      }, function (err) {
        alert('Error2');
      });

      app.hideMap = function () {
        var foreMapDiv = dom.byId('foreMapDiv');
        if (foreMapDiv) {
          domStyle.set(foreMapDiv, { 'display': 'none' });
        }
      };

      app.buildPage = function () {
        var divForecast = dom.byId('divForecast'),
            table, tbody, tr, th, td, anch;

        // Helper function: returns a sort list of keys found in elements object.
        var buildElementKeys = function () {
          elementKeys = [];
          for (var k in elements) {
            elementKeys.push(k);
          }
          elementKeys.sort();
          return elementKeys;
        };

        // Helper function:  builds the header for for the specified type ('County' or 'Region').
        var buildHeader = function (s) {
          tr = domConstruct.create('tr', {}, tbody);
          th = domConstruct.create('th', { 'innerHTML': s }, tr);
          array.forEach([0, 1, 2, 3], function (i) {
            th = domConstruct.create('th', { 'innerHTML': headerDate(i) + '<br>' }, tr);
            anch = domConstruct.create('a', { 'innerHTML': 'view map', 'href': 'javascript:void(0)' }, th);
            on(anch, 'click', function (e) {
              viewMap(i);
            });
          });
        };

        var viewMap = function (col) {
          //alert('Will show forecast map for ' + dObj.format('dddd, MMMM D') + '.');
          // First step:  query the table cells for this column (col)
          var dObj = app.forecast_data['d' + col],
              qs = 'td.data[id$="_' + col + '"]',
              cells = query(qs),
              mapShading = {},
              foreMapDiv = dom.byId('foreMapDiv'),
              foreMapHeader = dom.byId('foreMapHeader'),
              foreMapContainer = dom.byId('foreMapContainer'),
              airdataTable = dom.byId('airdataTable'),
              posTable = domGeom.position(airdataTable, true),
              hStr = 'Air Quality Forecast map for:<br>';
          hStr += dObj.format('dddd') + ', ' + dObj.format('MMMM D, YYYY') + '<br>';
          hStr += 'Primary Pollutant: ' + app.forecast_data.poc;
          mDate = dObj;

          var aCnt = 0;
          array.forEach(cells, function (n) {
            aCnt += 1;
            var nID = n.id,
                region = nID.split('_')[1] + '',
                aqi = domAttr.get(n, 'class').replace('data aqi_', '');
            mapShading[region] = aqi;
          });
          MmapShading = mapShading;

          // Second step:  create foreMapDiv if it does not already exist
          if (!foreMapDiv) {
            foreMapDiv = domConstruct.create('div', {
              'id': 'foreMapDiv', 'style':
              { 'top': posTable.y + 'px', 'visibilty': 'hidden' }
            }, winbase.body());
            var w = domStyle.get(foreMapDiv, 'width'),
                h = domStyle.get(foreMapDiv, 'height'),
                valLeft = (posTable.x - (w + 10));
            domStyle.set(foreMapDiv, { 'left': Math.max(0, valLeft) + 'px', 'visibility': 'visible' });
            foreMapHeader = domConstruct.create('div', { 'id': 'foreMapHeader', 'innerHTML': hStr }, foreMapDiv);
            h = h - domGeom.position(foreMapHeader, true).h;
            on(foreMapHeader, 'mouseover', function () {
              app.dnd = new Moveable(foreMapDiv);
            });
            on(foreMapHeader, 'mouseout', function () {
              app.dnd.destroy();
            });
            foreMapContainer = domConstruct.create('div', {
              'id': 'foreMapContainer',
              'style': { 'width': (w - 5) + 'px', 'height': (h - 5) + 'px' }
            }, foreMapDiv);
          } else {
            domStyle.set(foreMapDiv, { 'display': 'block' });
            if (foreMapHeader) {
              domConstruct.destroy(foreMapHeader);
            }
            foreMapHeader = domConstruct.create('div', { 'id': 'foreMapHeader', 'innerHTML': hStr }, foreMapDiv, 'first');
            on(foreMapHeader, 'mouseover', function () {
              app.dnd = new Moveable(foreMapDiv);
            });
            on(foreMapHeader, 'mouseout', function () {
              app.dnd.destroy();
            });
          }

          if (typeof (myMap) !== 'undefined') {
            if (myMap.panTo) {
              myMap.remove();
            }
          }

          myMap = L.map(foreMapContainer, {
            center: [43.99676629896825, -71.619873046875],
            zoom: 7.40,
            dragging: false,
            touchZoom: false,
            scrollWheelZoom: false,
            doubleClickZoom: false,
            boxZoom: false,
            trackResize: false,
            zoomControl: false,
            attributionControl: false
          });

          L.geoJson(monareasJSON, {
            onEachFeature: function (feature) {
              null;
            },
            style: function (feature) {
              // Base style
              var myStyle = { 'color': '#000', 'weight': 1, 'fillOpacity': 1 },
                aqiVal = mapShading[feature.properties.FIPS],
                shadeColor = app.aqiColor(aqiVal);
              myStyle.fillColor = shadeColor;
              myStyle.color = shadeColor;
              return myStyle;
            }
          }).addTo(myMap);
          L.geoJson(countyBoundaryJSON, { style: { 'color': '#000', 'weight': 1 } }).addTo(myMap);

          var labels = [{ 'name': 'Coos', 'latlng': [44.7, -71.35] },
                        { 'name': 'Grafton', 'latlng': [43.88, -71.96] },
                        { 'name': 'Carroll', 'latlng': [43.83, -71.30] },
                        { 'name': 'Sullivan', 'latlng': [43.37, -72.35] },
                        { 'name': 'Belknap', 'latlng': [43.53, -71.55] },
                        { 'name': 'Merrimack', 'latlng': [43.31, -71.80] },
                        { 'name': 'Strafford', 'latlng': [43.29, -71.18] },
                        { 'name': 'Cheshire', 'latlng': [42.90, -72.39] },
                        { 'name': 'Hillsborough', 'latlng': [42.97, -71.92] },
                        { 'name': 'Rockingham', 'latlng': [43.03, -71.30] }];

          array.forEach(labels, function (l) {
            var lIcon = L.divIcon({
              html: l.name,
              className: 'countyLabel'
            });
            var lDiv = L.marker(l.latlng, { icon: lIcon }).addTo(myMap);
          });

          var getLegendVals = function () {
            var legendVals = [];
            for (k in MmapShading) {
              if (legendVals.indexOf(MmapShading[k]) === -1) {
                legendVals.push(MmapShading[k]);
              }
            }
            legendVals.sort();
            return legendVals;
          };

          var MapLegend = L.Control.extend({
            options: {
              position: 'topleft'
            },
            onAdd: function (map) {
              var legendVals = getLegendVals(),
                  div = domConstruct.create('div', { 'className': 'mapLegend infoPanel' }),
                  i, br, span;
              domConstruct.create('div', { 'innerHTML': 'Map Legend', 'style': { 'fontWeight': 'bold' } }, div);
              domConstruct.create('br', {}, div);
              array.forEach(legendVals, function (aqi) {
                var c = app.aqiColor(aqi),
                    n = app.aqiLU(aqi).replace(/<br>/ig, ' ');
                i = domConstruct.create('i', { 'style': { 'background': c } }, div);
                span = domConstruct.create('span', { 'innerHTML': n }, div);
                br = domConstruct.create('br', {}, div);
              });
              return div;
            }
          });
          var CloseButton = L.Control.extend({
            options: {
              position: 'bottomright'
            },
            onAdd: function (map) {
              var div = domConstruct.create('div', { 'id': 'divCloseForeMap' }),
          button = domConstruct.create('input', { 'type': 'button', 'value': 'Close Map' }, div);
              on(button, 'click', function () {
                app.hideMap();
              });
              return div;
            }
          });

          myMap.addControl(new MapLegend());
          myMap.addControl(new CloseButton());
        };



        // Helper function:  builds the date string for display in the header.
        var headerDate = function (n) {
          var dObj = app.forecast_data['d' + n],
              s = dObj.format('dddd') + '<br>' + dObj.format('MMMM D');
          return s;
        };

        // Elements in the table
        var elements = {
          '01': { 'name': 'Belknap' },
          '03': { 'name': 'Carroll' },
          '05': { 'name': 'Cheshire' },
          '07': { 'name': 'Coos' },
          '09': { 'name': 'Grafton' },
          '11': { 'name': 'Hillsborough' },
          '13': { 'name': 'Merrimack' },
          '15': { 'name': 'Rockingham' },
          '17': { 'name': 'Strafford' },
          '19': { 'name': 'Sullivan' },
          '21': { 'name': 'Seacoast*' },
          '23': { 'name': 'High Elevations**' }
        };
        var elementKeys = buildElementKeys();
        divForecast.innerHTML = '';
        dom.byId('spanPOC').innerHTML = app.forecast_data.poc;
        table = domConstruct.create('table', { 'id': 'airdataTable', 'cellpadding': '5', 'border': '2' }, divForecast);
        tbody = domConstruct.create('tbody', {}, table);

        buildHeader('County');

        array.forEach(elementKeys, function (k) {
          if (k === '21') {
            buildHeader('Region');
          }

          tr = domConstruct.create('tr', {}, tbody);
          var tdVals = app.forecast_data.data[k];
          td = domConstruct.create('td', { 'innerHTML': elements[k].name }, tr);

          var dCnt = 0;
          array.forEach(tdVals, function (d) {
            td = domConstruct.create('td', {
              'innerHTML': app.aqiLU(d, true),
              'id': 'dt_' + k + '_' + dCnt,
              'className': 'data aqi_' + d
            }, tr);
            dCnt += 1;
          });
        });
        table = domConstruct.create('table', { 'style': {'marginLeft':'auto', 'marginRight':'auto'}}, divForecast);
        tbody = domConstruct.create('tbody', {}, table);
        tr = domConstruct.create('tr', {}, tbody);
        td = domConstruct.create('td', { 'innerHTML': '* coastal Rockingham county<br>** elevations above 3000 ft in Carroll, Coos and Grafton counties' }, tr);
      };

      app.aqiLU = function (aqi, upper) {
        if (typeof (upper) !== 'boolean') {
          upper = false;
        }
        var lut = { '1': 'Good', '2': 'Moderate', '3': 'Unhealthy<br>for Sensitive<br>People', '4': 'Unhealthy', '5': 'Very<br>Unhealthy', '6': 'Hazardous' },
            retVal = '';
        if (lut.hasOwnProperty(aqi)) {
          if (upper) {
            retVal = lut[aqi].toUpperCase();
          } else {
            retVal = lut[aqi];
          }
        }
        return retVal;
      };
      app.aqiColor = function (aqi) {
        var retVal;
        switch (parseInt(aqi, 10)) {
          case 1:
            // Green
            retVal = '#80ff80';
            break;
          case 2:
            // Yellow
            retVal = '#ffff66';
            break;
          case 3:
            // Orange
            retVal = '#ff8040';
            break;
          case 4:
            // Red
            retVal = '#ff0000';
            break;
          case 5:
            // Purple
            retVal = '#993366';
            break;
          case 6:
            // what color?
            retVal = '#7e0023';
            break;
          default:
            retVal = '#ffffff';
            break;
        }
        return retVal;
      };

    });
  </script>
</head>
