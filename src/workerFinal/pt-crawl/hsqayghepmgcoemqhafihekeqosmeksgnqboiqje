      <div class="views-element-container"><div class="hunting-fishing-regulations-page-1 js-view-dom-id-07516724e9da865f539c11887acd27fa76e18c4b9446b3fc3dc0aacab57de02b">
  
  
  

  
  <form class="views-exposed-form top-form" data-drupal-selector="views-exposed-form-hunting-fishing-regulations-page-1" action="/hunting-trapping/regulations" method="get" id="views-exposed-form-hunting-fishing-regulations-page-1" accept-charset="UTF-8">
  <div class="js-form-item form-item js-form-type-select form-item-species-topic-filter js-form-item-species-topic-filter form-group">
      <label for="edit-species-topic-filter">Find a Regulation by Species</label>
        <select data-drupal-selector="edit-species-topic-filter" id="edit-species-topic-filter" name="species_topic_filter" class="form-select form-control"><option value="All" selected="selected">- Any -</option><option value="49">Deer</option><option value="54">Badger</option><option value="55">Beaver</option><option value="59">Bobcat</option><option value="61">Coyote</option><option value="62">Crow</option><option value="63">Dove</option><option value="65">Elk</option><option value="66">Fox</option><option value="68">Goose</option><option value="69">Groundhog</option><option value="70">Mink</option><option value="71">Muskrat</option><option value="72">Opossum</option><option value="73">Otter</option><option value="74">Pheasant</option><option value="75">Quail</option><option value="76">Rabbit</option><option value="77">Raccoon</option><option value="78">Sora and Virginia Rail</option><option value="79">Squirrel</option><option value="80">Striped Skunk</option><option value="81">Teal</option><option value="82">Turkey</option><option value="83">Waterfowl</option><option value="84">Wilson&#039;s Snipe</option><option value="85">Woodcock</option><option value="199">Bear</option></select>
