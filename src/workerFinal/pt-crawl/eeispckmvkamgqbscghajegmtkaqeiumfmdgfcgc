<script>
  document.getElementById('reset-password1').style.display = 'none';
  document.getElementById('zephr-error1').style.display = 'none';
  document.getElementById('zephr-success1').style.display = 'none';
  

  var loginButton = document.getElementById('login1');
  loginButton.addEventListener('click', function(event) {
    event.preventDefault();
    let form = event.target.parentNode;
    let emailAddress = form.querySelector('[name="email_address"]').value;
    let password = form.querySelector('[name="password"]').value;
    let loginError = form.querySelector('#zephr-error1');
    
    loginError.style.display = 'none';

    var xhrLogin = new(XMLHttpRequest || ActiveXObject)('MSXML2.XMLHTTP.3.0');
    xhrLogin.open('POST', '/blaize/login', true);
    xhrLogin.setRequestHeader('Content-type', 'application/json');
    xhrLogin.onreadystatechange = function() {
      if (xhrLogin.readyState === 4) {
        if (xhrLogin.status === 200) {
          location.reload(true);
        } else if (xhrLogin.status === 401) {
          loginError.innerHTML = 'The email address or password you entered are incorrect.';
        } else {
          loginError.innerHTML = 'Something went wrong! Please try again later.';
        }
        loginError.style.display = 'block';
      }
    };
    xhrLogin.send(JSON.stringify({
      identifiers: {
        email_address: emailAddress
      },
      validators: {
        password: password
      }
    }));
  });

  var toLogin = document.getElementById('zephr-to-login1');
  toLogin.onclick = function(event) {
      event.preventDefault();
      document.getElementById('password_identifier1').style.display = 'block';
      document.getElementById('login1').style.display = 'block';
      document.getElementById('zephr-to-reset-password1').style.display = 'block';
      document.getElementById('reset-password1').style.display = 'none';
      document.getElementById('zephr-to-login1').style.display = 'none';
      document.getElementById('zephr-success1').style.display = 'none';
  };

  var toResetPassword = document.getElementById('zephr-to-reset-password1');
  toResetPassword.onclick = function(event) {
      event.preventDefault();
      document.getElementById('password_identifier1').style.display = 'none';
      document.getElementById('login1').style.display = 'none';
      document.getElementById('zephr-to-reset-password1').style.display = 'none';
      document.getElementById('reset-password1').style.display = 'block';
      document.getElementById('zephr-to-login1').style.display = 'block';
  };

  var handleResetPassword = function(event) {
      event.preventDefault();

      let form = event.target.parentNode;
      let emailAddress = form.querySelector('[name="email_address"]').value;
      let loginError = form.querySelector('#zephr-error1');
      let zephrSuccess = form.querySelector('#zephr-success1');
      let resetButton = form.querySelector('#reset-password1');

      var xhrResetPassword = new(XMLHttpRequest || ActiveXObject)('MSXML2.XMLHTTP.3.0');
      xhrResetPassword.open('POST', '/blaize/users/reset', true);
      xhrResetPassword.setRequestHeader('Content-type', 'application/json');
      xhrResetPassword.onreadystatechange = function() {
        if (xhrResetPassword.status < 300 && xhrResetPassword.status >= 200) {
          zephrSuccess.style.display = 'block';
          resetButton.style.display = 'none';
        } else {
          loginError.innerHTML = (response.message) ? response.message : response;
          loginError.style.display = 'block';
        }
      };
      xhrResetPassword.send(JSON.stringify({
        identifiers: {
            email_address: emailAddress
        }
      }));
  };

  var resetButton = document.getElementById('reset-password1');
  resetButton.addEventListener('click', handleResetPassword);
</script><script type="text/javascript">
