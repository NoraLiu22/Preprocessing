                    var _d = $("#tabs a#tab1");
                    _d.after('<a aria-controls="tab-panel-activities" aria-selected="false" href="#tab-panel-activities" id="tab-activities" data-toggle="tab" role="tab" class="nav-item nav-link">Activities</a>');
