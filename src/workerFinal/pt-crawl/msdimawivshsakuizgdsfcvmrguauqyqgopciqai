    <script>
        const img = new Image;
        img.crossOrigin="anonymous";
        const c = document.createElement('canvas');
        const ctx = c.getContext('2d');

        function setCanvasImage(path,func){
            img.onload = function(){
                c.width = this.naturalWidth
                c.height = this.naturalHeight
                ctx.drawImage(this,0,0)
                c.toBlob(blob=>{
                    func(blob)
                },'image/png')
            }
            img.src = path
        }

      function getWidth() {
        return Math.max(
          document.body.scrollWidth,
          document.documentElement.scrollWidth,
          document.body.offsetWidth,
          document.documentElement.offsetWidth,
          document.documentElement.clientWidth
        );
      }
      async function copy(url){
        try {
          gtag('event', 'Copy', {
                          'event_category' : 'Image',
                          'event_label' : String(location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1'))
                        });
          } catch(e) {
            console.log(e);
          }
        setCanvasImage(url,(imgBlob)=>{
              try{

                  navigator.clipboard.write(
                      [
                          new ClipboardItem({'image/png': imgBlob})
                      ]
                  ).then(e=>{console.log('Image copied to clipboard')})
              } catch(e){
                  console.log(e)
                  $('#alertmodal').modal('show');
              }
            });
       
      }
      

      function launchFullScreen(element) {
        try {
          // statements
        gtag('event', 'View', {
                              'event_category' : 'Fullscreen',
                              'event_label' : location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')
                            });  
        } catch(e) {
          // statements
          console.log(e);
        }
          // Supports most browsers and their versions.
          var requestMethod = element.requestFullScreen || element.webkitRequestFullScreen || element.mozRequestFullScreen || element.msRequestFullScreen;

          if (requestMethod) { // Native full screen.
              requestMethod.call(element);
          }else {
            fullscreenNewWindow();
          }
          isFullScreen = true;
      }
      function fullscreenNewWindow() {
          if ( !window.opener ) {
              window.open(location.href,"_blank",'fullscreen=yes');
          } else {
              window.close();
          }
      } 
      function exitFullScreen(element) {
          var exitMethod = element.exitFullscreen || element.webkitExitFullscreen || element.mozCancelFullScreen;
          if (exitMethod) {
            console.log('Here')
              exitMethod.call(element);
          }else {
            try {
              document.exitFullscreen()
            } catch(e){

            }
          }
           isFullScreen = false;
      }
      var isFullScreen = false;
      $('#fullscreen').on('click',fullscreen);
      function fullscreen() { 
      
          if (!window.isFullScreen){
            
            var maxResUrl="https://bugwoodcloud.org/images/1536x1024/1366086.jpg";
            
            
            if(maxResUrl != $('#mainimage').attr('src')){
              $('#mainimage').attr('src',maxResUrl);
            }
              launchFullScreen(document.getElementsByClassName('fullscreenelement')[0]);
          }
          else
          {
              exitFullScreen(document.getElementsByClassName('fullscreenelement')[0]);
          }
      }

        $('[data-toggle=popover]').popover({html:true,
          trigger:'manual', container:'body'
        });
        $('[data-toggle=popover]').on('click',function(e){
          $(e.delegateTarget).popover('toggle');
        });
        $('[data-toggle=popover]').on('focusout',function(e){
          setTimeout(function(){
                $(e.delegateTarget).popover('toggle');
          },200);
        });

          $('.contextlightbox').on('click',function(){
                try{
                    gtag('event', $(this).text(), {
                                            'event_category' : 'Lightbox',
                                            'event_label' : location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')
                                          });  
                  } catch (e){
                    console.log(e)
                  }
                  eval($(this).data('method')+'($(this))');
                });

          $(document).on('click', function(){
            if( $("#context-menu").hasClass("show")){
               $("#context-menu").removeClass("show").hide()
            }
          });

             $('#mainimage').on('contextmenu',function(e){
                    var top = e.pageY - 10;
                      var left = e.pageX - 90;
                      left = left > getWidth() ? left-getWidth() : left;
                      console.log(left, getWidth());
                      $("#context-menu").css({
                        display: "block",
                        top: top,
                        left: left
                      }).addClass("show");
                    return false;
                }).on("click", function() {
              $("#context-menu").removeClass("show").hide();
            });       

        $(document).on('click','.downloadimage', function(){
          try {
          gtag('event', 'Download', {
                          'event_category' : 'Image',
                          'event_label' : String($(this).data('id')) + ' | ' + $(this).text()
                        });
          } catch(e) {
            console.log(e);
          }
        } );

         $(document).on('click','.text-link', function(){
          try {
          gtag('event', 'Share', {
                          'event_category' : $(this).attr('id'),
                          'event_label' : String($(this).data('id')) + (($(this).attr('id') == 'htmlcode' || $(this).attr('id') == 'imagelink') ?  ' | ' + $('#shareres').children("option:selected").text() : '' )
                        });
          } catch(e) {
            console.log(e);
          }
        } );
         $(document).on('click','.icon-link', function(){
          try {
          gtag('event', 'Share' , {
                          'event_category' : $(this).attr('title'),
                          'event_label' : location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')
                        });
          } catch(e) {
            console.log(e);
          }
        } );

         $(document).on('click','.collapsed', function(){
          try {
          gtag('event', 'View', {
                          'event_category' : 'Details',
                          'event_label' : $(this).text()
                        });
          } catch(e) {
            console.log(e);
          }
        } );
        var lightboxpop=$("#lightboxadd").data('bs.popover');
        var downloadpop=$("#download").data('bs.popover');
        var versions={};
        $.ajax({
            dataType: 'json',
            url: apiroot+'version/.json?include=name,size&fmt=datatable&contentid='+location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')+'&systemid=2',
            success: function (response) {
               versions=response.data;
               var maxversion = versions.reduce(function(max, arr) {
                            return max >= arr[0] ? max : arr[0];
                        }, -Infinity);
               $('[data-target=".collectionmodal"]').data('versionid',maxversion);
               $('[data-target=".collectionmodal"]').data('hide','[name=versionid]');
                lightbox();
                download();
            },
        });
        function lightbox(){
          if(!$("#lightboxadd").length){
            return false;
          }
          var options='';
          for(var i=0;i<versions.length;i++){
              if(versions[i][1] != ''){
                options+=getlightboxstring(versions[i][0],versions[i][1]);
              }
          }
          lightboxpop.options.content=options;
          function getlightboxstring(versionid,versionname){
              return "  <button class='lightbox  btn btn-link' data-versionid='"+versionid+"' data-contentid='"+location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')+"' data-method='Lightbox.add' data-systemid='2' data-title='"+versionname+"'>"+versionname+" </button><br/>";
          }
          $('[data-target=".collectionmodal"]').data('contentid',$(this).data('id'));
           $("#lightboxadd").on('shown.bs.popover',function(){
              Lightbox.setbuttons();
              $('.lightbox').on('click',function(){
                  eval($(this).data('method')+'($(this))');
              });
          });
          $("#lightboxadd").on('shown.bs.popover',function(){
            Lightbox.setbuttons();
          });
        }
        function download(){
          var options='';
          for(var i=0;i<versions.length;i++){
                if(versions[i][0] <=3 ||$("#lightboxadd").length ){
                   options+=getdownloadstring(versions[i][0],versions[i][2]);
                 }else{
                   //options +="<a href='#' class='btn btn-link disabled' title=' '>"+versions[i][2]+"</a><br/>"
                  
                 }
          }
          if(versions.length >2 && !$("#lightboxadd").length){
             options +="<a href='#' class='btn btn-link disabled' title=' '>Login for additional resolutions</a><br/>";
          }
          downloadpop.options.content=options;
          function getdownloadstring(versionid,versionname){
                 return "  <a href='/browse/imgdownjoe.cfm?img="+location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')+"&res="+versionid+"'  title=' Download "+versionname+"'   class='btn btn-link downloadimage' data-id='"+location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')+"'>"+versionname+" </a><br/>";
            }
        }
      function setimagelink(){
        var url="http://bugwoodcloud.org/images/"+$('#shareres :selected').text()+"/"+location.href.replace(/.*imgnum=([0-9]+).*/,'$1')+".jpg";
        $('#imagelink').val(url);
      }
      function sethtmlcode(){
        var url='<a title="title" href="'+$('#directlink').val()+'" target="_blank"><img src="'+$('#imagelink').val()+'" alt="#title#"/> </a>';
        $('#htmlcode').val(url);
      }
      function updatelinks(){
        setimagelink();
        sethtmlcode();
      }
      $(document).ready(function(){
        $('#shareres').change(updatelinks);
      });
    </script>
