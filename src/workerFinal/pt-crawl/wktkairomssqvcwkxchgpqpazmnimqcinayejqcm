				link.addEventListener('click', (e) => {
					eVar94 = "header-search-icon-mobile";
					eVar95 = link.textContent.trim();
					eVar96 = "img";
					eVar97 = document.title;
					tmgComponentString = eVar94+";"+eVar95+"_"+eVar96+";"+eVar97;
