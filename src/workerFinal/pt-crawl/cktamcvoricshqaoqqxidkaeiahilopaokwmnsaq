    const newMidAdDiv = document.createElement("div");
    newMidAdDiv.setAttribute('style', 'text-align: center; margin-bottom: 2rem; min-width: 300px;');
    newMidAdDiv.setAttribute('id', 'div-gpt-ad-1651497971711-0')
