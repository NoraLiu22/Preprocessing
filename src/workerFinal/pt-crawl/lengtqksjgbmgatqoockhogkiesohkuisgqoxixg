    <script>
        window.addEventListener("DOMContentLoaded", (event) => {
            var select = document.getElementById("id_workcourse_authors");
            var check = document.getElementById("id_workcourse_text");
            if(check.value == "1" || check.value == "3") select.disabled = true 
            var memory = []
            check.addEventListener("change", function(){
                if(check.value == "3" || check.value == "1"){
                    if(!select.disabled){
                        select.disabled = true
                        for(var o in select.options){
                            if(select.options[o].selected) memory.push(o)
                            select.options[o].selected = false
                        }
                    }
                }else{
                    if(select.disabled){
                        for(var o in memory){
                            select.options[memory[o]].selected = true
                        }
                        memory = []
                        select.disabled = false
                    }
                }
            })
        });
    </script>

