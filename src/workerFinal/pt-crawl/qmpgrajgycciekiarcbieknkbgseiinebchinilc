            <div class="header-search">
            	<label for="q">Search for:</label>
                <input type="text" name="q" class="hsearch-input" value="What would you like to search for?" onkeypress="return entsub(event)" />
                <a href="javascript:void(SearchSubmit())">
	                <img src="/PublishingImages/masterpage/content/blank.gif" class="hsearch-btn" alt="Search" />
                </a>
			  <script type="text/javascript">
			      function SearchSubmit() {
			       window.location.href = "https://www.itu.int/search#?q=" + document.forms['aspnetForm'].q.value; 
			        //  document.searchform.q.value = document.forms['aspnetForm'].q.value;
			        //  document.searchform.submit();
			      }

			      function entsub(e) {
			          var keyPressed;
			          if (window.event)
			              keyPressed = window.event.keyCode; // IE
			          else
			              keyPressed = e.which; // Firefox

			          if (keyPressed == 13) {
			              //alert("you hit enter key");
			              SearchSubmit();
			              return false;
			          } else
			              return true;
			      }
			  </script>
            </div><!--/.header-search-->
