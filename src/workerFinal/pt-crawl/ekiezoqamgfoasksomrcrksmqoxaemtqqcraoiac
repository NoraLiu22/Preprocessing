    <!-- This needs to run AFTER the page is loaded! -->
    <script type="text/javascript">

        $(document).ready(function ()
        {
            $(".results").hide();

            var renderTextToLeft = $(".packagePreview .renderTextToLeft");
            //Create a container for the structure.
            renderTextToLeft.wrap("<div class='renderTextToLeftContainer'></div>");

            //This code will take any control wih the specified class and  prepend a span directly to
            //the right of the control and copies the controls value into the span.
            //Used for rending button labels to the right of buttons without a css nightmare.
            renderTextToLeft.each(function (index, element)
            {
                $("<span>" + $(element).val() + "</span>").insertBefore($(element));
                $(element).css("color", "transparent");
            });

            //Move the footer into the absolute positioned background so it is
            //visible in the correct location.
            $("#footerLayout").detach().appendTo('.formHeader div.background');

            //Position required for Image Database
            //TODO: Need better path than just login.
            $('div.loginContainer div.login').detach().prependTo("div#headerNavigation");
            $("div#headerNavigation div.login").addClass("globalNavItem");

            //Set up product window if the user is already logged in.
            $.getJSON("handlers/RoleCheck.ashx")
                .done(function (roleToken) { if (roleToken.IsAuthenticated) { PrepareImageDatabasePurchaseScreen(); }})
                .fail(function () { alert("error setting up product purchase screen."); });

        });

    </script>

