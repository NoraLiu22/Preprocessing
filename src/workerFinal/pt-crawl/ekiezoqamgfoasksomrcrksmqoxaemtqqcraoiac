                                        <script type="text/javascript">
                                            function login_onClick()
                                            {

                                                var loginRequest = new Object();
                                                loginRequest.UserId = $(".loginRequired .login .field.userName .value input[type='text']").val();
                                                loginRequest.Password = $(".loginRequired .login .field.password .value input[type='password']").val();
                                                //loginRequired login .field.rememberMe .value
                                                loginRequest.RememberMe = false;
                                                var jsonobj = JSON.stringify(loginRequest);
                                                $.post("\login.axd", jsonobj, function (response)
                                                {
                                                    if (response.LoginSuccessful)
                                                    {
                                                        if (response.ImageDatabaseUser)
                                                        {
                                                            closeMembershipRequiredPrompt();
                                                            var actionToTake = $(".membershipRequiredPromptContainer").data("actionToTake");
                                                            $(".membershipRequiredPromptContainer").removeData("actionToTake");
                                                            actionToTake();
                                                        }
                                                        else
                                                        {
                                                            //User is logged in, but does not hold a subscription
                                                            //Prepate the Purchase screen.
                                                            PrepareImageDatabasePurchaseScreen();

                                                            $(".membershipRequiredPromptContainer").css("display", "block");
                                                            $(".membershipRequiredPromptContainer .backDrop").css("display", "block");
                                                            $(".membershipRequiredPromptContainer .content .loginRequired").css("display", "none");
                                                            $(".membershipRequiredPromptContainer .content .membershipRequired").css("display", "block");
                                                        }
                                                    }
                                                    else
                                                    {
                                                        alert("Login Failed!");
                                                    }

                                                }, "JSON")
                                                .fail(function (response, errorTitle, errorMessage)
                                                {
                                                    alert("There was an error attempting to log you in.");
                                                });
                                            }

                                        </script> 
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>

                    <div>
