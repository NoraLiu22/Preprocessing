    <script type="text/javascript">


        function PrepareImageDatabasePurchaseScreen()
        {
            PreparePurchaseScreen("IMGDB")

        }


        function PreparePurchaseScreen(productCodes)
        {
            $(".controls input[type='button'].cancel").click(closeMembershipRequiredPrompt);
            $(".controls input[type='button'].purchase").click(makePurchase);
            $.getJSON("\AddProduct.axd?pl=" + productCodes, function (data)
            {
                var availableProducts = $("div.availableProducts");
                $(data.PriceLookupList).each(function (index, element)
                {
                    var html = '<div class="productRow">';
                    html += '<span><input type="checkbox" class="rowSelect" value="' + element.Code + '" /></span>';
                    html += '<span class="code">' + element.Code + '</span>';
                    html += '<span class="description">' + element.Description + '</span>';
                    html += '<span class="price" price="' + element.ProratedPrice + '">' + element.FormattedProratedPrice + '</span>';
                    html += '</div>';
                    availableProducts.append(html)
                });

                availableProducts.append('<div class="productRow total"><span value="label">Total</span><span class="value"></span></div>');
                availableProducts.find("input[type='checkbox']").click(SelectProduct);

                //If there is only one possible option then default the selection and hide the total and checkbox.
                if (data.PriceLookupList.length == 1)
                {
                    availableProducts.find(".productRow input[type='checkbox'].rowSelect").prop("checked", true);
                    availableProducts.find(".productRow input[type='checkbox'].rowSelect").css("display", "none");
                    availableProducts.find(".productRow.total").css("display", "none");
                }

                var cctypeselect = $(".creditCard  .cardType select.value");
                cctypeselect.html("");
                cctypeselect.append($('<option></option>').val("").html("Select a credit card type"));
                $(data.CashAccounts)
                    .each(function (index, element)
                    {
                        cctypeselect.append($('<option></option>').val(element.key).html(element.value));
                    });
                populateYearSelect();

            });
        }

        function SelectProduct(sender)
        {
            var total = 0;
            var x = $("div.availableProducts input[type = 'checkbox']:checked");
            var y = x.parents('.productRow').find('span.price');
            y.each(function (index, element) { total += Number($(element).attr("price")) });
            $("div.availableProducts .productRow.total .value").text("$" + total.toFixed(2));
        }

        function populateYearSelect()
        {
            var d = new Date();
            var currentYear = d.getFullYear();
            var yearSelect = $(".creditCard .expirationDate .year select.value");
            yearSelect.html("");
            for (var y = currentYear; y < currentYear + 10; y++)
            {
                yearSelect.append($('<option></option>').val(y).html(y));
            }
        }

        function makePurchase()
        {


            var cmdPurchase =  $(".controls input[type='button'].purchase");
            
            
            cmdPurchase.attr("disabled", "disabled");



            //Get selected products
            var selectedProductCodes = [];
            var x = $("div.availableProducts input[type = 'checkbox']:checked");
            x.each(function (index, element) { selectedProductCodes[selectedProductCodes.length] = $(element).val() });

            var obj = new Object();
            obj.NewProductCodes = selectedProductCodes;
            obj.CardHolderName = $(".creditCard .name input.value").val();
            obj.CashAccount = $(".creditCard .cardType select.value").val();
            obj.CardNumber = $(".creditCard .number input.value").val();
            obj.CardSecurityValue = $(".creditCard .cvv input.value").val();
            obj.CardExpirationMonth = $(".creditCard .expirationDate .month select.value").val();
            obj.CardExpirationYear = $(".creditCard .expirationDate .year select.value").val();
            var json = JSON.stringify(obj);







            $.ajaxSetup({
                crossDomain: true,
                xhrFields: {
                    withCredentials: true
                }
            });
            
            //$.post("\AddProduct.axd", json)
            $.post("https://my.apsnet.org/AddProduct.axd", json)
            .done
            (
                function (response)
                {
                    response = $.parseJSON(response);
                    if (response.Successful)
                    {
                        alert("Payment Authorized.");
                        closeMembershipRequiredPrompt();

                        //Call the originl method reference from a data element
                        var actionToTake = $(".membershipRequiredPromptContainer").data("actionToTake");
                        $(".membershipRequiredPromptContainer").removeData("actionToTake");
                        actionToTake();
                    }
                    else if(response.Declined)
                    {
                        alert("Your card was declined for the following reason - " + response.Message)
                    }
                    else
                    {
                        alert(response.Message)
                    }
                }
            )
            .fail
            (
                function(response)
                {
                    alert("We're sorry, but there was an error processing your credit card.");
                }
            )
            .always
            (
                function (response)
                {
                    cmdPurchase.removeAttr("disabled");
                }
            )
        }

    </script> 


                </div>
            </div>
            <div class="footer"></div>
        </div>
        <div class="backDrop"></div>
    </div>

