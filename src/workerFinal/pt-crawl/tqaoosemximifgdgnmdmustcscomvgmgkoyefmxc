<script>
    if (!ShdCache.get("pdf-notice")) {
        $('#pdfclick').trigger("click");
        ShdCache.set("pdf-notice", 1, true);
    }
    if (!ShdCache.get("terms-notice")) {
        $(".user-terms-hint").show();
        $(".user-terms-hint .glyphicon-remove").click(function () {
            $(".user-terms-hint").remove();
        });
        ShdCache.set("terms-notice", 1, true);
    } else {
        $(".user-terms-hint").remove();
    }
</script>
    <script src="https://obj.shine.cn/website/js/bottom.min.js?20221116-0001"></script>
<script src="https://obj.shine.cn/website/js/pako.min.js" ></script>
<script>
    function unzip(b64Data) {
        var strData = atob(b64Data);
        var charData = strData.split('').map(function (x) { return x.charCodeAt(0); });
        var binData = new Uint8Array(charData);
        var data = pako.inflate(binData);
        strData = String.fromCharCode.apply(null, new Uint16Array(data));
        return strData;
    }
    function zip(str) {
        var binaryString = pako.gzip(str, { to: 'string' });
        return btoa(binaryString);
    }
</script>
