<script src="https://www.google.com/recaptcha/api.js" async defer></script>

<script>
	var captcha_validated = false;
	function Captcha(k) {
		console.log('k=',k)
		document.getElementById('captcha').value=k;
		captcha_validated = k >'';
		IsFormReady();
	}
	function IsEmailValid() {
		return String(document.getElementById('email').value)
			.toLowerCase()
			.match(
				/^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
			);;
	}

	function IsFormReady() {
		console.log(document.getElementById('email').value);
		document.getElementById('btn').className = IsEmailValid() && captcha_validated ? 'button' :'button disabled';
	}

	function button_click(frm) {
		if (IsEmailValid() && captcha_validated) {
			frm.submit();
			return;
		}
		if (!IsEmailValid())
			alert('Please enter a valid email address');
		else if (!captcha_validated)
			alert('please validate yourself as a human');
	}
</script>
*By submitting your email address and clicking Go, you indicate that you have read and agree to the terms of our <a href="https://www.thefreedictionary.com/privacy-policy.htm">Privacy Policy</a> and our <a href="https://www.thefreedictionary.com/about.htm">Terms of Use</a>.
		 
			<br><br>
			<input class="button disabled" id="btn" type="button" onclick="button_click(this.form)" value="Sign Up" />
