                function getParameterByName(name, url) {
                  if (!url) url = window.location.href;
                  name = name.replace(/[\[\]]/g, "\\$&");
                  var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                    results = regex.exec(url);
                  if (!results) return null;
                  if (!results[2]) return '';
                  return decodeURIComponent(results[2].replace(/\+/g, " "));
                }
                
                var dynamicContent = getParameterByName('lang');
                var validValues = ['de','it','fr','es','pt'];                 
                
                jQuery(document).ready(function() {
                   
                  if (validValues.indexOf(dynamicContent) < 0) {
                    dynamicContent='default-content';
                  }

                  jQuery(`#${dynamicContent}`).show();
                  e=document.getElementById(`email-based-opt-out-container_${dynamicContent}`);
                  e.setAttribute('id', 'email-based-opt-out-container');
