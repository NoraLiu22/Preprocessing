        <script src="/static/js/d3.v3.min.js"></script>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".se-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1826, frequency:39 },{ year:1827, frequency:49 },{ year:1828, frequency:49 },{ year:1829, frequency:34 },{ year:1830, frequency:78 },{ year:1831, frequency:36 },{ year:1832, frequency:73 },{ year:1833, frequency:33 },{ year:1834, frequency:75 },{ year:1835, frequency:52 },{ year:1836, frequency:26 },{ year:1837, frequency:60 },{ year:1838, frequency:25 },{ year:1839, frequency:19 },{ year:1840, frequency:49 },{ year:1841, frequency:16 },{ year:1842, frequency:43 },{ year:1843, frequency:44 },{ year:1844, frequency:55 },{ year:1845, frequency:14 },{ year:1846, frequency:106 },{ year:1847, frequency:40 },{ year:1848, frequency:40 },{ year:1849, frequency:20 },{ year:1850, frequency:59 },{ year:1851, frequency:61 },{ year:1852, frequency:38 },{ year:1853, frequency:42 },{ year:1854, frequency:46 },{ year:1855, frequency:50 },{ year:1856, frequency:31 },{ year:1857, frequency:64 },{ year:1858, frequency:22 },{ year:1859, frequency:31 },{ year:1860, frequency:27 },{ year:1861, frequency:49 },{ year:1862, frequency:16 },{ year:1863, frequency:38 },{ year:1864, frequency:30 },{ year:1865, frequency:27 },{ year:1866, frequency:46 },{ year:1867, frequency:26 },{ year:1868, frequency:47 },{ year:1869, frequency:30 },{ year:1870, frequency:44 },{ year:1871, frequency:34 },{ year:1872, frequency:43 },{ year:1873, frequency:41 },{ year:1874, frequency:40 },{ year:1875, frequency:38 },{ year:1876, frequency:22 },{ year:1877, frequency:51 },{ year:1878, frequency:39 },{ year:1879, frequency:44 },{ year:1880, frequency:41 },{ year:1881, frequency:26 },{ year:1882, frequency:32 },{ year:1883, frequency:55 },{ year:1884, frequency:32 },{ year:1885, frequency:43 },{ year:1886, frequency:34 },{ year:1887, frequency:39 },{ year:1888, frequency:35 },{ year:1889, frequency:26 },{ year:1890, frequency:30 },{ year:1891, frequency:30 },{ year:1892, frequency:36 },{ year:1893, frequency:45 },{ year:1894, frequency:33 },{ year:1895, frequency:36 },{ year:1896, freque