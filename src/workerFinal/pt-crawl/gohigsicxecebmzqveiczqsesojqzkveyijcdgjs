<script type="text/javascript">
    $(document).ready(function () {
        FixHeightsMedia();
        $(window).resize(FixHeightsMedia);
    });
    function FixHeightsMedia() {

        var mq = window.matchMedia("(min-width: 991px)");
        if (mq.matches) {
            var maxHeight = 0;
            $('.divMediaContent').each(function () {
                var h = $(this).height();
                if (h > maxHeight) maxHeight = h;
            });
            $('.divMediaContent').height(maxHeight);
        } else {
            $('.divMediaContent').css('height', 'auto');
        }
    }
</script>
