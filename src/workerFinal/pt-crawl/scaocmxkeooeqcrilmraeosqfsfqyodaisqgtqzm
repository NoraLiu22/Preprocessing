<script src="https://kit.fontawesome.com/c3187ed982.js" crossorigin="anonymous"></script>

<script>
    var isBCpub = true;
    var chargePeriod = "annual";
    var selectedProductId;

    document.addEventListener("DOMContentLoaded", function(event) { 
        showAnnualPrice();
        if (!isBCpub) {
            Array.from(document.querySelectorAll(".todayinbc"))
                .forEach(function(val) {
                    val.style.display = 'none';
            });
        }
    });

    var toggleButton = (buttonId) => {
        document.querySelector("." + buttonId).classList.add('is-active');

        const regForm = document.getElementById("bpz-sub-page-reg-form");
        regForm.style.display = 'none';

        if (buttonId == "monthly-button") {
            document.querySelector(".annual-button").classList.remove('is-active');
            showMonthlyPrice();
            chargePeriod = "monthly";
        } else {
            document.querySelector(".monthly-button").classList.remove('is-active');
            showAnnualPrice();
            chargePeriod = "annual";
        }
    }

    var showMonthlyPrice = () => {
        document.querySelector('.bpz-price-description').innerHTML = 'Prices subject to <span>monthly</span> flexible plan';

        var standardMonthlyPrice = 4.99;
        var premiumMonthlyPrice = 5.37;
        var premiumPlusMonthlyPrice = 7.54;
        
        document.querySelector('.bpz-standard span.monthly-price').innerHTML = formatPrice(standardMonthlyPrice); 
        document.querySelector('.bpz-standard span.annual-price').innerHTML = formatPrice(standardMonthlyPrice*12);
        document.querySelector('.bpz-standard span.weekly-price').innerHTML = formatPrice(standardMonthlyPrice*12/52);
        
        document.querySelector('.bpz-premium span.monthly-price').innerHTML = formatPrice(premiumMonthlyPrice);
        document.querySelector('.bpz-premium span.annual-price').innerHTML = formatPrice(premiumMonthlyPrice*12);
        document.querySelector('.bpz-premium span.weekly-price').innerHTML = formatPrice(premiumMonthlyPrice*12/52);

        document.querySelector('.bpz-premium-plus span.monthly-price').innerHTML = formatPrice(premiumPlusMonthlyPrice);
        document.querySelector('.bpz-premium-plus span.annual-price').innerHTML = formatPrice(premiumPlusMonthlyPrice*12);
        document.querySelector('.bpz-premium-plus span.weekly-price').innerHTML = formatPrice(premiumPlusMonthlyPrice*12/52);
        
        Array.from(document.querySelectorAll('.package-description')).forEach(function(val) {
            val.innerHTML = 'Monthly Subscription';
        });

    }
    
    var showAnnualPrice = () => {
        document.querySelector('.bpz-price-description').innerHTML = 'Prices subject to <span>annual</span> plan';

        var standardAnnualPrice = 40.04;
        var premiumAnnualPrice = 51.48;
        var premiumPlusAnnualPrice = 72.28;

        document.querySelector('.bpz-standard span.annual-price').innerHTML = formatPrice(standardAnnualPrice);
        document.querySelector('.bpz-standard span.weekly-price').innerHTML = formatPrice(standardAnnualPrice/52);
        document.querySelector('.bpz-standard span.monthly-price').innerHTML = formatPrice(standardAnnualPrice/12);

        document.querySelector('.bpz-premium span.annual-price').innerHTML = formatPrice(premiumAnnualPrice);
        document.querySelector('.bpz-premium span.weekly-price').innerHTML = formatPrice(premiumAnnualPrice/52);
        document.querySelector('.bpz-premium span.monthly-price').innerHTML = formatPrice(premiumAnnualPrice/12);
        
        document.querySelector('.bpz-premium-plus span.annual-price').innerHTML = formatPrice(premiumPlusAnnualPrice);
        document.querySelector('.bpz-premium-plus span.weekly-price').innerHTML = formatPrice(premiumPlusAnnualPrice/52);
        document.querySelector('.bpz-premium-plus span.monthly-price').innerHTML = formatPrice(premiumPlusAnnualPrice/12);
        
        Array.from(document.querySelectorAll('.package-description')).forEach(function(val) {
            val.innerHTML = 'Annual Subscription';
        });
    }

    var formatPrice = (priceInput) => {
        var price = priceInput.toFixed(2).toString();

        // default display with $ sign
        var priceOutput = `<span class="bpz-price-sup">&#36;</span>${price}`;
        
        // checks if price starts with "0."
        // if yes, strip "0." and output value after the decimal in cent format. 0.99 -> 99c
        if (price.startsWith("0.")) {
            var priceInCents = price.replace("0.", "");
            priceOutput = `${priceInCents}<span class="bpz-price-sup">&#162; </span>`;
        } 

        return priceOutput;
    }

    var setSelectedProductSessionStorage = () => {
        sessionStorage.setItem("selectedProductId", selectedProductId);
        sessionStorage.setItem("selectedChargePeriod", chargePeriod);
    }

    function openRegForm(productId) { 
        const regForm = document.getElementById("bpz-sub-page-reg-form");
        regForm.style.display = 'block';
        scrollToElement(regForm);
        selectedProductId = productId;
    }

    function scrollToElement(element){
        var headerOffset = 125;
        var elementPosition = element.getBoundingClientRect().top;
        var offsetPosition = elementPosition + window.pageYOffset - headerOffset;
    
        window.scrollTo({
            top: offsetPosition,
            behavior: "smooth"
        });
    }

    //-------------- Zephr's Registration Form Script 

    window.blaizeRecaptchaClientId = '';
    function onRecaptchaLoadCallback() {
        window.blaizeRecaptchaClientId = grecaptcha.render('blaize-recaptcha-badge', {
            sitekey: '',
            badge: window.blaizeRecaptchaPosition,
            size: 'invisible'
        });
    }
    (function() {
        if (! window.Blaize) {
            window.Blaize = {
                formPreProcessors: {},
                formPostProcessors: {},
                registerFormPreProcessor: function(formSlug, processor) {
                    if (! window.Blaize.formPreProcessors[formSlug]) window.Blaize.formPreProcessors[formSlug] = [];

                    if (typeof processor === "function") {
                        window.Blaize.formPreProcessors[formSlug].push(processor);
                    }
                },
                registerFormPostProcessor: function(formSlug, processor) {
                    if (! window.Blaize.formPostProcessors[formSlug]) window.Blaize.formPostProcessors[formSlug] = [];

                    if (typeof processor === "function") {
                        window.Blaize.formPostProcessors[formSlug].push(processor);
                    }
                },
                preProcessFormData: function(formSlug, formData) {
                    if (window.Blaize.formPreProcessors[formSlug] || window.Blaize.formPreProcessors['*']) {
                        var processors = [];
                        if (window.Blaize.formPreProcessors['*']) {
                            window.Blaize.formPreProcessors['*'].forEach(function(processor) {
                                processors.push(processor);
                            });
                        }
                        if (window.Blaize.formPreProcessors[formSlug]) {
                            window.Blaize.formPreProcessors[formSlug].forEach(function(processor) {
                                processors.push(processor);
                            });
                        }
                        processors.forEach(function(processor) {
                            processedFormData = processor(formData);
                            if (processedFormData != undefined) {
                                formData = processedFormData;
                            }
                        });
                    }

                    return formData;
                },
                postProcessFormData: function(formSlug, formData, response) {
                    if (window.Blaize.formPostProcessors[formSlug] || window.Blaize.formPostProcessors['*']) {
                        var processors = [];
                        if (window.Blaize.formPostProcessors['*']) {
                            window.Blaize.formPostProcessors['*'].forEach(function(processor) {
                                processors.push(processor);
                            });
                        }
                        if (window.Blaize.formPostProcessors[formSlug]) {
                            window.Blaize.formPostProcessors[formSlug].forEach(function(processor) {
                                processors.push(processor);
                            });
                        }
                        processors.forEach(function(processor) {
                            processor(formData, response);
                        });
                    }
                }
            }
        }

        var section = document.getElementById("form-M-nWlMVb");
        var formRegister = section.querySelector('.blaize-form-register');
        var formLogin = section.querySelector('.blaize-form-login');
        var registerSsiError = formRegister.querySelector('.blaize-ssi-error');
        var registerError = formRegister.querySelector('.blaize-error');
        var registerSuccess = formRegister.querySelector('.blaize-success');
        var loginError = formLogin.querySelector('.blaize-error');
        var loginSuccess = formLogin.querySelector('.blaize-success');
        var identifiersFieldset = formRegister.querySelector('.blaize-identifiers');
        var validatorsFieldset = formRegister.querySelector('.blaize-validators');
        var formResetPassword = section.querySelector('.blaize-form-reset-password');
        var passwordResetError = formResetPassword.querySelector('.blaize-error');
        var passwordResetSuccess = formResetPassword.querySelector('.blaize-success');

        section.querySelector('.blaize-to-login a').onclick = function(event) {
            event.preventDefault();
            formRegister.style.display = 'none';
            formResetPassword.style.display = 'none';
            formLogin.style.display = 'block';
        };

        section.querySelector('.blaize-to-register a').onclick = function(event) {
            event.preventDefault();
            formLogin.style.display = 'none';
            formResetPassword.style.display = 'none';
            formRegister.style.display = 'block';
        };

        section.querySelector('.blaize-to-reset-password a').onclick = function(event) {
            event.preventDefault();
            formLogin.style.display = 'none';
            formRegister.style.display = 'none';
            formResetPassword.style.display = 'block';
        };

        section.querySelector('.blaize-reset-to-login a').onclick = function(event) {
            event.preventDefault();
            formRegister.style.display = 'none';
            formResetPassword.style.display = 'none';
            formLogin.style.display = 'block';
        };

        section.querySelector('.blaize-reset-to-register a').onclick = function(event) {
            event.preventDefault();
            formLogin.style.display = 'none';
            formResetPassword.style.display = 'none';
            formRegister.style.display = 'block';
        };

        var blaizeRequest = function({ url, action, data, callback, method = 'POST' }) {
            var xhr = new(XMLHttpRequest || ActiveXObject)('MSXML2.XMLHTTP.3.0');
            xhr.open(method, url, true);
            xhr.setRequestHeader('Content-type', 'application/json');
            xhr.setRequestHeader('Accept', 'application/json');
            xhr.onreadystatechange = function() {
                if (xhr.readyState === 4) {
                    var response;
                    try {
                        response = JSON.parse(xhr.response);
                    } catch (e) {
                        response = xhr.response;
                    }

                    return callback(xhr.status, response);
                }
            };
            if (typeof grecaptcha !== 'undefined') {
                grecaptcha.ready(function() {
                    grecaptcha.execute(window.blaizeRecaptchaClientId, { action: action }).then(function(recaptchaToken) {
                        xhr.setRequestHeader('X-Recaptcha-Token', recaptchaToken);
                        return (!data) ? xhr.send() : xhr.send(data);
                    });
                });
            } else {
                return (!data) ? xhr.send() : xhr.send(data);
            }
        };

        blaizeRequest({ url: '/zephr/media/registration/config', action: 'registrationConfig', method: 'GET', callback: function(status, response) {
            if (status !== 200) {
                return;
            }
            if (response.oauthProvidersEnabled) {
                var oauthLinks = section.querySelectorAll("a.blaize-oauth");
                for (var i = 0; i < oauthLinks.length; i++) {
                    if (!response.oauthProviders.includes(oauthLinks[i].dataset.oauthProvider.toUpperCase())) {
                        oauthLinks[i].setAttribute("disabled", "");
                    }
                };
            }
            if (response.recaptchaEnabled) {
                var badge = document.getElementById('blaize-recaptcha-badge');
                var tag = document.createElement('script');
                tag.src = 'https://www.google.com/recaptcha/api.js?render=explicit&onload=onRecaptchaLoadCallback';
                tag.async = true;
                tag.defer = true;
                badge.insertAdjacentElement('afterend', tag);
                window.blaizeRecaptchaPosition = response.recaptchaPosition;

                if (response.recaptchaPosition === 'hidden') {
                    var tcs = section.querySelectorAll(".blaize-recaptcha-tcs");
                    tcs.forEach((el) => el.classList.add("show"));
                } else {
                    badge.classList.add("show");
                }
            }
        }});

        var handleRegister = function(event) {
            event.preventDefault();
            registerError.style.display = 'none';
                var identifier = registrationAuthenticationIdentifier ? registrationAuthenticationIdentifier : formRegister.querySelector('input[name="email_address"]').value;
                var identifierKey = "email_address";

            var attributes = {};
            var inputs = formRegister.querySelector('.blaize-user-attributes').querySelectorAll('input, textarea, select');
            for (var i = 0; i < inputs.length; i++) {
                if (inputs[i].type === 'checkbox') {
                    attributes[inputs[i].name] = inputs[i].checked;
                } else if (inputs[i].type === 'radio') {
                    if (inputs[i].checked === true) {
                        attributes[inputs[i].name] = inputs[i].value;
                    }
                } else {
                    attributes[inputs[i].name] = inputs[i].value;
                }
            }

            var registerData = {
                identifiers: {},
                validators: {},
                attributes: attributes
            };


            if (registrationAuthenticationToken) {
                registerData.validators["token_exchange"] = registrationAuthenticationToken;
            } else {
                registerData.identifiers[identifierKey] = identifier;

                var activeElementAction = document.activeElement.getAttribute("formaction");
                if (activeElementAction === "password") {
                    registerData.validators["password"] = formRegister.querySelector('input[name="password"]').value;
                }
            }

            registerData = window.Blaize.preProcessFormData(formRegister.getAttribute('data-blaize-form'), registerData);

            blaizeRequest({ url: '/blaize/register', action: 'register', data: JSON.stringify(registerData), callback: function(status, response) {
                window.Blaize.postProcessFormData(formRegister.getAttribute('data-blaize-form'), registerData, response);

                if (status === 200) {
                    setSelectedProductSessionStorage();
                    pushToDataLayer();
                    location.reload(true);
                } else if (status === 409) {
                    if (identifierKey === 'username') {
                        registerError.innerHTML = 'Username already taken, please try a different one.';
                    } else {
                        registerError.innerHTML = 'Email address already taken, please try a different one.';
                    }
                    registerError.style.display = 'block';
                } else if (status === 400 || status === 402) {
                    registerError.innerHTML = response.message;
                    registerError.style.display = 'block';
                } else {
                    registerError.innerHTML = 'Something went wrong! Please try again later.';
                    registerError.style.display = 'block';
                }
            }});
        };

        var handleOauthRegister = function(e) {
            e.preventDefault();
            registerError.style.display = 'none';

            var registerData = {
                identifiers: {},
                validators: {},
                attributes: {}
            };

            if (registrationAuthenticationToken) {
                registerData.validators["token_exchange"] = registrationAuthenticationToken;
            } else {
                registerError.innerHTML = 'Something went wrong! Please try again later.';
                registerError.style.display = 'block';
            }

            registerData = window.Blaize.preProcessFormData(formRegister.getAttribute('data-blaize-form'), registerData);

            blaizeRequest({ url: '/blaize/register', action: 'register', data: JSON.stringify(registerData), callback: function(status, response) {
                window.Blaize.postProcessFormData(formRegister.getAttribute('data-blaize-form'), registerData, response);

                if (status === 200) {
                    async function fetchExtendedProfile() {
                        const response = await fetch(`/blaize/profile/_${oauthProvider}`, {
                                        method: 'GET',
                                        credentials: 'include'
                                    });
                        
                        const extendedProfile = await response.json();

                        return extendedProfile;
                    }

                    fetchExtendedProfile()
                        .then(extendedProfile => {
                            var lastName;
                            var firstName;

                            if (oauthProvider == "google") {
                                lastName = extendedProfile.family_name;
                                firstName = extendedProfile.given_name;
                            } else if (oauthProvider == "facebook"){
                                var fullName = extendedProfile.name.split(" ");
                                lastName = fullName.pop();
                                firstName = fullName.join(' ');
                            }

                            fetch('/blaize/profile', {
                                method: 'PATCH',
                                headers: {
                                    "Content-type": "application/json"
                                },
                                credentials: 'include',
                                body: JSON.stringify({
                                    "name": firstName ? firstName : " ",
                                    "last-name": lastName ? lastName : " ",
                                })})
                                .then( () => {
                                    setSelectedProductSessionStorage();
                                    pushToDataLayer();
                                    location.reload(true);
                                }).catch(function (error) {
                                    console.log('Request failed', error);
                                    registerSuccess.style.display = 'none';
                                    registerError.innerHTML = 'Something went wrong! Please try again later.';
                                    registerError.style.display = 'block';
                                });
                        }).catch (err => {
                            console.log(err);
                            registerSuccess.style.display = 'none';
                            registerError.innerHTML = 'Something went wrong! Please try again later.';
                            registerError.style.display = 'block';
                        });
                } else {
                    registerSuccess.style.display = 'none';
                    registerError.innerHTML = 'Something went wrong! Please try again later.';
                    registerError.style.display = 'block';
                }
            }});
        };

        var handleLogin = function(event) {
            event.preventDefault();
            loginError.style.display = 'none';

                var identifier = formLogin.querySelector('input[name="email_address"]').value;
                var isAUserName = false;

            var endpoint;
            var payloadData;

            if (event.target.className.indexOf("passwordless") > -1) {
                endpoint = '/blaize/token-exchange';
                payloadData = JSON.stringify({
                    identifiers: {
                        email_address: identifier
                    },
                    delivery: {
                        method: 'email',
                        destination: identifier,
                        action: 'login',
                        redirect: window.location.href.replace(window.location.origin, "")
                    }
                });
            } else {
                endpoint = '/blaize/login';


                    if (isAUserName) {
                        payloadData = JSON.stringify({
                            identifiers: {
                                username: identifier
                            },
                            validators: {
                                password: formLogin.querySelector('input[name="password"]').value
                            }
                        });
                    } else {
                        payloadData = JSON.stringify({
                            identifiers: {
                                email_address: identifier
                            },
                            validators: {
                                password: formLogin.querySelector('input[name="password"]').value
                            }
                        });
                    }
            }

            blaizeRequest({ url: endpoint, action: 'login', data: payloadData, callback: function(status, response) {
                if (status === 200) {
                    if (response.cookie) {
                        document.cookie = response.cookie;
                        setSelectedProductSessionStorage();
                        pushToDataLayer();
                        location.reload(true);
                    }
                } else if (status === 201) {
                    if (response.message) {
                        loginSuccess.innerHTML = response.message;
                        loginSuccess.style.display = 'block';
                        if (event.target.className.indexOf("passwordless") > -1) {
                            formLogin.querySelector('.blaize-login-passwordless').style.display = 'none';
                        } else {
                            formLogin.querySelector('.blaize-submit').style.display = 'none';
                        }
                        formLogin.querySelector('.blaize-validators').style.display = 'none';
                        formLogin.querySelector('.blaize-to-register').style.display = 'none';
                    }
                } else if (status === 401) {
                    if (isAUserName) {
                        loginError.innerHTML = 'The username or password you entered are incorrect.';
                    } else {
                        loginError.innerHTML = 'The email address or password you entered are incorrect.';
                    }
                    loginError.style.display = 'block';
                } else if (status === 404) {
                    loginError.innerHTML = 'User not found.';
                    loginError.style.display = 'block';
                  } else if (status === 429) {
                      loginError.innerHTML = 'To gain login access, please contact your administrator.';
                      loginError.style.display = 'block';
                  } else {
                    loginError.innerHTML = 'Something went wrong! Please try again later.';
                    loginError.style.display = 'block';
                  }
              }});
            };

            var handleResetPassword = function(event) {
                event.preventDefault();

                var payloadData = JSON.stringify({
                    identifiers: {
                        email_address: formResetPassword.querySelector('input[name="email_address"]').value
                    }
                });

                blaizeRequest({ url: '/blaize/users/reset', action: 'password_reset', data: payloadData, callback: function(status, response) {
                    if (status < 300 && status >= 200) {
                        passwordResetSuccess.style.display = 'block';
                    } else {
                        passwordResetError.innerHTML = (response.message) ? response.message : response;
                        passwordResetError.style.display = 'block';
                    }
                }});
            };

            formResetPassword.addEventListener('submit', handleResetPassword);

            var registrationAuthenticationToken;
            var registrationAuthenticationIdentifier;
            var loginAuthenticationToken;
            var oauthProvider;

            var oauthProviderWindowFeatures = "menubar=no,location=yes,resizable=no,scrollbars=no,status=no,width=500,height=600";

            var registerClickHandler = function(link) {
                link.addEventListener("click", function(event) {
                    event.preventDefault();

                    var provider = link.getAttribute("data-oauth-provider");
                    oauthProvider = provider;

                    if (link.hasAttribute("disabled")) {
                        console.log(provider + " provider is not configured.");
                        return;
                    }

                    window.open("/blaize/oauth/" + provider, provider + " sign-in", oauthProviderWindowFeatures);
                });
            }

            var oauthLinks = section.querySelectorAll("a.blaize-oauth");
            for (var i = 0; i < oauthLinks.length; i++) {
                registerClickHandler(oauthLinks[i]);
            }

            window.addEventListener("message", function(message) {
                if (message.data.action == "register") {
                    window.zephrSocialSigninRegistration = true;

                    registrationAuthenticationToken = message.data.stateKey;
                    registrationAuthenticationIdentifier = message.data.identifier;

                    // identifiersFieldset.style.opacity = 0;
                    // validatorsFieldset.style.opacity = 0;
                    // identifiersFieldset.style.display = "block";
                    // validatorsFieldset.style.display = "block";
                    // identifiersFieldset.style.transform = "scale(1, 0)";
                    // validatorsFieldset.style.transform = "scale(1, 0)";
                    // identifiersFieldset.style.display = "none";
                    // validatorsFieldset.style.display = "none";

                    // formRegister.querySelector('.blaize-complete-registration').style.display = 'block';
                    // registerSuccess.style.display = 'none';

                    // formRegister.querySelector('input[name="email_address"]').required = false;
                    // formRegister.querySelector('input[name="password"]').required = false;
                    
                    registerSuccess.innerHTML = 'Please wait while we set up your account... ';
                    registerSuccess.style.display = 'block';
                    
                    setTimeout(function() {
                        registerError.style.display = 'none';
                        loginError.style.display = 'none';
                    }, 600);

                    handleOauthRegister(event);
                    
                } else if (message.data.status == "failure") {
                    if (formLogin.style.display == 'none' || (formLogin.style.display == '' && formRegister.style.display == '')) {
                        registerError.innerHTML = message.data.message;
                        registerError.style.display = 'block';
                    } else {
                        loginError.innerHTML = message.data.message;
                        loginError.style.display = 'block';
                    }
                } else if (message.data.action == "login") {
                    document.cookie = message.data.cookie;
                    setTimeout(function() {
                        setSelectedProductSessionStorage();
                        pushToDataLayer();
                        window.location.reload();
                    }, 20);
                } else if (message.data.action == "otp") {
                }
            });

        var pushToDataLayer = function () {
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({
                'event' : 'sub-page-signup-login',
            });
        }

        formRegister.addEventListener('submit', handleRegister);
        formLogin.addEventListener('submit', handleLogin);
    })();
</script><style>
