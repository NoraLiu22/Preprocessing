<article  class="tribe-events-calendar-month__calendar-event post-10013801 tribe_events type-tribe_events status-publish hentry tribe_events_cat-westchester cat_westchester tribe-recurring-event tribe-recurring-event-parent" >

	
	<div class="tribe-events-calendar-month__calendar-event-details">

		<div class="tribe-events-calendar-month__calendar-event-datetime">
		<time datetime="08:00">
		8:00 am	</time>
			<span class="tribe-events-calendar-month__calendar-event-datetime-separator"> - </span>
		<time datetime="17:00">
			5:00 pm		</time>
		
