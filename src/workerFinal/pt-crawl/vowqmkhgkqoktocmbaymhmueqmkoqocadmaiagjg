<article  class="tribe-events-calendar-month__calendar-event post-10015719 tribe_events type-tribe_events status-publish has-post-thumbnail hentry tag-dutchess-county tag-hudson-valley tag-stormville-flea-market tribe_events_cat-dutchess cat_dutchess tribe-recurring-event tribe-recurring-event-parent" >

	
	<div class="tribe-events-calendar-month__calendar-event-details">

		<div class="tribe-events-calendar-month__calendar-event-datetime">
		<time datetime="08:00">
		8:00 am	</time>
			<span class="tribe-events-calendar-month__calendar-event-datetime-separator"> - </span>
		<time datetime="16:00">
			4:00 pm		</time>
		
<span class="tribe-events-calendar-series-archive__container">
	<a
