<script>
	function trackBrowsergame(action = 'startBrowserGame') {
		const options = {
			event: "kr3m",
			eventAction: action,
			eventCategory: "Click",
			hitType: "Button"
		}

		try {
			trackGTMCustom(options);
		} catch (e) {
			console.error("trackGTMCustom:", e);
		}
	}
</script>


