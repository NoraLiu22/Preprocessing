<script>

    whenScriptReady('jquery', function()
    {

        let isMobile = (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent));

        if(true)
        {
            initNav();

            window.addEventListener("resize", initNav);

            function initNav()
            {
                setTimeout(()=>
                {
                    dragElement(document.getElementById('partnerNavbar'));
                }, 1000);
            }



            function dragElement(navElement)
            {
                const btnNavRight = document.getElementById("btnNavRight");
                const btnNavLeft = document.getElementById("btnNavLeft");

                btnNavLeft.onclick = moveElement;
                btnNavRight.onclick = moveElement;

                const wrapper = document.getElementById('partnerNavbarWrapper');
                var dragSpace = navElement.clientWidth - wrapper.offsetWidth;
                var posLeft = 0, posLeftStart = 0;


                if(isMobile)
                {
                    navElement.addEventListener('touchstart', dragMouseDown, {passive: true});
                }
                else
                {
                    navElement.onmousedown = dragMouseDown;
                }



                function dragMouseDown(e)
                {
                    //console.log("onmousedown", e)
                    e = e || window.event;

                    const target = isMobile ? e.changedTouches[0] : e;

                    posLeftStart = parseInt(target.clientX);
                    navElement.style.transition = null;

                    if(isMobile)
                    {
                        //navElement.style.backgroundColor = '#ecf0f1';
                        navElement.addEventListener('touchmove', elementDrag, {passive: true});
                        navElement.addEventListener('touchend', closeDragElement, {passive: true});
                    }
                    else
                    {
                        document.onmouseup = closeDragElement;
                        document.onmousemove = elementDrag;
                    }
                }


                function elementDrag(e)
                {
                    e = e || window.event;

                    const target = isMobile ? e.changedTouches[0] : e;

                    posLeft = posLeftStart - parseInt(target.clientX);
                    posLeftStart = parseInt(target.clientX);

                    let value = getTranslateX() - posLeft;
                    navElement.style.transform = 'translateX(' + value + 'px)';
                }


                function getTranslateX()
                {
                    const style = window.getComputedStyle(navElement)
                    const matrix = new DOMMatrixReadOnly(style.transform)
                    return matrix.m41;
                }


                function closeDragElement(e)
                {
                    e = e || window.event;
                    const target = isMobile ? e.changedTouches[0] : e;

                    navElement.style.transition = 'transform .3s ease-out';
                    btnNavLeft.style.display = 'flex';
                    btnNavRight.style.display = 'flex';

                    if(getTranslateX() > 0)
                    {
                        navElement.style.transform = 'translateX(0px)';
                        btnNavLeft.style.display = 'none';
                    }

                    if(getTranslateX() < -dragSpace)
                    {
                        navElement.style.transform = 'translateX(' +  (-dragSpace)  + 'px)';
                        btnNavRight.style.display = 'none';
                    }

                    if(isMobile)
                    {
                       // navElement.style.backgroundColor = 'transparent';
                        navElement.ontouchend = null;
                        navElement.ontouchmove = null;
                    }
                    else
                    {
                        document.onmouseup = null;
                        document.onmousemove = null;
                    }
                }



                function moveElement(e)
                {
                    e = e || window.event;

                    let btn = e.currentTarget;
                    let value = 0;

                    if(btn.id == 'btnNavRight')
                    {
                        value = getTranslateX() - 95;
                    }
                    else
                    {
                        value = getTranslateX() + 95;
                    }

                    //console.log("move to translatex value : ", value);

                    navElement.style.transition = 'transform .3s ease-out';

                    navElement.style.transform = 'translateX(' + value + 'px)';
                    btnNavLeft.style.display = 'flex';
                    btnNavRight.style.display = 'flex';

                    if(value < (-dragSpace))
                    {
                        navElement.style.transform = 'translateX(' +  (-dragSpace)  + 'px)';
                        btnNavRight.style.display = 'none';
                    }

                    if(value > 0)
                    {
                        navElement.style.transform = 'translateX(0px)';
                        btnNavLeft.style.display = 'none';
                    }
                }
            }
        }

    });
</script>


    <div id="partnerSidebar" class="partner-sidebar">
    <div id="partnerSidebarBg" class="partner-sidebar-bg"></div>

    <script>
        whenScriptReady('jquery', function()
        {
