                            <script type="text/javascript">
                                $(document).ready(() => $("#validationMessage").hide());

                                function get_action(form) {
                                    var error = "";
                                    var errorField = "These required fields are missing data:\n";

                                    //if (!document.getElementById("type-of-request_Device").checked && !document.getElementById("type-of-request_Bio/Pharm").checked) {
                                    //    var error = "y";
                                    //    errorField = errorField + "\nType Of Request";
                                    //}
                                    if ($("#name").val() == "" || $("#name").val() == 0 || $("#name").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nName";
                                    }


                                    if ($("#email").val() == "" || $("#email").val() == 0 || $("#email").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nEmail: ";
                                    }
                                    if ($("#phone").val() == "" || $("#phone").val() == 0 || $("#phone").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nPhone: ";
                                    }
                                    if ($("#address-1").val() == "" || $("#address-1").val() == 0 || $("#address-1").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nAddress : ";
                                    }

                                    if ($("#city").val() == "" || $("#city").val() == 0 || $("#city").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nCity :";


                                    }
                                    if ($("#zip").val() == "" || $("#zip").val() == 0 || $("#zip").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nZip Code:";


                                    }

                                    if ($("#country").val() == "" || $("#country").val() == 0 || $("#country").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nCountry:";
                                    }

                                    if (!document.getElementById("member_Yes").checked && !document.getElementById("member_No").checked) {
                                        var error = "y";
                                        errorField = errorField + "\nAre you a member of an AUA Section?";
                                    }


                                    if ($("#job-title").val() == "" || $("#job-title").val() == 0 || $("#job-title").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nJob title:";
                                    }
                                    if ($("#employer-name").val() == "" || $("#employer-name").val() == 0 || $("#employer-name").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nEmployer Name:";
                                    }

                                    if ($("#undergraduate").val() == "" || $("#undergraduate").val() == 0 || $("#undergraduate").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nName of Undergraduate Institution:";
                                    }


                                    if ($("#undergraduate-degree").val() == "" || $("#undergraduate-degree").val() == 0 || $("#undergraduate-degree").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nUndergraduate Degree: ";
                                    }
                                    if ($("#graduate").val() == "" || $("#graduate").val() == 0 || $("#graduate").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nName of Graduate Institution: ";
                                    }

                                    if ($("#graduate-degree").val() == "" || $("#graduate-degree").val() == 0 || $("#graduate-degree").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nGraduate Degree?";
                                    }



                                    if ($("#years-practice").val() == "" || $("#years-practice").val() == 0 || $("#years-practice").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nYears in Practice";
                                    }

                                    if ($("#years-urology").val() == "" || $("#years-urology").val() == 0 || $("#years-urology").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nYears in Urology:";
                                    }
                                    if (!document.getElementById("areas-expertise_General Urology").checked && !document.getElementById("areas-expertise_Genomics").checked
                                        && !document.getElementById("areas-expertise_Adrenal").checked && !document.getElementById("areas-expertise_Andrology").checked
                                        && !document.getElementById("areas-expertise_BPH").checked && !document.getElementById("areas-expertise_BPH").checked
                                        && !document.getElementById("areas-expertise_Calculus").checked && !document.getElementById("areas-expertise_Female Urology").checked
                                        && !document.getElementById("areas-expertise_Impotence").checked && !document.getElementById("areas-expertise_Infertility").checked
                                        && !document.getElementById("areas-expertise_Integration/Utilization of Advanced Practice Providers").checked && !document.getElementById("areas-expertise_Neurogenic Bladder").checked
                                        && !document.getElementById("areas-expertise_Oncology: Adrenal/Kidney").checked && !document.getElementById("areas-expertise_Oncology: Bladder/Urothelium/Urethra").checked
                                        && !document.getElementById("areas-expertise_Oncology: Prostate").checked && !document.getElementById("areas-expertise_Oncology: Testis/Penis").checked
                                        && !document.getElementById("areas-expertise_Pediatrics").checked && !document.getElementById("areas-expertise_Surgical Assist").checked
                                        && !document.getElementById("areas-expertise_Transgender Care").checked && !document.getElementById("areas-expertise_Upper Tract Obstruction").checked
                                        && !document.getElementById("areas-expertise_Urethral Stricture").checked && !document.getElementById("areas-expertise_Urinary Diversion").checked
                                        && !document.getElementById("areas-expertise_Urodynamics").checked && !document.getElementById("areas-expertise_Voiding Dysfunction").checked
                                        && !document.getElementById("areas-expertise_Other").checked) {
                                        var error = "y";
                                        errorField = errorField + "\nAreas of Expertise?";
                                    }

                                    if ($("#bio").val() == "" || $("bio").val() == 0 || $("#bio").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nProvide a brief Bio:";
                                    }
                                    // Initialize the word counter
                                    var count = 0;

                                    // Split the words on each
                                    // space character
                                    var split = $("#bio").val().split(' ');

                                    // Loop through the words and
                                    // increase the counter when
                                    // each split word is not empty
                                    for (var i = 0; i < split.length; i++) {
                                        if (split[i] != "") {
                                            count += 1;
                                        }
                                    }

                                    if (count > 250) {
                                        var error = "y";
                                        errorField = errorField + "\n Please limit the Bio under 250 words";
                                    }

                                    var file1 = document.getElementById("cv");
                                    if (file1.files.length == 0) {
                                        var error = "y";
                                        errorField = errorField + "\nCurriculum Vitae:";
                                    }
                                    var file2 = document.getElementById("loi-1");
                                    if (file2.files.length == 0) {
                                        var error = "y";
                                        errorField = errorField + "\nLetters of Recommendation?";
                                    }



                                    if ($("#speaker-reason").val() == "" || $("#speaker-reason").val() == 0 || $("#speaker-reason").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nWhy do you want to be on the AUA Speakers Bureau?:";
                                    }
                                    var ct = 0;
                                    var split = $("#speaker-reason").val().split(' ');

                                    // Loop through the words and
                                    // increase the counter when
                                    // each split word is not empty
                                    for (var i = 0; i < split.length; i++) {
                                        if (split[i] != "") {
                                            ct += 1;
                                        }
                                    }

                                    if (ct > 250) {
                                        var error = "y";
                                        errorField = errorField + "\n Please limit the Why do you want to be on the AUA Speakers Bureau under 250 words";
                                    }

                                    if ($("#signature").val() == "" || $("signature").val() == 0 || $("#signature").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nSignature ";
                                    }

                                    if ($("#date").val() == "" || $("#date").val() == 0 || $("#date").val() == null) {
                                        var error = "y";
                                        errorField = errorField + "\nDate:";
                                    }

                                    if (error == "y") {
                                        $("#validationMessage").show();
                                        alert(errorField);
                                        return false;
                                    }
                                    var v = grecaptcha.getResponse();
                                    //alert(v);
                                    if (v.length == 0 || v == "") {
                                        document.getElementById('captcha').innerHTML = "You must complete the Captcha to submit";
                                        return false;
                                    }
                                    else {
                                        //document.getElementById('captcha').innerHTML = "Captcha completed";
                                        $("#btnSubmit").attr("type", "submit");
                                        $("#btnSubmit").click();
                                        //document.getElementById("exchangeForm").submit();
                                    }
                                }
                            </script>
                            <script>
                                function UploadVerificationFunction1() {
                                    var browse = $("#cv");

                                    var size = parseFloat($("#cv")[0].files[0].size).toFixed(2);
                                    var Maxvalue = 4096000;

                                    if (size > Maxvalue) {
                                        // $('#dvMsg')[0].style.display = 'block';
                                        alert("Maximun allowed file size: 4 MB");
                                        $("#cv").val('');
                                        $("#lbl_uploadMessage").val('Maximun allowed file size: 4 MB');
                                        //  browse.attr("disabled", "disabled");
                                        return false;
                                    }
                                    for (var i = 0; i < browse.length; i++) {
                                        var oInput = browse[i];
                                        var _validFileExtensions = [".jpg", ".jpeg", ".pdf", ".gif", ".png", ".docx", ".doc"];
                                        if (oInput.type == "file") {
                                            var sFileName = oInput.value;
                                            //  alert(oInput.value);
                                            if (sFileName.length > 0) {
                                                var blnValid = false;
                                                // alert("blnValid");
                                                for (var j = 0; j < _validFileExtensions.length; j++) {
                                                    var sCurExtension = _validFileExtensions[j];
                                                    if (sFileName.substr(sFileName.length - sCurExtension.length, sCurExtension.length).toLowerCase() == sCurExtension.toLowerCase()) {
                                                        blnValid = true;
                                                        break;
                                                    }
                                                }

                                                if (!blnValid) {
                                                    alert("Allowed file types: PDF(preferred), JPG, GIF, PNG, DOCX, DOC");
                                                    $("#cv").val('');
                                                    return false;
                                                }
                                            }
                                        }
                                    }

                                }
                            </script>
