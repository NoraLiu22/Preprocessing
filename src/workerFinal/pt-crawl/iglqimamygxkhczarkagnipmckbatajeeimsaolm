    function errorMsg(element, errmsg) {
        alert(errmsg);
        element.focus();
        return false;
    }
    function regKontrol(pattern, value) {
        r = new RegExp(pattern, "g");
        return r.test(value);
    }

    function ekle123(form10) {
        var errmsg = "";
        if (form10.AdSoyad.value == "") {
            errmsg = " Please enter your name and surname. \n  ";
            errorMsg(form10.AdSoyad, errmsg);
            return false;
        }

        if (form10.email.value == "") {
            errmsg = " Please enter your e-mail address.\n ";
            errorMsg(form10.email, errmsg);
            return false;
        }

        patternEposta = "^" + "([abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0-9_\.\-]+)" + "@" + "([abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0-9_\.\-]+)" + "[\.]" + "([abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0-9_\.\-]+)" + "$";
        if (!regKontrol(patternEposta, form10.email.value)) {
            alert(" Wrong e-mail address. Please check it. ");
            return false;
        }
        if (form10.Konu.value == "") {
            errmsg = " Please enter a subject.\n ";
            errorMsg(form10.Konu, errmsg);
            return false;
        }
        if (form10.MyTextAreaName.value == "") {
            errmsg = " Please enter body text of your message.\n ";
            errorMsg(form10.MyTextAreaName, errmsg);
            return false;
        }

        if (form10.GKod.value == "") {
            errmsg = " Please enter the security code shown in this page.\n ";
            errorMsg(form10.GKod, errmsg);
            return false;
        }

        if (form10.GKod.value != form10.GKodkontrol.value) {
            errmsg = " Wrong security code. Please reenter the correct code.\n ";
            errorMsg(form10.GKod, errmsg);
            return false;
        }
    }                
