        <td width="119" valign="middle" style="width:89.4pt;border:outset #CC9900 .75pt; padding:0cm 1.4pt 0cm 1.4pt" align="center"><span lang="EN-US" style="font-size:8.0pt;mso-bidi-font-size:12.0pt;font-family:Arial; color:windowtext"><a href="class.htm"><span style="text-decoration:none; text-underline:none">Trombone
          Class</span></a></span></td>
        <td width="100" valign="middle" style="width:74.95pt;border:outset #CC9900 .75pt; padding:0cm 1.4pt 0cm 1.4pt" align="center"><span lang="EN-US" style="font-size:8.0pt;mso-bidi-font-size:12.0pt;font-family:Arial; color:windowtext"><a href="misc.htm"><span style="text-decoration:none; text-underline:none">Miscellaneous</span></a></span></td>
