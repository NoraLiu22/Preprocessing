<script type="text/javascript">

    function submitEverythingSearch() {
        document.location = "default.aspx?searchtype=everything&searchfor=" + document.getElementById("ctl00_ContentMain_rptMain_ctl00_ctl00_searchfor").value + "&exactphrase=" + document.getElementById("ctl00_ContentMain_rptMain_ctl00_ctl00_chkExactPhrase").checked;
    }

    function runScript(e) {
        $(document).keypress(function(e) {
            if (e.keyCode == 13) {
                submitEverythingSearch();
                return false;
            }
            return;
        });
    }
</script>

