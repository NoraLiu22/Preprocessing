					sorting: app.cfg.SORTINGS.ALPHABETICALLY,
					//dataLocation: 'data/',
					//assetLocation: 'theme/img/viz/'
				});
			} else if ($('#viz').hasClass('viz-countries')) {
				// launch visualization for country page
				$('#viz').flowerVis({
					mode: app.cfg.MODES.COUNTRY,
					locale: 'en',
					selectedCountryID: (countryCode?countryCode.toUpperCase():undefined),
					selectedDimensionID: undefined,
					forceIncludedCountryID: undefined,
					sorting: app.cfg.SORTINGS.ALPHABETICALLY,
					//dataLocation: 'data/',
					//assetLocation: 'theme/img/viz/'
				});
			} else if ($('#viz').hasClass('viz-bli-responses')) {
				// launch visualization for country page
				$('#viz').flowerVis({
					mode: app.cfg.MODES.RESPONSES,
					locale: 'en'
				});
			}
		}
	});	
</script>

