		function showMenu() {
			document.querySelector('#menu_mobile').classList.remove('hidden');
			document.querySelector('body').classList.add('modal-open');
		}	
		function showLogin() {
			document.querySelector('#login_mobile').classList.remove('hidden');
			document.querySelector('body').classList.add('modal-open');
		}	
		function showContact() {
			try {
    // Mozilla, Opera, Safari sowie Internet Explorer (ab v7)
    xmlHttp = new XMLHttpRequest ();
} catch(e) {
    try {
        // MS Internet Explorer (ab v6)
        xmlHttp  = new ActiveXObject("Microsoft.XMLHTTP");
    } catch(e) {
        try {
            // MS Internet Explorer (ab v5)
            xmlHttp  = new ActiveXObject("Msxml2.XMLHTTP");
        } catch(e) {
            xmlHttp  = null;
        }
    }
}
if (xmlHttp) {
    xmlHttp.open('GET', '/contactform.asp', true);
    xmlHttp.onreadystatechange = function () {
        if (xmlHttp.readyState == 4) {
		resultText = xmlHttp.responseText;
            document.getElementById('contact_mobile').innerHTML=resultText;
        }
    };
    xmlHttp.send(null);
}


			document.querySelector('#contact_mobile').classList.remove('hidden');
			document.querySelector('body').classList.add('modal-open');
		}	

    
    function playAudio(songId) {
    	document.getElementById('play_mobile').style.height='70px';
    	document.getElementById('play_mobile_close').style.bottom='60px';
      if(songId.substr(0,1)=='n') {
        pSound = "n/" + songId.substr(1,3) + "0000/";
      } else {
        pSound = songId.substr(0,3) + "0000/";
      }
      
      aSound = "https://tools2.hitparade.ch/tools/audio/";
      bSound = String.fromCharCode(46,109,112,51)

      document.querySelector('#play_mobile').classList.remove('hidden');
      document.querySelector('#play_mobile_close').classList.remove('hidden');
      document.querySelector('#play_mobile_left').classList.add('hidden');
      document.querySelector('#play_mobile_right').classList.add('hidden');
      document.querySelector('#play_mobile_inner').innerHTML='<audio controls id="play_audio" controlsList="nodownload" style="width:100%;"><source src="' + aSound + pSound + songId + bSound +'"></audio>';
      //document.querySelector('#play_audio').load();
      document.querySelector('#play_audio').play();
      document.querySelector('#play_audio').onended = function() {
        hideAudio();
      };
    }

    function hideAudio() {
      document.querySelector('#play_audio').pause();
      document.querySelector('#play_mobile').classList.add('hidden');
      document.querySelector('#play_mobile_close').classList.add('hidden');
      document.querySelector('#play_mobile_left').classList.add('hidden');
      document.querySelector('#play_mobile_right').classList.add('hidden');
    }
    
    function playAudioMulti(mediumId,trackNum) {
    	document.getElementById('play_mobile').style.height='120px';
    	document.getElementById('play_mobile_close').style.bottom='110px';

    	tracks = document.querySelectorAll('.audio'+mediumId);
    	songId = tracks[trackNum].getAttribute('data-audio');
    	trackName = tracks[trackNum].getAttribute('data-display');
     	if(songId.substr(0,1)=='n') {
        pSound = "n/" + songId.substr(1,3) + "0000/";
      } else {
        pSound = songId.substr(0,3) + "0000/";
      }    	
      aSound = "https://tools2.hitparade.ch/tools/audio/";
      bSound = String.fromCharCode(46,109,112,51)
      document.querySelector('#play_mobile').classList.remove('hidden');
      document.querySelector('#play_mobile_close').classList.remove('hidden');
      if (trackNum==0) {
      	document.querySelector('#play_mobile_left').classList.add('hidden');
      } else {
      	document.querySelector('#play_mobile_left').classList.remove('hidden');
      }
      if (trackNum+1<tracks.length) {
	      document.querySelector('#play_mobile_right').classList.remove('hidden');
      } else {
	      document.querySelector('#play_mobile_right').classList.add('hidden');
      }
      document.querySelector('#play_mobile_inner').innerHTML='<div class="audioplayertext" data-mediumid="'+mediumId+'" data-track="'+trackNum+'">'+trackName+'</div><audio controls id="play_audio" controlsList="nodownload" style="width:100%;"><source src="' + aSound + pSound + songId + bSound +'"></audio>';
      //document.querySelector('#play_audio').load();
      document.querySelector('#play_audio').play();
      document.querySelector('#play_audio').onended = function() {
      	if (trackNum+1<tracks.length) {
					playAudioMulti(mediumId,trackNum+1)
        } else {
        	hideAudio();
        }
      };
    }

		function playAudioNavigate(interval) {
			playAudioMulti(document.querySelector('.audioplayertext').getAttribute('data-mediumid'),parseInt(document.querySelector('.audioplayertext').getAttribute('data-track'))+interval)
		}    

    function playVideo(videoEmbed) {
      document.querySelector('#video_mobile_dark').classList.remove('hidden');
      document.querySelector('#video_mobile').classList.remove('hidden');
      if (window.innerWidth<=480) {
      	videoWidth = 480;
      } else {
      	videoWidth = window.innerWidth-100;
      	if (videoWidth>1280) videoWidth=1280;
	      document.querySelector('#video_mobile_inner').style.width=videoWidth+'px';
      }
      videoHeight = Math.round(videoWidth/16*9);
      document.querySelector('#video_mobile_inner').style.height=videoHeight+'px';
      document.querySelector('#video_mobile_inner').innerHTML='<iframe src="'+videoEmbed+'" style="width:100%;height:100%;border:0;" allow="autoplay" allowfullscreen>';
    }

    
	</script>
	
	
