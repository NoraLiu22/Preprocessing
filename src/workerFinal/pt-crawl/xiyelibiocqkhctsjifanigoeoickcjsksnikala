<script>
	function clickTab(iArtist, sCat) {
		inav = document.querySelectorAll('.active.artist' + iArtist);
		for (i=0;i<inav.length;i++) {
			inav[i].classList.remove('active')
		}
		document.querySelector('#' + sCat + iArtist).classList.add('active');
		icontent = document.querySelectorAll('.content' + iArtist);
		for (i=0;i<icontent.length;i++) {
			icontent[i].classList.add('hidden')
		}
		document.querySelector('#' + sCat + iArtist + 'content').classList.remove('hidden');
	}
</script>



