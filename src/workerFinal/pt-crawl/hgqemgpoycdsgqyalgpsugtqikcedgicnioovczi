    <a class="mpwl wishlist-msg" title="Add this item to your wishlist" data-bind="click: wishlistClick">
        <span class="icon"><svg width="15px" height="13px" viewBox="0 0 20 20"><use xlink:href="#collect-control-wishlist"></svg></span>
        <span class="txt">wishlist</span>
    </a>
    <!-- alt wl -->
    <a class="mpwl alt wishlist-msg" title="Add this item to your wishlist" data-bind="click: wishlistClick">
        <!--span class="txt">wishlist</span-->
        <span class="icon"><svg width="15px" height="13px" viewBox="0 0 20 20"><use xlink:href="#collect-control-wishlist"></svg></span>
