    <mpmerch class="popupImageGallery" data-bind="css: merchClasses">
        <mpmerchformats>
                Merch for this release:<br/>
                
                    Vinyl LP
                
        </mpmerchformats>
        
        
            
                
                    <mpmerchitem>
