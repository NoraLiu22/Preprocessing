        return false; 
    }); 

    function loadDeferredIframe() {
        $(".spotify-lazy").hide();
        $(".spotify-player").html(`<iframe style="border-radius:12px" title="Hustle Culture" src="https://open.spotify.com/embed/episode/74Aakj9v4RXTidba9kiCY9?utm_source=generator" width="100%" height="232" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"></iframe>`);
    }

    $(window).on('load', function(){
        setTimeout(() => {
            loadDeferredIframe();
        }, 1000);
    })

    $( document ).ready(function() {

        $("#btn-apple-sign-in").click(function() {
            $('#appleid-signin').click();
        })

        // $("#closeiklanBottom").click(function() {
        //     $(".expandable-bottom-sticky").addClass('upper');
        // })
        
                    var isLogin = false;

            async function appendAds() {
                let wording =   '<div class="loginAds columns">' +
                                '<a href="https://www.thejakartapost.com/user/account/login">' +
                                    '<img src="/img/adblock-pop-up.png" alt="">' +
                                '</a>' +
                            '</div>';
                let leaderboard =   '<div class="columns">' +
                                    '<a href="https://www.thejakartapost.com/user/account/login">' +
                                        '<img src="/img/adblock-leaderboard.png" alt="">' +
                                    '</a>' +
                                '</div>';
                $('[class*="ads-showcase"]').append(wording);
                $('.tjp-placeholder-ads--leaderboard').append(leaderboard);
            }

            async function detectAdBlock() {
                let adBlockEnabled = false
                const googleAdUrl = 'https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js'
                try {
                    await fetch(new Request(googleAdUrl)).catch(_ => adBlockEnabled = true)
                } catch (e) {
                    adBlockEnabled = true
                } finally {
                    if(adBlockEnabled == true){
                        appendAds();
                    } 
                    // else if(navigator.userAgent.match(/firefox|fxios/i)) {
                    //     whenNoTrackingProtection();
                    // }
                }
            }
            detectAdBlock();

            async function whenNoTrackingProtection() {
                if (!whenNoTrackingProtection.promise) {
                    whenNoTrackingProtection.promise = new Promise(function(resolve, reject) {
                        var time = Date.now();
                        var img = new Image();
                        img.onload = resolve;
                        img.onerror = function() {
                            if ((Date.now() - time) < 50) {
                                reject(false);
                            } else {
                                resolve(false);
                            }
                        };
                        img.src = '//www.facebook.com/tr/';
                    }).then((result) => {
                        if (!result) {
                            appendAds()
                        }
                    }).catch(e => {
                        appendAds()
                    });
                }
            }
            
            });
</script>
