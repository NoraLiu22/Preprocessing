        deferjquery(function () {
            var loadedrollupimage = $('#rollupimageonload > img').first().attr('src');
            if (loadedrollupimage) {
                var newimage = $('<img/>');
                $(newimage).attr('src', loadedrollupimage);
                $('#rollupthumbimage').html("");
                $('#rollupthumbimage').append($(newimage));
