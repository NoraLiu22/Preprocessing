  function OpenWindow(fileName, width, height)
  {
    var locx = (screen.height / 2) - (height / 2);
    var locy = (screen.width / 2) - (width / 2);
 
    window.open(fileName, "intro", "width=" + width + 
                ",height=" + height + ",top=" + locx + 
                ",left=" + locy);
  }
</SCRIPT>

