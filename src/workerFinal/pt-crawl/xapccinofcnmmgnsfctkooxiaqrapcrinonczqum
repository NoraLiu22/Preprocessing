            <section>
    <div class="wfp-u-1 layout-content page-body">
              <div>
    <div data-drupal-messages-fallback class="hidden"></div>
  <div class="node node--type-page node--view-mode-full">
      
<div class="field field--node-title wfp-wrapper">

            
        
</div>
  
<div class="field field--field-paragraphs">

        
          



<section class="hero js--hero" style="background-image: linear-gradient(to top, rgba(0, 0, 0, 0.7), transparent), url('/sites/default/files/images/WFP-performance-management-accountability.jpg')" role="banner">
  <div class="hero__content">
    <div class="wfp-wrapper hero__inner-wrapper ph3 ph0-lg">
                    <h1 class="hero__title">Performance management and accountability</h1>
                    <div class="hero__subtitle"></div>
                    <div class="hero__actions">
                      <a href="https://www.wfp.org/annual-country-reports-2023"   class="button-new button-new--white">Annual Country Reports 2023</a>
                            </div>
          </div>
  </div>

  
      <aside class="hero__credit">
      <div id="credit--2064462506" class="hero__credit-panel" aria-hidden="true">
