<fieldset  class="collapsible group-shows field-group-htab panel panel-default form-wrapper" id="show-clips">
          <legend class="panel-heading">
      <a href="#show-clips-body" class="panel-title fieldset-legend" data-toggle="collapse">Shows</a>
    </legend>
        <div class="panel-body panel-collapse collapse fade in" id="show-clips-body">
        
