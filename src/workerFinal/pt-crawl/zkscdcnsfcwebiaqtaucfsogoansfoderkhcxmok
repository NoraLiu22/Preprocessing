    <div class="widget-dynamic-inner-wrap">

    <div class="widget-TrendMD widget-instance-Article_RightRailB0B6trendmd">
            <script type='text/plain' defer src='//js.trendmd.com/trendmd.min.js' data-trendmdconfig='{"journal_id":"44074","element":"#trendmd-suggestions"}' class='optanon-category-C0002'></script>

<div id="trendmd-suggestions"></div> 
    </div>

    </div>

