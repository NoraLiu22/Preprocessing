<script type="text/javascript">
(function ($){

	/* add alt class to alternating tbody instances in tables
	$('.main-column table tbody:nth-child(even)').addClass('alt');*/
	
	// enable colorbox for links with this class
	$('.colorbox').colorbox();
	
    // disable colorbox if less than 530 wide 
    $('.colorbox').click( function( e ) {
        if ( window.innerWidth < 530 ) {
            e.stopPropagation();
        }
    })
})(jQuery);
</script>
