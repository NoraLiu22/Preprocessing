// Checks the form data
function checkForm ( ) {

    var frm = document.forms[1];

    // var returnVal = checkIntervalNames();
    // if (!returnVal) {
    //     return false;
    // }

    var fields = new Array('collection_names','section_name','reference_no','person_reversed','research_group','taxon_name','country','state','county','max_interval','min_interval','group_formation_member','environment','lithification','lithologies','field_includes');

    if (checkAllEmpty(frm,fields)) {
        alert ("Please enter a search term");
        return false;
    } else if (frm.field_name.selectedIndex == 0 && frm.field_includes.value != '')	{
        alert ("Please select a field to search");
        return false;
    } else {
        return true;
    }
}


