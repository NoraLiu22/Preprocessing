<script type="text/javascript" src="/media/dataportal/scripts/application.js"></script> <script type="text/javascript">
$(function() {
	$(".home-jumbotron-search-form").submit(function() {
	    var form = $(this);
	    var url = $(this).attr('action');
	    // to fix a bug related to empty parameter 
	    var $q = form.find('input[name=q]');
	    if ( !$q.val() ) $q.detach(); 
	    if (url != '') {
	        var attributesString = url.split('?')[1];
	        var attributes = attributesString.split('&');
	        $.each(attributes, function(index, value) {
	            var elemName = value.split('=')[0];
	            var elemValue = value.split('=')[1];
	            var str = "<input type='hidden' name='"+elemName+"' value='"+elemValue+"' />";
	            form.append(str);
	        });
	    }
	});

	/*$("a.catalogue-oecd").click(function(event){
		var sPath = $(this).attr("href"); 
		if(sPath.indexOf("?") >= 0) {
			var sPathSplited = sPath.split("?");
			var attributes = sPathSplited[1].split("&");

			sPath = sPathSplited[0] + "?";
			$.each(attributes, function(idx, value) {
				if(idx > 0) sPath += "&";
				sPath += attributes[idx];
			});
			sPath += "&";	
		}else sPath += "?";
		sPath += "r=+f/type/datasets";	
		$(this).attr("href", sPath);
	});*/

	$('ul.list-filters>li').click(function() {
		$(this).addClass('current').siblings().removeClass('current');
	});

	$('.main-nav ul.tab>li>a').attr('href','#');

	/*FIX Language Switch on Home Page*/
	$('.language-switch a').each(function(idx, item) { 
		var $item = $(item); 
		if ( $item.attr('href').indexOf('/fr/accueil') > -1 ) { 
			$item.attr('href','/fr');
		} else if ( $item.attr('href').indexOf('/home') > -1 ) { 
			$item.attr('href','/');
		}
	});

});
</script><!--search bar -->
