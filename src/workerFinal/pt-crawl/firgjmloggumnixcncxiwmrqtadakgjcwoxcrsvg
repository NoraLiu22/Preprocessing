        font-weight: 400;
        font-size: 18px;
        padding-bottom: 6px;
        margin-bottom: 10px;
      }
      
      .STAFF-PROFILE-LIST .profile_job-title,
      .STAFF-PROFILE-LIST .profile_email,
      .STAFF-PROFILE-LIST .profile_phone {
        font-size: 14px;
      }
      
      .STAFF-PROFILE-LIST .profile_summary,
      .STAFF-PROFILE-LIST .profile_description {
        font-size: 12px;
      }
      
      .STAFF-PROFILE-LIST .profile_thumbnail {
        margin: 0.5em 0 0 0.5em;
      }
    </style>
    
