   timer = setInterval(updateDiv,100);
   function updateDiv(){        
       var editorText = CKEDITOR.instances.description.getData();
       var txt = document.createElement('textarea');
	     txt.innerHTML = editorText;
       $('#description').text(txt.value);
    }
