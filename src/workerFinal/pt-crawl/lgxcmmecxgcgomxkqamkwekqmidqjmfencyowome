		function trackOtherSubscriptions (data) {
			data = otherSubscriptionPackages[0]
			console.log('other subscription packages', otherSubscriptionPackages[0])
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'Product Impression',
				'eventLabel': promote_channel,
				'eventValue': 0
			})
			dataLayer.push({
				'ecommerce': {
				'currencyCode': 'AUD',
					'impressions': [
					{
						'name': data.name,
						'id': data.id,
						'price': data.price,
						'brand': data.brand,
						'variant': data.variant,
						'list_position': data.list_position
					}]
				}
			});
		}

		function trackPackage1Arrow (data) {
			if (document.querySelector('.card-position').classList.contains('annual-view')) {
				data = afrPackages[1]
				console.log('> package 1 arrow annual: ', afrPackages[1])
			} else {
				data = afrPackages[0]
				console.log('> package 1 arrow weekly: ', afrPackages[0])
			}
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'Product Impression',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
				'currencyCode': 'AUD',
						'impressions': [
