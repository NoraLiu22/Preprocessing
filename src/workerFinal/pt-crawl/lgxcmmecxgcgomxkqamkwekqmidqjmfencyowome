	var cardPosition = document.querySelector(".card-position")
	document.querySelector('.visual-status').addEventListener('click', function () {
		if (cardPosition.classList.contains('annual-view')) {
			// console.log('weekly view')
			document.querySelector('.card-holder.annual').style.display = 'none'
			document.querySelector('.card-holder.weekly').style.display = 'flex'
			cardPosition.classList.remove("annual-view")
			cardPosition.classList.add("weekly-view")

			var trackPackage1Button = document.querySelector('.card-content .package-1')
			if (trackPackage1Button.classList.contains('package-1')) {
				trackPackage1Button.setAttribute("href", "https://www.afr.com/subscription/account/?bundle_key=afr_alldig&callback_uri=http%3A%2F%2Fwww.afr.com&channel_key=9ME3ACTT4ZYY1fEMfvR2EA&promotion_code=afralldig28for3m-kiiwkw&single_use_token=true" + PROMOTE_CHANNEL)
			}
			var trackPackage2Button = document.querySelector('.card-content .package-2')
			if (trackPackage2Button.classList.contains('package-2')) {
				trackPackage2Button.setAttribute("href", "https://www.afr.com/subscription/account/?bundle_key=afr_alldigsat&channel_key=9ME3ACTT4ZYY1fEMfvR2EA&single_use_token=true&callback_uri=http%3A%2F%2Fwww.afr.com" + PROMOTE_CHANNEL)
			}
			var trackPackage3Button = document.querySelector('.card-content .package-3')
			if (trackPackage3Button.classList.contains('package-3')) {
				trackPackage3Button.setAttribute("href", "https://www.corporatesubscriptions.com.au/?utm_source=afr_sub_lp&utm_medium=link&utm_campaign=corporate_subscriptions" + PROMOTE_CHANNEL)
			}
			var termsWeekly = document.querySelector('.disclaimer')
			termsWeekly.innerHTML = '*Discount is 56% off the regular subscription price for the first 3 months on the Premium Digital package only. Offer is available for new subscribers only. At the end of the promotional period the subscription rate will increase to the regular rate of $65 per month, billed monthly. This offer is subject to change. Displayed prices are expressed as weekly rounded figures. Subscriptions automatically renew unless the subscriber changes or cancels their subscription in My Account or by calling customer service on 13 66 66. Home delivery is available in selected areas only. Please allow 10 days for delivery to commence.For full terms and conditions, please visit  <a href="https://www.afr.com/digital-subscription-terms">afr.com/conditions</a>.'

		} else {
			// console.log('annual view')
			document.querySelector('.card-holder.weekly').style.display = 'none'
			document.querySelector('.card-holder.annual').style.display = 'flex'
			cardPosition.classList.add("annual-view")
			cardPosition.classList.remove("weekly-view")

			var trackPackage1Button = document.querySelector('.card-content .package-1')
			if (trackPackage1Button.classList.contains('package-1')) {
				trackPackage1Button.setAttribute("href", "https://www.afr.com/subscription/account/?bundle_key=afr_alldig_12m&channel_key=9ME3ACTT4ZYY1fEMfvR2EA&single_use_token=true&callback_uri=http%3A%2F%2Fwww.afr.com" + PROMOTE_CHANNEL)
