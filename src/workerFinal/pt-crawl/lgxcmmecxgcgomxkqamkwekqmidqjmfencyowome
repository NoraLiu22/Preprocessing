							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
					}
				});
		}

		function trackPackage1 (data) {
			if (document.querySelector('.card-position').classList.contains('annual-view')) {
				data = afrPackages[1]
				console.log('package 1 annual: ', afrPackages[1])
			} else {
				data = afrPackages[0]
				console.log('package 1 weekly: ', afrPackages[0])
			}
			dataLayer.push({
				'event': 'event-Ecommerce',
				'eventCategory': 'Ecommerce',
				'eventAction':  'addToCart',
				'eventLabel': promote_channel,
				'eventValue': 0
			});
			dataLayer.push({
				'ecommerce': {
				'event': 'addToCart',
				'currencyCode': 'AUD',
						'add': [
						{
							'name': data.name,
							'id': data.id,
							'price': data.price,
							'brand': data.brand,
							'variant': data.variant,
							'list_position': data.list_position
						}]
