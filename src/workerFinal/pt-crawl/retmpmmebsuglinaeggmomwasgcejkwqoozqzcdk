    $(function () {
        $('#btn1').on('click', function () {
            search()
        })
        $('#search1').keydown(function (e) {
            if (e.keyCode == 13) {
                search()
            }
        })
    })

    function search() {
        if (!$('#search1').val) { return }
        let searchText = $('#search1').val()
        let searchUrl = encodeURI("/BoxOffice/search.html?key=" + searchText); //使用encodeURI编码
        location.href = searchUrl;
    }
