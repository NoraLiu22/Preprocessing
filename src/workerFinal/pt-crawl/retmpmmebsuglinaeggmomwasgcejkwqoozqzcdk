<script>
    var _FilterInfo = {
        DayType: "1",
        sDate: new Date().DateAdd('d', -9).DateFormat("yyyy-MM-dd"),
        eDate: new Date().DateFormat("yyyy-MM-dd")
    }
    $(function () {
        //加载数据
        PageLoad_Data();
    });

    function PageLoad_Data() {
        $Common.Popup.LoadingShow('bodyinfo');//Loading显示
        $Common.Ajax.PostAPI("BoxOffice_GetTrendScreenData", "sCityId=ALL&sdate=" + _FilterInfo.sDate + "&edate=" + _FilterInfo.eDate, function (data) {
            $Common.Popup.LoadingHide('bodyinfo');//Loading显示
            if (data && data["Table"] && data["Table"].length > 0) {
                PageLoad_DataChart(data["Table1"]);
                PageLoad_DataTable(data);
            } else $("#TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        }, function (msg) {
            $("#TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        });
    }

    //图表
    function PageLoad_DataChart(data) {


        var sArr = $Common.Json.GetByKey(data, 'm_name,m_date,m_daysum');
        var lArr = $Common.Format.Array.Unique(sArr['m_name']);
        var xArr = $Common.Format.Array.Sort($Common.Format.Array.Unique(sArr['m_date']), true);

        var _ChartObj = new EChartsHelper(), _ParamArr = [], _ParamArrPie = [];
        _ChartObj.xAxis[0].data = xArr;//X轴
        _ChartObj.xAxis[0].boundaryGap = true;
        _ChartObj.xAxis[0].axisLabel.interval = 0;//x轴始终显示
        _ChartObj.legend.data = lArr;//图例
        var jsonSelected = '{';
        for (var i = 0; i < _ChartObj.legend.data.length; i++) {
            if (i > 4) {
                jsonSelected += '"' + _ChartObj.legend.data[i] + '":false,'
            }
        }
        jsonSelected = jsonSelected.substring(0, jsonSelected.length - 1);
        jsonSelected += '}';

        _ChartObj.legend.selected = JSON.parse(jsonSelected);
        _ChartObj.title[0].text = _FilterInfo.sDate + '至' + $Common.Format.ToDate(_FilterInfo.eDate).DateFormat('MM-dd') + '地域分布排片统计';

        for (var i = 0; i < lArr.length; i++) {
            var arr = [];
            for (var n = 0; n < xArr.length; n++) {
                var isnone = true;
                for (var m = 0; m < sArr['m_daysum'].length; m++) {
                    if (sArr['m_name'][m] == lArr[i] && sArr['m_date'][m] == xArr[n]) {
                        arr.push(sArr['m_daysum'][m]);
                        isnone = false;
                        break;
                    }
                }
                if (isnone) arr.push("0");
            }
            _ChartObj.series.push(_ChartObj.GetSeries('line', lArr[i], arr, _ParamArr));
        }


        _ChartObj.DrawChart('dvChart', ''); //生成图片
    }

    //列表
    function PageLoad_DataTable(data) {

        var tableData = { data1: data['Table'], data2: data['Table1'], data3: data['Table2'] };
        $Common.Data.InitTemplate("TP_TableList", tableData, function (html) {
            $("#TableList").html(html);
        });
    }
</script> contentType 9 text/html url 59 https://www.endata.com.cn:443/BoxOffice/RowPiece/Trend.html responseCode 3 200 