        <div class="taxonomy-node" data-id="GroupType/30">
            <!-- NODE PARENT -->
            
            <!-- NODE CHILDREN -->
            <!-- NOTE: removed a div and applied the classes to the ul inside it -->
            <ul class="taxonomy-child-list taxonomy-lvl-0">
                        <li class="taxonomy-child-list-item">
