                    css: { 'seeking': seeking },
                    click: clickSeek,
                    event: {
                        'mousedown': function () { seeking(true); }
                    }">
                    <div class="progress-bar-buffer" data-bind="style: { 'width': bufferBarWidth }"></div>
                    <div class="progress-bar-inner">
                        <div class="progress-bar-played" data-bind="style: { 'width': progressBarWidth }"></div>
                        <div class="progress-bar-handle" data-bind="
                            event: {
                                'mousedown': function () { seeking(true); }
                            }
                        "
                        ></div>
                    </div>
                </div>
            <!-- /ko -->
        </mplayer-inner>
        <mplayer-inner-tracklist data-bind="foreach: player2.currentTracklist()">
            <trtrack data-bind="css: { 'playing': $parent.player2.currentTrack().trackId == trackId}, click: goToThisTrack">
                <trnumber data-bind="text: trackNumber">. </trnumber>
                <trtrackname data-bind="text: trackTitle"></trtrackname>
                <trtime>00:10 / 00:58</trtime>
