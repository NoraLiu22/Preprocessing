                <trnumber data-bind="text: trackNumber">. </trnumber>
                <trtrackname data-bind="text: trackTitle"></trtrackname>
                <trtime>00:10 / 00:58</trtime>
            </trtrack>
        </mplayer-inner-tracklist>

    
    </mplayer>
