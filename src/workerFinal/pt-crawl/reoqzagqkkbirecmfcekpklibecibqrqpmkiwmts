        <span class="txt" data-bind="visible: !currentTrack().isPreorder">buy</span>
    </a>
    <!-- alt buy/pre -->
    <a class="mpbuy alt buy-now" data-bind="attr: { 'href': currentTrack().tralbumBuyUrl }, click: buyClick">
        <span class="txt" data-bind="visible: currentTrack().isPreorder" style="display: none;">pre-order</span>
        <span class="txt" data-bind="visible: !currentTrack().isPreorder">buy</span>
    </a>
    <a class="mpbuy purchased-msg" href="">
        <span class="icon"><svg width="15px" height="13px" viewBox="0 0 20 20"><use xlink:href="#collect-control-purchased"></svg></span>
        <span class="txt">you own this</span>
    </a>
    <!-- alt you own this -->
    <a class="mpbuy alt purchased-msg" title="You own this" href="">
        <!--span class="txt">you own this</span-->
        <span class="icon"><svg width="15px" height="13px" viewBox="0 0 20 20"><use xlink:href="#collect-control-purchased"></svg></span>
    </a>
