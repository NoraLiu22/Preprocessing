<script>
    function changeDiv(tmp) {
        var tmp1 = document.getElementById('heading-'+tmp);
        var tmp2 = document.getElementById('caret-' + tmp);
        switch (tmp1.className) {
            case 'card-header':
                tmp1.className = 'card-header open';
                tmp2.className = "fas fa-chevron-up";
                $(this).prev(".card-header open").find(".fas").removeClass("fas fa-chevron-down").addClass("fas fa-chevron-up");
                break;
				
            case 'card-header open':
                tmp1.className = 'card-header';
                tmp2.className = "fas fa-chevron-down";
                $(this).prev(".card-header open").find(".fas").removeClass("fas fa-chevron-up").addClass("fas fa-chevron-down");
                break;
        }


    }

</script>

