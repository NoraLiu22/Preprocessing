<script type="text/javascript">

    function RefreshCaptcha() {

        var img = document.getElementById("imgCaptcha");
        var num = randomstring(5)
        $('#randomcapcha').val(num);
        img.src = "/ExamSite/GetCaptchaImage?query=" + num;

    }
    function randomstring(L) {
        return Math.floor(Math.random() * (Math.pow(10, L)));
        //var s = '';
        //var randomchar = function () {
        //    var n = Math.floor(Math.random() * 62);
        //    if (n < 10) return n; //1-10
        //   if (n < 36) return String.fromCharCode(n + 55); //A-Z
        //  return String.fromCharCode(n + 61); //a-z
        //}
        //while (s.length < L) s += randomchar();
        //return s;
    }
    $('#captcha_code').keypress(function (event) {
        console.log(event.which);
        if (event.which != 8 && isNaN(String.fromCharCode(event.which))) {
            event.preventDefault();
        }
    });

    function SaveDetails() {
        var selectedProfile = $("input[name='profile']:checked").val();
        var selectedUsers = $("input[name='approximateusers']:checked").val();
        $.ajax({
            //method: "Post",
            url: "/Account/ContactUs",
            data: { FirstName: $("#firstName").val(), LastName: $("#lastname").val(), email: $("#email").val(), phoneno: $("#phone_no").val(), Comments: $("#comments").val(), Institution: $("#NameofInstitution").val(), Users: selectedUsers, Profile: selectedProfile },
            //url: "/Outer/ContactUsPost?FirstName=" + $("#firstName").val() + "&LastName=" + $("#lastname").val() + "&email=" + $("#email").val() + "&phoneno=" + $("#phone_no").val() + "&Comments=" + $("#comments").val() + "&Institution=" + $("#NameofInstitution").val() + "&Users=" + selectedUsers + "&Profile=" + selectedProfile,
            //data: {},
            dataType: "json",
            success: function (data) {
                console.log("OK");
                // console.log(response);
                //alert("Form submitted successfully");
                $('#successmsg').show();
                // Hide the span element after 3 seconds (adjust as needed)
                setTimeout(function () {
                    $('#successmsg').hide();
                }, 6000);
                $(".modal-body").modal('hide');
                //window.location.href = "/usmle-step-1";
            },
            failure: function (response) {
                console.log("WRONG");
                return false;
            }
        });
        //window.location.href = "/usmle-step-1";
    }


    function isNumber(evt) {
        evt = (evt) ? evt : window.event;
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        placedtextcode = charCode;
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        return true;
    }



    $(document).ready(function () {
        $('.pricing-tab-as').click(function () {
            $('.pricing-tab-as').removeClass("active");
            $(this).addClass("active");
            var dataTarget = $(this).attr("data-target");
            if (dataTarget == "comp-course") {
                $('.pricingLEn.new-sectn-graybg').addClass("graybgrd");
            } else {
                $('.pricingLEn.new-sectn-graybg').removeClass("graybgrd");
            }
            dataTarget = "." + dataTarget;
            $('.newPricingTabs').removeClass("activeTab");
            $(dataTarget).addClass("activeTab");
        });
    });
</script>
<script>
    //accordion active
    var acc = document.getElementsByClassName("accordion");
    var i;

    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {

            var panel = this.nextElementSibling;
            if (panel.style.display === "block") {
                panel.style.display = "none";
                $(".accordion").removeClass("active");
            } else {
                $(".accordion").removeClass("active");
                $('.panel').css("display", "none");
                this.classList.toggle("active");
                panel.style.display = "block";
            }
        });
    };




</script>

