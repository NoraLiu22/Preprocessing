var notSelectedImage = new Array(2);
notSelectedImage[0] = new Image(8,8);
notSelectedImage[0].src = "images/red-arrow.gif";
notSelectedImage[0] = new Image(11,11);
notSelectedImage[0].src = "images/red-diamond.gif";


//Change an image on mouse rollover
function swapImage(i,j) {
  if (j=='r') {
    var onName = eval('document.button' + i);
    onName.src = "images/red-arrow.gif";
  }
}

//Change an image on mouse rolloff
function restoreImage(i,j) {
  if (j=='r') {
    var offName = eval('document.button' + i);
    offName.src = "images/red-diamond.gif";
  }
}

// Blank the status bar so mouse rollovers don't display image name
function clearStatus(msg) {
  window.status = msg;
  return true;
}

