	<script type="text/javascript">
		//ham click sort
		function btnGetWatchedList($that, cinema_id) {
			if ($that.data("ajax")) {
				return;
			}
			var evenclk = getloading($that);
			var data = "{'movie_cd': " + cinema_id + ""
							+ "}";
			$.ajax({
				url: 'detail.aspx/btnGetWatchedListData',
				data: data,
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				timeout: base_ajax_time_out,
				beforeSend: function (xhr) {
					$that.data("ajax", true); //2重送信回避のためdisableに
				},
				success: function (data) {
					
					if (data.d != '') {
						var datatemp = data.d;
						openBoxModal('#lightbox_listmember_watched');
						$("#ul_get_watched_list").empty();
						$("#ul_get_watched_list").html(datatemp.toString());
						
					}
					$that.data("ajax", false);
					addEven($that, evenclk);
					$that.find(".indicator").remove();
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					$that.data("ajax", false);
					$that.find(".indicator").remove();
				}
			});
		}
		function btnGetWishList($that, cinema_id) {
			if ($that.data("ajax")) {
				return;
			}
			var evenclk = getloading($that);
			var data = "{'movie_cd': " + cinema_id + ""
							+ "}";
			$.ajax({
				url: 'detail.aspx/btnGetWishListData',
				data: data,
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				timeout: base_ajax_time_out,
				beforeSend: function (xhr) {
					$that.data("ajax", true); //2重送信回避のためdisableに
				},
				success: function (data) {
					if (data.d != '') {
						var datatemp = data.d;
						openBoxModal('#lightbox_listmember_wish');
						$("#ul_get_wish_list").empty();
						$("#ul_get_wish_list").html(datatemp.toString());

					}
					$that.data("ajax", false);
					addEven($that, evenclk);
					$that.find(".indicator").remove();
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					$that.data("ajax", false);
					$that.find(".indicator").remove();
				}
			});
		}
		
	</script>

		
