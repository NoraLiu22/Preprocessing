			$media_cd.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				$("#media_subcd09").hide();
				newdeterminDisplay(selectedValue, displayListOfMethod)
				$media_subcd2.val("-1");
				if (selectedValue == 6)//floatR
				{
					$("#media_free_txt0").removeClass("floatR");
				}
				else {
					$("#media_free_txt0").addClass("floatR");
					if (selectedValue == 3) {
						$media_subcd1.show();
						$media_subcd2.hide();
						$media_subcd1.val("-1");
					}
					else if (selectedValue == 1) {
						$media_subcd3.show();
						$media_subcd3.val("-1");
						$media_subcd4.hide();
					}
					else if (selectedValue == 7) {
						$media_subcd3.val("-1");
						$media_subcd5.val("-1");
					}
					else if (selectedValue == 4) {
						$media_subcd7.val("-1");
						$media_subcd8.val("-1");
					}
				}
				$('.regist_review_class_block').jScrollPane({
					autoReinitialise: false,
					showArrows: true
				})
			}).change();

			//観賞方法2つ目パッケージのセレクトボックス
			$media_subcd1.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				var postData = "id=" + selectedValue;
				if (selectedValue < 0) {
					$media_subcd2.hide();
					$("#media_free_txt0").hide();
				};
				if (selectedValue > 0) {
					$media_subcd2.show();
				};
				newdynamicpulldownlist(newReturnLocationpath() + 'member/review/bysell.ashx', postData, $media_subcd2, media_subcd2_selected_value);
			}).change();

			//観賞方法2つ目テレビのセレクトボックス
			$media_subcd7.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				var postData = "id=" + selectedValue;
				$media_subcd8.hide();
				$media_subcd8_2.hide();
				if (selectedValue == 1) {
					$media_subcd8.show()
				}
				else if (selectedValue == 2) {
					$media_subcd8_2.show();
				}
				newdynamicpulldownlist(newReturnLocationpath() + 'member/review/channel.ashx', postData, $media_subcd8, media_subcd8_selected_value);
				$(document).ajaxSuccess(function (event, xhr, settings) {
					$media_subcd8.val("-1");
				});
			}).change();

			//観賞方法2つ目映画館のセレクトボックス
			$media_subcd3.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				var selectedValueMedia = $media_cd.find(':selected').val();
				if (selectedValueMedia == 1) {
					var postData = "pref=" + selectedValue;
					if (selectedValue < 0) {
						$media_subcd4.hide();
					};
					if (selectedValue > 0) {
						$media_subcd4.show();
					};
					newdynamicpulldownlist(newReturnLocationpath() + 'member/review/theater.ashx', postData, $media_subcd4, media_subcd4_selected_value);
					$(document).ajaxSuccess(function (event, xhr, settings) {
						$media_subcd4.val("-1");
					});
				}
			}).change();

			//観賞方法5つ目パッケージのセレクトボックス
			$media_subcd5.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				var selectedValueMedia = $media_cd.find(':selected').val();
				var postData = "id=" + selectedValueMedia;
				$("#media_subcd09").hide();
				if (selectedValue < 0) {
					$("#media_subcd01").hide();
					$media_subcd2.hide();
				};
				if (selectedValue > 0) {
					$("#media_subcd01").show();
					$media_subcd1.hide();
					$media_subcd2.show();
					$media_subcd2.val("-1");
				};
				newdynamicpulldownlist(newReturnLocationpath() + 'member/review/bysell.ashx', postData, $media_subcd2, media_subcd2_selected_value);
				$(document).ajaxSuccess(function (event, xhr, settings) {
					$media_subcd2.val("-1");
				});
			}).change();

			//edit by khoand  #5471
			$media_subcd2.bind('change', function () {
				var selectedValue = $(this).find(':selected').val();
				var selectedValueMedia = $media_cd.find(':selected').val();
				$("#media_subcd09").hide();
				$("#media_free_txt0").hide();
				$("#media_subcd02").hide();
				if (selectedValueMedia == 3) {
				}
				else if (selectedValueMedia == 7) {
					$media_subcd9.val("-1");
					if (selectedValue == 1 || selectedValue == 2) {
						$("#media_subcd09").show();
					}
					else {
						$("#media_subcd09").hide();
					}

				}
			}).change();


		});

		//convert numberic 2byte to 1byte
		function cast_num(a) {
			return a.replace(/[０１２３４５６７８９]/g, function (a) { var b = "０１２３４５６７８９".indexOf(a); return (b !== -1) ? b : a; });
		}

		//get data cinema
		function GetDataMovie(movie_cinema_id, $that) {
			var data = "{'_member_cd': '" + $("#ctl00_ctl00_container_include_container_include_member_review_member_cd").val() + "'"
					+ ",'_cinema_id': " + movie_cinema_id + ""
					+ "}";
			$.ajax({
				url: newReturnLocationpath() + "public/cinema/detail.aspx/GetCinemaDetail",
				type: 'post',
				contentType: "application/json; charset=utf-8",
				dataType: 'json',
				data: data,
				timeout: base_ajax_time_out,
				async: false,
				cache: false,
				success: function (data) {
					if (data.d != null) {
						var objJson = $.parseJSON(data.d);
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_title_review").text(objJson[0].movie_title_review);
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val(objJson[0].movie_cd);
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_time").val(objJson[0].movie_watch_time);
						$("#ctl00_ctl00_container_include_container_include_member_review_save_genre_cd").val(objJson[0].save_genre_cd);
						$("#ctl00_ctl00_container_include_container_include_member_review_release_date_year_review").text(objJson[0].release_date_year_review);
						$("#show_edit_review #watch-record .member-review-string-lblcountreview").html((parseInt(objJson[0].lblcountreview) + 1) + "回目");
						$("#lblcountreview_member_review_hiden").val(parseInt(objJson[0].lblcountreview).toString());

						$("#show_edit_review #watch-record .member-review-string-lblavgrapoint").html(objJson[0].lblavgrapoint + "点");
					}
					else {
						window.location.href = base_url_system_error;
					}
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					window.location.href = base_url_system_error;
				}
			});
		};
		//check input value
		function checkInputRating(varText) {
			var varTextByte = "０１２３４５６７８９";
			var varTextEn = "0123456789";
			for (var i = 0; i < varText.length; i++) {
				var item = varText[i];
				if (varTextByte.indexOf(item) >= 0) {
					alert("半角数字で入力してください。");
					return 1;
				}
				if (varTextEn.indexOf(item) == -1) {
					alert("フォーマットが正しくありません");
					return 1;
				}
			}
			return 0;
		}
		//function Load ligthbox confirm
		function loadconfirm($status_review) {
			//		if ($("#ctl00_ctl00_container_include_container_include_member_review_rating").val() == '') {
			//			alert("点を入力してください。");
			//			return;
			//		} else 
			//check input value rating
			var member_rating = $("#ctl00_ctl00_container_include_container_include_member_review_rating").val();
			if (parseInt(member_rating) > 100) {
				alert("０～１００点を入力してください。");
				return;
			}
			if (member_rating.length > 0) {
				if (checkInputRating(member_rating) == 1) {
					$("#ctl00_ctl00_container_include_container_include_member_review_rating").focus();
					return;
				}
			}
			if (member_rating.substring(0, 1) == ".") {
				alert("フォーマットが正しくありません");
				return;
			}

			//check input vaolue movie cost
			var member_movie_cost = $("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val();
			if (member_movie_cost.substring(0, 1) == ".") {
				alert("フォーマットが正しくありません");
				return;
			}
			if (member_movie_cost.length > 0) {
				if (checkInputRating(member_movie_cost) == 1) {
					$("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").focus();
					return;
				}
			}
			$("#confirm_rating").html(($("#ctl00_ctl00_container_include_container_include_member_review_rating").val() == "" ? "－" : $("#ctl00_ctl00_container_include_container_include_member_review_rating").val()) + "点  ※100点満点");
			$("#movie_cost").html(($("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val() == "" ? "" : $("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val() + " 円"));
			$("#movie_title").html($("#ctl00_ctl00_container_include_container_include_member_review_movie_title_review").text() + " 公開年 :" + $("#ctl00_ctl00_container_include_container_include_member_review_release_date_year_review").text());

			$("#movie_cd").html($("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val());

			var movie_watch_kbn = parseInt($('#movie_watch_kbn_span input[type="radio"]:checked').val());
			var toDayConfirm = $("#toDay_review").val();

			if (typeof toDayConfirm === undefined || toDayConfirm == "") {
				movie_watch_year = 0; movie_watch_month = 0; movie_watch_day = 0;
			}
			else {
				movie_watch_year = parseInt(toDayConfirm.substring(0, 4));
				movie_watch_month = parseInt(toDayConfirm.substring(5, 7));
				movie_watch_day = parseInt(toDayConfirm.substring(9, 11));
			}

			var movie_watch_ymd_confirm = "";
			if (movie_watch_kbn == 0) {
				if ((movie_watch_year > 0) && (movie_watch_month > 0) && (movie_watch_day > 0)) {
					movie_watch_ymd_confirm += movie_watch_year + "年" + (movie_watch_month < 10 ? "0" + movie_watch_month : movie_watch_month) + "月" + (movie_watch_day < 10 ? "0" + movie_watch_day : movie_watch_day) + "日" + "（年月日指定）";
				}
				else if ((movie_watch_year > 0) && (movie_watch_month > 0)) {
					movie_watch_ymd_confirm += movie_watch_year + "年" + (movie_watch_month < 10 ? "0" + movie_watch_month : movie_watch_month) + "月";
					movie_watch_ymd_confirm += "（年月指定）";
				}
				else if (movie_watch_year > 0) {
					movie_watch_ymd_confirm += "未指定";
				}
			}
			else if (movie_watch_kbn == 1) {
				var movie_watch_period = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_period").val());
				if (movie_watch_period > 0) {
					movie_watch_ymd_confirm += movie_watch_period.toString() + "年代 ";
					movie_watch_ymd_confirm += "（年代指定）";
				}
				else {
					movie_watch_ymd_confirm += "未指定";
				}
			}
			else {
				movie_watch_ymd_confirm += "選択しない";
			}
			$("#movie_watch_ymd").html(movie_watch_ymd_confirm);
			$("#label_name").html($("#checklist input:checkbox:checked").map(function () {
				return "-" + $(this).attr("title") + "<br />";
			}).get());
			var $input = $('#display_cd_list_div input[type="radio"]:checked').val();
			$("#display_name").html($("#display_cd_list_div input[type='radio']:checked").attr("title"));
			$("#spoiler_flg").html(($("#ctl00_ctl00_container_include_container_include_member_review_spoiler_flg_sv").is(':checked') ? "<img src='../../common/img/icon/icon_netabare_2.png' alt='ネタバレ' />" : ""));

			$("#memo").html((htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_memo").val())).replaceAll("\n", "<br />"));
			$("#memo2").html((htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_memo2").val())).replaceAll("\n", "<br />"));
			$("#up_kinema").html(($("#ctl00_ctl00_container_include_container_include_member_review_up_kinema").is(':checked') ? "キネマ旬報本誌の読者評にも投稿する<br />" : ""));
			var media_cd = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_media_cd").val());
			$("#cinema_name").html(htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_cinema_name").val()).replace("\r\n", "<br />"));
			var media_name_confirm = $("#ctl00_ctl00_container_include_container_include_member_review_media_cd option:selected").text() + "   ";
			if (media_cd == 1) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd3 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd3 option:selected").text() : "") + " ";
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd4 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd4 option:selected").text() : "") + " ";
			}
			else if (media_cd == 3) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1 option:selected").text() : "") + " ";
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2 option:selected").text() : "") + " ";
			}
			else if (media_cd == 4) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd7 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd7 option:selected").text() : "") + " ";
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8 option:selected").text() : "") + " ";
			}
			else if (media_cd == 7) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd5 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd5 option:selected").text() : "") + " ";
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2 option:selected").text() : "") + " ";
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd9 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd9 option:selected").text() : "") + " ";
			}
			else if (media_cd == 8) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_subcd6 option:selected").val() != "-1" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd6 option:selected").text() : "") + " ";
			}
			else if (media_cd == 6) {
				media_name_confirm += ($("#ctl00_ctl00_container_include_container_include_member_review_media_free_txt").val() != "" ? $("#ctl00_ctl00_container_include_container_include_member_review_media_free_txt").val() : "") + " ";
			}

			$("#media_name").html(media_name_confirm);
			$("#movie_watch_with_cd").html($("#watch-record .movie_watch_class_list input[type='radio']:checked").map(function () {
				return $(this).attr("title") + "  ";
			}).get() + htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_with_memo").val()));

			$("#check_3d").html($("#watch-record .subtitle input:checkbox:checked").map(function () { return $(this).attr("title") + "   "; }).get());

			$("#social_name").html($(".social_list input:checkbox:checked").map(function () {
				return "- " + $(this).attr("title") + "<br />";
			}).get());

			$("#status_review").val($status_review);
			closebox();
			openBoxModal("#showconfirm");

			$('.regist_review_confirm_class_block').jScrollPane({
				autoReinitialise: false,
				showArrows: true
			})
			$('.label_name_member_review_confirm').jScrollPane({
				autoReinitialise: false,
				showArrows: true
			})
			var api = $('.label_name_member_review_confirm').data("jsp").scrollToY(0);
		};

		//Khai bao bien
		var member_cd; var new_label; var cinema_cd;
		var review_id = 0; var display_cd; var memo;
		var movie_cd; var movie_title; var rating; var movie_watch_kbn; var movie_watch_year; var movie_watch_month;
		var movie_watch_day; var movie_watch_period; var movie_watch_time; var movie_watch_with_cd; var movie_watch_with_name;
		var movie_watch_with_memo; var media_cd; var media_cd_text; var save_genre_cd; var cinema_name; var label_cd_list; var label_cd_list_text;
		var spoiler_flg; var facebook; var twitter; var hatena; var mixi; var google; var is3D; var subtitle; var up_kinema; var up_evernote; var memo2;
		var movie_cost; var media_subcd3; var media_subcd4; var media_subcd1; var media_subcd2; var media_subcd5; var media_subcd6; var media_subcd7;
		var media_subcd8; var media_subcd9; var media_subcd10; var media_free_txt; var status_review; var toDay;
		//get value
		function voidGetValue() {
			member_cd = $("#ctl00_ctl00_container_include_container_include_member_review_member_cd").val();
			new_label = htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_new_label").val());
			cinema_cd = $("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val();
			if ($("#ctl00_ctl00_container_include_container_include_member_review_review_save_id").val() != "")
				review_id = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_review_save_id").val());
			display_cd = $('#display_cd_list_div input[type="radio"]:checked').val();
			memo = htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_memo").val());
			movie_cd = $("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val();
			movie_title = $("#ctl00_ctl00_container_include_container_include_member_review_movie_title_review").text();
			rating = ($("#ctl00_ctl00_container_include_container_include_member_review_rating").val() != "" ? $("#ctl00_ctl00_container_include_container_include_member_review_rating").val() : -1);
			movie_watch_kbn = $('#movie_watch_kbn_span input[type="radio"]:checked').val();
			toDay = $("#toDay_review").val();
			if (typeof toDay === undefined || toDay == "") {
				movie_watch_year = 0; movie_watch_month = 0; movie_watch_day = 0;
			}
			else {
				movie_watch_year = parseInt(toDay.substring(0, 4));
				movie_watch_month = parseInt(toDay.substring(5, 7));
				movie_watch_day = parseInt(toDay.substring(9, 11));
			}
			movie_watch_period = $("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_period").val();
			movie_watch_time = $("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_time").val();
			movie_watch_with_cd = $(".movie_watch_class_list input[type='radio']:checked").map(function () {
				return $(this).attr("value");
			}).get();

			movie_watch_with_name = $(".movie_watch_class_list input[type='radio']:checked").map(function () {
				return $(this).attr("title");
			}).get();

			movie_watch_with_memo = $("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_with_memo").val();
			media_cd = $("#ctl00_ctl00_container_include_container_include_member_review_media_cd").val();
			media_cd_text = $("#ctl00_ctl00_container_include_container_include_member_review_media_cd :selected").text();
			save_genre_cd = $("#ctl00_ctl00_container_include_container_include_member_review_save_genre_cd").val();
			cinema_name = htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_cinema_name").val());
			label_cd_list = $("#checklist input:checkbox:checked").map(function () {
				return $(this).attr("value");
			}).get();
			label_cd_list_text = $("#checklist input:checkbox:checked").map(function () {
				return $(this).attr("title");
			}).get();

			spoiler_flg = ($("#ctl00_ctl00_container_include_container_include_member_review_spoiler_flg_sv").is(':checked') ? true : false);
			facebook = ($("#facebook").is(':checked') ? true : false);
			twitter = ($("#twitter").is(':checked') ? true : false);
			hatena = ($("#hatena").is(':checked') ? true : false);
			mixi = ($("#mixi").is(':checked') ? true : false);
			google = ($("#google").is(':checked') ? true : false);
			is3D = ($("#ctl00_ctl00_container_include_container_include_member_review_check3D").is(':checked') ? true : false);
			if ($("#ctl00_ctl00_container_include_container_include_member_review_subtitle_00").is(':checked')) {
				subtitle = "1";
			}
			else {
				subtitle = "0";
			}
			if ($("#ctl00_ctl00_container_include_container_include_member_review_subtitle_01").is(':checked')) {
				subtitle += "1";
			}
			else {
				subtitle += "0";
			}
			subtitle = parseInt(subtitle);

			up_kinema = ($("#ctl00_ctl00_container_include_container_include_member_review_up_kinema").is(':checked') ? true : false);
			up_evernote = ($("#evernote").is(':checked') ? true : false);
			memo2 = htmlEncode($("#ctl00_ctl00_container_include_container_include_member_review_memo2").val());
			movie_cost = $("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val();

			media_subcd1 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1 :selected").val();
			media_subcd2 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2 :selected").val();
			media_subcd3 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd3 :selected").val();
			media_subcd4 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd4 :selected").val();
			media_subcd5 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd5 :selected").val();
			media_subcd6 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd6 :selected").val();
			media_subcd7 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd7 :selected").val();
			if (media_subcd7 != 2)
				media_subcd8 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8 :selected").val();
			else
				media_subcd8 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8_2 :selected").val();
			media_subcd9 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd9 :selected").val();
			media_subcd10 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd10 :selected").val();
			media_free_txt = $("#ctl00_ctl00_container_include_container_include_member_review_media_free_txt").val();
			status_review = parseInt($("#status_review").val());

		};


		//function save new label name
		function AddLabel_Member($that) {
			if ($that.data("ajax") == true) {
				return;
			}
			//Load image
			var evenclk = getloading($that);
			voidGetValue();
			if (new_label == "") {
				$that.find(".indicator").remove();
				return;
			}
			var data = "{'member_cd': '" + member_cd + "'"
					+ ",'new_label': '" + new_label + "'"
					+ ",'cinema_cd': " + cinema_cd + ""
					+ ",'review_id': " + review_id + ""
					+ "}";
			$.ajax({
				url: newReturnLocationpath() + 'public/cinema/detail.aspx/AddLabel_Javascript',
				type: 'post',
				contentType: "application/json; charset=utf-8",
				dataType: 'json',
				data: data,
				timeout: base_ajax_time_out,
				async: false,
				cache: false,
				success: function (data) {
					addEven($that, evenclk);
					$that.find(".indicator").remove();
					if (data.d != '') {
						if (data.d == '0') {
							$that.find(".indicator").remove();
							alert("このラックが存在しています。");
						}
						else {
							$("#ctl00_ctl00_container_include_container_include_member_review_page_count_label_name").val(2);
							$("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val(0);
							//						var label_cd_value = data.d.replaceAll("<li>", "");
							//						label_cd_value = label_cd_value.replaceAll("</li>", "<br/>");
							$("#checklist").empty();
							$("#checklist").append(data.d);
							$("#ctl00_ctl00_container_include_container_include_member_review_new_label").val('');

							$('.cont_label_cd_name_list').jScrollPane({
								autoReinitialise: false,
								showArrows: true
							})
							//movie scroll to up
							var api = $('.cont_label_cd_name_list').data("jsp").scrollToY(0);

						}
					}
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					window.location.href = base_url_system_error;
				}
			});
		};

		//function post data reivew
		function submit_review($that) {
			if ($that.data("ajax") == true) {
				return;
			}
			var evenclk = getloading($that);
			voidGetValue();
			var review_save_id = review_id;
			var data = "{'member_cd': '" + member_cd + "'"
					+ ",'review_save_id': '" + review_save_id + "'"
					+ ",'display_cd': '" + display_cd + "'"
					+ ",'memo': '" + memo + "'"
					+ ",'movie_cd': '" + movie_cd + "'"
					+ ",'movie_title': '" + movie_title + "'"
					+ ",'rating': '" + rating + "'"
					+ ",'movie_watch_kbn': '" + movie_watch_kbn + "'"
					+ ",'movie_watch_year': '" + movie_watch_year + "'"
					+ ",'movie_watch_month': '" + movie_watch_month + "'"
					+ ",'movie_watch_day': '" + movie_watch_day + "'"
					+ ",'movie_watch_period': '" + movie_watch_period + "'"
					+ ",'movie_watch_time': '" + movie_watch_time + "'"
					+ ",'movie_watch_with_cd': '" + movie_watch_with_cd + "'"
					+ ",'movie_watch_with_name': '" + movie_watch_with_name + "'"
					+ ",'movie_watch_with_memo': '" + movie_watch_with_memo + "'"
					+ ",'media_cd': '" + media_cd + "'"
					+ ",'media_cd_text': '" + media_cd_text + "'"
					+ ",'save_genre_cd': '" + save_genre_cd + "'"
					+ ",'cinema_name': '" + cinema_name + "'"
					+ ",'label_cd_list': '" + label_cd_list + "'"
					+ ",'label_cd_list_text': '" + label_cd_list_text + "'"
					+ ",'spoiler_flg': " + spoiler_flg + ""
					+ ",'is3D': " + is3D + ""
					+ ",'subtitle': " + subtitle + ""
					+ ",'facebook': " + facebook + ""
					+ ",'twitter': " + twitter + ""
					+ ",'hatena': " + hatena + ""
					+ ",'mixi': " + mixi + ""
					+ ",'google': " + google + ""
					+ ",'up_kinema': " + up_kinema + ""
					+ ",'up_evernote': " + up_evernote + ""
					+ ",'memo2': '" + memo2 + "'"
					+ ",'movie_cost': '" + movie_cost + "'"
					+ ",'media_subcd3': '" + media_subcd3 + "'"
					+ ",'media_subcd4': '" + media_subcd4 + "'"
					+ ",'media_subcd1': '" + media_subcd1 + "'"
					+ ",'media_subcd2': '" + media_subcd2 + "'"
					+ ",'media_subcd5': '" + media_subcd5 + "'"
					+ ",'media_subcd6': '" + media_subcd6 + "'"
					+ ",'media_subcd7': '" + media_subcd7 + "'"
					+ ",'media_subcd8': '" + media_subcd8 + "'"
					+ ",'media_subcd9': '" + media_subcd9 + "'"
					+ ",'media_subcd10': '" + media_subcd10 + "'"
					+ ",'media_free_txt': '" + media_free_txt + "'"
					+ ",'status_review': " + status_review + ""
					+ "}";
			$.ajax({
				url: newReturnLocationpath() + "public/cinema/detail.aspx/submit_review",
				type: 'post',
				contentType: "application/json; charset=utf-8",
				dataType: 'json',
				data: data,
				timeout: base_ajax_time_out,
				async: false,
				cache: false,
				success: function (data) {
					addEven($that, evenclk);
					if (data.d) {
						//reload website after one secon
						window.location.reload();
					}
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (xhr.status == 401) {
						if (xhr.responseText.indexOf("{\"d\":true}")) {
							window.location.reload();
							return false;
						}
					}
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					window.location.href = base_url_system_error;
				}
			});
		};

		//Load data default when show lightbox
		function LoadDefaultValue() {
			$('input:checkbox').removeAttr('checked');

			$('#toDay_review').datepicker().datepicker('setDate', 'today');

			$("#ctl00_ctl00_container_include_container_include_member_review_memo").val("");

			$("#ctl00_ctl00_container_include_container_include_member_review_label_name_review_id").val("0");
			$("#ctl00_ctl00_container_include_container_include_member_review_page_count_label_name").val("2");
			$("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val("0");


			$("#ctl00_ctl00_container_include_container_include_member_review_rating").val('');
			$("#ctl00_ctl00_container_include_container_include_member_review_memo2").val("");
			$("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val("");
			$("#ctl00_ctl00_container_include_container_include_member_review_cinema_name").val('');
			$("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_with_memo").val("");

			//いつ観た 
			$("#dateselector0").hide();
			$("#dateselector1").hide();
			$("#dateselector2").hide();
			$("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_kbn02").prop('checked', true);
			//持っている 
			//		$("#mediaselector1").hide();
			//		$("#mediaselector2").hide();
			//		$("#mediaselector3").hide();

			//鑑賞方法 
			$("#media_subcd01").hide();
			$("#media_subcd02").hide();
			$("#media_subcd03").hide();
			$("#media_subcd04").hide();
			$("#media_subcd05").hide();
			$("#media_subcd09").hide();
			$("#media_free_txt0").hide();

			$("#ctl00_ctl00_container_include_container_include_member_review_media_cd").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1").val("1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd3").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd4").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd5").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd6").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd7").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd9").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd10").val("-1");
			$("#ctl00_ctl00_container_include_container_include_member_review_media_free_txt").val("");
			$("#btneditreview").hide();
			$("#btndeleteconfrim").hide();

		}

		function getloading_showreview($that) {
			var evenonclick = $that.attr("onclick");
			$that.removeAttr("onclick");
			$that.find("span").css({
				//backgroundColor: "#D6D3CA"
				//			position: "relative"
			});
			var indicator = $("<span>").addClass("indicator").css({
				width: 40,
				height: 20,
				backgroundColor: "#ffffff",
				opacity: 0.3,
				display: "block",
				position: "absolute",
				top: 0,
				left: 0
			}).append($("<img>", {
				src: newReturnLocationpath() + "images/indicator.white.gif"
			}).css({
				position: "absolute",
				top: 3,
				left: 10
			}));
			$that.find("span").append(indicator);
			return evenonclick;
		}
		//function getdate review set to control ajax
		function LoadDataReview($member_cd, $review_id, $movie_cd, $that) {
			//		if ($that.data("ajax") == true) {
			//			return;
			//		}
			var evenclk = "";
			var _member_cd = $member_cd.toString();
			var _review_id = parseInt($review_id);
			var _movie_cd = parseInt($movie_cd);
			if (_member_cd == "") {
				$that.find(".indicator").remove();
				return;
			}
			var data = "{'_member_cd': '" + _member_cd + "'"
					+ ",'_review_id': " + _review_id + ""
					+ ",'_movie_cd': " + _movie_cd + ""
					+ "}";
			$.ajax({
				url: newReturnLocationpath() + "public/cinema/detail.aspx/GetReviewMember",
				data: data,
				timeout: base_ajax_time_out,
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				beforeSend: function () {
					evenclk = getloading_showreview($that);
					//$that.data("ajax", true);
				},
				success: function (data, status, xhr) {
					if (data.d != "") {
						var objJson = $.parseJSON(data.d);
						//$that.data("ajax", false);
						//Set data to control

//						$('.cinema3 > .block_body').each(function (index) {
//							var $self = $(this).next();
//							$self.parent().find('.block_body').css({
//								'margin-left': '10px',
//								'margin-right': '10px',
//								'padding-top': '10px',
//								'padding-bottom': '10px'
//							});
//							$self.addClass('active');
//							// $self.parent().find('.block_body').height(targetHeight);
//							$self.parent().find('.block_body').css({ 'overflow': 'hidden', 'height': 'auto' });
//						});
						openBoxModal("#show_edit_review");

						//$that.find(".indicator").remove();
						$('input:radio').removeAttr('checked');

						LoadDefaultValue();

						//memer_cd
						$("#ctl00_ctl00_container_include_container_include_member_review_member_cd").val(objJson[0].member_cd);
						$("#ctl00_ctl00_container_include_container_include_member_review_review_save_id").val(objJson[0].id);
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val(objJson[0].movie_cd);

						//いつ観た 
						if (parseInt(objJson[0].movie_watch_kbn) == 0) {
							$("#dateselector1").show();
							var toDayLoad = new Date(objJson[0].movie_watch_year + "/" + objJson[0].movie_watch_month + "/" + objJson[0].movie_watch_day);
							$("#toDay_review").datepicker('setDate', toDayLoad);
						}
						else if (parseInt(objJson[0].movie_watch_kbn) == 1) {
							$("#dateselector2").show();
							$("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_period").val(objJson[0].movie_watch_period);
						}
						$("#movie_watch_kbn_span input:radio").map(function () {
							if ($(this).val() == objJson[0].movie_watch_kbn) {
								$(this).prop('checked', true);
							}
						}).get();

						//レビューを書く
						$("#ctl00_ctl00_container_include_container_include_member_review_memo").val((objJson[0].memo));
						//メモ 
						$("#ctl00_ctl00_container_include_container_include_member_review_memo2").val((objJson[0].memo2));
						$("#countUp").text(objJson[0].memo.length + "文字");
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_title_review").text(objJson[0].movie_title);

						//誰と観た 
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_watch_with_memo").val(objJson[0].movie_watch_with_memo);
						$("#watch-record .movie_watch_class_list input:radio").map(function () {
							if ($(this).attr("value") == objJson[0].movie_watch_with_cd) {
								$(this).prop('checked', true);
							}
						}).get();

						//評価 
						$("#ctl00_ctl00_container_include_container_include_member_review_rating").val((objJson[0].movie_point != -1 ? objJson[0].movie_point : ""));

						//鑑賞費用（半角数値） 
						$("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").val(objJson[0].movie_cost);

						//ラック名 
						var label_cd_value = objJson[0].label_cd_list; //.replaceAll("<li>", "")
						$("#checklist").empty();
						$("#checklist").append(label_cd_value);

						//レビュータイトル 
						$("#ctl00_ctl00_container_include_container_include_member_review_cinema_name").val(objJson[0].cinema_name);
						if (objJson[0].spoiler_flg == "1")
							$("#ctl00_ctl00_container_include_container_include_member_review_spoiler_flg_sv").prop("checked", true);
						else
							$("#System.Web.UI.HtmlControls.HtmlInputCheckBox").prop("checked", false);

						//投稿 
						if (objJson[0].up_kinema == "1")
							$("#ctl00_ctl00_container_include_container_include_member_review_up_kinema").prop("checked", true);
						else
							$("#ctl00_ctl00_container_include_container_include_member_review_up_kinema").prop("checked", false);

						//鑑賞方法 
						if (objJson[0].media_cd != "-1" && objJson[0].media_cd != "0") {
							$("#ctl00_ctl00_container_include_container_include_member_review_media_cd").val(objJson[0].media_cd);

							if (objJson[0].media_cd == "3") {
								$("#media_subcd01").show();
								var postData = "id=" + objJson[0].media_cd;
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1").val(objJson[0].media_subcd1);

								var $media_subcd2 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2");
								var media_subcd2_selected_value = "";
								newdynamicpulldownlist(newReturnLocationpath() + 'member/review/bysell.ashx', postData, $media_subcd2, media_subcd2_selected_value);
								$(document).ajaxSuccess(function (event, xhr, settings) {
									$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2").val(objJson[0].media_subcd2);
								});

								$media_subcd2.show();
							}
							else if (objJson[0].media_cd == "1") {
								$("#media_subcd02").show();
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd3").val(objJson[0].media_subcd3);
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd4").val(objJson[0].media_subcd4);
							}

							else if (objJson[0].media_cd == "4") {
								$("#media_subcd05").show();
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd7").val(objJson[0].media_subcd7);
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd8").val(objJson[0].media_subcd8);
							}

							else if (objJson[0].media_cd == "7") {
								$("#media_subcd03").show();
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd5").val(objJson[0].media_subcd5);
								var postData = "id=" + objJson[0].media_subcd5;

								var $media_subcd2 = $("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2");

								var media_subcd2_selected_value = "";
								newdynamicpulldownlist(newReturnLocationpath() + 'member/review/bysell.ashx', postData, $media_subcd2, media_subcd2_selected_value);
								$(document).ajaxSuccess(function (event, xhr, settings) {
									$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd2").val(objJson[0].media_subcd2);
								});

								$media_subcd2.show();
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd1").hide();
								$("#media_subcd01").show();

								if (parseInt(objJson[0].media_subcd2) > 0) {
									$("#media_subcd09").show();
									$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd9").val(objJson[0].media_subcd9);
								}
							}
							else if (objJson[0].media_cd == "8") {
								$("#media_subcd04").show();
								$("#ctl00_ctl00_container_include_container_include_member_review_media_subcd6").val(objJson[0].media_subcd6);
							}
							else if (objJson[0].media_cd == "6") {
								$("#ctl00_ctl00_container_include_container_include_member_review_media_free_txt").val(objJson[0].media_free_txt);
								$("#media_free_txt0").show();
							}
						}
						else {
							$("#ctl00_ctl00_container_include_container_include_member_review_media_cd").val("-1");
							$("#media_subcd01").hide();
							$("#media_subcd02").hide();
							$("#media_subcd03").hide();
							$("#media_subcd04").hide();
							$("#media_subcd05").hide();
							$("#media_subcd09").hide();
							$("#media_free_txt0").hide();
						}

						//3D/字幕/吹替 
						if (objJson[0].is3d == "1")
							$("#ctl00_ctl00_container_include_container_include_member_review_check3D").prop("checked", true);
						else
							$("#ctl00_ctl00_container_include_container_include_member_review_check3D").prop("checked", false);
						//set check button -subtitle

						$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_01").prop("checked", false);
						$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_00").prop("checked", false);
						var jsubtitle = parseInt(objJson[0].subtitle);
						if (jsubtitle == 11) {
							$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_00").prop("checked", true);
							$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_01").prop("checked", true);
						}
						else if (jsubtitle == 10) {
							$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_00").prop("checked", true);
						}
						else if (jsubtitle == 1) {
							$("#ctl00_ctl00_container_include_container_include_member_review_subtitle_01").prop("checked", true);
						}

						//レビュー公開／下書き 
						//set check radio button - display_cd
						if (parseInt(objJson[0].display_cd) == 9)
						{ $("#ctl00_ctl00_container_include_container_include_member_review_display_cd09").prop('checked', true); }
						else
						{ $("#ctl00_ctl00_container_include_container_include_member_review_display_cd00").prop('checked', true); }

						//social Load checked
						var i;
						var str = objJson[0].up_evernote;
						for (i = 0; i <= str.length; ++i) {
							if ((str.length - i) == 6) {
								if (str.substr(i, 1) == "1")
									$("#facebook").prop("checked", true);
								else
									$("#facebook").prop("checked", false);
							}
							else if ((str.length - i) == 5) {
								if (str.substr(i, 1) == "1")
									$("#twitter").prop("checked", true);
								else
									$("#twitter").prop("checked", false);
							}
							else if ((str.length - i) == 4) {
								if (str.substr(i, 1) == "1")
									$("#google").prop("checked", true);
								else
									$("#google").prop("checked", false);
							}
							else if ((str.length - i) == 3) {
								if (str.substr(i, 1) == "1")
									$("#mixi").prop("checked", true);
								else
									$("#mixi").prop("checked", false);
							}
							else if ((str.length - i) == 2) {
								if (str.substr(i, 1) == "1")
									$("#hatena").prop("checked", true);
								else
									$("#hatena").prop("checked", false);
							}
							else if ((str.length - i) == 1) {
								if (str.substr(i, 1) == "1")
									$("#evernote").prop("checked", true);
								else
									$("#evernote").prop("checked", false);
							}
						}
						GetDataMovie(_movie_cd, $that);
						$("#show_edit_review #watch-record .member-review-string-lblcountreview").html($("#lblcountreview_member_review_hiden").val() + "回目");
						$that.find(".indicator").remove();
						//show button delete and edit
						$("#btneditreview").show();
						$("#btndeleteconfrim").show();
						$("#btnconfirmreview").hide();
						$("#btnback").hide();


						//Load label_cd
						$("#ctl00_ctl00_container_include_container_include_member_review_label_name_review_id").val(_review_id.toString());

						$('.cont_label_cd_name_list').jScrollPane({
							autoReinitialise: true,
							showArrows: true
						})
						$('.regist_review_class_block').jScrollPane({
							autoReinitialise: false,
							showArrows: true
						})
						var api = $('.cont_label_cd_name_list').data("jsp").scrollToY(0);

						$("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val(0);
					}
					addEven($that, evenclk);
					$that.find(".indicator").remove();
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					window.location.href = base_url_system_error;
				}
			});
		};

		function closebox() {
			$('.modal__close').click();
		};
		function openbox($boxname) {
			openBoxModal($boxname);
		};

		function UpdateJScrollPaneDataLable() {
			$('.cont_label_cd_name_list').jScrollPane({
				autoReinitialise: true,
				showArrows: true
			}).bind('jsp-scroll-y', function (event, scrollPositionY, isAtTop, isAtBottom) {
				$(this).trigger('scroll');
				$that = $(this);
				var api = $that.data('jsp');
				if (($that.outerHeight() + scrollPositionY) >= (api.getContentHeight() - base_height_auto_load)) {
					var this_page = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_page_count_label_name").val());
					LoadLabel_Cd_Name($(this), this_page);
				}
			});
		}
		function LoadLabel_Cd_Name($that, page_this) {
			var is_endpage = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val());
			if (is_endpage == 1) {
				return;
			}

			if ($that.data("ajax")) {
				return;
			}
			var evenclk = getloading($(".cont_label_cd_name_list"));
			var member_cd = $("#ctl00_ctl00_container_include_container_include_member_review_member_cd").val();
			//var this_page = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_page_count_label_name").val());
			var this_page = parseInt(page_this);
			var review_id = parseInt($("#ctl00_ctl00_container_include_container_include_member_review_label_name_review_id").val());
			var movie_cd_label = $("#ctl00_ctl00_container_include_container_include_member_review_movie_cd").val();
			var data = "{'member_cd': '" + member_cd + "'"
					+ ",'page': " + this_page + ""
					+ ",'review_id': " + review_id + ""
					+ ",'movie_cd': " + movie_cd_label + ""
					+ "}";
			$.ajax({
				url: newReturnLocationpath() + "public/cinema/detail.aspx/GetDataLable_Name",
				data: data,
				type: "POST",
				timeout: base_ajax_time_out,
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				beforeSend: function (xhr) {
					$that.data("ajax", true); //2重送信回避のためdisableに
				},
				success: function (data, status, xhr) {
					addEven($(".cont_label_cd_name_list"), evenclk);
					$(".cont_label_cd_name_list").find(".indicator").remove();
					if (data.d != null) {
						var valhtml = data.d.html;
						var datais_end = data.d.is_end;
						$("#checklist").append(valhtml);

						$("#ctl00_ctl00_container_include_container_include_member_review_page_count_label_name").val((this_page + 1).toString());
						if (datais_end)
							$("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val(1);
						else
							$("#ctl00_ctl00_container_include_container_include_member_review_page_isend_label_name").val(0);
						$('.cont_label_cd_name_list').jScrollPane({
							autoReinitialise: false,
							showArrows: true
						})
					}
					$that.data("ajax", false);
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					$that.data("ajax", false);
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					window.location.href = base_url_system_error;
				}
			});
		};
	</script>
</div>

