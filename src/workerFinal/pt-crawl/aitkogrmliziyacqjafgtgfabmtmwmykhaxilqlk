			//check input numberic japan in  rating
			$("#ctl00_ctl00_container_include_container_include_member_review_rating,#ctl00_ctl00_container_include_container_include_member_review_movie_cost").blur(function () {
				var varText = $(this).val();
				var idRating = $("#ctl00_ctl00_container_include_container_include_member_review_rating").attr('id');
				if (varText.length > 0) {
					if (checkInputRating(varText) == 0) {
						if (this.id == idRating) {
							if (parseInt(varText) > 100) {
								alert("０～１００点を入力してください。");
								$("#ctl00_ctl00_container_include_container_include_member_review_rating").val(100);
								return;
							}
						}
					}
					else {
						$(this).val('');
						if (this.id == idRating) {
							$("#ctl00_ctl00_container_include_container_include_member_review_rating").focus();
						}
						else {
							$("#ctl00_ctl00_container_include_container_include_member_review_movie_cost").focus();
						}
					}
				}
			});

			function testLength(ta) {
				if (ta.value.length > maxLength) {
					ta.value = ta.value.substring(0, 3000);
				}
			}
			//celendar
			$(function () {

				/* JS DatePicker
				-------------------------*/
				getDatePicker('#fromDay');
				getDatePicker('#toDay_review');
				getDatePicker('#fromDay01');
				getDatePicker('#toDay01');

				function getDatePicker(item) {
					if (item.length > 0) {
						$(item).datepicker({
							showOn: 'both',
							dayNames: ['日', '月', '火', '水', '木', '金', '土'],
							dateFormat: 'yy年mm月‎dd日 (DD)',
							buttonImage: newReturnLocationpath() + 'img/icon/calendar.png',
							buttonImageOnly: true,
							changeMonth: true,
							changeYear: true

						});
					}
				}
			});

