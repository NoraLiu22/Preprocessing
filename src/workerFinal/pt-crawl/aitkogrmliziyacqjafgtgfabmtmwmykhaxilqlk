	<script type="text/javascript">
		$(document).ready(function () {
			//レビュー文字数チェック
			$("#ctl00_ctl00_container_include_container_include_member_review_memo").keyup(function () {
				var varText = $("#ctl00_ctl00_container_include_container_include_member_review_memo").val();
				var counter = $(this).val().length;
				if (counter == 3000) {
					$("#countUp").text(counter + "文字");
					$("#countUp").css("color", "red");
				}
				if (counter < 3000) {
					$("#countUp").text(counter + "文字");
					$("#countUp").css("color", "#393939");
					return;
				};
				if (counter > 3000) {
					var temp = varText.substring(0, 3000);
					$("#ctl00_ctl00_container_include_container_include_member_review_memo").val(temp);
					$("#countUp").text(temp.length + "文字");
					$("#countUp").css("color", "red");
				}
			}).keyup();

			$("#ctl00_ctl00_container_include_container_include_member_review_new_label").keypress(function (event) {
				if (event.which == 13) {
					event.preventDefault();
					$("#member_review_btnaddnewRack").click();
					return false;
				}
			});

			UpdateJScrollPaneDataLable();


			$("#watch-record").find("input[type='text']").each(function () {
				if (this.id != "ctl00_ctl00_container_include_container_include_member_review_rating" && this.id != "ctl00_ctl00_container_include_container_include_member_review_new_label") {
					$(this).keypress(function (event) {
						if (event.which == 13) {
							event.preventDefault();
							return false;
						}
					});
				}
			});
			$("#ctl00_ctl00_container_include_container_include_member_review_rating").keypress(function (event) {
				if (event.which == 13) {
					event.preventDefault();
				}
			});
			//input not string
			//			$("#ctl00_ctl00_container_include_container_include_member_review_rating, #ctl00_ctl00_container_include_container_include_member_review_movie_cost").keydown(function (event) {
