	<input type="hidden" name="ctl00$ctl00$container_include$container_include$hidthis_page" id="ctl00_ctl00_container_include_container_include_hidthis_page" value="1" />
	<input type="hidden" id="cinema_detail_hiden_scroll_class_togger" value="50" />
	<script type="text/javascript">

		$(function () {
			var dropDown11 = new DropDown($('#dropDown11'));
		});

		$(document).ready(function () {
			$("#ctl00_ctl00_container_include_container_include_hidthis_page").val(1);
			do_load_movie_review($('#clickandsort_review_cinema'), true,false);
		});

		function do_load_movie_review($that, is_first_post,is_click_sort) {
			if ($that.data("ajax") || $that.data("isEnd")) {
				return;
			}
			var evenclk = getloading($("#clickandsort_review_cinema"));
			var member_cd = $("#ctl00_ctl00_container_include_container_include_hidmember_cd").val();
			var movie_cd = parseInt($("#ctl00_ctl00_container_include_container_include_hidmovie_cd").val());
			var this_sort = parseInt($("#ctl00_ctl00_container_include_container_include_hidthis_sort").val());
			var this_page = parseInt($("#ctl00_ctl00_container_include_container_include_hidthis_page").val());
			var data = {
				member_cd: "'" + member_cd + "'",
				movie_cd: movie_cd,
				sort: this_sort,
				page: this_page
			};
			loading_ajax_get("detail.aspx/GetSelectReview", data,
				function () { $that.data("ajax", true); },
				function (data_res) {
					addEven($("#clickandsort_review_cinema"), evenclk);
					$("#clickandsort_review_cinema").find(".indicator").remove();
					var data = jQuery.parseJSON(data_res);
					$that.data("isEnd", data.d.trim() == '');
					if (data.d != '') {
						var datatemp = data.d;
						if (is_first_post == true) {
							$("#movie-review .movie_list").html(datatemp);
						}
						else {
							$("#movie-review .movie_list").append(datatemp);
						}
						$('#movie-review .movie_list .cinema50 > .block_body').each(function (index) {
							if ($(this).data("isExpander"))
								return;
							$(this).parent().find('a.block_tg').unbind('click');
							toggleExpander($(this), { 'rows': 1.2 });
							$(this).parent().find('a.block_tg').on("click", UpdateJScrollPaneSearchReview_Cinema);
							$(this).data("isExpander", true);
						});

						$("#ctl00_ctl00_container_include_container_include_hidthis_page").val((this_page + 1).toString());
						$("#cinema_detail_hiden_value_scroll").val(1);
						if (is_click_sort == true) {

							$('#clickandsort_review_cinema').jScrollPane({
								autoReinitialise: true,
								showArrows: true
							});
							$('#clickandsort_review_cinema').data("jsp").scrollToY(0);
						}
						else {
							UpdateJScrollPaneSearchReview_Cinema();
						}
					}
					$that.find(".indicator").remove();
					$('a').find(".indicator").remove();
					$that.data("ajax", false);
				},
				function () {
					$that.data("ajax", false);
					$('a').find(".indicator").remove();
					addEven($that, evenclk);
					$that.find(".indicator").remove();
				}
			);
			}

			function UpdateJScrollPaneSearchReview_Cinema() {
				$('#clickandsort_review_cinema').jScrollPane({
					autoReinitialise: true,
					showArrows: true
				}).bind('jsp-scroll-y', function (event, scrollPositionY, isAtTop, isAtBottom) {
					$(this).trigger('scroll');
					$that = $(this);
					var api = $that.data('jsp');
					if (($that.outerHeight() + scrollPositionY) >= (api.getContentHeight() - base_height_auto_load)) {
						do_load_movie_review($that, false,false);
					}
				});
			}

			function expandblock_tg_cinema(el) {
				var parent = $(el).parent(".block_list");
				var isactive = $(el).hasClass("active");
				if (isactive) {
					parent.find(".block_body").attr("style", "padding: 0px; overflow: hidden; height: 0px; margin-left: 10px; margin-right: 10px;");
					$(el).removeClass("active");
				} else {
					parent.find(".block_body").attr("style", "padding: 10px 0px; overflow: hidden; height: auto; margin-left: 10px; margin-right: 10px;");
					$(el).removeClass("block_tg").addClass("block_tg active");
				}
				$('#clickandsort_review_cinema').jScrollPane({
					autoReinitialise: true,
					showArrows: true
				})
				return false;
			}

			//ham click sort
			function btnclicktosort($valuesort, $that) {
				if ($that.data("ajax")) {
					return;
				}
				var evenclk = getloading($that);
				$("#ctl00_ctl00_container_include_container_include_hidthis_sort").val($valuesort.toString());
				$("#ctl00_ctl00_container_include_container_include_hidthis_page").val(1);
				$('#clickandsort_review_cinema').data("isEnd", false);
				do_load_movie_review($('#clickandsort_review_cinema'), true, true);
				addEven($that, evenclk);
				var title_sort = $that.text();
				$("#cinema_detail_button_sort_review_list").text(title_sort);
		};

		function stekine_like_review($that, _review_id, _member_to_cd, _member_cd) {
			if ($.trim(_member_cd) == "" || $.trim(_member_to_cd) == "") {
				window.location.href = "../../public/entry/";
				return false;
			}
			var evenclk = getloading($that);
			var labelStekine = "review_id=";
			var data = "{'review_id': '" + labelStekine + _review_id + "'"
						+ ",'member_to_cd': '" + _member_to_cd + "'"
						+ ",'member_cd': '" + _member_cd + "'"
						+ "}";
			$.ajax({
				url: 'detail.aspx/getCountLikeReview',
				data: data,
				type: "POST",
				contentType: "application/json; charset=utf-8",
				dataType: "json",
				timeout: base_ajax_time_out,
				//				beforeSend: loadStart,
				//				complete: loadStop,
				success: function (data) {
					addEven($that, evenclk);
					$that.find(".indicator").remove();
					if (data.d != '') {
						var count = data.d.substring(1, data.d.length);
						var like = data.d.substring(0, 1);
						$that.next().text(count);
						if (like == "1") {
							$that.removeClass();
							$that.addClass("like_o demo-tip-yellowsimple");
							$that.attr("title", base_title_iine);
							$that.poshytip({
								className: 'tip-yellowsimple',
								showTimeout: 1,
								alignTo: 'target',
								alignX: 'center',
								offsetY: 5,
								allowTipHover: false
							});
						} else {
							$that.removeClass();
							$that.addClass("like");
							$that.removeAttr("title");
							$that.removeClass("demo-tip-yellowsimple");
							$that.poshytip('destroy');
							$("div.tip-yellowsimple").removeAttr("style");
						}
					}
						// Error
					else {

					}
					return false;
				},
				error: function (xhr, textStatus, errorThrown) {
					// add code here
					addEven($that, evenclk);
					$that.find(".indicator").remove();
					if (textStatus === "timeout") {
						alert(base_ajax_time_out_message);
						return;
					}
					//window.location.href = base_url_system_error;
				}
			});
		}
	</script>

		</div>
	</div>

            </div>
            

