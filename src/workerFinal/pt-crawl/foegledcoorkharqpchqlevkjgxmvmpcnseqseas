                $.ajax({
                    dataType: 'json',
                    url: apiroot+'version/.json?include=name,size&fmt=datatable&contentid='+current_selection+'&systemid=2',
                    success: function (response) {
                       versions=response.data;
                       var maxversion = versions.reduce(function(max, arr) {
