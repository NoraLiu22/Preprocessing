                                }, -Infinity);
                       if($(".lightboxadd").length){
                        lightbox();
                       }
                        download();
                    },
                });
                    function lightbox(){
                      var options="<li><a class='dropdown-item copyable'><b>Add to lightbox</b></a></li>";
                      for(var i=0;i<versions.length;i++){
                          if(versions[i][1] != ''){
                            options+=getlightboxstring(versions[i][0],versions[i][1]);
                          }
                      }
                      function getlightboxstring(versionid,versionname){
                          return "  <button class='contextlightbox  btn btn-link' data-versionid='"+versionid+"' data-contentid='"+location.href.replace(/^.*imgnum=([0-9]+).*$/i,'$1')+"' data-method='Lightbox.add' data-systemid='2' data-title='"+versionname+"'>"+versionname+" </button><br/>";
                      }
                        $('.contextdetail').append(options)
                    }
                    function download(){
                      var options="<li><a class='dropdown-item copyable'><b>Download</b></a></li>";
                      for(var i=0;i<versions.length;i++){
                            if(versions[i][0] <=3 ||$(".lightboxadd").length ){
                               options+=getdownloadstring(versions[i][0],versions[i][2]);
                             }
                      }
                      if(versions.length >2 && !$(".lightboxadd").length){
                         options +="<a href='#' class='btn btn-link disabled' title=' '>Login for additional resolutions</a><br/>";
                      }
                      function getdownloadstring(versionid,versionname){
                             return "  <li><a href='/browse/imgdownjoe.cfm?img="+current_selection+"&res="+versionid+"'  title=' Download "+versionname+"'   class='btn btn-link downloadimage' data-id='"+current_selection+"'>"+versionname+" </a></li>";
                        }
                        $('.contextdetail').append(options)
                    }

                     var top = e.clientY - 90;
                     // var left = e.clientX;
                      var left = e.screenX - e.pageX  + e.offsetX - 45;
                     // var top = e.pageX - $(this).position().left;
                      // var left = e.pageY - $(this).position().top;
                      left = left> getWidth() ? left-getWidth() : left;
                      console.log(left);
                      $("#context-menu2").css({
                        display: "block",
                        top: top,
                        left: left
                      }).addClass("show");
                    return false;
                }).on("click", function() {
              $("#context-menu2").removeClass("show").hide();
            });
