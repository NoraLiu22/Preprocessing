              <h1 id="page-title" class="title eight columns ">Wampanoag Tribe Of Gay Head</h1>
               </div><!-- /.title-search -->
  <div id="content-wrapper">
  <div class="row">
   <div id="content" class="twelve columns">
    <div class="row left-main">
     
           <div class="tabs"></div>
                          
          
          
          
     <div class="row content">
      <div class="twelve columns" id="content-main"> 
       <div class="region region-content">
    <div id="block-system-main" class="block block-system">

    
  <div class="content">
    <div class="view view-timemachine-featured-collections view-id-timemachine_featured_collections view-display-id-page_1 articles top-spotlight view-dom-id-2b96dd741d7f894024d06c9f87a564a7">
      
  
  
      <div class="view-content">
      
<div class="header-embed">
