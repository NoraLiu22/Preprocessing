		function resizeColumns() {			
			$('.col-eq').height('auto');
			if ($(window).width() > 767) {
				var maxHeight = Math.max.apply(null, $('.col-eq').map(function () {
					return $(this).outerHeight();
				}).get());
				$('.col-eq').height(maxHeight);
			}
		}
		
