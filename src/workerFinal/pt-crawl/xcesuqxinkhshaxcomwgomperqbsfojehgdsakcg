<div id="mobile-search-container"></div>

<script type="text/javascript">

$(document).ready(function () {
    
    // Add block display to focused nav links - allows keyboard users to tab through links

    $('.nav-link').on('focus', function() {
       $(this).parent().addClass('dropdown-active')
    });
    
    // Check when the last link loses focus and hide dropdown
    
    $('.nav-item li:last-child a').on('blur', function() {
        if ($('.nav-item').hasClass('dropdown-active')) {
            $('.nav-item').removeClass('dropdown-active');
        } 
    });
    
    $(document).mouseup(function(e) {
        var container = $(".nav-item");
    
        // if the target of the click isn't the container nor a descendant of the container
        if (!container.is(e.target) && container.has(e.target).length === 0) {
            $('.nav-item').removeClass('dropdown-active');
        }
    });
            
    // If the user presses escape, hide dropdown again
    $(document).keyup(function(e) {
        if (e.keyCode === 27) {
    		$(".nav-item").removeClass("dropdown-active")
        }
    })
    
    // Animate hamburger menu
    
    var forEach=function(t,o,r){if("[object Object]"===Object.prototype.toString.call(t))for(var c in t)Object.prototype.hasOwnProperty.call(t,c)&&o.call(r,t[c],c,t);else for(var e=0,l=t.length;l>e;e++)o.call(r,t[e],e,t)};

    var hamburgers = document.querySelectorAll(".hamburger");
    if (hamburgers.length > 0) {
        forEach(hamburgers, function(hamburger) {
            hamburger.addEventListener("click", function() {
                this.classList.toggle("is-active");
            }, false);
        });
    }
    
    // Add search button to MSL search widget
    
    newSearchButton = $('<button aria-label="search" id="ctl00_search_btnSubmit" name="ctl00$search$btnSubmit" type="submit" class="search-button"><span class="icon-switch"><i class="fas fa-search fa-2x fa-fw icon-default"></i><i class="fas fa-arrow-right fa-2x fa-fw icon-hover"></i></span></button>');

    $('#search-container #search > .mslwidget > div').append(newSearchButton);
    
    // If the window size is 
    
        if ($(window).width() < 992) {
            $('#search').appendTo( $('#mobile-search-container') )
        } else {
            $('#search').appendTo( $('#search-container li') )
        }
    
    // Check if window has been resized and move

    $( window ).resize(function() {
        if ($(window).width() < 992) {
            $('#search').appendTo( $('#mobile-search-container') )
        } else {
            $('#search').appendTo( $('#search-container li') )
        }
    })
    
});

</script>
<!-- end of Nav2020 snippet -->


