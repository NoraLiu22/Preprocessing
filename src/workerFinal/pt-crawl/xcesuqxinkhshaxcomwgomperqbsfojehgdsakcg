<script type="text/javascript">
$(document).ready(function() {
    
    // Check if user is logged out, hide admin and basket if they are

    if ($(".toolbar a").hasClass("msl-loggedout")) {
        console.log("hello");
        $("#ul-toolbar li").slice(0, 3).remove();
    }
    
    // Add right arrow to review basket buttons and elections buttons
    
    $(' #product .basket, #ctl00_basket_lnkReviewBasket').append(' <i class="fas fa-chevron-right"></i> ');
    
    // Append price to product name for better user experience
    
    $(' #ctl00_basket_pnlBasket .price ').each(function() {
        $(this).appendTo( $(this).prev( 'dt' ) );
    });
    
    // Toggle admin link dropdowns
    
	$('a#tb-controls').on('click keypress', function() {
		$('div#controls').slideToggle("fast");	
		$('div#admin').css('display', 'none');
		$('div#basket').css('display', 'none');
        
        $(document).keyup(function(e) {
            if (e.keyCode === 27) {
		        $('div#controls').slideUp("fast");	
            }
        });
	});
   	
	$('a#tb-admin').on('click keypress', function() {
		$('div#controls').css('display', 'none');
		$('div#admin').slideToggle("fast");				
		$('div#basket').css('display', 'none');	
        
        $(document).keyup(function(e) {
            if (e.keyCode === 27) {
		        $('div#admin').slideUp("fast");
            }
        });											 
	});
   	
	$('a#tb-basket').on('click keypress', function() {
		$('div#controls').css('display', 'none');
		$('div#admin').css('display', 'none');		
		$('div#basket').slideToggle("fast");
        
        $(document).keyup(function(e) {
            if (e.keyCode === 27) {
		        $('div#basket').slideUp("fast");
            }
        });														 
	});

});
</script>
