let footerWrapper = document.querySelector('.footer');

const visibilityToggler = (target) => {
  target.classList.toggle('open');
}

searchButton.onclick = function() {
  visibilityToggler(searchModal);
};

searchClose.onclick = function() {
  visibilityToggler(searchModal);
};
// Close searchbar by press ESC
window.onkeydown = function( event ) {
  if ( event.keyCode == 27 && searchModal.classList == 'search-modal open') {
    visibilityToggler(searchModal);
  }
};

adfooterClose.onclick = function() {
  visibilityToggler(adfooterWrapper);
  visibilityToggler(footerWrapper);
};

mobmenuButton.onclick = function() {
  visibilityToggler(mobmenuButton);
  visibilityToggler(bodyWrapper);
  visibilityToggler(mobMenu);
};
    

});

window.REBELMOUSE_LOWEST_TASKS_QUEUE.push(function(){

    
        const initiatePlayer = (paragraphCount = 2, src = 'https://www.caffeine.tv/embed.js') => {
  // CaffeineTV container
  
  const container = '<blockquote style="width:100%" data-cafurl="https://www.caffeine.tv/okayplayer/domain?sticky=true" class="caffeine-embed"></blockquote>';
  const indexForPaste = paragraphCount - 1;
  // Second
  const placeToPaste = Array.from(document.querySelectorAll('.widget .body-description p'))[indexForPaste]; 
  
  placeToPaste.insertAdjacentHTML('afterend', container);
  
  //Add script to page
  let caffeineTVScript = document.createElement('script');
  caffeineTVScript
  
  const loadScript = (src) => {
    return new Promise(function (resolve, reject) {
        let script = document.createElement('script');
        script.src = src;
        script.onload = resolve;
        script.onerror = reject;
        document.head.insertAdjacentElement('beforeend', script);
    });
  }
  
  return loadScript(src);
}

initiatePlayer();
    

});

window.REBELMOUSE_LOWEST_TASKS_QUEUE.push(function(){

    
        let adDiv = document.getElementsByClassName("rblad-okp_sidebar")[0];
let shift = document.querySelector('.sidebar .js-scroll-trigger').dataset.offset;
let touchObject = document.querySelector(".subscribe-form");
let adHeight = 650;

document.addEventListener("scroll", (e) => {
  let screenHeight = document.documentElement.clientHeight;
  let adDivBottom = screenHeight - adDiv.clientHeight - shift;
  let touchObjectTop = window.innerHeight - touchObject.getBoundingClientRect().top;

  if (touchObjectTop>=adDivBottom) {
    adDiv.style.position = "fixed";
    adDiv.style.bottom = `calc(${touchObjectTop}px - ${adHeight}px - ${adDivBottom}px)`;
    adDiv.style.top = "auto";
  } else {
    adDiv.style.bottom = "auto";
    adDiv.style.top = `${shift / 2}px`;
  }
})
    

