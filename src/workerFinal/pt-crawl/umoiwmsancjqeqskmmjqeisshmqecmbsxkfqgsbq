  if (!window.Zephr.outcomes) window.Zephr.outcomes = {};
  window.Zephr.outcomes['a-v4-datalayer'] = {
    featureLabel: 'a-v4-brand-datalayer',
    outcomeId: 'transformation/2',
    outcomeLabel: 'datalayer v2'
  };
</script>
