					name="email_address"
					id="reg_email"
					type="text"
					required
					pattern="^(([^\<\>\(\)\[\]\.,;:\s@\x22]+(\.[^\<\>\(\)\[\]\.,;:\s@\x22]+)*)|(\x22.+\x22))@(([^\<\>\(\)\[\]\.,;:\s@\x22]+\.)+[^\<\>\(\)\[\]\.,;:\s@\x22]{2,50})$"
					title="Work e-mail"
					placeholder="Work e-mail"
				/>
			</label>
		</fieldset>

		<fieldset class="blaize-user-attributes">
			<label for="first_name" class="field-container">
				<input
					name="first_name"
					id="first_name"
					type="text"
					placeholder="First name"
					required
