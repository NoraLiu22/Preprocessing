		<script>
			$(document).ready(function() {
			// _spBodyOnLoadFunctionNames.push("targetZWS");
			  $(".collapsible-body").hide();
				$(".collapsible-header").click(function(){
					$(this).next(".collapsible-body").slideToggle("normal");
					$(this).toggleClass("open");
				});
