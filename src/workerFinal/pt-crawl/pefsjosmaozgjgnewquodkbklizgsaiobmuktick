<script>

    $("#accordion").on("hide.bs.collapse show.bs.collapse", e => {
        $(e.target)
            .prev()
            .find("i:last-child")
            .toggleClass("fa-minus fa-plus");
    });

    $("#accordion").on("shown.bs.collapse", e => {
        $("html, body").animate(
            {
                scrollTop: $(e.target)
                    .prev()
                    .offset().top
            },
            400
        );
    });

    $("#accordion2").on("hide.bs.collapse show.bs.collapse", e => {
        $(e.target)
            .prev()
            .find("i:last-child")
            .toggleClass("fa-minus fa-plus");
    });

    $("#accordion2").on("shown.bs.collapse", e => {
        $("html, body").animate(
            {
                scrollTop: $(e.target)
                    .prev()
                    .offset().top
            },
            400
        );
    });

    $("#accordion3").on("hide.bs.collapse show.bs.collapse", e => {
        $(e.target)
            .prev()
            .find("i:last-child")
            .toggleClass("fa-minus fa-plus");
    });

