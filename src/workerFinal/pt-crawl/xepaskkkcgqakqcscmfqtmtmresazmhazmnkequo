            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1977, frequency:7 },{ year:1978, frequency:11 },{ year:1979, frequency:11 },{ year:1980, frequency:13 },{ year:1981, frequency:23 },{ year:1982, frequency:27 },{ year:1983, frequency:30 },{ year:1984, frequency:36 },{ year:1985, frequency:67 },{ year:1986, frequency:73 },{ year:1987, frequency:92 },{ year:1988, frequency:113 },{ year:1989, frequency:111 },{ year:1990, frequency:131 },{ year:1991, frequency:140 },{ year:1992, frequency:152 },{ year:1993, frequency:173 },{ year:1994, frequency:183 },{ year:1995, frequency:195 },{ year:1996, frequency:258 },{ year:1997, frequency:269 },{ year:1998, frequency:249 },{ year:1999, frequency:268 },{ year:2000, frequency:311 },{ year:2001, frequency:334 },{ year:2002, frequency:381 },{ year:2003, frequency:392 },{ year:2004, frequency:515 },{ year:2005, frequency:594 },{ year:2006, frequency:736 },{ year:2007, frequency:783 },{ year:2008, frequency:1076 },{ year:2009, frequency:1140 },{ year:2010, frequency:1195 },{ year:2011, frequency:1269 },{ year:2012, frequency:1412 },{ year:2013, frequency:1306 },{ year:2014, frequency:1522 },{ year:2015, frequency:1467 },{ year:2016, frequency:1605 },{ year:2017, frequency:1317 },{ year:2018, frequency:1437 },{ year:2019, frequency:1510 },{ year:2020, frequency:1523 },{ year:2021, frequency:1660 },{ year:2022, frequency:1674 },{ year:2023, frequency:1483 },{ year:2024, frequency:313 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=rj%3A523+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                        </div>
                    </div>
                
            
        </article>
    </div>
        </div>
    

                    <div class="clearfix"></div>
                </div>
            </div>

