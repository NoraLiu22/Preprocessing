<main class="" data-img-base="https://s4.bcbits.com/bundle/bundle/1/static-1c571081b31bcd3d73ea36a0d0be9ee2.static">

<section id="intro">
    <div class="mark-cluster">
        <svg viewBox="0 0 100 100"><use href="#chladni-mark-01"></use></svg>
        <svg viewBox="0 0 100 100"><use href="#chladni-mark-02"></use></svg>
        <div></div>
        <svg viewBox="0 0 100 100"><use href="#chladni-mark-03"></use></svg>
