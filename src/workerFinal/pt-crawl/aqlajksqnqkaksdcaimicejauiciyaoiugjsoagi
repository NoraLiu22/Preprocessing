  <h5 _ngcontent-sc229="" class="simple-view-element-header ng-star-inserted">Description</h5><!---->
  <div _ngcontent-sc229="" class="simple-view-element-body">
    
    
    <span _ngcontent-sc231="" class="dont-break-out ng-star-inserted">
      <span _ngcontent-sc231="">research</span><!---->
    </span><!---->

  </div>
</div>
</ds-metadata-field-wrapper>
</ds-metadata-values>
