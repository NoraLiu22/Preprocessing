<div id="outbrain-holder">
    <div id="DFP_standard_mpu_3"></div>
    <div id="DFP_standard_mpu_4"></div>
    <div id="DFP_standard_mpu_5"></div>
    <div id="DFP_standard_mpu_6"></div>
    <div id="DFP_standard_mpu_7"></div>
