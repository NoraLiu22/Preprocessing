<script type="text/javascript">
function stoggle(toggleId)
{
    
    if (!document.getElementById) {
        return false;
    }
    var body = document.getElementById(toggleId);
    if (!body) {
        return false;
    } 
    if (body.className == "detailOff") {
        body.className = "detailOn";
    } 
    else {
        body.className = "detailOff";
    }
}
</script>
