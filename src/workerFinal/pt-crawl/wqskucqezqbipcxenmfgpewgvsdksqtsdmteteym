      //<![CDATA[
      $(function(){
        $('.list-group-item', '.vdsPlayList').click(function(){
          var $self = $(this),$parent = $self.parents('.vdsPlayList'),arr=$('#vds1 li'),
              vdHtm='<iframe src="'+ $self.data('vdpsrc') + '&i=' + 'vds1 li:eq(' +$.inArray(this,arr)+ ')&a=1&p='+ $self.data('vdpimg') +'" width="100%" height="100%" scrolling = "no" frameborder="0"></iframe>';
          $('.embed-responsive', $parent).html(vdHtm);

          $('.selected', $parent).removeClass('selected');
          $self.addClass('selected');
        });


        function is_weixin(){
          var ua = navigator.userAgent.toLowerCase();
          if(ua.match(/MicroMessenger/i)=="micromessenger") {
            return true;
          } else {
            return false;
          }
        }
        var url =location.href;
        if(is_weixin()){
          $.post('http://watchthis.chinadaily.com.cn/pub/wx/api/wxinit', {url:url}, function(data){
            wx.config({
              debug: false,
              appId: data.appid,
              timestamp: data.timestamp,
              nonceStr: data.noncestr,
              signature: data.sign,
              jsApiList: ['onMenuShareTimeline','onMenuShareAppMessage']
            }),
              wx.ready(function(){
              wx.onMenuShareTimeline({
                title: 'WATCH THIS! - China Daily Video',
                link: url,
                imgUrl: '/2024watchthis/img/share.jpg'
              });
                var data = {
                title: 'WATCH THIS! - China Daily Video',
                desc: "",
                link: url,
                imgUrl: 'http://'  + window.location.host + '/2024watchthis/img/share.jpg'
              };

                                     wx.onMenuShareAppMessage(data);
            }),
              wx.error(function(res){
              console.log("微信验证失败!");
            })
          });
        }
      });
      //]]>
