<script type="text/javascript">
	// <![CDATA[

		

		Liferay.currentURL = '\x2fen\x2fweb\x2fabc\x2ftools\x2fdisclaim';
		Liferay.currentURLEncoded = '\x252Fen\x252Fweb\x252Fabc\x252Ftools\x252Fdisclaim';

	// ]]>
