        display();

        $('.bookListHeader').on("click", function()
        {
            $(this).next().toggle();
            $(this).attr('aria-expanded', function (i, attr) {return attr == 'true' ? 'false' : 'true'});
            return false;
        }).next().show();

        $('.bookListHeader a').on("keydown", function(e) {
            return e.keyCode != 13 || $(this).click();
        });

        $('#list_control').on('keydown', function(e) {
            return e.keyCode != 13 || $(this).click();
        });

        $('#list_control').on("click", function()
        {
            document.getElementById('list_control').innerHTML == "Collapse All" ? collapseAll() : expandAll();
        });

        collapseAll(); // Moved from inside if-statement below to default to all collapsed.
        var pathName = document.location.toString();
        if (pathName.match('#')) { // the URL contains an anchor
            var anchorName = pathName.split('#')[1];
            $( jq(anchorName) ).next().show();
            $( jq(anchorName) ).attr('aria-expanded', "true");
            window.location.hash = anchorName;  // scroll to the anchor name
        }

        // Cache selectors outside callback for performance.
        /*var $window = $(window),
                $stickyEl = $('#book-list-header-sticky'),
                elTop = $stickyEl.offset().top;

        $window.scroll(function() {
            $stickyEl.toggleClass('sticky', $window.scrollTop() > elTop);
        });*/
    });

    function jq(id) {
        id = id.replace(/,/g,'');
        id = id.replace(/( |%20)/g,'_');
        return "#" + id;
    }

    function expandAll()
    {
        document.getElementById('list_control').innerHTML = "Collapse All";
        $('.bookListHeader').next().show();
        $('[aria-expanded="false"]').attr('aria-expanded', "true");
    }

    function collapseAll()
    {
        document.getElementById('list_control').innerHTML = "Expand All";
        $('.bookListHeader').next().hide();
        $('[aria-expanded="true"]').attr('aria-expanded', "false");
    }
</script><noindex>
