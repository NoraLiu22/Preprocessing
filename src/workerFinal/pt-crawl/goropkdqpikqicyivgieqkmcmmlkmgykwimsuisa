  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-7684421-2']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

  /** 
   * Recommended by Google to delay loading outbound link to allow tracking code more time to load completely
   * Source: http://support.google.com/analytics/bin/answer.py?hl=en&answer=1136920
   */
  function recordOutboundLink(link, category, action) {
    try {
      /* var myTracker=_gat._getTrackerByName(); */
      /* _gaq.push(['myTracker._trackEvent', category ,  action ]); */
      _gaq.push(['_trackEvent', category ,  action ]);
      setTimeout('document.location = "' + link.href + '"', 100)
    }catch(err){}
  }
  
</script>


