    jQuery('.menu-close').click(function(){
      jQuery(this).parents().closest('.sub-menu').addClass("closed");
      
      setTimeout(function(){
        jQuery('.sub-menu').removeClass("closed");
      },500);
      
      jQuery('.sub-menu').removeClass("visible");
    });
    
