                srcset="https://assets.simpleviewinc.com/simpleview/image/upload/c_fill,f_webp,h_427,q_65,w_640/v1/clients/newmexico-redesign/default_img_98a68045-676b-466b-9390-d2d4a26daa6c.jpg"
                
                type="image/webp"
            />
        
    
        
            <img
                class="slide-img"
                src="https://assets.simpleviewinc.com/simpleview/image/upload/c_fill,f_jpg,h_427,q_65,w_640/v1/clients/newmexico-redesign/default_img_98a68045-676b-466b-9390-d2d4a26daa6c.jpg"
                alt=""
                loading="lazy"
                fetchpriority="low"
                width="640"
                height="427"
                onload="this.classList.add('loaded')" 
            />
        
    
