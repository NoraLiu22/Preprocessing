        googletag.cmd.push(function() {
        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
		if (event.slot.getSlotElementId() == 'div-gpt-ad-8773544-1') {
 //       console.log("Rendered Ad Size..: " + event.size);

          if(event.size!=null){
  //      	console.log("HR Ad ON");
        	window.scrollTo(0,0); 
            $('.leaderboard-top').remove();    						// Hiding Header Leader Ad while Header Reveal Ad displaying
			var cssPaddingVal = $('body').css("padding-top");   // fix for removing space above Header Reveal Ad - Start
            if(cssPaddingVal == "55px")
            {
				$('body').css("padding-top","0px");
				$('.mobile-nav').css("margin-top", "0px");  	// fix for remove space which is displaying between navigation and Header reveal Ad 
				$(window).scroll(function()
                {
					$('.mobile-nav').css("margin-top", "0px");
                });
			}
																// fix for removing space above Header Reveal Ad - End
        }
        else if(event.size===null){
    //    	 console.log("HR Ad OFF");  
        	 $('#div-gpt-ad-8773544-1').remove();  // Removing Header Reveal Ad DIV tag if Ad respose null (if enable 24hr cap functionality)
    	      }
			}
		});
       });
</script>
