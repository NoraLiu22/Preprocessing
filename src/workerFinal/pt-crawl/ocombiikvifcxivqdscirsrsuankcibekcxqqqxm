                            <script>
                            jQuery('input.quiz__question-radio').change(function() {
                                var button_id = $(this).attr('id');
                                if ($(this).is(':checked')) {
                                    $("label.quiz__question-label.button[for='" + button_id + "']").addClass(
                                        "selected_answer");
                                } else {
                                    $("label.quiz__question-label.button[for='" + button_id + "']").removeClass(
                                        "selected_answer");
                                }
                            });
                            </script>

