                        <script>
                        //open social sharing links in new popup windows
                        jQuery('div.quiz__social-icon.quiz__popup>a').click(function() {
                            event.preventDefault();
                            var url = jQuery(this).attr('href');
                            var window_height = jQuery(this).attr('window_height');
                            if (window_height == undefined) {
                                window_height = "500";
                            }

                            window.open(url, 'Share_Results', 'width=500,height=' + window_height +
                                ',resizable=1;scrollbars=0,status=0,menubar=0');
                        });

                        jQuery('form#need_more_info>button').on('click', function() {
                            event.preventDefault();
                            var first_name = jQuery('input#first_name').val();
                            var last_name = jQuery('input#last_name').val();
                            var email = jQuery('input#email').val();

                            if (first_name == "" || last_name == "" || email == "") {
                                jQuery('form#need_more_info div#result').text('Please complete all fields.');
                            } else if (email.indexOf('@') == -1) {
                                jQuery('form#need_more_info div#result').text(
                                    'Please enter a valid email address.');
                            } else {
                                //proceed with submitting
                                var to = jQuery('input#recipient').val();
                                var quiz_url = jQuery('a#quiz__share-results').attr('href');
                                var url = 'information_request.php';
                                posting = jQuery.post(url, {
                                    first_name: first_name,
                                    last_name: last_name,
                                    email: email,
                                    quiz_url: quiz_url,
                                    recipient: to
                                });
                                posting.success(jQuery('form#need_more_info div#result').text(
                                    'Your Request has Been Submitted.'));
                                console.log(posting);
                            }
                        });
                        </script>

                    </div>
