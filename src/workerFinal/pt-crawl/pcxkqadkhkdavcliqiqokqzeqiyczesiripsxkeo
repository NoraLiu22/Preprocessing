    document.oncontextmenu=new Function("return false")
    function SetPopup()
    {  
        var w = screen.width;
        var h = screen.height;
        var popW = 200, popH = 100;
        var leftPos = (w-popW)/2;
        var topPos = (h-popH)/2;                    
        document.getElementById('MessageDiv').style.top = topPos + 'px'; 
        document.getElementById('MessageDiv').style.left = leftPos + 'px'; 
    }     
    
        function KeyCheck(btn,e)
        {         
              var code = (window.event) ? event.keyCode : e.keyCode;
              if(code == '13')
              {                    
                    document.getElementById(btn).click(); 
                     return false;  
              }
        }
    
    function ResetGlobalSearchText()
    {
        var txtSearch = document.getElementById('Master1_txtSearch');
        if (txtSearch.value == '')
        {
            txtSearch.value = 'بحث في كل الأقسام';
        }
    }
    
    function ClearGlobalSearchText()
    {
        var txtSearch = document.getElementById('Master1_txtSearch');
        if (txtSearch.value == 'بحث في كل الأقسام')
        {
            txtSearch.value = '';
        }
    }
     function Hide(obj)
        {        
            if( obj.className == "dimmed") return;
            if(obj.disabled!=true ) 
                document.getElementById('hidepage').style.display = 'block';
        }
        
        
        function opacity(id, opacStart, opacEnd, millisec) {
	        //speed for each frame
	        var speed = Math.round(millisec / 100);
	        var timer = 0;

	        //determine the direction for the blending, if start and end are the same nothing happens
	        if(opacStart > opacEnd) {
		        for(i = opacStart; i >= opacEnd; i--) {
			        setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
			        timer++;
		        }
		        setTimeout("document.getElementById('" + id + "').style.display = 'none';", (timer * speed));
	        } else if(opacStart < opacEnd) {
		        for(i = opacStart; i <= opacEnd; i++)
			        {
			        setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
			        timer++;
		        }
		        document.getElementById(id).style.display = 'block';
	        }
        }

        //change the opacity for different browsers
        function changeOpac(opacity, id) {
	        var object = document.getElementById(id).style; 
	        object.opacity = (opacity / 100);
	        object.MozOpacity = (opacity / 100);
	        object.KhtmlOpacity = (opacity / 100);
	        object.filter = "alpha(opacity=" + opacity + ")";
        }
        
        function showTip(abbr)
        {
            var countElement;
            var title;
            var content;
            var width;
            
            switch (abbr)
            {
                case 'IM':
                    countElement = document.getElementById('Master1_lblCntIM');
                    title = 'صور';
                    content = 'العدد: ' + countElement.innerHTML + ' صورة';
                    break;
                case 'DC':
                    countElement = document.getElementById('Master1_lblCntDC');
                    title = 'وثائق';
                    content = 'العدد: ' + countElement.innerHTML + ' وثيقة';
                    break;
                case 'VI':
                    countElement = document.getElementById('Master1_lblCntVI');
                    title = 'أفلام';
                    content = 'العدد: ' + countElement.innerHTML + ' فيلم';
                    break;
                case 'AU':
                    countElement = document.getElementById('Master1_lblCntAU');
                    title = 'صوتيات';
                    content = 'العدد: ' + countElement.innerHTML + ' صوتية';
                    break;
                case 'SP':
                    countElement = document.getElementById('Master1_lblCntSP');
                    title = 'خطب';
                    content = 'العدد: ' + countElement.innerHTML + ' خطبة';
                    break;
                case 'PS':
                    countElement = document.getElementById('Master1_lblCntPS');
                    title = 'صحافة';
                    content = 'العدد: ' + countElement.innerHTML + ' صحيفة';
                    break;
                case 'AR':
                    countElement = document.getElementById('Master1_lblCntAR');
                    title = 'مقدمات و أبحاث';
                    content = 'العدد: ' + countElement.innerHTML + ' مقدمة';
                    break;
                case 'MP':
                    countElement = document.getElementById('Master1_lblCntMP');
                    title = 'خرائط';
                    content = 'العدد: ' + countElement.innerHTML + ' خريطة';
                    break;
                case 'MN':
                    countElement = document.getElementById('Master1_lblCntMN');
                    title = 'نقود';
                    content = 'العدد: ' + countElement.innerHTML + ' عملة';
                    break;
                case 'ST':
                    countElement = document.getElementById('Master1_lblCntST');
                    title = 'طوابع';
                    content = 'العدد: ' + countElement.innerHTML + ' طابع';
                    break;
                case 'MD':
                    countElement = document.getElementById('Master1_lblCntMD');
                    title = 'أوسمة';
                    content = 'العدد: ' + countElement.innerHTML + ' وسام';
                    break;
                case 'BK':
                    countElement = document.getElementById('Master1_lblCntBK');
                    title = 'كتب';
                    content = 'العدد: ' + countElement.innerHTML + ' كتاب';
                    break;
                case 'CV':
                    countElement = document.getElementById('Master1_lblCntCV');
                    title = 'أغلفة';
                    content = 'العدد: ' + countElement.innerHTML + ' غلاف';
                    break;
                case 'AD':
                    countElement = document.getElementById('Master1_lblCntAD');
                    title = 'إعلانات';
                    content = 'العدد: ' + countElement.innerHTML + ' إعلان';
                    break;
            }
            
            if (countElement.innerHTML == '')
            {
                content = '';
                width = 60;
             }
             else
                 width = 110;
           
            return tooltip(content, title, 'width:'+width+',backcolor:#aa9669,xoffset:-30,yoffset:20');
        }
        
        window.onresize = function()
        {
            var w = document.body.offsetWidth;
            var h = document.body.offsetHeight;
            document.getElementById('hidepage').style.width = w + 'px';
            document.getElementById('hidepage').style.height = h + 'px';
            document.getElementById('tdAlignment').style.height = parseInt(h / 2 - 25) + 'px';

            document.getElementById('tdMessageBoxAlignment').style.width = w + 'px';
            document.getElementById('tdMessageBoxAlignment').style.height = h + 'px';
            SetPopup();
        }
    </script>
    
