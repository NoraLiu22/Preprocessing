 <script type="text/javascript">
 
  function RemoveRuler(iRulerID , strRulerName)
    {
   
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedID').value = iRulerID;
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedName').value = strRulerName;
        document.getElementById('Master1_ContentPlaceHolder1_Button5').click();
        
    }
        
    function RemovePrimeMinister(iPrimeMinisterID,strPrimeMinisterName)
    {
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedID').value = iPrimeMinisterID;
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedName').value = strPrimeMinisterName;
        document.getElementById('Master1_ContentPlaceHolder1_Button6').click();
    }
    
    function RemoveEvent(iEventID , strEventName)
    {
         document.getElementById('Master1_ContentPlaceHolder1_txtRemovedID').value = iEventID;
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedName').value = strEventName;
        document.getElementById('Master1_ContentPlaceHolder1_Button2').click();
    }
     function RemoveSubject(iSubjectID , strSubjectName)
    {
         document.getElementById('Master1_ContentPlaceHolder1_txtRemovedID').value = iSubjectID;
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedName').value = strSubjectName;
        document.getElementById('Master1_ContentPlaceHolder1_Button3').click();
    }    
    
    function RemoveGeneralPerson(iGeneralPersonID,strGeneralPersonName)
    {
         document.getElementById('Master1_ContentPlaceHolder1_txtRemovedID').value = iGeneralPersonID;
        document.getElementById('Master1_ContentPlaceHolder1_txtRemovedName').value = strGeneralPersonName;
        document.getElementById('Master1_ContentPlaceHolder1_Button1').click();
    }   
