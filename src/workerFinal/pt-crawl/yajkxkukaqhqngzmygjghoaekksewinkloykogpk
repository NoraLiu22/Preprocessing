<script> 
	var $buoop = {
		required:{e:0,f:0,o:0,s:-2,c:-2},
		insecure:true,
		style:"corner",
		api:2021.01,
		text: {
			'msg': 'Your browser {brow_name} is outdated!',
			'msgmore': 'Update your browser for security purposes and to get the most enhanced user experience.',
			'bupdate': 'Update Browser',
			'remind': 'You will be reminded again in {days} days.',
			'bnever': 'Ignore'
		}
	}; 
	function $buo_f(){ 
		var e = document.createElement("script"); 
		e.src = "//browser-update.org/update.min.js"; 
		document.body.appendChild(e);
	};
	try {document.addEventListener("DOMContentLoaded", $buo_f,false)}
	catch(e){window.attachEvent("onload", $buo_f)}
</script>	

