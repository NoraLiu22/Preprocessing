function Go_Step_Two()
{
	Step_One_Check();
	
	if(Step_One_Complete)
	{
		var Country = document.TopUp_Form_Widget.Mobile_Country.value;
		var CountryCode = Country_Codes[Indz];
		var AreaCode = Area_Codes[Indz];
		var MobileNumber = document.TopUp_Form_Widget.MobileNumber.value;
		var Provider = document.TopUp_Form_Widget.Operator.value;
		var Currency = document.TopUp_Form_Widget.Currency.value;
		var Amount = document.TopUp_Form_Widget.amount.value;
		
		
		
		var urlz = "/topup/topup_widget_validate.cfm?Country=" + Country + "&CountryCode=" + CountryCode + "&AreaCode=" + AreaCode + "&MobileNumber=" + MobileNumber + "&Provider=" + Provider + "&Currency=" + Currency + "&Amount=" + Amount;
		var Divz = "topup_widgz";
		
		Set_Navz(urlz,Divz);
		
	}
}


function ValidEmailZx(FormField)
{
    //url = prompt("Enter your email address ONLY.","");
	url = FormField.value;
    if (url != null && url != "")
	{
        a = url.lastIndexOf("@");
        b = url.lastIndexOf(".");
        c = url.indexOf(":");
        d = url.indexOf("/");
        e = url.substring(0,a);
        f = e.indexOf("@");
        g = url.substring(a+1,url.length);
        h = g.indexOf("[");
        i = g.indexOf("]");
        j = g.indexOf("<");
        k = g.indexOf(">");
        l = url.substring(a+1,b);
        m = url.substring(b+1,url.length);
        n = url.substring(0,a);
        o = 0;
		p = url.indexOf(" ");
		q = url.indexOf("`");
		r = url.indexOf("~");
		s = url.indexOf("!");
		t = url.indexOf("#");
		u = url.indexOf("$");
		v = url.indexOf("%");
		w = url.indexOf("^");
		x = url.indexOf("&");
		y = url.indexOf("*");
		z = url.indexOf("(");
		ab = url.indexOf(")");
		ac = url.indexOf("+");
		ad = url.indexOf("=");
		//ae = url.indexOf("-");
		af = url.indexOf("|");
		ag = url.indexOf("\\");
		ah = url.indexOf("}");
		ai = url.indexOf("{");
		aj = url.indexOf("?");
		ak = url.indexOf(",");
		al = url.indexOf(";");
		am = url.indexOf("\"");
		an = url.indexOf("'");
		
		
        if (a > b) {o++};
        if (c != -1) {o++};
        if (d != -1) {o++};
        if (f != -1) {o++};
        if (h != -1) {o++};
        if (i != -1) {o++};
        if (j != -1) {o++};
        if (k != -1) {o++};
        if (l.length < 2) {o++};
        if (m.length < 2) {o++};
        if (n.length < 1) {o++};
		if (p != -1) {o++};
		if (q != -1) {o++};
		if (r != -1) {o++};
		if (s != -1) {o++};
		if (t != -1) {o++};
		if (u != -1) {o++};
		if (v != -1) {o++};
		if (w != -1) {o++};
		if (x != -1) {o++};
		if (y != -1) {o++};
		if (z != -1) {o++};
		if (ab != -1) {o++};
		if (ac != -1) {o++};
		if (ad != -1) {o++};
		//if (ae != -1) {o++};
		if (af != -1) {o++};
		if (ag != -1) {o++};
		if (ah != -1) {o++};
		if (ai != -1) {o++};
		if (aj != -1) {o++};
		if (ak != -1) {o++};
		if (al != -1) {o++};
		if (am != -1) {o++};
		if (an != -1) {o++};
		
        if (o == 0)
		{
			if(IsEmpty(document.TopUp_Form_Widget.address1,6) || IsEmpty(document.TopUp_Form_Widget.city,4))
			{
				if(IsEmpty(document.TopUp_Form_Widget.address1,6))
				{
					alert("Please Enter A Valid Billing Address Line 1!\n\nEnter Billing Address Information AS REGISTERED with your bank!");
					document.TopUp_Form_Widget.address1.focus();
					document.TopUp_Form_Widget.address1.select();
				}
				else if(IsEmpty(document.TopUp_Form_Widget.city,4))
				{
					alert("Please Enter A Valid City / Town!\n\nEnter Billing Address Information AS REGISTERED with your bank!");
					document.TopUp_Form_Widget.city.focus();
					document.TopUp_Form_Widget.city.select();
				}
				return false;
			}
			else
			{
				if(confirm("You Are About To Add " + document.TopUp_Form_Widget.Currency.value + " $" + document.TopUp_Form_Widget.amount.value + " Credit To: " + document.TopUp_Form_Widget.Area.value + " " + document.TopUp_Form_Widget.MobileNumber.value + ". Do You Wish To Proceed?"))
				{
					document.all.Subz.style.visibility = 'hidden';
					document.all.Subzz.style.visibility = 'visible';
					return true;
				}
				else return false;
			}
        }
        else
		{
            alert("Please Enter A Valid Email Address In Order To Proceed!");
			return false;
        }
    }
    else
	{
        alert("Please Enter A Valid Email Address In Order To Proceed!");
		return false;
    }
}

function formatCurrency(num) {
num = num.toString().replace(/\$|\,/g,'');
if(isNaN(num))
num = "0";
sign = (num == (num = Math.abs(num)));
num = Math.floor(num*100+0.50000000001);
cents = num%100;
num = Math.floor(num/100).toString();
if(cents<10)
cents = "0" + cents;
for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++)
num = num.substring(0,num.length-(4*i+3))+','+
num.substring(num.length-(4*i+3));
return (((sign)?'':'-') + num + '.' + cents);
}

