function Setup_Step_One()
{ 
	Indz = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	
	Provider = Mobile_Provider[Indz];
	
	var Special_Selectz = 0;
	var lastvalz = 0;
	
	if (Provider != null)
	{
		Providerz = document.TopUp_Form_Widget.Operator;
		Providerz.disabled = false;
		
		for (j = 0; j < Provider.length; j++)
		{
			Providerz.options[j] = new Option(Provider[j], Provider[j]);
		}
		
		if(j < 1)
		{
			Providerz.length = 0;
		}
		else
		{
			Providerz.length = j;
		}
	}
	
	Providerz.selectedIndex = Special_Selectz;
	
	Change_Logo();
	Disable_Enable_One();
	
	document.TopUp_Form_Widget.Area.value = Area_Codes_Masked[Indz];
	document.TopUp_Form_Widget.AreaCode.value = Area_Codes[Indz];
	document.TopUp_Form_Widget.CountryCode.value = Country_Codes[Indz];
	
	jQuery(function($){ $("#MobileNumber").unmask(); });
	jQuery(function($){ $("#MobileNumber").mask(Number_Mask[Indz],{placeholder:"X"}); });
	
	Change_Currency();
	
}

function parseDouble(value){
  if(typeof value == "string") {
    value = value.match(/^-?\d*/)[0];
  }

  return !isNaN(parseInt(value)) ? value * 1 : NaN;
}

function Validate_Amount()
{
	var Indz = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	var Oidz = document.TopUp_Form_Widget.Operator.selectedIndex;
	var Cidz = document.TopUp_Form_Widget.Currency.selectedIndex;
	var Total = document.TopUp_Form_Widget.amount.value;
	
	var Lowest_US = parseInt(Low_Valz[Indz][Oidz]);
	var Highest_US = parseInt(High_Valz[Indz][Oidz]);
	//alert(Lowest_US);alert(Highest_US);
	var Lowest_EC = parseDouble(formatCurrency((Lowest_US * 2.6882) + 1));
	var Highest_EC = parseDouble(formatCurrency(Highest_US * 2.6882));
	//alert(Lowest_EC);alert(Highest_EC);
	
	if(Cidz == 1)
	{
		//alert(Lowest_US);alert(Highest_US);alert(Total);;
		if(Total >= Lowest_US && Total <= Highest_US)
		{
			Step_One_Complete = true;
			InvalidAmountForRange = false;
			Disable_Enable_Two();
		}
		else
		{
			//alert("DIGICEL TOPUP: Enter amounts ranging from: USD $5.00 - $55.00!");
			alert("IMPORTANT: Enter amounts ranging from: USD $" + Lowest_US + " - $" + Highest_US + "!");
			Step_One_Complete = false;
			InvalidAmountForRange = true;
		}
	}
	else if(Cidz == 2)
	{
		if(Total >= Lowest_EC && Total <= Highest_EC)
		{
			Step_One_Complete = true;
			InvalidAmountForRange = false;
			Disable_Enable_Two();
		}
		else
		{
			//alert("DIGICEL TOPUP: Enter amounts ranging from: EC $13.00 - $150.00!");
			alert("IMPORTANT: Enter amounts ranging from: EC $" + Lowest_EC + " - $" + Highest_EC + "!");
			Step_One_Complete = false;
			InvalidAmountForRange = true;
		}
	}
	else if(Total > 0)
	{
		//if(DirectTopUpPage == false){alert("Please Select A Currency For The Amount Requested!");}
		document.TopUp_Form_Widget.Currency.focus();
		Step_One_Complete = false;
	}
	
	if(document.TopUp_Form_Widget.MobileNumber.value.length < 5)
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		//alert("Please Enter The Mobile Number You Wish To Top Up!");
	}
}

function Change_Currency()
{
	var Oidz = document.TopUp_Form_Widget.Operator.selectedIndex;
	var Cidz = document.TopUp_Form_Widget.Currency.selectedIndex;
	var Indz = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	
	Lowest_US = formatCurrency(Low_Valz[Indz][Oidz]);
	Highest_US = formatCurrency(High_Valz[Indz][Oidz]);
	
	Lowest_EC = parseDouble(formatCurrency((Lowest_US * 2.6882) + 1));
	Highest_EC = parseDouble(formatCurrency(Highest_US * 2.6882));
	//alert(Lowest_EC);alert(Highest_EC);
	if(Cidz == 1)
	{
		document.all.gtotal.innerHTML = "Range: USD $" + Lowest_US + " - $" + Highest_US;
	}
	else if(Cidz == 2)
	{
		document.all.gtotal.innerHTML = "Range: EC $" + Lowest_EC + " - $" + Highest_EC;
	}
	else
	{
		document.all.gtotal.innerHTML = ""
	}
}

function DisplayTaxez()
{
	var Indz = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	var Oidz = document.TopUp_Form_Widget.Operator.selectedIndex;
	var CurIdz = document.TopUp_Form_Widget.Currency.selectedIndex;
	var Currency = document.TopUp_Form_Widget.Currency.value;
	var Amount = document.TopUp_Form_Widget.amount.value;
	var ShowTaxDiv = true;
	
	CountryCode = document.TopUp_Form_Widget.Mobile_Country.value;
	OperatorCode = Mobile_Provider_Codez[Indz][Oidz];
	
	Lowest_US = parseInt(Low_Valz[Indz][Oidz]);
	Highest_US = parseInt(High_Valz[Indz][Oidz]);
	Lowest_EC = parseDouble(formatCurrency((Lowest_US * 2.6882) + 1));
	Highest_EC = parseDouble(formatCurrency(Highest_US * 2.6882));
	
	/* GENERAL CHECK: HIDES TAXES DIV IF AMOUNT IS NOTHING AND NO COUNTRY OR CURRENCY SELECTED */
	if(Amount == 0 || Amount == "" || CurIdz == 0 || Indz == 0){document.all.TaxAmountz.style.display = "none";var ShowTaxDiv = false;}
	
	/* USD CURRENCY CHECK: HIDES TAXES DIV IF US AMOUNT OUT OF US RANGE */
	if(CurIdz == 1)
	{
		//Amount = formatCurrency(Amount / 1);
		if(Amount < Lowest_US || Amount > Highest_US){document.all.TaxAmountz.style.display = "none";var ShowTaxDiv = false;}
	}
	/* ECD CURRENCY CHECK: HIDES TAXES DIV IF EC AMOUNT OUT OF EC RANGE */
	else if(CurIdz == 2)
	{
		//Amount = formatCurrency(Amount / 2.6882);
		if(Amount < Lowest_EC || Amount > Highest_EC){document.all.TaxAmountz.style.display = "none";var ShowTaxDiv = false;}
	}
	
	if(ShowTaxDiv)
	{
		/* Calculate Taxes And Display To Customer */
		var urlz = "/topup/online_target_amount.cfm?Cdz=" + CountryCode + "&Optz=" + OperatorCode + "&amount=" + Amount + "&currency=" + Currency;
		var Divz = "TaxAmountz";
		document.all.TaxAmountz.style.display = "block";
		Set_Navz(urlz,Divz);
	}
}

function Change_Logo()
{
	Indz = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	Provider = Mobile_Provider[Indz];
	
	var Idz = document.TopUp_Form_Widget.Operator.selectedIndex;
	
	//alert(Provider[Idz]);
	if(Provider[Idz] == "LIME" || Provider[Idz] == "Lime" || Provider[Idz] == "Digicel")
	{
		var Logoz = new Array();
		
		if(Provider[Idz] == "LIME" || Provider[Idz] == "Lime")
		{
			Logoz[0] = "/topup/images/LIME_Logo.gif";
			Logoz[1] = "/topup/images/LIME_Logo.gif";
		}
		else
		{
			Logoz[0] = "/topup/images/Digicel_Logo.gif";
			Logoz[1] = "/topup/images/LIME_Logo.gif";
		}
		
		if(document.all)
		{
			document.images.Mobilz_Provz_Logo.style.filter="blendTrans(duration=1)";
			document.images.Mobilz_Provz_Logo.filters.blendTrans.Apply();
		}
		
		document.images.Mobilz_Provz_Logo.src = "/topup/images/" + Provider[Idz] + "_Logo.gif";
		
		if(document.all)
		{
			document.images.Mobilz_Provz_Logo.filters.blendTrans.Play();
		}
		
		document.all.Mobilz_Provz_Logo.style.visibility = 'visible';
	}
	else
	{
		document.images.Mobilz_Provz_Logo.src = "/topup/images/" + Provider[Idz] + "_Logo.jpg";
		
		if(document.all)
		{
			document.images.Mobilz_Provz_Logo.filters.blendTrans.Play();
		}
		
		document.all.Mobilz_Provz_Logo.style.visibility = 'visible';
		//document.all.Mobilz_Provz_Logo.style.visibility = 'hidden';
	}
}

function Step_One_Check()
{
	Validate_Amount();
	
	var Country_Index = document.TopUp_Form_Widget.Mobile_Country.selectedIndex;
	
	Currency_Index = document.TopUp_Form_Widget.Currency.selectedIndex;
	
	if(Step_One_Complete)
	{
		//Disable_Enable_Two();
	}
	else if(Country_Index == 0)
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		alert("Please Select A Country Before You Proceed!");
	}
	else if(document.TopUp_Form_Widget.MobileNumber.value.length < 7)
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		alert("Please Enter The Mobile Number You Wish To Top Up!");
	}
	else if(Currency_Index == 0)
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		alert("Please Select A Currency For Amount Requested!");
	}
	else if(document.TopUp_Form_Widget.amount.value.length < 1)
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		if(InvalidAmountForRange == false){alert("Please Enter A Valid Amount Before You Proceed!");}
	}
	else
	{
		Step_One_Complete = false;
		//Disable_Enable_Two();
		alert("Please Enter A Valid Amount Before You Proceed!");
	}
}

function StripNonDigits(n)
{
	var s = String(n);
	var n2 = String();
	var cnt = 0;
	
	for(var i = 0; i < s.length; i++)
	{
   		var ss = s.substr(i,1);
		if(ss == '.') {cnt += 1;}
   		if((ss >= '0' && ss <= '9') || (ss == '.' && cnt <= 1)) { n2 = n2 + ss; }
   	}
	
	return n2;
}

function IsEmpty(aTextField,Nz)
{
   if((aTextField.value.length < Nz) || (aTextField.value==null))
   {
      return true;
   }
   else { return false; }
}

