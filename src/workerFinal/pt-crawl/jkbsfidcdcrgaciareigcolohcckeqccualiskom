function genTopicDisp() {
	var comboBox = document.getElementById('comboBoxService');
	var inputBox = document.getElementById('inputBoxService');
	
	if($("#comboBoxService").is(":visible")) {/*Hide ComboBox And Display TextBox*/
		$('#inputBoxService').attr('name','Service');
		$('#comboBoxService').attr('name','ServiceSec');
		$('#comboBoxService').hide();
		$('#inputBoxService').show();
		if(inputBox.value == " "){inputBox.value = "";}
		inputBox.focus();
		document.getElementById('genTopicNotice').innerHTML = "<b>CLICK TO CHOOSE FROM A LIST OF TYPES</b>";
	} else {/*Hide TextBox And Display ComboBox*/
		$('#comboBoxService').attr('name','Service');
		$('#inputBoxService').attr('name','ServiceSec');
		$('#inputBoxService').hide();
		$('#comboBoxService').show();
		if(inputBox.value == ""){inputBox.value = " ";}
		document.getElementById('genTopicNotice').innerHTML = "<b>CLICK TO ENTER A DIFFERENT TYPE</b>";
	}
}

function inputValidate() {
	var inputBox = document.getElementById('inputBoxService');
	
	if($("#inputBoxService").is(":visible")) {
		if(inputBox.value == "") {
			alert("Please Enter The Type of Transaction For This Payment...");
			inputBox.focus();
			return false;
		}
	}
}
