<script>

	// set IDs of selectors to use
	var firstNameID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_firstname_datatel_firstname";
	var middleNameID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_middlename_datatel_middlename";
	var lastNameID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_lastname_datatel_lastname";
	var nickNameID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_nickname_datatel_nickname";
	var maidenNameID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_wsu_maidenformername_wsu_maidenformername";
	var cellPhoneID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_address1_cellphone_datatel_address1_cellphone";
	var phonePrefID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_elluc_preferredphone_elluc_preferredphone";
	var homePhoneID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_address1_telephone1_datatel_address1_telephone1";
	var cityID = "ctl00_mainContent_ApplicationForm_ApplicationForm_ApplicationFormControl_datatel_address1_city_datatel_address1_city";
	
	var pFirstNameID = "ctl00_mainContent_CreateAccountUserControl_CreateUserControl_ProspectForm_firstname_firstname";
	var pLastNameID = "ctl00_mainContent_CreateAccountUserControl_CreateUserControl_ProspectForm_lastname_lastname";
	var pHomePhoneID = "ctl00_mainContent_CreateAccountUserControl_CreateUserControl_ProspectForm_address1_telephone1_address1_telephone1";
	var pCellPhoneID = "ctl00_mainContent_CreateAccountUserControl_CreateUserControl_ProspectForm_address1_telephone2_address1_telephone2";
	var pCityID = "ctl00_mainContent_CreateAccountUserControl_CreateUserControl_ProspectForm_address1_city_address1_city";
	
	// attach onChange event listener to the fields
	var firstName = document.getElementById(firstNameID);
	var middleName = document.getElementById(middleNameID);
	var lastName = document.getElementById(lastNameID);
	var nickName = document.getElementById(nickNameID);
	var maidenName = document.getElementById(maidenNameID);
	var phonePref = document.getElementById(phonePrefID);
	var cellPhone = document.getElementById(cellPhoneID);
	var phonePref = document.getElementById(phonePrefID);
	var homePhone = document.getElementById(homePhoneID);
	var city = document.getElementById(cityID);
	
	var pFirstName = document.getElementById(pFirstNameID);
	var pLastName = document.getElementById(pLastNameID);
	var pHomePhone = document.getElementById(pHomePhoneID);
	var pCellPhone = document.getElementById(pCellPhoneID);
	var pCity = document.getElementById(pCityID);
	
	if (firstName) {
		
		firstName.addEventListener("change", function () {fixData(firstNameID);}, false);
		middleName.addEventListener("change", function () {fixData(middleNameID);}, false);
		lastName.addEventListener("change", function () {fixData(lastNameID);}, false);
		nickName.addEventListener("change", function () {fixData(nickNameID);}, false);
		maidenName.addEventListener("change", function () {fixData(maidenNameID);}, false);
		city.addEventListener("change", function () {fixData(cityID);}, false);
		cellPhone.addEventListener("change", function () {formatPhone(cellPhoneID);}, false);
		homePhone.addEventListener("change", function () {formatPhone(homePhoneID);}, false);
				
	} else if (pFirstName) {
		
		pFirstName.addEventListener("change", function () {fixData(pFirstNameID);}, false);
		pLastName.addEventListener("change", function () {fixData(pLastNameID);}, false);
		pCity.addEventListener("change", function () {fixData(pCityID);}, false);
		pHomePhone.addEventListener("change", function () {formatPhone(pHomePhoneID);}, false);		
		pCellPhone.addEventListener("change", function () {formatPhone(pCellPhoneID);}, false);
		
	}
	// for IE8 and earlier versions
	else {
				
		if (firstName.attachEvent) {
			
			firstName.attachEvent("onchange", function () {fixData(firstNameID);});
			middleName.attachEvent("onchange", function () {fixData(middleNameID);});
			lastName.attachEvent("onchange", function () {fixData(lastNameID);});
			nickName.attachEvent("onchange", function () {fixData(nickNameID);});
			maidenName.attachEvent("onchange", function () {fixData(maidenNameID);});
			city.attachEvent("onchange", function () {fixData(cityID);});
			cellPhone.attachEvent("onchange", function () {formatPhone(cellPhoneID);});
			homePhone.attachEvent("onchange", function () {formatPhone(homePhoneID);});
						
		} else if (firstName.onChange) {
			
			firstName.onLoad = fixData(firstNameID);
			middleName.onLoad = fixData(middleNameID);
			lastName.onLoad = fixData(lastNameID);
			nickName.onLoad = fixData(nickNameID);
			maidenName.onLoad = fixData(maidenNameID);
			city.onLoad = fixData(cityID);
			cellPhone.onLoad = formatPhone(cellPhoneID);
			homePhone.onLoad = formatPhone(homePhoneID);
			
		} else if (pFirstName.attachEvent) {
			
			pFirstName.attachEvent("onchange", function () {fixData(pFirstNameID);});
			pLastName.attachEvent("onchange", function () {fixData(pLastNameID);});
			pCity.attachEvent("onchange", function () {fixData(pCityID);});
			pCellPhone.onLoad = formatPhone(pCellPhoneID);
			pHomePhone.onLoad = formatPhone(pHomePhoneID);
			
		} else if (pFirstName.onChange) {
			
			pFirstName.onLoad = fixData(pFirstNameID);
			pLastName.onLoad = fixData(pLastNameID);
			pCity.onLoad = fixData(pCityID);
			pCellPhone.onLoad = formatPhone(pCellPhoneID);
			pHomePhone.onLoad = formatPhone(pHomePhoneID);
			
		}
	}

	// get the data and then case it properly
	function fixData(dataToFixID) {
				
		var field = document.getElementById(dataToFixID);		
		var data = field.value;
		
		// remove special characters
		data = data.replace(/[0-9`~!@#$%^&*()_|+=?;:'",.<>\{\}\[\]\\\/]/g, "");
		
		// only properly case if all data is upper or lower
		if (data === data.toUpperCase() || data === data.toLowerCase()) {
			
			data = data.replace(/\w+/g, function (txt) {
			return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
			
			});
			
		}
		
		// set field to the cleaned up value
		field.value = data;
	}

	// format phone number to ###-###-####
	function formatPhone(phoneToFix) {
		
		var field = document.getElementById(phoneToFix);
		var data = field.value;
		
		// check regex is valid
		if (data.match(/^\D?(\d{3})\D?\D?(\d{3})\D?(\d{4})$/)) {
			// strip special characters
			data = data.replace(/\D/g, '');
			// format to how we want
			data = data.substr(0, 3) + "-" + data.substr(3, 3) + "-" + data.substr(6);
			field.value = data;
		
		}
	}
	
</script>
	</div>
</section>

