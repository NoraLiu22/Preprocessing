            		'viewMode': viewMode
            	});
            }
            if(contractId != "undefined"){
            	ga('set','dimension6', contractId);
            	gtag('config', 'G-5H36ZEETNT',{
            		'custom_map': {'dimension6': 'contractId'
            		},
            		'contractId': contractId
            	});
            }
            
            // Send view
            ga('send', 'pageview');
            
         </script>
