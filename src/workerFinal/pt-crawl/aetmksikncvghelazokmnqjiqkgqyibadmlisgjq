            <div class="PageHeading">
              <!-- #BeginEditable "PageHeading" -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
            Emission Factor<!-- #EndEditable --></div>
            <!-- #BeginEditable "Content" -->


<!-- 
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! 
!!!!!!!! REDIRECT TO CMS !!!!!!!! 
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! 
-->
<script type="text/javascript">
  location.replace('https://ww2.valleyair.org/permitting/emission-factors/');
