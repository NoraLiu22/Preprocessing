<script>
    // Function to send an API request or log when iframe is detected
    function trackIframeLoad(iframeSource) {
        // Make an API request to your backend or log the event
        console.log(`Iframe from ${iframeSource} loaded on page: ${window.location.href}`);
        let formData = new FormData();
        formData.append("_token", "kktqsTZznZji5taz0EWPDJ2XEMB6uJy7h59bl3oJ" );
        formData.append("source_url", `${window.location.href}`);
        $.ajax({
            type: "POST",
            url: "https://www.thejakartapost.com/api/iframe-tracker",
            data: formData,
            processData: false,
            contentType: false,
            cache: false,
            success: (res) => {
                console.log("iframe tracked");
            },
            error: (err) => {
                console.log(err);
            }
        });
    }

    // Set up MutationObserver to detect changes in the DOM
    const observer = new MutationObserver(mutations => {
        mutations.forEach(mutation => {
            // Check if a new node is added
            if (mutation.type === 'childList' && mutation.addedNodes.length > 0) {
                // Iterate through added nodes
                mutation.addedNodes.forEach(node => {
                    // Check if the node is an iframe
                    if (node.tagName && node.tagName.toLowerCase() === 'iframe') {
                        // Extract iframe source (you may need to adjust this based on your actual structure)
                        const iframeSource = new URL(node.src).hostname;

                        // Perform actions based on the iframe source
                        if (iframeSource == 'korfo.org' || iframeSource == 'powered-by-revidy.com' || iframeSource == 'hlmiq.com' || iframeSource == 'nethcdn.com') {
                            // Track the iframe load
                            trackIframeLoad(iframeSource);
                        }
                    }
                });
            }
        });
    });

    // Start observing the entire document and its descendants
    observer.observe(document.documentElement, { childList: true, subtree: true });
</script>
