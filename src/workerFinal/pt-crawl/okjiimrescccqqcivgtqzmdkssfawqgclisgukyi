    <div id="name-section" >
        <h2 class="trackTitle">
            April Fools (2021 Mix)
            
        </h2>

        <h3 style="margin:0px;">by 
        <span> 
          <a href="https://thescaryjokes.bandcamp.com">the scary jokes</a>
          </span>
        
        </h3>

    

    
    </div>
    <div class="middleColumn">
        
        

        <div id="tralbumArt">
        
            <a class="popupImage" href="https://f4.bcbits.com/img/a4164378372_10.jpg">
                <img src="https://f4.bcbits.com/img/a4164378372_16.jpg">
            </a>
        
        </div>

        
            <div class="share-panel-wrapper-desktop signup-tooltip-parent" data-tooltip-id="x"></div>            
            
                
<div class="collected-by tralbum collectors">
    
