                    const frameWrapper = window.document.getElementById('iframe-wrapper');

                    if(event.data && typeof event.data === 'string'){   
                        if(event.data === 'scrollToTop'){
                            const container = window.document.getElementById('main-container');
                            const containerYPos = frameWrapper
                                ? window.scrollY + frameWrapper.getBoundingClientRect().top
                                : 0;
                            const GlobalNavHeight = window.document.getElementsByClassName('GlobalNav-wrapper')[0].getBoundingClientRect().height;
                            const pageNavHeight = container
                                ? container.querySelector('nav')?.getBoundingClientRect().height
                                : 0;
                            const scrollOffset = GlobalNavHeight + (pageNavHeight || 0);

                            window.scroll(0, containerYPos - scrollOffset);    
                        }

                        if(event.data.includes('iframeHeight')){
                            const newFrameHeight = JSON.parse(event.data).iframeHeight + "px";

                            frameWrapper.style.height = newFrameHeight;
                            frameWrapper.querySelector('div').style.height = newFrameHeight;
                            frameWrapper.querySelector('iframe').height = newFrameHeight;
                        }

                        if(event.data.includes('heightxPYMx')){
                            const lastIndex = event.data.lastIndexOf('x');
                            const newFrameHeight = event.data.slice(lastIndex + 1) + "px";

                            frameWrapper.style.height = newFrameHeight;
                            frameWrapper.querySelector('div').style.height = newFrameHeight;
                            frameWrapper.querySelector('iframe').height = newFrameHeight;
                        }
                    }
                });</script><script>window.__APOLLO_STATE__ = {}</script><script async subscriptions-control="manual" src="https://news.google.com/swg/js/v1/swg.js"></script></head><body id="top" data-build-number="518459" data-view-name="react-page"class="Page Page--sundaytimes          "><div class="AccessibilityLinks"><h2 class="u-hide">Accessibility Links</h2><a href="#main-container" class="u-hide" tabindex="1">Skip to content</a></div><div class="GlobalNav "><div id="new-nav" class="NewGlobalNav opHide_newGlobalNav "> </div><div class="GlobalNav-wrapper opHide_newGlobalNav "><div class="GlobalNav-container"><button data-tracking='{"event_navigation_name":"mobile menu:open"}'  class="GlobalNav-menuButton js-tracking" id="global-nav-toggle" aria-haspopup="true" aria-controls="global-menu-mobile" aria-label="view all sections"><span class="GlobalNav-menuButtonContent"><span class="GlobalNav-menuButtonIcon"></span><span class="GlobalNav-menuButtonText">Menu</span><span class="GlobalNav-menuButtonText GlobalNav-menuButtonText--close" aria-label="close all sections">Close</span></span></button><div class="GlobalNav-logoHolder"><div class="GlobalNav-logo"><a data-tracking='{"event_navigation_name":"header:global navigation:logo"}'  href="/" title="Link to homepage" class="js-tracking"><img src="/d/img/logos/sundaytimes-white-small-d8581d335c.png" alt="sundaytimes logo" class="GlobalNav-logoImg GlobalNav-logoImg--sundaytimes"></a></div><time datetime="2024-05-05">Sunday May 5 2024</time></div><form action="/search" role="search" method="GET" class="GlobalSearch GlobalSearch--desktop" accept-charset="utf-8"><label for="nav-search-desktop" class="u-hide">Search The Times and The Sunday Times</label><input type="hidden" id="source" name="source" value="nav-desktop" /><input type="text" id="nav-search-desktop" class="GlobalSearch-searchInput" placeholder="Search" name="q" tabindex="3"><button class="GlobalSearch-submitInput" aria-label="Submit search" tabindex="3"><i clas