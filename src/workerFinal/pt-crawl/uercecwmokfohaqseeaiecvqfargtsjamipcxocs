                clearInterval(timer);
                $(".Choices_banner ul").stop().animate({
                    left: -i * imgWidth
                });
                $(this).addClass("onclick").siblings().removeClass("onclick");

            }, function() {
                timer = setInterval(function() {
                    toRight();
                }, 5000)
            });

            $(".Choicesprev,.Choicesnext,.Choices_banner").mouseenter(function() {
                clearInterval(timer);
            }).mouseleave(function() {
                timer = setInterval(function() {
                    toRight();
                }, 5000);
            });

            var timer = setInterval(function() {
                toRight();
            }, 5000);

            function toLeft() {
                i--;
                if (i == -1) {
                    $(".Choices_banner ul").css({
                        left: -(size - 1) * imgWidth
                    });
                    i = size - 2;
                }
                $(".Choices_banner ul").animate({
                    left: -i * imgWidth
                }, 1000);
                $(".Choicestel div").eq(i).addClass("onclick").siblings().removeClass("onclick");
            };

            function toRight() {
                i++;
                if (i == size) {
                    $(".Choices_banner ul").css({
                        left: 0
                    });
                    i = 1;
                }
                $(".Choices_banner ul").stop().animate({
                    left: -i * imgWidth
                }, 1000);
                if (i == size - 1) {
                    $(".Choicestel div").eq(0).addClass("onclick").siblings().removeClass("onclick");

                } else {
                    $(".Choicestel div").eq(i).addClass("onclick").siblings().removeClass("onclick");
                }
            };

            $('.list li').each(function() {
                $(this).click(function() {
                    $(".list li").removeClass("change");
                    $(this).addClass("change");
                    $(".l1").removeClass("show");
                    n = $(this).index();
                    $(".l1").eq(n).addClass("show");
                })
            });

            $(".data1 dl").eq(0).hover(function() {
                $(".op").eq(0).css("background-color", "#053763");
                $(".data1 dl dd p a").eq(0).css("color", "#fff");
                $(".data1 dl dd span a").eq(0).css("color", "#eff2f6");
            }, function() {
                $(".op").eq(0).css("background-color", "#eff2f6");
                $(".data1 dl dd p a").eq(0).css("color", "#000");
                $(".data1 dl dd span a").eq(0).css("color", "rgba(0,0,0,0.7)");
            });
            $(".data1 dl").eq(1).hover(function() {
                $(".op").eq(1).css("background-color", "#053763");
                $(".data1 dl dd p a").eq(1).css("color", "#fff");
                $(".data1 dl dd span a").eq(1).css("color", "#eff2f6");
            }, function() {
                $(".op").eq(1).css("background-color", "#eff2f6");
                $(".data1 dl dd p a").eq(1).css("color", "#000");
                $(".data1 dl dd span a").eq(1).css("color", "rgba(0,0,0,0.7)");
            });
        });
    </script>
</body>

