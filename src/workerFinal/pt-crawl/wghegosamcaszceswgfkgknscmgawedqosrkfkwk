	<h1 id="firstHeading" class="firstHeading" lang="en">Animal</h1>
	<div id="bodyContent" class="mw-body-content">
		<div id="siteSub" class="noprint">From RationalWiki</div>
		<div id="contentSub"></div>
		<div id="contentSub2"></div>
		
