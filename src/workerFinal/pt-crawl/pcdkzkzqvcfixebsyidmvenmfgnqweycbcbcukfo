                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Curriculum Instruction and Testing" class="ms-sitemapdirectional" href="/TeachingAndLearning/Pages/home.aspx">Teaching And Learning</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/TeachingAndLearning/StudentLearning/Pages/home.aspx">Student Learning</a></span><span> &gt; </span><span class="current">Multilingualism</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">Multilingualism</h2>
                
    



				
				<p class="intro">
					The vision of the Multilingualism for Equitable Education Policy is that all Hawai‘i State Department of Education (HIDOE) schools embrace, promote, and perpetuate multilingualism and multiculturalism to support and advance learners, families, and communities.  
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

