          }

      });

      $("#PasswordTextBox").change(function () {
          // Check if other textboxes have value
          var emailInput = $('#EmailTextBox').val();
          var firstNameInput = $('#ContentContainer_MainContent_MainContent_first_name_text_box').val().trim();
          var lastNameInput = $('#ContentContainer_MainContent_MainContent_last_name_text_box').val().trim();
          var passwordInput = $('#PasswordTextBox').val();

          if (emailInput !== "" && firstNameInput !== "" && lastNameInput !== "" && passwordInput !== "") {
              $("#CustomSubmitButton").prop('disabled', false);
          } else {
              $("#CustomSubmitButton").prop('disabled', true);
          }

      });

      let OnLoad = () => {

          let emailInput = $('#EmailTextBox').val();

          if (emailInput !== ''){
              $("#UserNameTextBox").val(emailInput);
          }

          var saveButtonEvent = document.getElementById("SubmitButton").onclick;
          document.getElementById("CustomSubmitButton").onclick = function () {
              var firstNameInput = $('#ContentContainer_MainContent_MainContent_first_name_text_box').val().trim();
              var lastNameInput = $('#ContentContainer_MainContent_MainContent_last_name_text_box').val().trim();
              localStorage.setItem("firstname", firstNameInput);
              localStorage.setItem("lastname", lastNameInput);
              console.log("First Name" + localStorage.getItem("firstname"));
              console.log("Last Name" + localStorage.getItem("lastname"));

              saveButtonEvent();
          }

          $(document).on('keypress',function(e) {
              if(e.which == 13) {
                var firstNameInput = $('#ContentContainer_MainContent_MainContent_first_name_text_box').val().trim();
                var lastNameInput = $('#ContentContainer_MainContent_MainContent_last_name_text_box').val().trim();
                localStorage.setItem("firstname", firstNameInput);
                localStorage.setItem("lastname", lastNameInput);
                console.log("First Name" + localStorage.getItem("firstname"));
                console.log("Last Name" + localStorage.getItem("lastname"));

                saveButtonEvent();
              }
          });
      }

      OnLoad();
  })
</script>

