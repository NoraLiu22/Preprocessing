          var lastNameInput = $('#ContentContainer_MainContent_MainContent_last_name_text_box').val().trim();
          var passwordInput = $('#PasswordTextBox').val();

          if (emailInput !== "" && firstNameInput !== "" && lastNameInput !== "" && passwordInput !== "") {
              $("#CustomSubmitButton").prop('disabled', false);
          } else {
              $("#CustomSubmitButton").prop('disabled', true);
          }

      });

