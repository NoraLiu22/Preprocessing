<script>
  function addHours(milisec, h) {
    return milisec + h*60*60*1000;    
  }

  function isPastEventByTimestamp(p){
    
    // We need to compare GMT - 7 with timestamps of events ( that are specified in GMT but represents GMT -7)
    var currentTimeStamp = 1714876341754;

    if(p.name.indexOf("sample test") >= 0 ){
      console.log("----------------------------2---------------");
      console.log("canada_now : 2024-05-05 03:32:21");
                  console.log("canada_now_ts_original : 1714901541754");
                  console.log("canada_now_tsx : 1714876341754");
                  console.log("sample");          
      console.log("---------------------------- start_bool : ");
    }

    var comp_start_time = p.timestamp_data.start,
        comp_end_time = p.timestamp_data.end;


    var result = false;
    if(currentTimeStamp > comp_start_time && currentTimeStamp > comp_end_time){
      result = true;
    }    
    console.log("The Event | " + p.name + " | hasPassed? = " + result);
    return result;

  }
</script>


<!-- UpcomingShows_v3 -->
<script>
  function UpcomingShowsHTML(post, color_theme, start_date, end_date, series, company){
    console.log("UpcomingShowsHTML called! : " + start_date + " | " + end_date);
    var font_color_global = '#fff';
    var font_color_company = '#999';
    if (color_theme == 'event-items-dark-item'){
      var font_color_global = '#fff';
    }
    if (color_theme == 'event-items-light-item'){
      var font_color_global = '#8C8278';
    }
    if (color_theme == 'event-items-sand-item'){
      var font_color_global = '#4B4B4B';
    }

    /* Let's define old & current & new seasons */
    // 1. Is TODAY before or after September 1st? 
    var today = new Date();
    const currentMonth = today.getMonth() + 1; 
    const currentYear = today.getFullYear();

    var newSeasonStarted = false;

    var currentSeasonEvent = true;
    var prevSeasonEvent = false;    
    var nextSeasonEvent = false;
    // 1.1 Before September 1st -> So we are still in the new season since last year's September 1st till this year's August 31st    

    var startDate = new Date(start_date);
    var endDate = new Date(end_date);
    var thisEventStartMonth = startDate.getMonth() + 1;   
    var thisEventEndMonth =  endDate.getMonth() + 1;   
    var thisEventStartYear = startDate.getFullYear();
    var thisEventEndYear =  endDate.getFullYear();
    console.log(" -------------------------------- post.name : " +  post.name + " -------------------------------- ");    

    if(post.name.indexOf("U2: The Joshua Tree") >= 0){
      console.log("1U2: The Joshua Tree");
    }


    console.log("This Event : " + thisEventStartMonth + " - " + thisEventEndMonth + " - " + thisEventStartYear);
    if(currentMonth >= 9) newSeasonStarted = true;
    console.log("newSeasonStarted ? : " + newSeasonStarted);
    if(newSeasonStarted){
      // 1.2 It is already after September 1st -> So it is going to be the new season till next year's August 31st
      // 1.2.1 Thus, we need to evaluate the events accordingly -> E.g : Any events before September 1st of this year will be considered in the prev. season
      // 1.2.2 And any events until next year's August 31st will be considered in this season.

      if(thisEventStartYear < currentYear){
        prevSeasonEvent = true;         
      }
      if(thisEventStartYear === currentYear){
        if(thisEventStartMonth >= 9 && thisEventEndMonth >= 9){
          // This event is in the previous season now
          currentSeasonEvent = true;         
        } 
        else{
          prevSeasonEvent = true;
        }
      }
      if(thisEventStartYear > currentYear){
        if(thisEventStartMonth >= 9 && thisEventEndMonth >= 9){
          nextSeasonEvent = true;
        }
        else currentSeasonEvent = true;    
      }
    }
    else{
      // Still in the old season = Meaning that it is not September 1st yet. Thus, evaluate events accordingly
      if(thisEventStartYear > currentYear){
        nextSeasonEvent = true;
      }      
      if(thisEventStartYear < currentYear){
        if(thisEventStartMonth < 9 && thisEventEndMonth < 9){
          // This event is in the previous season now
          prevSeasonEvent = true;         
        }  
        else{
          currentSeasonEvent = true;
        }
      }
      if(thisEventStartYear === currentYear){
        if(thisEventStartMonth >= 9 && thisEventEndMonth >= 9){
          // This event is in the previous season now
          nextSeasonEvent = true;         
        } 
        else{
          currentSeasonEvent = true;
        }
      }
    }
    if(nextSeasonEvent || prevSeasonEvent) currentSeasonEvent = false;
    if(currentSeasonEvent){
      nextSeasonEvent = false;
      prevSeasonEvent = false;
    }
    console.log("This Event : currentSeasonEvent? " + currentSeasonEvent );
    console.log("prevSeasonEvent? : " + prevSeasonEvent + ", if true - then EXIT!");
    // IF prevSeasonEvent -> then do not display this one
    if(prevSeasonEvent) return;

    post.url = post.url;

    var ended_style = '';
    var event_ended = false;
    var subliner = '';
    var venue = post.venue;
    var presented_by = post.company;

    console.log("post.name : " + post.name);
    if(post.name.indexOf("U2") >= 0){
      console.log("U2 found!");
    }
    var hasPassed = isPastEventByTimestamp(post);
    console.log("hasPassed : " + hasPassed);
    var hasEnded = hasPassed || ( start_date.toString() == "" && end_date.toString() == "" ) || (start_date.toString() == "Invalid Date" && end_date.toString() == "Invalid Date");
    console.log("hasEnded : " + hasEnded);
    console.log("The Event | " + post.name + " | hasEnded? = " + hasEnded + " | hasPassed? = " + hasPassed);
    if( hasEnded ){
      event_ended = true;
      ended_style = '-webkit-filter: grayscale(100%); filter: grayscale(100%);';
    }

    var html = '<li class="list-item upcoming-show-item" data-current-season="' + currentSeasonEvent + '" data-next-season="' + nextSeasonEvent + '" data-prev-season="' + prevSeasonEvent + '">';
    html += '<ul style="overflow: hidden; color:' + font_color_global + ';" class="w-full-mod ' + color_theme + ' p-12px">';
    html += '<li style="post-img"><div style="position: relative;"><a href="' + post.url + '"><img style="' + ended_style + '" src="' + post.img + '" /></a></div></li>';

    var orange_text = '';
    if(!event_ended){
      var TBD_cuttoff_date = Date.now() + 65750400000;
      if (start_date > TBD_cuttoff_date) {
        html += '<li class="pb-4 date-line">TBD</li>';
      } else { 
        if (start_date == end_date && start_date != 'Invalid Date'){
          orange_text = 'Get Tickets';
          html += '<li class="pb-4 date-line">' + post.timestamp_data.start_date_string + '</li>';
        } else if (start_date != 'Invalid Date' && end_date == 'Invalid Date') {
          orange_text = 'Get Tickets';
          html += '<li class="pb-4 date-line">' + post.timestamp_data.start_date_string + '</li>';
        } else if (start_date != null && end_date != null && end_date != 'Invalid Date'){
          orange_text = 'Showtimes';
          html += '<li class="pb-4 date-line">' + post.timestamp_data.start_date_string + ' - ' + post.timestamp_data.end_date_string + '</li>';
        } else {
          html += '<li class="pb-4 date-line"></li>';
        }
      }
    } else {
      html += '<li style="color:#999;" class="pb-4 date-line">Event Has Ended</li>';
    }

    if (post.button_cta_text != '' && post.button_cta_text != null){
      orange_text = post.button_cta_text;
    }
    // console.log("hideCardLabels.presentedBy : " + hideCardLabels.presentedBy);
    // console.log("hideCardLabels.venue : " + hideCardLabels.venue);
    // console.log("hideCardLabels.pastEvents : " + hideCardLabels.pastEvents);

    if(!hideCardLabels.presentedBy) html += '<li> <p class="resident-company here">' + presented_by + '</p></li>';   

    html += '<li style="height: 120px; overflow: hidden;" class="pb-4 event-title">' + post.name + '</li>';
    //console.log("venue : " + venue + " , visibilityOfCardLabels : " + hideCardLabels.venue);
    if(!hideCardLabels.venue) html += '<h5 style="color:' + font_color_company + '; font-size: 14px;">' + venue + '</h5>';
    html += '<li style="overflow:hidden;" class="flex-row between-center pt-4 cta-col"><a href="' + post.url + '" class="cta_button orange event-items-sand-main">Learn More</a>';        
    html += '</ul>';
    html += '</li>';

    return html;
  }
</script>

<!-- WhatsOnListing_v3.html -->
<script>
  function isValidDate(d) {
    return d instanceof Date && !isNaN(d);
  }
  function WhatsOnListingHTML(i, color_theme, start_date, end_date, pre_date, manual_show_date, has_expired, should_be_listed){    
    var html = '<div id="t-id-' + i.tess_id + '" data-timestamp="' + i.timestamp_data.start  + '" data-company="' + i.company + '" data-genre="' + i.genre + '" data-series="' + i.series + '" data-venue="' + i.venue + '" class="item post-item" data-listed="' + should_be_listed + '" data-expired="' + has_expired + '">';
    // Dsiplay Featured Picture as Link
    html += '<a href="' + i.url + '" class="image-col">';
    if (i.img != "") {
      html += '<img loading="lazy" style="width:100%;" src="' + i.img + '" alt="' + i.name + '">';
    } else {
      html += '<img src="https://f.hubspotusercontent00.net/hubfs/8919754/assets_theme_2021/placeholders/placeholder-5x6.jpg" />';
    }
    html += '</a>';

    html += '<div class="content-col">';
    html += '<div class="heading">';
    // Display Date or TBD    
    var manual_show_date_formatted = new Date(manual_show_date);    

    if (manual_show_date && manual_show_date.length > 0 && !isValidDate(manual_show_date_formatted)) {
      html += '<p class="event-performance-date type1">TBD</p>';
    } 
    else if(manual_show_date && manual_show_date.length > 0 && isValidDate(manual_show_date_formatted)){
      html += '<p class="event-performance-date type2">' + manual_show_date_formatted.toDateString() + '</p>';
    }
    else if(has_expired) {
      // Why? : Events whose last perf. date has passed today should be hidden in whats-on listing templates. Can still be displayed as a result of the search, though.     
      html += '<p class="event-performance-date expired type3"></p>';
    }    
    else {
      if (start_date && start_date == end_date && start_date != 'Invalid Date'){
        html += '<p class="event-performance-date type1a">' + i.timestamp_data.start_date_string + '</p>';
      } else if (start_date && !end_date) {
        //html += '<p class="event-performance-date type1b">' + i.timestamp_data.start_date_string + '</p>';
        html += '<p class="event-performance-date type1b">' + "" + '</p>';
      } else if (start_date != null && end_date != null && end_date != 'Invalid Date'){
        html += '<p class="event-performance-date type1c">' + i.timestamp_data.start_date_string + ' - ' + i.timestamp_data.end_date_string + '</p>';
      } 
    }

    // Display Company/Presenter Name   
    if (i.company != "null"){
      html += '<p class="resident-company">' + i.company + '</p>';
    }
    // Display Event Name as Link
    html += '<h3 class="bloglistingHeading"><a style="color: #323232; text-decoration: none;" href="' + i.url + '">' + i.name + '</a></h3>';
    // Display Venue
    if (i.venue != "null"){
      html += '<h4 class="venue">' + i.venue + '</h4>';
    }
    html += '</div>';
    html += '<div class="content">';
    // Display Event Post Content Snippet
    html += '<p>' + i.content + '</p>';
    html += '</div>';
    html += '</div>';

    // Display Tag List
    html += '<div class="tag-col">';
    i.tags.forEach(function(tag){
      html += '<a class="tag" href="' + tag.url + '" rel="tag">' + tag.name + '</a>';
    });
    html += '</div>';

    // Display Gray CTA Button
    html += '<div class="cta-col">';
    html += '<a href="' + i.url + '" class="cta_button orange event-items-sand-main">Learn More</a>';    
    html += '</div>';
    html += '</div>';

    return html;
  }

</script>

<!-- Event Display -->
<script>
  shown_count = 0;

  function setView(pl, view, color_theme, _count, series, company){



    var pl_post_list = [];
    if (_count > pl.length){
      shown_count = pl.length;      
    }
    console.group("EventData_Display_v3 | setView() : shown_count.length : " + shown_count);

    for (let x = 0; x < shown_count; x++){
      if (x <= pl.length) {
        var i = pl[x];

        var start_date = i.timestamp_data.start;
        // If there is no proper timestamp for this event ( because it is an old event without any timestamp property , THEN USE the date string.)
        if(start_date === 0){
          start_date =  new Date(i.timestamp_data.start_date_string).getTime();
        }
        //console.log("start_date*** : " + start_date);
        var end_date = i.timestamp_data.end;
        if(end_date === 0){
          end_date =  new Date(i.timestamp_data.end_date_string).getTime();
        }

        var pre_date = new Date(i.presale_date);
        var manual_show_date = i.manual_show_date;               
        var is_manual_show_date = new Date(manual_show_date);

        var whats_on_listing_date;
        if(i.whats_on_listing_date){
          whats_on_listing_date = i.timestamp_data.whats_on_listing_date;
        }

        // Expired events do not get displayed on listing page. ( only on search )
        var has_expired = false; 
        if(isPastEventByTimestamp(i)){
          has_expired = true;          
        } 
        console.log("whats_on_listing_datex ----> " + i.timestamp_data.whats_on_listing_date + ", i.name : " + i.name + ", has_expired: " + has_expired);
        // Load correct view for display location
        if (view == 'v'){
          // replaced_element_container = '#GlobalList';        
          var todays_date = 1714926741754;      
          // some events are not listed until the show-list-time is hit. 
          var should_be_listed = true;

          /* Check if a Specific What's on Listing date is specified. It means that the post will be hidden until we reach that specific date. */         
          /* Rules here 
           * 1) IF the Display Manual Date is checked, then whatever date is in that field should display as the show date and the system should ignore any dates in the Show Dates fields. 
           * If Display Manual Date is unchecked, even if there is something in the field, it should not use that data.
           * */
          if(manual_show_date && is_manual_show_date){
            end_date = is_manual_show_date;            
            console.log("has_expired check will be done by [the Manual Date entered]");            
          }                

          if(i.whats_on_listing_date && i.whats_on_listing_date > todays_date){
            // Keep this Event hidden as the editor wants to hide it till the specified tae.
            console.log("whats_on_listing_date : " + i.timestamp_data.whats_on_listing_date + ", not reached yet - so hide it!");
            should_be_listed = false;            
          }
          var html = WhatsOnListingHTML(i, color_theme, start_date, end_date, pre_date, manual_show_date, has_expired, should_be_listed);
        }
        if (view == 'h'){
          //replaced_element_container = '.post-list';
          console.log("hideCardLabels.pastEvents : " + hideCardLabels.pastEvents);
          var skipThisEvent = has_expired && hideCardLabels.pastEvents;
          if(skipThisEvent){
            console.log("Hide it! : " + i.name);
            continue;
          }
          if(i.whats_on_listing_date > 1){
            console.log("Hide it! : " + i.name);
            continue;
          } 
          else{
            console.log("ADD IT TO CAROUSEL : " + i.name);
            var html = UpcomingShowsHTML(i, color_theme, start_date, end_date, series, company);
          }
        }
        //console.log("FINAL HTML HERE FOR THE CAROUSEL SLIDER.");
        if (html != null)
        pl_post_list.push(html);
      }
    }
    console.groupEnd();

    return pl_post_list;
  }

  function loadMoreButton(){
    loadMore('v','None', 'All', 12, 'color_theme', 'All', '#GlobalList', 'No');
  }



  function loadMore(view, control, company, _count, color_theme, series, replaced_element_container, is_series, hidden_tags = []){
    console.log("eventData_display_v3.html | loadMore() called : " + _count + " series : " + series + ", global_start : " + global_start);
    filtered_post_list = getEventsByDate(global_start, global_end, series, is_series);
    console.log("eventData_display_v3.html | postsFiltered : filtered_post_list : " + filtered_post_list.length);

    if (filtered_post_list.length <= _count){
      shown_count = filtered_post_list.length;
    } else {
      shown_count += _count;
    }

    if (shown_count > filtered_post_list.length){
      shown_count = filtered_post_list.length;
    }

    var _fpl = [];
    var _fpl_past = [];
    var _fpl_future = [];
    var _fpl_combined = [];
    var _fpl_untagged = [];

    // Hide Tagged Posts
    filtered_post_list.forEach(function(p){
      var hidden = false;
      hidden_tags.forEach(function(htag){
        p.tags.forEach(function(ptag){
          if (htag == ptag.name){
            hidden = true;
          }
        });
      });

      if(!hidden){
        _fpl_untagged.push(p);
      }
    });

    // Move Expired and TBD Events to End of List
    _fpl_untagged.forEach(function(p){   



      if(isPastEventByTimestamp(p)){
        _fpl_past.push(p);  
      }
      else {
        _fpl_combined.push(p);
      }

    });
    console.log("_fpl_past : " + _fpl_past);
    //   console.log(_fpl_future);

    _fpl_future.forEach(function(p){
      _fpl_combined.push(p);
    });

    _fpl_past.forEach(function(p){
      _fpl_combined.push(p);
    });


    if (control == 'slider'){
      var post_counter = 0;
      _fpl_combined.forEach(function(p){
        if (post_counter < _count) {

          console.log("--------------------------------------------");
          console.log("p.company: " + p.company + " , p.path_company : " + p.path_company + " | VS | " + company );          
          console.log("--------------------------------------------");
          if (p.company == company || company == "All" || p.path_company == company){
            if (p.series == series || series == "All"){
              post_counter++;
              _fpl.push(p);
              console.log("add this slide : " + p.name + " | counter : " + post_counter); 
            }
          }
        }
      });
    } else {
      _fpl = _fpl_combined;
    }



    console.log("total slide : " + _fpl.length); 
    post_list_html = setView(_fpl, view, color_theme, _count, series, company);

    post_view = "";
    console.log("post_list_html.length : " + post_list_html.length);
    post_list_html.forEach(function(post){
      post_view += post;            
    });

    if (post_view == ""){
      //     post_view = '<h3>Darn!</h3>';
      post_view += '<h4 style="text-align:center;">There are no events currently scheduled for your selection.<br>We are adding events all the time, so check back again soon!</h4>';
      if (view == 'h'){
        document.body.scrollTop = 0; // For Safari
        document.documentElement.scrollTo({top: 0, behavior: 'smooth'}); // For Chrome, Firefox, IE and Opera
      }
    }

    console.log("deploying carousel content...");
    $(replaced_element_container).html(post_view);

    // If this is a search result -> then display the expired events too!
    var entered_keyword = $( "#filter-search input" ).val() ? $( "#filter-search input" ).val().toLowerCase() : "";    
    console.log("entered_keyword : " + entered_keyword);
    if(entered_keyword != ""){
      console.log("enteredKeyword not empty!");      
      $(".arts_ev_listing .list .item[data-expired=true]").addClass("temporaryShow");
    }

    if (shown_count >= _fpl.length) {
      $('#LoadMore').hide();
    } else {
      $('#LoadMore').show();
    }

    return _fpl;
  }

  function UpdateTitle(){
    var text = '';
    if (selected_genre != '') {
      text += selected_genre;
    }

    if (selected_venue != '') {
      if (text != '') {
        text += ' + ';
      }
      text += selected_venue;
    }
    console.log(selected_company);
    if (selected_company != '') {
      if (text != '') {
        text += ' + ';
      }
      text += selected_company;
    }
    if (selected_series != '') {
      if (text != '') {
        text += ' + ';
      }
      text += selected_series;
    }
    if (text == '') {
      text = 'All Shows & Events';
    }
    $('#TitleOverride').text(text);
  }

</script>

<!-- End partial --></div>
<script>
  
  
  
  

  /////////////////////////////
  today = moment();
  global_start = 1714876341754;
  console.log("global_start set : " + global_start);
  global_end = moment(today).add(2, 'Y').endOf('day');

  var datePicker;
  var selected_genre = '';
  var selected_venue = '';
  var selected_company = '';
  var selected_series = '';
  var _numberFilters = 0;
  var url_string;
  var cat_string;
  var event_cat;
  var url_tag;
  var all_posts;

  let PageTitle = '';

  //////////////////////////////

  $(document).ready(function(){
    console.log("Event-Listing.html | - doc-ready. Call loadMore().")
    PageTitle = 'All Shows &amp; Events';
    showFilters();    
    post_loader = loadMore('v','None', 'All',12, 'color_theme','All', '#GlobalList', 'No');


    // It is a hack that we apply for now ( the idea is to select ALL Dates from the filter so that we can have the right order )
    //  Without it, the sorting is wrong.
    //$('input[type="radio"]#all_dates').click();    
    
    console.log("query_tag : Free");
                
    $("input[name='event_genre'][value='Free']")[0].click()
                

  });

  ////////////////////////////////

  //Date Picker
  datePicker = new Lightpick({
    field: document.getElementById('datefrom'),
    secondField: document.getElementById('dateto'),
    selectBackward: false,
    startDate: moment(),
    endDate: moment().add(1, 'month'),
    inline: true,
    onSelect: function(start, end){
      global_start = start.startOf('day').subtract(1, "days");
      global_end = end.endOf('day');
      console.log("global_start set2: " + global_start + " | global_end : " + global_end);

      loadMore('v','None', 'All', 12, 'color_theme','All', '#GlobalList', 'No');
    }
  });



  /* Filtering CTAs, Apply and Clear Data */

  window.onload = function(event){
    console.log("pagex:  onLoad");
    //  event.preventDefault();   
    //     clearAll(); 
  }  

  $(".clear_all").click(function(event){
    event.preventDefault();   
    console.log("clearing all!");
    clearAll();
  });

  $("#apply").click(function(event){
    event.preventDefault();     
    var field = new Date($('#filter-modal #datefrom').val());
    var secondField = new Date($('#filter-modal #dateto').val());    
    $.magnificPopup.close();

  });

  function clearAll(){
    console.log("CLEAR ALL!--------------------")
    $( "#filter-search input" ).val("");
    entered_keyword = 0;
    console.log("cleared? : " + $( "#filter-search input" ).val() + ", entered_keyword : " + entered_keyword);
    $("#all_dates").click();
    $("#all_genres").click();
    $("#all_venues").click();
    $("#all_companies").click();
    $("#all_series").click();

    $(".arts_ev_listing .list .item[data-expired=true]").removeClass("temporaryShow");
  }  

  /* Mobile â€”Â Popup Modal Filter */  
  $(function () {
    $('.popup-modal-filter').magnificPopup({
      type: 'inline',
      preloader: false,
      focus: '#username',
      modal: true
    });
    $(document).on('click', '.popup-modal-dismiss', function (e) {
      e.preventDefault();
      $.magnificPopup.close();
    });
  });



</script>
