  $(document).ready(function(){
    console.log("EventData_Aggregation.html | doc.ready initEventList()");
    initEventList();
  });

  ////////////////////////////////

  function initEventList(start, end){
    var fpl = [];
    console.log("Where we create the OBJECT ARRAY by all HubL Data AND SORT THEM!");
    console.group("EventData_Aggregation.html | initEventList()  | raw data | post_list : " + post_list.length);
    post_list.forEach(function(post){
      //console.log("-- (post.name) -- : " + post.name);
      if(post.name.indexOf("U2: The Joshua Tree") >= 0){
        console.log("1U2: The Joshua Tree");
      }

      var p = post;      
      var isTess = (p.tess_id && p.tess_id !== 'null')? true : false;              
      if(isTess) {        
        p.timestamp_start_id_new = Number(post.t_start_time_ts);              
        p.timestamp_end_id_new = Number(post.t_end_time_ts);      
        //console.log("--------- t_start_time_string ------------");
        p.start_date_string = formatDateLongStringToShort(post.t_start_time_string);
        //console.log("--------- t_end_time_string ------------");
        p.end_date_string = formatDateLongStringToShort(post.t_end_time_string) ;
      }
      else {
        // console.log("BEFORE -> : " + post.start_dates[0] + " - " + post.start_dates[1]);        
        p.timestamp_start_id_new = Number(post.nont_start_time_ts);        
        p.timestamp_end_id_new = Number(post.nont_end_time_ts);            
        //console.log("--------- NONt_start_time_string ------------ : " + post.start_dates_string[0]);
        p.start_date_string = post.start_dates_string[0];
        //console.log("--------- NONt_end_time_string ------------");
        p.end_date_string = post.end_dates_string[0];
      }


      // Let's merge timestamp data in an array  | From Now on - Use this structure for any DATE comparison or presentation
      p.timestamp_data = {
        "start" : p.timestamp_start_id_new,
        "end" : p.timestamp_end_id_new,
        "start_date_string" : p.start_date_string,
        "end_date_string" : p.end_date_string,
        "isTess" : isTess,
        "name" : p.name,
        "whats_on_listing_date" : p.whats_on_listing_date
      };

      fpl.push(p);
    });    
    fpl.sort(function(x, y){
      return x.timestamp_start_id_new - y.timestamp_start_id_new;
    })
    fpl.forEach(function(post){
      date_sorted_post_list.push(post);
    });
    console.log("EventData_Aggregation.html | initEventList() | first Sort | done with date_sorted_post_list : " + date_sorted_post_list.length);
    console.groupEnd();
  }
</script>

<!-- End partial --></div>

<script>
  var category = "";

  updated_post_list = [];
  filtered_post_list = [];

  selected_company = "";
  selected_genre = "";
  selected_series = "";
  selected_venue = "";
  entered_keyword = "";

  loaded = 0;

  $(document).ready(function(){
    console.log("EventData_Filtering.html | doc. ready -> updated_post_list = date_sorted_post_list! : " + date_sorted_post_list.length);
    updated_post_list = date_sorted_post_list;
  });

  function getEventsByDate(start, end, series, is_series){
    // Start -> Starting from when
    // End -> The last day for filtering events

    // ALSO
    // Start = global_start
    // End = global_end

    console.group("EventData_Filtering.html | getEventsByDate | start : " + start + " end : " + end);
    var fpl = [];

    if (start != null && start != 0){
      if (end != null && end != 0){
        global_start = start;
        global_end = end;
      }
    }

    var filter_starts = start;
    var filter_ends = end;
    console.log("CALL FilterList first ");
    updated_post_list = filterList();
    console.log("CALL FilterList ends - ALL FILTERED");


    console.log("filter_starts : " + filter_starts);
    console.log("filter_ends : " + filter_ends);
    updated_post_list.forEach(function(post){
      var p = post;            
      var event_start_date = post.timestamp_data.start;
      var event_end_date = post.timestamp_data.end;
      var start_bool = false;
      var end_bool = false;




      //IF start date or end date of the filter is empty, then return all items including this one         
      if(!filter_starts || !filter_ends){
        console.log("empty-*----------------");
        fpl.push(p);
      }
      else{       
        if (event_start_date >= filter_starts && event_start_date <= filter_ends) {
          //console.log("EVENT START DATE in the RANGE!");
          start_bool = true;
        } 
        else if (event_end_date >= filter_starts && event_end_date <= filter_ends) {
          //console.log("EVENT END DATE in the RANGE!");
          end_bool = true;
        } 
        else {
          console.log("EVENT NOT IN THE RANGE")
        }

        if ( start_bool == true || end_bool == true ) {
          start_bool = true;
          end_bool = true;
        }

        if(p.name.indexOf("sample test") >= 0 ){
          console.log("----------------------------");
          console.log("sample");
          console.log("event_start_date : " + event_start_date + " | event_end_date : " + event_end_date );
          console.log("filter_starts : " + filter_starts + " | filter_ends : " + filter_ends );
          console.log("---------------------------- start_bool : " + start_bool + " , end_bool : " + end_bool);
        }

        var is_TBD = event_start_date > Date.now() + 65750400000;
        if (is_TBD){
          start_bool = true;
          end_bool = true;
        }

        console.log("3. getEventsByDate() called | eventData_filtering_v3.html | " + post.name + "  result? " + start_bool + " " + end_bool);
        if(p.name.indexOf("Immigrants of Distinction Awards") >= 0){
          console.log("Immigrants here2 -> venue -> : " + post.name);
        }


        if (start_bool || is_series == 'Yes' || is_TBD) {
          if (end_bool || is_series == 'Yes' || is_TBD) {
            fpl.push(p);
          }
        }

      }
    });
    console.groupEnd();

    return fpl.sort(function(a, b){return a.timestamp_data.start - b.timestamp_data.start});
  }

  function filterList(){
    console.group("EventData_Filtering | filterList()");

    var upl = [];
    if (category != ""){
      if (category == "multi" || category == "whats-on"){
        // Do Nothing
      } else {
        if(loaded <= 3) {
          switch(category) {
            case "comedy":
              selected_genre = "Comedy";
              break;
            case "community":
              selected_genre = "Community";
              break;
            case "dance":
              selected_genre = "Dance";
              break;
            case "education":
              selected_genre = "Education";
              break;
            case "family":
              selected_genre = "Family";
              break;
            case "free":
              selected_genre = "Free";
              break;
            case "music":
              selected_genre = "Music";
              break;
            case "talks":
              selected_genre = "Talks";
              break;
            case "theatre":
              selected_genre = "Theatre";
              break;
            case "virtual":
              selected_genre = "Virtual";
              break;
            case "student":
              selected_genre = "Student";
              break;
            default:
              selected_company = category;
          }
          loaded++;
        }
      }
    }

    date_sorted_post_list.forEach(function(post){
      var selected = true;
      var tagged = false;
      post.tags.forEach(function(tag){
        //console.log("here tag check! : " +  post.tags.length);
        if (tag.name == selected_genre || selected_genre == ""){
          tagged = true;
        }
      });

      if (post.tags.length == 0){
        tagged = true;
      }

      if (tagged == false) {
        selected = false;
      }

      if (selected_venue != ""){
        if (post.venue != selected_venue){
          selected = false;
        }
      }

      if (selected_company == "arts-commons-presents"){
        selected_company = "Arts Commons Presents";
      }
      if (selected_company == "alberta-theatre-projects"){
        selected_company = "Alberta Theatre Projects";
      }
      if (selected_company == "calgary-philharmonic-orchestra"){
        selected_company = "Calgary Philharmonic Orchestra";
      }
      if (selected_company == "downstage"){
        selected_company = "Downstage";
      }
      if (selected_company == "one-yellow-rabbit"){
        selected_company = "One Yellow Rabbit";
      }
      if (selected_company == "theatre-calgary"){
        selected_company = "Theatre Calgary";
      }

      if (selected_company != ""){
        if (post.company != selected_company){
          selected = false;
        }
      }
      //console.log("checking entered keyword match? " + entered_keyword);
      if(entered_keyword != ""){
        if (post.name.toLowerCase().indexOf(entered_keyword) < 0 && post.content.toLowerCase().indexOf(entered_keyword) < 0){
          selected = false;
        }
        else selected = true;
      }

      if (selected_series != ""){
        if (post.series != selected_series){
          selected = false;
        }
      }
      if (selected_genre != ""){
        if (post.tags.length == 0){
          console.log("no tags found for this EVENT!");
          selected = false;
        }
      }

      if(post.name.indexOf("Disney") >= 0 ){
        console.log("Disney test");
      }
      if (selected == true) {
        upl.push(post);
      }
    });


    console.log("4. filterList() called | eventData_filtering_v3.html | category : " + category + ", selected_genre: " + selected_genre + ", selected_venue: " + selected_venue + ", selected_company : " + selected_company);
    console.log("4.1 filterList() filters date_sorted_post_list[] and returns upl[] :" + upl.length + " events." );
    console.groupEnd();

    return upl;
  }

  function searchAgain(){
    shown_count = 0;
    loadMore('v','None', selected_company, 12, 'color_theme', selected_series, '#GlobalList');
    UpdateTitle();
  }
  
  
  
  
  // Date Filtering
  function getNextYearEvents(){    
    global_start = 1714876341754;
    console.log("--global_start : " + global_start);
    global_end = moment(today).add(10, 'Y').endOf('day');    
    console.log("getNextYearEvents | global_start : " + global_start + " | global_end : " + global_end);
    loadMore('v','None', selected_company, 12, 'color_theme', selected_series, '#GlobalList');
    UpdateTitle();
  }
  function getNextMonthEvents(){
    global_start = 1714876341754;
    console.log("--global_start : " + global_start);
    global_end = moment(today).add(1, 'M').endOf('day');
    console.log("getNextMonthEvents | global_start : " + global_start + " | global_end : " + global_end);
    loadMore('v','None', selected_company, 12, 'color_theme', selected_series, '#GlobalList');
    UpdateTitle();
  }
  function getNextWeekEvents(){
    global_start = 1714876341754;
    console.log("--global_start : " + global_start);
    global_end = moment(today).add(1, 'W').endOf('day');
    console.log("getNextWeekEvents | global_start : " + global_start + " | global_end : " + global_end);
    loadMore('v','None', selected_company, 12, 'color_theme', selected_series, '#GlobalList');
    UpdateTitle();
  }
  function getTodayEvents(){
    global_start = 1714876341754;
    console.log("--global_start : " + global_start);
    global_end = today.endOf('day');
    console.log("getTodayEvents | global_start : " + global_start + " | global_end : " + global_end);
    loadMore('v','None', selected_company, 12, 'color_theme', selected_series, '#GlobalList');
    UpdateTitle();
  }

  function scrollToTopNow(){
    $('html, body').animate({scrollTop:(0)}, 1000);    
  }
  //Genre / Hubspot Tag URL
  $('input[name="event_genre"]').click(function(){  
    selected_genre = $(this).attr("data-genre");
    console.log("1. Genre clicked : " + selected_genre);
    searchAgain();
    scrollToTopNow();
  });
  //Venue
  $('input[type=radio][name=event_venue]').click(function() {
    selected_venue = $(this).val();
    searchAgain();
    scrollToTopNow();
  });
  //Company
  $('input[type=radio][name=event_company]').click(function() {
    selected_company =  $(this).val();
    searchAgain();
    scrollToTopNow();
  });
  //Series
  $('input[type=radio][name=event_series]').click(function() {
    selected_series =  $(this).attr("data-series");
    searchAgain();
    scrollToTopNow();
  });

  //Search
  $( "#filter-search" ).submit(function( event ) {
    console.log( "Handler for .submit() called." + $(this).find("input").val());
    entered_keyword = $(this).find("input").val().toLowerCase();    
    searchAgain();    
    event.preventDefault();    
  });
  $( "#filter-search input" ).keyup(function(e) {
    console.log("val --> " + $(this).val() + " | " + $(this).val().length);    



    if($(this).val().length == 0){
      console.log("clear result");
      entered_keyword = "";    
      searchAgain();      
    }
    else if(e.keyCode == 46 || e.keyCode == 8){
      // Delete Key or Backspace   
    }
    else{
      entered_keyword = $(this).val().toLowerCase();    
      searchAgain();        
    }
  });

  function showFilters(){
    $('.filter-column').each(function(){
      $(this).removeClass('hidden');
    });
    $('.lmbtn').each(function(){
      $(this).removeClass('hidden');
    });
  }

</script>

<!-- End partial --></div>
