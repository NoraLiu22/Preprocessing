	<script type="text/javascript">
    ;(function ($) {
      // creates/adds scrolled classes
      $(window).scroll(function () {
        if ($(window).scrollTop() > 100) {
          $("body").addClass('scrolled');
        } else { $("body").removeClass('scrolled');}
      });
      $('.col').has('.col').addClass('parent');
      $('.columns').has('.columns').addClass('parent');
      $('.row').not(':has(.columns)').addClass('has-no-columns');

      // Slicknav callback to create mobile menu
      $(function(){
        $('#main-menu-container .box div > ul.menu').slicknav({
          allowParentLinks: true,
          nestedParentLinks: false,
          afterOpen: function(){
            $('body').keyup(function() {
              if (!$('.slicknav_menu').has(document.activeElement).length == 1) {
                $('.slicknav_nav').css('display','none');
                $('.slicknav_btn').removeClass('slicknav_open');
                $('.slicknav_nav').addClass('slicknav_hidden');
                $('.slicknav_btn').removeClass('slicknav_collapsed');
              }
            });
          }
        });
      });

      //Adds keyboard accessibility support to main menu
      $(function () {
        $("#main-menu > ul > li.expanded > a").each(function(){
          var labelText = $(this).text();
          $(this).after("<a href='#' class='add-subnav-exposed'>expand "+labelText+" submenu</a>");
        });

        $("a.add-subnav-exposed").click(function() {
          $(this).parent().toggleClass("focused");
          if ($(this).parent().hasClass("focused")) {
            $("#main-menu > ul").keyup(function(e){
              if(e.keyCode == 9){
                if(!$(".expanded.focused > ul",this).has(document.activeElement).length == 1){
                  $(".expanded.focused").removeClass("focused");
                }
              }
              else if(e.keyCode == 27){
                $(".expanded.focused").removeClass("focused");
              }
            });
          }
          return false;
        });
      });
    }(jQuery));
  </script>

