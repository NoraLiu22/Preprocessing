, Reflect—and Try Out New Ingredients","tags":{"nodes":[{"name":"Cakes","uri":"/tags/cakes/","slug":"cakes"},{"name":"Food","uri":"/tags/food/","slug":"food"},{"name":"Gluten-Free","uri":"/tags/gluten-free/","slug":"gluten-free"},{"name":"Recipe Roundups","uri":"/tags/recipe-roundups/","slug":"recipe-roundups"},{"name":"Recipes","uri":"/tags/recipes/","slug":"recipes"},{"name":"roundup","uri":"/tags/roundup/","slug":"roundup"},{"name":"Spring","uri":"/spring-recipes","slug":"spring"}]},"slug":"passover-desserts-recipe"},{"id":"cG9zdDoxMzExNDY=","databaseId":131146,"authorTags":{"nodes":[{"name":"Ellen Gray","uri":"/authors/ellen-gray/","count":37}]},"categories":{"nodes":[{"uri":"/category/recipes/","name":"Recipes","slug":"recipes","databaseId":16,"parentDatabaseId":null},{"uri":"/category/recipes-by-course/","name":"Recipes by Course","slug":"recipes-by-course","databaseId":5608,"parentDatabaseId":16},{"uri":"/category/desserts/","name":"Desserts","slug":"desserts","databaseId":5640,"parentDatabaseId":5608},{"uri":"/category/recipes-by-ingredient/","name":"Recipes by Ingredient","slug":"recipes-by-ingredient","databaseId":5659,"parentDatabaseId":16},{"uri":"/category/fruit/","name":"Fruit","slug":"fruit","databaseId":5670,"parentDatabaseId":5659},{"uri":"/category/rhubarb/","name":"Rhubarb","slug":"rhubarb","databaseId":5695,"parentDatabaseId":5670},{"uri":"/category/vegetables/","name":"Vegetables","slug":"vegetables","databaseId":5759,"parentDatabaseId":5659},{"uri":"/category/beets/","name":"Beets","slug":"beets","databaseId":5764,"parentDatabaseId":5759},{"uri":"/category/recipes-by-cuisine/","name":"Recipes by Cuisine","slug":"recipes-by-cuisine","databaseId":5787,"parentDatabaseId":16},{"uri":"/category/european/","name":"European","slug":"european","databaseId":5836,"parentDatabaseId":5787}]},"primaryCategory":{"uri":"/category/recipes/","name":"Recipes","slug":"recipes","databaseId":16,"parentDatabaseId":null},"excerpt":"\u003cp\u003eFor this year’s Passov