    $( document ).ready(function() {
        function handleJumpLink(hashString) {
            var urlFragmentElement = $(hashString);
            if (urlFragmentElement.length > 0) {
                var scrollOffset = urlFragmentElement.offset().top;
                scrollOffset -= 60;
                $("html, body").animate({
                    scrollTop: scrollOffset
                }, 800);
            }
        }
        handleJumpLink(location.hash);

        $(document).on('click', '#toc__section li a', function () {
            handleJumpLink($(this).prop("hash"));
        });

        var deviceType = getDeviceTypeInNumber(window.deviceType);
        if ( deviceType !== 3 ) {
            $(document).on('click', '.toc__show-more span,.toc__fade', function () {
                $('.toc__wrap').toggleClass('show-show-more');
                $('.toc__show-more span').text(function(i, text){
                    return text === "Show more" ? "Show less" : "Show more";
                })
            });
            if($('ul#toc__section li').length >= 6 && $('.toc__wrap').hasClass('hide-show-more')){
                $('.toc__wrap').removeClass('hide-show-more');
                $('.toc__wrap').addClass('show-show-more');
            } else{
                $('.toc__wrap').addClass('hide-show-more');
            }
        } else {
            $('.toc__wrap').removeClass('show-show-more');
            $('.toc__wrap').addClass('hide-show-more');

            $('.toc__title').on('click', function () {
                $('.toc__toggle-icon').toggleClass('toggleClosed');
                $('.toc__inner-wrap').slideToggle();
            });
        }
    });

