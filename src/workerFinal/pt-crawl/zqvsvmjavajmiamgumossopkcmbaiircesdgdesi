<script id="crdl_script-js-after" type="text/javascript">
window.crdlSaveEmail = function(params, shouldSendDatalayerEvent = false){
    var fetchUrl = `${window.faglobal.site_url}/wp-json/mmp/v1/saveLead`;
    var isProduction = parseInt(window.faglobal.is_production, 10) > 0;
	var dataParams =  {
        email_id : params.email,
        first_name: params.firstName,
        last_name: params.lastName,
        source : params.source,
        vertical: 'advisor_us' || 'Advisor_us',
    };
    var interest = window.newsletterMailingList ||  "Forbes-Advisor-US-Weekly-Newsletter";
    if(interest){
        dataParams['interest'] = interest;
    }
    var user_id =  __mpctc.userid();
    if(user_id){
        dataParams['user_id'] = user_id;
    }
    if(__mpResponseHeaders['x-city-code'] || __mpResponseHeaders['x-country-code']){
        dataParams['comment'] = {
            "city" : __mpResponseHeaders['x-city-code'],
            "country": __mpResponseHeaders['x-country-code']
        };
    }
    dataParams['comment']['page_url'] = window.location.href;
    dataParams['comment']['article_title'] = 'How Much Does It Cost To Start An LLC? (2024 Guide)';
    dataParams['comment']['title'] = params.title;
    if(params.internalTitle){
        dataParams['comment']['internalTitle'] = params.internalTitle;
    }
    var visitor_id =  __cookieController.get('client_id');
    if(visitor_id){
        dataParams['visitor_id'] = visitor_id;
    }
	if(!isProduction){
		dataParams["noauthfilter1"] = 1
	}
    $.ajax({
        url: fetchUrl,
        method: 'POST',
        data: dataParams,
        success: function(response){
            if(response && shouldSendDatalayerEvent){
                window.eventOnSuccess(response.success, response.data, params.scrollDepth);
            }
        },
        error: function(xhr, status, error){
            let response = xhr.responseJSON
            if(shouldSendDatalayerEvent){
                window.eventOnSuccess(response.success, response.data, params.scrollDepth);
            }
        }
    });
}
</script>
