<ul><li><sup><small>1</small></sup> with <a href="/wiki/Hungary" title="Hungary">Hungary</a></li>
<li><sup><small>2</small></sup> with <a href="/wiki/France" title="France">France</a>, <a href="/wiki/Germany" title="Germany">Germany</a>, <a href="/wiki/Italy" title="Italy">Italy</a>, <a href="/wiki/Slovenia" title="Slovenia">Slovenia</a> and <a href="/wiki/Switzerland" title="Switzerland">Switzerland</a></li>
<li><sup><small>3</small></sup> with <a href="/wiki/Albania" title="Albania">Albania</a>, <a href="/wiki/Belgium" title="Belgium">Belgium</a>, <a href="/wiki/Bosnia_and_Herzegovina" title="Bosnia and Herzegovina">Bosnia and Herzegovina</a>, <a href="/wiki/Croatia" title="Croatia">Croatia</a>, <a href="/wiki/Czech_Republic" title="Czech Republic">Czech Republic</a>, <a href="/wiki/France" title="France">France</a>, <a href="/wiki/Germany" title="Germany">Germany</a>, <a href="/wiki/Italy" title="Italy">Italy</a>, <a href="/wiki/North_Macedonia" title="North Macedonia">North Macedonia</a>, <a href="/wiki/Poland" title="Poland">Poland</a>, <a href="/wiki/Romania" title="Romania">Romania</a>, <a href="/wiki/Slovakia" title="Slovakia">Slovakia</a>, <a href="/wiki/Slovenia" title="Slovenia">Slovenia</a>, <a href="/wiki/Spain" title="Spain">Spain</a>, <a href="/wiki/Switzerland" title="Switzerland">Switzerland</a> and <a href="/wiki/Ukraine" title="Ukraine">Ukraine</a></li></ul>
</div></td></tr></tbody></table></div>
