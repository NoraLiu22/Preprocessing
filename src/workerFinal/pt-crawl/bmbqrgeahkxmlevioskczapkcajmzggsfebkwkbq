	<script type="text/javascript">
		// Helper to grab URL query parameters and convert to js object
		function getQueryParameters()
		{
			var vars = [], hash;
			var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
			for(var i = 0; i < hashes.length; i++)
			{
				hash = hashes[i].split('=');
				vars.push(hash[0]);
				vars[hash[0]] = hash[1];
			}
			return vars;
		}
		
		// Grab the utm email if it exists
		var queryParamUtmEmail = decodeURIComponent(getQueryParameters()['utm_email']);

		// Set the Nosto email tag with email query param
		if(queryParamUtmEmail != 'undefined'){
			$(".nosto_customer").find(".email").text(queryParamUtmEmail);
            $(".nosto_customer").find(".marketing_permission").text('true');
		} else {
            $(".nosto_customer").find(".marketing_permission").text('false');
        }
	</script>

	<script type="text/javascript">
	$(document).ready(function(){
		var tagMetaOgTitle = $("meta[property='og:title']").attr("content");
		var tagMetaOgType = $("meta[property='og:type']").attr("content");
		var tagMetaOgDescription = $("meta[property='og:description']").attr("content");
		var tagMetaOgUrl = $("meta[property='og:url']").attr("content");
		var tagMetaOgSiteName = $("meta[property='og:site_name']").attr("content");
		var tagMetaOgCategory = $("meta[property='og:category']").attr("content");
		var tagMetaOgSubCategory = $("meta[property='og:sub-category']").attr("content");
		var tagMetaOgAges = $("meta[property='og:ages']").attr("content");
		var tagMetaOgBrands = $("meta[property='og:brands']").attr("content");
		var tagMetaProductPriceAmount = $("meta[property='product:price:amount']").attr("content");
		var tagMetaSku = $('.skuitemnumber').text();
		var tagMetaPresale = $('.product.mainContentWell').data("presale");

		//myscienceshop.com Page Visit
		olytics.fire({
			behaviorId : '8898I1345578A0H',
			category : tagMetaOgCategory ? tagMetaOgCategory : '',
			tag : '',
			entitlements : '',
			type : tagMetaOgType ? tagMetaOgType : '',
			title : tagMetaOgTitle ? tagMetaOgTitle : '',
			description : tagMetaOgDescription ? tagMetaOgDescription : '',
			url : tagMetaOgUrl ? tagMetaOgUrl : '',
			sitename : tagMetaOgSiteName ? tagMetaOgSiteName : '',
			publishedtime : '',
			pageaccess : '',
			subcategory : tagMetaOgSubCategory ? tagMetaOgSubCategory : '',
			ages : tagMetaOgAges ? tagMetaOgAges : '',
			brands : tagMetaOgBrands ? tagMetaOgBrands : '',
			productprice : tagMetaProductPriceAmount ? tagMetaProductPriceAmount : '',
			ecompresale : tagMetaPresale ? tagMetaPresale : '',
			modifiedtime : '',
			slug : '',
