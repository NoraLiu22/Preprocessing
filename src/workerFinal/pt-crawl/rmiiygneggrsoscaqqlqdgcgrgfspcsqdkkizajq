<script>
  var adWrappers = document.getElementsByClassName('ad-bottom');
  
  var getAdWrapper = () => {
    if (adWrappers.length === 1) {
      return adWrappers[0];
    } else {
      for (adWrapper of adWrappers) {
        var childNodes = adWrapper.children;
        if (childNodes.length === 0) {
          var grandParentNode = adWrapper.parentNode.parentNode;
          var isVisibleNode = !(grandParentNode.style.display === 'none')
          if (isVisibleNode) {
            return adWrapper;
          }
        }
      }
    }
  }

  var adWrapper = getAdWrapper()

  if (adWrapper) {
    var adUnit = document.createElement('div');
    if (isSovrn) {
      adUnit.classList.add('dictionary_content_728x90_noflex_1');
      adUnit.setAttribute('data-text', 'Sovrn Advertisement');
      adWrapper.appendChild(adUnit);

      var hasWindow = typeof window !== 'undefined';
      var hasProperBuildSlots = hasWindow && typeof window.properBuildSlots !== 'undefined';
      if (hasProperBuildSlots) {
        window.properBuildSlots();
      };
    } else {
      adUnit.id = 'crb-ad-slot-bottom'
      adUnit.setAttribute('data-text', 'GPT Advertisement');
      var adContainer = document.createElement('div');
      adContainer.classList.add("ad__container");
      if ("") {
        adContainer.classList.add("");
      }
      adContainer.id = "crb-ad-container-bottom";
      adContainer.setAttribute('data-text', 'GPT Advertisement');
  
      var adContainerInner = document.createElement('div');
      adContainerInner.classList.add('ad__container-inner');
      adContainerInner.id = "crb-ad-container-inner-bottom";
      adContainerInner.setAttribute('data-text', 'GPT Advertisement');
  
              var mainShell = document.createElement('div');
        mainShell.classList.add('main');
        mainShell.classList.add('shell');
        var contentDiv = document.createElement('div');
        contentDiv.classList.add('content');
        contentDiv.appendChild(adUnit);
        mainShell.appendChild(contentDiv);
        adContainerInner.appendChild(mainShell);
        
      adContainer.appendChild(adContainerInner);
      adWrapper.appendChild(adContainer);
    }
  }
</script>

