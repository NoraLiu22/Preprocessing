$(document).ready(function() {

// properties
var props = {
  type: "uaex-home-article",
  feed: "/_resources/feeds/extension-news.xml",                                    
  numberToDisplay: 3
};                 
                      
                      
var params_to_send = { 
   type: props.type, feed: props.feed, numberToDisplay: props.numberToDisplay
};

$.ajax({
url : "/_resources/wnl/rss.ashx",	
data: params_to_send,
success: function(data){    
	$('#uaex-home-article-httpswwwuadaeduresourcesfeedsextensionnewsxml').before(data);
	$('#uaex-home-article-httpswwwuadaeduresourcesfeedsextensionnewsxml').remove();
	
}
});

});			
