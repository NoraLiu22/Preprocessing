urceUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;callToActionUrl&quot;:null,&quot;captionCredit&quot;:&quot;Mohammed Salem\/World Press Photo via AP&quot;,&quot;altText&quot;:&quot;Mohammed Salem won the World Press Photo Award of the Year&quot;,&quot;callToActionText&quot;:null,&quot;width&quot;:2519,&quot;caption&quot;:&quot;Mohammed Salem won the World Press Photo Award of the Year&quot;,&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/08\/38\/17\/26\/{{w}}x{{h}}_cmsv2_e76e320e-eb65-52b1-952a-ec3aa72e5b5f-8381726.jpg&quot;,&quot;captionUrl&quot;:null,&quot;height&quot;:3778}],&quot;authors&quot;:{&quot;journalists&quot;:[{&quot;urlSafeValue&quot;:&quot;mouriquand&quot;,&quot;twitter&quot;:null,&quot;title&quot;:&quot;David Mouriquand&quot;}],&quot;producers&quot;:[],&quot;videoEditor&quot;:[]},&quot;keywords&quot;:[{&quot;urlSafeValue&quot;:&quot;photography&quot;,&quot;titleRaw&quot;:&quot;Photography&quot;,&quot;id&quot;:4147,&quot;title&quot;:&quot;Photography&quot;,&quot;slug&quot;:&quot;photography&quot;},{&quot;urlSafeValue&quot;:&quot;israel-hamas-war&quot;,&quot;titleRaw&quot;:&quot;Israel Hamas war&quot;,&quot;id&quot;:29226,&quot;title&quot;:&quot;Israel Hamas war&quot;,&quot;slug&quot;:&quot;israel-hamas-war&quot;},{&quot;urlSafeValue&quot;:&quot;israel&quot;,&quot;titleRaw&quot;:&quot;Israel&quot;,&quot;id&quot;:157,&quot;title&quot;:&quot;Israel&quot;,&quot;slug&quot;:&quot;israel&quot;},{&quot;urlSafeValue&quot;:&quot;palestine&quot;,&quot;titleRaw&quot;:&quot;Palestine&quot;,&quot;id&quot;:13122,&quot;title&quot;:&quot;Palestine&quot;,&quot;slug&quot;:&quot;palestine&quot;},{&quot;urlSafeValue&quot;:&quot;world-press-photo&quot;,&quot;titleRaw&quot;:&quot;World Press Photo&quot;,&quot;id&quot;:13888,&quot;title&quot;:&quot;World Press Photo&quot;,&quot;slug&quot;:&quot;world-press-photo&quot;}],&quot;widgets&quot;:[{&quot;count&quot;:6,&quot;slug&quot;:&quot;image&quot;}],&quot;related&quot;:[{&quot;id&quot;:2529118}],&