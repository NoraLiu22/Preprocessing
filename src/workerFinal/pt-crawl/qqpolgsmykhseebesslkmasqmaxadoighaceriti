<script>
var j = jQuery.noConflict();
j(document).ready(function(){
	if(j('#theOtherButton').is(':checked')) { j('#theOtherInput').show(); j('#theOtherInput').focus(); }
		else { j('#theOtherInput').hide(); }
	
	j('input[type=radio][name=amt]').change(function() {
		if(j('#theOtherButton').is(':checked')) { j('#theOtherInput').show(); j('#theOtherInput').focus(); }
		else { j('#theOtherInput').hide(); }
	});
	
	j('#startButton').click(function() {
		var amtFinal;
		if(j('#theOtherButton').is(':checked')) {
			var amtHold = j('#theOtherInput').val();
			amtFinal = amtHold.replace(/[^\d.]/g, '');
			j('#passAmt').val(amtFinal);
			j('form#giveStart').submit();
		}
		else if (j('input[name=amt]:checked').val() != undefined) {
			amtFinal = j('input[name=amt]:checked').val();
			j('#passAmt').val(amtFinal);
			j('form#giveStart').submit();
		}
		else {
			j('.error').show();
		}
	});

});
</script>    </main><!-- /.main -->
    
