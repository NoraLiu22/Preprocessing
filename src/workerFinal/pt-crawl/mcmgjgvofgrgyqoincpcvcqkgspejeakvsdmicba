    <script nonce="80547988964499906" type="module">
        import { getCLS, getFID, getLCP } from '/static/js6/web-vitals.js';

        const queue = new Set();
        function addToQueue(metric) {
            queue.add(metric);
        }
        getCLS(addToQueue);
        getFID(addToQueue);
        getLCP(addToQueue);

        function getSelector(node, maxLen = 100) {
            if (!node) return 'null';
            let sel = '';
            try {
                while (node && node.nodeType !== 9) {
                    const part = node.id ? '#' + node.id : node.nodeName ? node.nodeName.toLowerCase() : '' + (
                        (node.className && node.className.length) ?
                            '.' + Array.from(node.classList.values()).join('.') : '');
                    if (sel.length + part.length > maxLen - 1) return sel || part;
                    sel = sel ? part + '>' + sel : part;
                    if (node.id) break;
                    node = node.parentNode;
                }
            } catch (err) {
                //console.log(err);
            }
            return sel || 'empty';
        }

        const locationHref = window.location.href;
        function flushQueue() {
            if (queue.size > 0 && navigator.sendBeacon) {
                let cls = null, fid = null, lcp = null, lcpElement = '', lcpUrl = '';
                for (let metric of queue) {
                    let delta = Math.round(metric.name === 'CLS' ? metric.delta * 1000 : metric.delta);
                    switch (metric.name) {
                        case 'CLS':
                            cls = delta;
                            break;
                        case 'FID':
                            fid = delta;
                            break;
                        case 'LCP':
                            lcp = delta;

                            const lastEntry = metric.entries[metric.entries.length - 1];
                            lcpElement = getSelector(lastEntry.element);
                            lcpUrl = lastEntry.url;

                            if (typeof gtag !== 'undefined') {
                                gtag('event', 'web_vitals', {
                                    metricName: metric.name,
                                    metricValue: delta,
                                    metricId: metric.id
                                });
                            }
                            break;
                    }
                }
                if (lcp) {
                    const payload = {
                        cls: cls,
                        fid: fid,
                        lcp: lcp,
                        lcpElement: lcpElement,
                        lcpUrl: lcpUrl,
                        isCacheable: !!$('body').data('cdn-cacheable'),
                        url: locationHref,
                        version: 2
                    };
                    console.log(payload);
                    globalThis.navigator.sendBeacon(`/CoreWebVitals?${new URLSearchParams(payload)}`);
                }
                queue.clear();
            }
        }
        addEventListener('visibilitychange', () => {
            if (document.visibilityState === 'hidden') flushQueue();
        });
        addEventListener('pagehide', flushQueue); //safari :(
    </script>


</body>
</html>


