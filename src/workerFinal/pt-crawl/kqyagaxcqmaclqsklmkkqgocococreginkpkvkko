<script>// tab interface is handled with jquery-accessible-hide-show-aria.js
jQuery(function($){
    var current_url = location.href;

    $(document).on('click', '.share-controls button', function(event){
        var input = $(event.currentTarget).
            parents('.share-form-row').
            find('input')[0];
        if(input){
            input.focus();
            input.setSelectionRange(0, input.value.length);
        }
    });

    function update_share_controls(){
        current_url = location.href;
        if(!location.search){
            current_url += '?';
        }
        $('.share-controls input.share-media-embed').attr(
            'value',
            '<frame src="' + current_url + '&embed=resources" ' +
                'width="697" height="298" frameborder="0" ' +
                'scrolling="no"></iframe>'
        );
        $('.share-controls input.share-media-link').attr(
            'value',
            current_url
        );
    };

    setInterval(function(){
        if(current_url != location.href){
            update_share_controls();
        }
    }, 1000);
});
</script>


