<script>jQuery(function($){

    // This is a critical fallback for now.  Chrome and firefox use hls.js
    // and hls.js stiches together our .ts a/v segments without audio drift
    // but... safari native hls doesnt
    var source_node = $(".audio-player source");

    // Copy the title attribute of the first video source element to the
    // video element because on safari if there is only one source element
    // safari will remove it an put the url directly into the audio source
    // element, but not copy the title attribute (which we need for metrics)
    $(".audio-player").attr(
        'title',
        source_node.attr('title')
    );

    var mediaelement = $('.audio-player').mediaelementplayer({
        audioVolume: 'vertical',
        autoStartLoad: true,
        loop: false,
        
        success: function(media, node){
            // Bind all of our metrics tracking handlers
            var title = $('source', media).length ?
                $('source', media).get(0).title:
                $('audio', media).get(0).title;
            UX.track_player(media, 'mediaElement', title, true);

            media.addEventListener('loadedmetadata', function() {
               //console.log('loadedmetadata %s', arguments);
            }, true);
            media.addEventListener('progress', function() {
               //console.log('progress %s', arguments);
            }, true);
            media.addEventListener('timeupdate', function() {
               //console.log('timeupdate %s', arguments);
            }, true);
            media.addEventListener('seeking', function() {
               //console.log('seeking %s', arguments);
            }, true);
            media.addEventListener('seeked', function() {
               //console.log('seeked %s', arguments);
            }, true);
            media.addEventListener('canplay', function() {
                //console.log('canplay %s', arguments);
            }, true);
            media.addEventListener('play', function() {
                //console.log('play %s', arguments);
                jQuery(document).trigger('audio-player-play', [media, node]);
            }, true);
            media.addEventListener('playing', function() {
               //console.log('playing %s', arguments);
            }, true);
            media.addEventListener('pause', function() {
                //console.log('pause %s', arguments);
                jQuery(document).trigger('audio-player-pause', [media, node]);
            }, true);
            media.addEventListener('ended', function() {
               //console.log('ended %s', arguments);
            }, true);
            media.addEventListener('volumechange', function() {
               //console.log('volumechange %s', arguments);
            }, true);
            media.addEventListener('captionschange', function() {
               //console.log('captionschange %s', captionschange);
            }, true);

            setTimeout(function(){
                $(document).trigger('media-player-ready', [media, node]);
            }, 100);
        }
    });
});
</script>

