                    <div class="diagram svg-diagram">
                        <h3>Publications by Year</h3>
                        <div class="diagram se-diagram" aria-label="publications by year"></div>
                        
    <style>
        rect:hover {
            fill: steelblue;
        }
        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }
        .bar {
            fill: steelblue;
        }
        a:hover .bar, a:focus .bar {
            stroke: black;
            fill-opacity: 60%;
        }
    </style>
    
        <script src="/static/js/d3.v3.min.js"></script>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".se-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1960, frequency:2 },{ year:1961, frequency:8 },{ year:1962, frequency:6 },{ year:1964, frequency:6 },{ year:1965, frequency:8 },{ year:1966, frequency:6 },{ year:1967, frequency:6 },{ year:1968, frequency:9 },{ year:1969, frequency:3 },{ year:1970, frequency:13 },{ year:1971, frequency:14 },{ year:1972, frequency:15 },{ year:1973, frequency:10 },{ year:1974, frequency:18 },{ year:1975, frequency:6 },{ year:1976, frequency:13 },{ year:1977, frequency:15 },{ year:1978, frequency:19 },{ year:1979, frequency:15 },{ year:1980, frequency:15 },{ year:1981, frequency:18 },{ year:1982, frequency:28 },{ year:1983, frequency:9 },{ year:1984, frequency:24 },{ year:1985, frequency:26 },{ year:1986, frequency:18 },{ year:1987, frequency:9 },{ year:1988, frequency:20 },{ year:1989, frequency:16 },{ year:1990, frequency:9 },{ year:1991, frequency:19 },{ year:1992, frequency:17 },{ year:1993, frequency:10 },{ year:1994, frequency:19 },{ year:1995, frequency:5 },{ year:1996, frequency:10 },{ year:1997, frequency:9 },{ year:1998, frequency:23 },{ year:1999, frequency:18 },{ year:2000, frequency:12 },{ year:2001, frequency:13 },{ year:2002, frequency:10 },{ year:2003, frequency:14 },{ year:2004, frequency:11 },{ year:2005, frequency:13 },{ year:2006, frequency:12 },{ year:2007, frequency:17 },{ year:2008, frequency:19 },{ year:2009, frequency:19 },{ year:2010, frequency:15 },{ year:2011, frequency:17 },{ year:2012, frequency:11 },{ year:2013, frequency:13 },{ year:2014, frequency:15 },{ year:2015, frequency:18 },{ year:2016, frequency:13 },{ year:2017, frequency:15 },{ year:2018, frequency:11 },{ year:2019, frequency:17 },{ year:2020, frequency:20 },{ year:2021, frequency:22 },{ year:2022, frequency:19 },{ year:2023, frequency:20 },{ year:2024, frequency:7 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=se%3A33+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Publication" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

