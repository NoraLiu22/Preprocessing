<tr>
    <th><label for="id_title">Original title:</label></th>
    <td>
        

        <input type="text" name="title" placeholder="Type here all or part of the title of the work" id="id_title">

<script type="text/javascript">
    window.onload = function() {
        if (!('placeholder' in document.createElement('input'))) {
            var input = document.getElementById('id_title');
            input.value = 'Type here all or part of the title of the work';

            input.onblur = function() {
                if (this.value == '')
                    this.value='Type here all or part of the title of the work';
            };

            input.onfocus = function() {
                if (this.value == 'Type here all or part of the title of the work')
                    this.value = '';
            };
        }
    };
</script>

        
        
    </td>
</tr>


            
                
                

