    <th><label for="id_parts_title">Section title:</label></th>
    <td>
        

        <input type="text" name="parts_title" placeholder="Type here the partial or complete title of a part of the work" id="id_parts_title">

<script type="text/javascript">
    window.onload = function() {
        if (!('placeholder' in document.createElement('input'))) {
            var input = document.getElementById('id_parts_title');
            input.value = 'Type here the partial or complete title of a part of the work';

            input.onblur = function() {
                if (this.value == '')
                    this.value='Type here the partial or complete title of a part of the work';
            };

            input.onfocus = function() {
                if (this.value == 'Type here the partial or complete title of a part of the work')
                    this.value = '';
            };
        }
    };
</script>

        
        
