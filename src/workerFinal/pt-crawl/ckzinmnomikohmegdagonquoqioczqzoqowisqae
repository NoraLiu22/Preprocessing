        <div class="widget widget-dynamic FiguresTablesCss" data-count="2">
    
        <div class="widget-dynamic__header">
            <h3>
                Data &amp; Figures
                <div class="widget-dynamic__underbar journal-bg"></div>
            </h3>
        </div>

    <div class="widget-dynamic-inner-wrap">

