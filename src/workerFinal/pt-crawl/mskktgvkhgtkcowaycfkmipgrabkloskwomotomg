<script>
function showAdv() {
  $('#advsearch').toggle();
  $('#isadv').val($('#advsearch').is(":visible")?"yes":"no");
}
function showHelp() {
  $('#searchhelp').toggle();
}
function btnSubmit() {
  $("#fam").val("");
  $("#pgno").val(1);
  $("#avbform").submit();
}
</script>

