              <source srcset="/sites/default/files/styles/text_image_single/public/content-image/2023-09/raul_tolosa_vereda_sabanas_de_la_fuga_credit_luis_cano_-_wwf_colombia_1.jpg?itok=EdB0RCx3 1x" media="all and (min-width: 25em)" type="image/jpeg" width="636" height="424">
