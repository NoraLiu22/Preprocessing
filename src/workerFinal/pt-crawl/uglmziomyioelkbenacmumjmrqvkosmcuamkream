<meta name="DC.subject" content="Rate Control" xml:lang="es" />
<meta name="DC.title" content="Parallel Efficient Rate Control Methods for JPEG 2000" xml:lang="es" />
<meta name="DC.type" content="info:eu-repo/semantics/conferenceObject" xml:lang="es" />
<meta name="DC.type" content="info:eu-repo/semantics/submittedVersion" xml:lang="es" />
