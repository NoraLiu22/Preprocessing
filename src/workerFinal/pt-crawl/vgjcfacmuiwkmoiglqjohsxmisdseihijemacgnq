<script id="loopnav-a-script">
    var listA = document.getElementById('loopnav-a-script').parentNode.previousElementSibling;
    var divA = document.createElement('div');
    divA.classList.add('loopnav', 'loopnav-a');
    divA.appendChild(listA);
    var header = document.querySelector('.l-header');
    header.after(divA);
  
    var links = document.querySelectorAll('.loopnav-a li a');
    links.forEach(function(link) {
      link.dataset.analyticsViewport = 'loopnav';
      link.dataset.analyticsLink = 'loopnav';
    })

    var loopnavTime = Date.now();

    function sendViewEvent() {
      var loopnavDelay = 1000;

      if((loopnavTime + loopnavDelay - Date.now()) <= 0) { 
          if (window.scrollY >= 75) {
              var dataLayer = window.dataLayer || [];
              dataLayer.push({
                  event: 'Loopnav View'
              })
            window.removeEventListener('scroll', sendViewEvent);
          }

          loopnavTime = Date.now(); 
        } 
  }
     
    window.addEventListener('scroll', sendViewEvent); 
</script>

