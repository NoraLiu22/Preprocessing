<script type="text/javascript">
function carousel_initCallback(carousel) {
    // Disable autoscrolling if the user clicks the prev or next button.
    carousel.buttonNext.bind('click', function() {
        carousel.startAuto(0);
    });

    carousel.buttonPrev.bind('click', function() {
        carousel.startAuto(0);
    });

    // Pause autoscrolling if the user moves with the cursor over the clip.
    carousel.clip.hover(function() {
        carousel.stopAuto();
    }, function() {
        carousel.startAuto();
    });

    // Move buttons out of main container so they can be positioned above he carousel
    $('.jcarousel-next,.jcarousel-prev').appendTo('#carousel-wrap');

    // Add div for gradients on sides and to suppress hover effect
    $("#carousel-wrap").append("<div id='carousel-gradient-left'></div><div id='carousel-gradient-right'></div>");
}

    jQuery.noConflict();
    (function( $ ) {
        $(document).ready(function() {
            try {
                if (null != $('#carousel ul')) {
                    $('#carousel ul').jcarousel( {
                        auto: 4,
                        scroll: 1,
                        wrap: 'circular',
                        initCallback: carousel_initCallback,
                        animation: 2500,
                        easing: 'easeInOutCubic'
                    });
                    console.log("carousel ul: " + $('#carousel ul').jcarousel());
                }
            } catch(e1) {
                if (window.console) {
                    console.log("error with carousel ul, error is: " + e1 );
                }
            }

            try {
                $(".cq-carousel-link").mouseenter(function() {
                    $(this).find(".slide-details").slideDown();
                });

                $(".cq-carousel-link").mouseleave(function() {
                    $(this).find(".slide-details").slideUp();
                });
            } catch(e2) {
                if (window.console) {
                    console.log("error with cq-carousel-link, error is: " + e2 );
                }
            }
        });
    })(jQuery);
</script>


