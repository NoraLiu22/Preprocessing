	<script>
		function mediaQueryDesktop(desktopWidth) {
			if (desktopWidth.matches) {
				document.querySelector('.mid-section .title').innerHTML = 'Student and Corporate Subscriptions'
			} else {
				document.querySelector('.mid-section .title').innerHTML = 'Student and Corporate Subscriptions'
			}
		}
		var desktopWidth = window.matchMedia("(min-width: 1024px)")
		mediaQueryDesktop(desktopWidth)
		desktopWidth.addListener(mediaQueryDesktop)
	</script>

