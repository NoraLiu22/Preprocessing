&quot;,&quot;artist&quot;:&quot;Charlotte Day Wilson &amp; Snoh Aalegra&quot;,&quot;art_id&quot;:1834890262,&quot;preorder_download&quot;:1,&quot;album_id&quot;:3350628812,&quot;streaming&quot;:1,&quot;audio_track_duration&quot;:196.006,&quot;audio_url&quot;:{&quot;mp3-128&quot;:&quot;https://t4.bcbits.com/stream/4deb15ed75c7ec02a434ba790abdc8f3/mp3-128/238286581?p=0&amp;ts=1714979340&amp;t=e2858610d734a21df748f526416b59da2f829567&amp;token=1714979340_efc623ed2b6d7492a2db82938a559e8501444ce5&quot;},&quot;track_number&quot;:4},{&quot;track_id&quot;:2945771111,&quot;track_title&quot;:&quot;Canopy&quot;,&quot;artist&quot;:&quot;Charlotte Day Wilson&quot;,&quot;art_id&quot;:1834890262,&quot;preorder_download&quot;:1,&quot;album_id&quot;:3350628812,&quot;streaming&quot;:1,&quot;audio_track_duration&quot;:151.8,&quot;audio_url&quot;:{&quot;mp3-128&quot;:&quot;https://t4.bcbits.com/stream/7d99cdf1c7cf50333cc368b104f99d66/mp3-128/2945771111?p=0&amp;ts=1714979340&amp;t=a424953a1c7aac86442b147de233173b5a2ebe00&amp;token=1714979340_f0e6d08c2339ecbda91e88a25c42e537052182d2&quot;},&quot;track_number&quot;:8},{&quot;track_id&quot;:1321466165,&quot;track_title&quot;:&quot;I Don&#39;t Love You&quot;,&quot;artist&quot;:&quot;Charlotte Day Wilson&quot;,&quot;art_id&quot;:1834890262,&quot;preorder_download&quot;:1,&quot;album_id&quot;:3350628812,&quot;streaming&quot;:1,&quot;audio_track_duration&quot;:251.01,&quot;audio_url&quot;:{&quot;mp3-128&quot;:&quot;https://t4.bcbits.com/stream/1731d9798497d0706be36d93786047d5/mp3-128/1321466165?p=0&amp;ts=1714979340&amp;t=f46d80fd3115f4189e7ec4635da832a4c3709c25&amp;token=1714979340_3f01eb050e6144cfaf801d0739e75a57fe3034be&quot;},&quot;track_number&quot;:11}],&quot;size&quot;:&quot;s&quot;,&quot;is_merch&quot;:1,&quot;featured_track_number&quot;:11,&quot;art_id&quot;:1834890262,&quot;band_id&quot;:3759723135,&quot;band_name&quot;:&quot;Charlotte Day Wilson&quot;,&quot;band_image_id&quot;:33332764,&quot;band_url&quot;:&quot;https://charlotteday