     data-tooltips="{&quot;tooltips&quot;:[],&quot;dash_seen&quot;:null}">
    
    <div id="name-section" >
        <h2 class="trackTitle">
            everything is alive
            
        </h2>

        <h3 style="margin:0px;">by 
        <span> 
          <a href="https://slowdive.bandcamp.com">Slowdive</a>
          </span>
        
        </h3>

    

    
