            </div>
            
            
                <script type="text/javascript" nonce="wlEqw2BMZPxQu626STBqxg==">
                    $(".tralbum-credits").last().bcTruncate(TruncateProfile.get("tralbum_long"), "more", "less");
                </script>
            
                
            

            

