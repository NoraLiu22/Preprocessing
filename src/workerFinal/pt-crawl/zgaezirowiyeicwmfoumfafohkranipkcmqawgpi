<script>
                var App = App || {};
                App.ArticleSplitScreen = App.ArticleSplitScreen || {};
                App.ArticleSplitScreen.siteName = 'jamadermatology';
                App.ArticleSplitScreen.siteId = 12;
                App.ArticleSplitScreen.journalId = 68;
                App.ArticleSplitScreen.adDisplay = 'ArticleContentsSkyBox';
                App.ArticleSplitScreen.adSection = "scholarly";
                App.ArticleSplitScreen.adPosition = "contab";
                App.ArticleSplitScreen.controlId = "AMA_AdTag_Contents_Skybox";
                App.ArticleSplitScreen.fromGoogle = "False";
