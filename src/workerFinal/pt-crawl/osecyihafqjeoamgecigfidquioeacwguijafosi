	<meta itemprop="width" content="1877"/>
	<meta itemprop="height" content="1877"/>

	
	



	
	

		<div class="article-body-image__container">

			
			

			
				
				

	
	
	
	

	
	

	
	

	
	

	
	
	
	
	
	
	


	

	

	

	


	
	

	
	
	<picture class="article-body-image__source  ">
		
			<source srcset="/content/dam/fashion/2023/10/17/TELEMMGLPICT000353424738_16975502130890_trans_NvBQzQNjv4BqGqOnpus_Z0s4GBSeV2ZKDdeiXeeLUzAdXZYWlQaJ_g0.jpeg?imwidth=960" media="( min-width: 1280px )"/>
		
			<source srcset="/content/dam/fashion/2023/10/17/TELEMMGLPICT000353424738_16975502130890_trans_NvBQzQNjv4BqGqOnpus_Z0s4GBSeV2ZKDdeiXeeLUzAdXZYWlQaJ_g0.jpeg?imwidth=680" media="( min-width: 768px )"/>
		
			<source srcset="/content/dam/fashion/2023/10/17/TELEMMGLPICT000353424738_16975502130890_trans_NvBQzQNjv4BqGqOnpus_Z0s4GBSeV2ZKDdeiXeeLUzAdXZYWlQaJ_g0.jpeg?imwidth=480&imdensity=2" media="( min-width: 350px ) and ( -webkit-min-device-pixel-ratio: 2 )"/>
		
			<source srcset="/content/dam/fashion/2023/10/17/TELEMMGLPICT000353424738_16975502130890_trans_NvBQzQNjv4BqGqOnpus_Z0s4GBSeV2ZKDdeiXeeLUzAdXZYWlQaJ_g0.jpeg?imwidth=480" media="( min-width: 350px )"/>
		
