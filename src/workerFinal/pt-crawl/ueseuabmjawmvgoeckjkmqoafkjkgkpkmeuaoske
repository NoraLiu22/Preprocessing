      <div class="column">
        <a class="clr" href="/theme/genomics-lite-genomic-cartography-generating-a-human-cell-atlas/" data-wpel-link="internal">
          <div class="card" >
              <div class="card-img">
    <picture>
        <source type="image/webp" media="(max-width: 375px)" srcset="/wp-content/uploads/2024/03/GL24_05_CellGen1_website-768x292.png.webp">
        <source type="image/webp" media="(max-width: 768px)" srcset="/wp-content/uploads/2024/03/GL24_05_CellGen1_website-768x292.png.webp">
