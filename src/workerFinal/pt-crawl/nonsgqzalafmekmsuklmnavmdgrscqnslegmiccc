    <span data-js="country-selector__button-icon" class="all-sections-country-selector__icon-flag">
        <img src="/etc.clientlibs/settings/wcm/designs/telegraph/core/clientlibs/ui/dist/static/resources/images/flags/USA-SVG.svg" fetchpriority="high" role="presentation" class="all-sections-country-selector__icon-flag-link" data-test="country-selector-flag-icon"/>
    </span>
    <span data-js="country-selector__button-text" data-test="country-selector-button-text">
