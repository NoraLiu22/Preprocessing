 <script>
    //global grecaptcha
    grecaptcha.ready(function() {
      grecaptcha.execute('6LelrMMcAAAAAGjgfa_7vNO772YI9PNimZ2FDi0e', {action: "/subscribe/"}).then(function(token) {

        if (document.getElementById('g-recaptcha-token-full')){
            // subscribe
            document.getElementById('g-recaptcha-token-full').value = token;
        }
        if (document.getElementById('g-recaptcha-token')){
            // footer
            document.getElementById('g-recaptcha-token').value = token;
        }
        else if (document.getElementById('g-recaptcha-token-kids')){
            // kids/submit
            document.getElementById('g-recaptcha-token-kids').value = token;
        }
      });
    });
  </script>




