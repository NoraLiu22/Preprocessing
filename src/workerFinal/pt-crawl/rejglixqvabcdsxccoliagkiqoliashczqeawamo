<style>.multi-volume-selection {
  display: none;
}
.search-results.list-view .multi-volume-selection {
  display: block;
}
.multi-volume-selection .input-group-small label {
  font-size: 1rem !important;
}
</style>
<script>
function openWindow(location, parameters, page_count){
    // Done this way because IE11 doesn't support default
    // parameters.
    parameters = parameters || "noparams"; 
    if(location == "#")return;
    var parameters = parameters.split("&");
    var location = location;
    for (i=0; i < parameters.length; i++){
        if(parameters[i].includes("st") && page_count > 1){
            location = location + "?" + parameters[i];
        }
    }
    window.open(location,'_self');
}

jQuery(function($){
    var pathname = location.pathname,
        multivolume_options = $('select[name="multi-volume-select"] > option'),
        found = false;

    if(pathname.indexOf('/resource/') == -1){
        return;
    }

    $(multivolume_options).each(function(index, option){
        if(!found && $(option).attr('value').indexOf(pathname) > -1){
            found = true;
            $(option).attr('selected', 'selected');
        } else {
            $(option).removeAttr('selected');
        }
    });
});
</script>


<!-- multi volume prototype -->
<form name="" class='multi-volume-selection select-flex
      '
      
      style="margin: 5px 0;"
      >
    <div class="input-group-small">
    <label for="multivolume_8"
            class='label-bold
            '>
            Resources:</label>
    <div class='select-wrapper
        '>
    <select name="multi-volume-select"
        style="width: 100%; color: #242424;"
        class='sort select-default
        '
        id="multivolume_8">
        
            <option value="https://www.loc.gov/collections/world-digital-library/?fa=segmentof:gdclccn.ltf90022694v1/&amp;sb=shelf-id&amp;st=gallery"
                >Volume 1 - 629 pages</option>

            

            
                <option value="https://www.loc.gov/resource/gdclccn.ltf90022694v1/?st=pdf">Volume 1 - PDF</option>
            

            

            
                <option disabled></option>}
            
        
            <option value="https://www.loc.gov/collections/world-digital-library/?fa=segmentof:gdclccn.ltf90022694v2/&amp;sb=shelf-id&amp;st=gallery"
                >Volume 2 - 369 pages</option>

            

            
                <option value="https://www.loc.gov/resource/gdclccn.ltf90022694v2/?st=pdf">Volume 2 - PDF</option>
            

            

            
                
            
        
    </select>
    </div><button type="button"
        onclick="
            multivolume = multivolume_8;
            if(window.location.search.replace(/^[?]/,'')){
                openWindow(multivolume.options[multivolume.selectedIndex].value, window.location.search.replace(/^[?]/,''), multivolume.options[multivolume.selectedIndex].getAttribute('data-page-count'))
            } else {
                openWindow(multivolume.options[multivolume.selectedIndex].value)
            }"
        >Go</button>
    </div>
</form>

            
        

        


        

