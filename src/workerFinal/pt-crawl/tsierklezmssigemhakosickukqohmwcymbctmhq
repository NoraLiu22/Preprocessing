                            <path class="lightbulb" id="long-ray-2" d="M32.2,10c0.1,0,0.3-0.1,0.4-0.2l2.3-2.3C35,7.3,35,7,34.8,6.8c-0.2-0.2-0.5-0.2-0.8,0l-2.3,2.3 c-0.2,0.2-0.2,0.5,0,0.8C31.9,9.9,32,10,32.2,10z"/>
                        </svg>
                    <span class="contribute-text">Request a Feature</span>
                </a>
            </div>
        </section>
    </div>
</section>
<script>
	//**********************
	//General js code
	//***




	//scroll delay appear code, arguments include the container that contains
	//the targetted object, an array of targeted objects(S), window appear location
	function scrollAnimate (scrollDelayContainer, targettedObjectClass, animationClass) {

		//get object
		var objects = Array.prototype.slice.call(document.querySelectorAll('.' + targettedObjectClass));

		objects.forEach(object => {
			//not expecting object properties to change but if they do
			//move object properties variable inside of the scroll event
			console.log(object);
			//get object document position
			var objectPosition = object.getBoundingClientRect().top;
			//get object height
			var objectHeight = object.scrollHeight;
			//set object animate location
			var appearLocation = objectPosition + (objectHeight/2);

			var addScroll = function() {
				//get browser vh on every scroll so if vh changes its refreshed on scroll
				var vh = window.innerHeight;

				//current scroll position
				var currentScrollPosition = window.scrollY;

				//debug code
				// console.log("objectPosition = " + objectPosition);
				// console.log("appearLocation = " + appearLocation);
				// console.log("currentScrollPosition = " + currentScrollPosition);


				if ( currentScrollPosition + vh >= appearLocation) {
					//removes the scroll event handler, so that it only fire once
					window.removeEventListener('scroll', addScroll);
					//add the animation class
					object.classList.add(animationClass);
				}
			};

			window.addEventListener('scroll', addScroll);


			//--if page is reloaded near the targeted object instead of scrolled towards it
			//get browser vh
			var vh = window.innerHeight;

			//set object animate location
			var appearLocation = objectPosition + (objectHeight/2);
			if ( window.scrollY + vh >= appearLocation) {
				//removes the scroll event handler if it exists
				window.removeEventListener('scroll', addScroll);

				//add the animation class
				object.classList.add(animationClass);
			}
		})


	}

	//scroll animate code
	if (document.querySelector('.contribute-icon')) {
		scrollAnimate("contribute-link","contribute-icon","animate");
	}

</script>
</div></div></div>  </div>

  
  
</div>
  </div>
</div>
</div>
        
        
        
