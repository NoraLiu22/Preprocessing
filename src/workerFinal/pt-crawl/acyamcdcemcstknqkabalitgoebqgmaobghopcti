    <script>
      // https://github.com/jgthms/bulma/issues/2361
      $(document).ready(function () {
        // Control the mobile burger menu show/hide
        $(".navbar-burger").click(function () {
          $(".navbar-burger").toggleClass("is-active");
          $(".navbar-menu").toggleClass("is-active");
        });

        // just control the "is-action" class for the drop down menu, and use the above css to control this display
        $(".navbar-item.has-dropdown").click(function () {
          var isThisDropDownActive = $(this).hasClass("is-active");

          // Turn off any other drop downs
          $(".navbar-item.has-dropdown").removeClass("is-active");
          $(".navbar-dropdown").removeClass("is-active");

          // Turn on this drop down if it is not active on click
          if (!isThisDropDownActive) {
            $(this).toggleClass("is-active");
            $(this).children(".navbar-dropdown").toggleClass("is-active");
          }
        });
      });
    </script>
    
