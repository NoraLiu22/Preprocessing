<script type="text/javascript">
    $(document).ready(function () {
        function ToogleDatesVisibility(isItChecked) {
            if (isItChecked == false) {
                //Show the Upcoming events label, slideDown = Show
                $("#evUpcomingDate").slideDown("fast");
                //Hide the Date container, unfortunately name 'evEndDate', slideUp = Hide
                $("#evEndDate").slideUp("fast");
            }
            else {
                //Hide the Upcoming events label, slideUp = Hide
                $("#evUpcomingDate").slideUp("fast");
                //Show the Date container, unfortunately name 'evEndDate', slideDown = Show
                $("#evEndDate").slideDown("fast");
            }
        };

        $("#evDateRangeSelector").on("change", function (e) {
            //Change event occur after change has occured, hence value reflect the currect change state
            var isItChecked = $(this).prop("checked");
            ToogleDatesVisibility(isItChecked);
        });

        //When page is load, base on  the checkbox state, toogle the visibility
        if ($("#evDateRangeSelector").length > 0) {
            var isItChecked = $("#evDateRangeSelector").prop("checked");
            ToogleDatesVisibility(isItChecked);
        }

        $(".location").removeAttr("href");
        $("input[type=radio]").on("change", function () {
            if ($(this).val() == "international") {
                $("#country").parent().show();
                $("#continent").parent().show();
                $("#ukregion").parent().hide();
                $("#country").val(''); $("#continent").val(''); $("#ukregion").val('');
                $("#country").empty();
                $("#country").append($('<option />', { value: '', text: 'All countries' }));
            }
            else if ($(this).val() == "ukireland") {
                $("#country").parent().hide();
                $("#continent").parent().hide();
                $("#ukregion").parent().show();
                $("#country").val(''); $("#continent").val('');
                GetUkRegions();
            }
            else {
                $("#country").parent().show();
                $("#continent").parent().hide();
                $("#ukregion").parent().hide();
                $("#country").val(''); $("#continent").val('');
                PopulateCountries('');
            }
        });

        if ($("input[name=EventType]:checked").length > 0) {
            $("#EventTypeAll").prop("checked", false);
        } else {
            $("#EventTypeAll").prop("checked", true);
        }

        function PopulateCountries(continent) {
            $.ajax({
                url: "/events/getcountries",
                data: { continent: continent },
                success: function (data) {
                    var select = $("#country");
                    select.empty();
                    select.append($('<option />', { value: '', text: 'All countries' }));
                    $.each(data, function (index, element) {
                        select.append($('<option />', {
                            value: element.Value,
                            text: element.id
                        }));
                    });
                },
                error: function () { }
            });
        };

        $("#continent").on("change", function () {
            var continent = $(this).val();
            if ($(this).val() == '') {
                var select = $("#country");
                select.empty();
                select.append($('<option />', { value: '', text: 'All countries' }));
            } else {
                PopulateCountries(continent);
            }

        });

        function GetUkRegions() {
            $.ajax({
                url: "/events/getukregions",
                success: function (data) {
                    var select = $("#ukregion");
                    select.empty();
                    select.append($('<option />', { value: '', text: 'All regions' }));
                    $.each(data, function (index, element) {
                        select.append($('<option />', {
                            value: element.Value,
                            text: element.id
                        }));
                    });
                },
                error: function () { }
            });
        }

        $("#subjectarea").on("change", function () {
            if ($(this).val() != "") {
                var subjectarea = $(this).val();
                if ($("#subjecttopic").length > 0) {
                    $.ajax({
                        url: "/events/getsubjecttopics",
                        data: { subjectarea: subjectarea },
                        success: function (data) {
                            var select = $("#subjecttopic");
                            select.empty();
                            select.append($('<option />', { value: '', text: 'All subject topics' }));
                            $.each(data, function (index, element) {
                                select.append($('<option />', {
                                    value: element.Value,
                                    text: element.id
                                }));
                            });
                            $("#subjecttopic").parent().show();
                        },
                        error: function () { }
                    });
                }
            } else {
                var select = $("#subjecttopic");
                select.empty();
                select.append($('<option />', { value: '', text: 'All subject topics' }));
            }
        });

        $(".membernetwork li a").on("click", function (e) {
            e.preventDefault();
            var id = $(this).attr("id").toString();
            var index = id.indexOf('-') + 1;
            id = id.substring(index);
            $(".tab-list li a").removeClass("selected");
            $(".ev-memnet-search-block").hide();
            $(this).addClass("selected");
            $("#memNetworkChild-" + id).show();
        });


        $("input[name=EventType]").on("click", function (e) {
            e.stopPropagation();
            if ($("input[name=EventType]:checked").length > 0) {
                $("#EventTypeAll").prop("checked", false);
            }
            else {
                $("#EventTypeAll").prop("checked", true);
            }
        });

        $("#EventTypeAll").on("click", function (e) {
            e.stopPropagation();
            if ($(this).is(":checked")) { $("input[name=EventType]").prop("checked", false); }
        });

        var months = ["january", "february", "march", "april", "may", "june", "july", "august", "september", "october", "november", "december"];

        $('input[type=submit]').on('click', function () {
            if ($('input[name="IsDateRangeSelected"]:checked').length > 0) {
                $("#errorStartDate").hide();
                $("#errorEndDate").hide();
                var startDateDay = $('#StartDateDay').val();
                var startDateMonth = $('#StartDateMonth').val().toString().toLowerCase();
                var startDateYear = $('#StartDateYear').val();
                var endDateDay = $('#EndDateDay').val();
                var endDateMonth = $('#EndDateMonth').val().toString().toLowerCase();
                var endDateYear = $('#EndDateYear').val();
                var startDateMonthIndex = $.inArray(startDateMonth, months);
                var endDateMonthIndex = $.inArray(endDateMonth, months);
                var startDate = startDateMonthIndex + 1 + '/' + startDateDay + '/' + startDateYear;
                var endDate = endDateMonthIndex + 1 + '/' + endDateDay + '/' + endDateYear;
                if (!isDate(startDate)) {
                    $("#errorStartDate").show();
                    $("#errorStartDate").text("Invalid start date");
                    return false;
                } else if (!isDate(endDate)) {
                    $("#errorEndDate").show();
                    $("#errorEndDate").text("Invalid end date");
                    return false;
                }
                if (Date.parse(endDate) < Date.parse(startDate)) {
                    $("#errorEndDate").show();
                    $("#errorEndDate").text("End date should be after the start date");
                    return false;
                }
            }
            return true;
        });

        $('#StartDateMonth').on("change", function () {
            $('#EndDateMonth').val($(this).val());
        });
        $('#StartDateYear').on("change", function () {
            $('#EndDateYear').val($(this).val());
        });

        $('#btnClear').on('click', function () {
            var currentDate = new Date();
            $('#evDateRangeSelector').prop('checked', false);
            $("#evEndDate").hide();
            $('select').val('');
            $('#StartDateDay').val(currentDate.getDay());
            $('#StartDateMonth').val(months[currentDate.getMonth()]);
            $('#StartDateYear').val(currentDate.getFullYear());
            $('#EndDateDay').val(currentDate.getDay());
            $('#EndDateMonth').val(months[currentDate.getMonth()]);
            $('#EndDateYear').val(currentDate.getFullYear());
            $('input[name=EventType]:checked').prop('checked', false);
            $('#EventTypeAll').prop('checked', true);
            $('input[type=text]').val('');
            $("#ukregion").parent().hide(); //$("#localsection").parent().hide();
            $("#ExcludeOnline").prop('checked', false);
            $("input[name = location][value = all]").prop("checked", true);
            $("#country").parent().show();
            $("#continent").parent().hide();
            $("#ukregion").parent().hide();
            //$("#localsection").parent().hide();
            $("#subjecttopic").parent().hide();
            return false;
        });
        function isDate(txtDate) {
            var currVal = txtDate;
            if (currVal == '')
                return false;

            var rxDatePattern = /^(\d{1,2})(\/|-)(\d{1,2})(\/|-)(\d{4})$/; //Declare Regex
            var dtArray = currVal.match(rxDatePattern); // is format OK?

            if (dtArray == null)
                return false;

            //Checks for mm/dd/yyyy format.
            dtMonth = dtArray[1];
            dtDay = dtArray[3];
            dtYear = dtArray[5];

            if (dtMonth < 1 || dtMonth > 12)
                return false;
            else if (dtDay < 1 || dtDay > 31)
                return false;
            else if ((dtMonth == 4 || dtMonth == 6 || dtMonth == 9 || dtMonth == 11) && dtDay == 31)
                return false;
            else if (dtMonth == 2) {
                var isleap = (dtYear % 4 == 0 && (dtYear % 100 != 0 || dtYear % 400 == 0));
                if (dtDay > 29 || (dtDay == 29 && !isleap))
                    return false;
            }
            return true;
        }

    });
</script>
</div>

            </div>

            <!-- End: Site Content -->
