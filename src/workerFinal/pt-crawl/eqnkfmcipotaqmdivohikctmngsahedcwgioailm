                <input type="hidden" name="resultsPagePath" value="/content/marketing/publish/de_de/search-results.html" id="resultsPagePath">
                <input type="hidden" name="accessToken" value="xxc839667d-dd5e-474e-b3ab-c84b434185fe" id="accessToken">
                <input type="hidden" name="searchUrl" value="https://platform.cloud.coveo.com/rest/search" id="searchUrl">
                <input type="hidden" name="searchWord" value="OCLC-Webseiten suchen" id="searchWord">
