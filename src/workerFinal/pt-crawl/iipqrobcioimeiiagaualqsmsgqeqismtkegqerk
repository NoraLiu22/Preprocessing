        }
    }
}
if (xmlHttp) {
    xmlHttp.open('GET', '/ajax_getlatestreviews.asp?artist=P%2DSquare', true);
    xmlHttp.onreadystatechange = function () {
        if (xmlHttp.readyState == 4) {
						resultText = xmlHttp.responseText;
            document.getElementById('divreviews').innerHTML=resultText;
        }
    };
    xmlHttp.send(null);
}

	
	}
	
	
	var hash = window.location.hash.substr(1);
	if (hash!='') {
		clickTab(hash.split("-")[0]);
		if (hash=='charts') {
			loadCharts('s');
		}
		if (hash=='reviews') {
			loadLatestReviews();
		}
	}
		
