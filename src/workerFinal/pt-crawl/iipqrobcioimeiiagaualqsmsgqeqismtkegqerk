<script>
	activeTab = "disco"
	

	videoIsDisplayed=0;

	function clickTab(tab) {
		if (videoIsDisplayed==1) stopVideo();
		document.getElementById('div'+activeTab).style.display='none';
		document.getElementById('nav-'+activeTab).style.backgroundColor='#DF1D27';
		activeTab = tab;
		document.getElementById('nav-'+tab).style.backgroundColor='#000000';
		document.getElementById('div'+tab).style.display='';
		if (tab=='videos') {
			setTimeout(function(){ playVideo(); }, 1000);
			videoIsDisplayed = 1;
		} else {
			videoIsDisplayed = 0;
		}
		window.location.hash = tab;
	}
</script>

