<script>
	function clickTabDisco(tab) {
		inav = document.querySelectorAll('.active.artistdisco');
		for (i=0;i<inav.length;i++) {
			inav[i].classList.remove('active')
		}
		document.querySelector('#navdisco-'+tab).classList.add('active');
		idiv = document.querySelectorAll('.disco');
		for (i=0;i<idiv.length;i++) {
			idiv[i].classList.add('hidden')
		}
		document.querySelector('#disco-'+tab).classList.remove('hidden');
	}
	
</script>
