    <script src="//ajax.googleapis.com/ajax/libs/dojo/1.10.4/dojo/dojo.js" data-dojo-config="async: true" type="text/javascript"></script>

    <script type="text/javascript">
        app = new Object();
        require(['dojo/domReady!'], function () {
            app.fetchJSON();
        });

        app.fetchJSON = function () {
            require(['dojo/dom', 'dojo/on', 'dojo/dom-attr', 'dojo/dom-style', 'dojo/dom-construct', 'dijit/Dialog', 'dojo/request', 'dojo/DeferredList'], function (dom, on, domAttr, domStyle, domConstruct, Dialog, request, DeferredList) {
                var ok1 = false,
                    ok2 = false;//,
                //data1,
                //data2;

                // Request 1
                var promise1 = request('GeoJSON/MapJSON.jgz', {
                    handleAs: 'json'
                }).then(function (response) {
                    ok1 = true;
                    data1 = response;
                }, function (err) {
                    if (console.error) {
                        console.error(err);
                    }
                    
                });

                // Request 1
                var promise2 = request('json/current_data.json', {
                    handleAs: 'json'
                }).then(function (response) {
                    ok2 = true;
                    data2 = response;
                }, function (err) {
                    if (console.error) {
                        console.error(err);
                    }
                });

                // Create a DeferredList to manage both of them
                var list = new DeferredList([promise1, promise2]);
                // When they're both resolved...
                list.then(function (result) {
                    // Do something!
                    if (ok1 && ok2) {
                        countyBoundaryJSON = data1['countyboundaries'];
                        monareasJSON = data1['monareas'];
                        countyCentroidJSON = data1['countycentroids'];
                        app.buildContent(data2);
                    } else {
                        app.buildContent(data2);
                        alert('There was a problem retrieving data that is required for the map.');
                    }

                });
            });
        };

        app.buildContent = function (tableData) {
            require(['dojo/dom', 'dojo/dom-style', 'AirTable_V0', 'AirMap', 'dojo/domReady!'], function (dom, domStyle, AirTable, AirMap) {
                var spanEdingAt = dom.byId('spanEndingAt'),
                    tableRelativeLevels = dom.byId('tableRelativeLevels'),
                    aq_table = dom.byId('aq_table');
                app.airTable = new AirTable({ 'tableData': tableData });
                if (spanEndingAt) {
                    spanEndingAt.innerHTML = app.airTable.getDisplayDate(app.airTable.tableData['effectiveDate']);
                }
                if (tableRelativeLevels) {
                    domStyle.set(tableRelativeLevels, { 'visibility': 'visible' });
                }
                if (aq_table) {
                    domStyle.set(aq_table, { 'visibility': 'visible' });
                }
                app.airMap = new AirMap({ 'name': 'george' });
                app.checkForUpdates();
            });

        };

        /**
        *  This function, called from app.buildContent, looks to handlers/DataCheck.ashx to see
        *  if a more recent source JSON file is available.  If it is, the user is asked if the page should be reloaded.
        */
        app.checkForUpdates = function () {
            var timerMS = 1*60*1000,
                url = 'Handlers/DataCheck.ashx';
            require(["dojo/request/xhr"], function (xhr) {
                xhr(url, {
                    handleAs: "json"
                }).then(function (response) {
                    // Have a response
                    if (response.success) {
                        // Response in successful
                        var hvJsonDate = document.getElementById('hvJsonDate');
                        if (hvJsonDate) {
                            var curValue = parseInt(hvJsonDate.value),
                                newValue = Date.parse(response.filedate);
                            if (!isNaN(curValue)) {
                                // Value of #hvJsonDate has been established:  compare to current UTC date.
                                if (newValue > curValue) {
                                    // Newer file
                                    app.confirmYesNo({
                                        'title': 'Updated Air Quality Data',
                                        'text': '<p>The air quality data was updated at ' + response.filetime + '.</p><p>Would you like to reload the page?</p>'
                                    }).then(function (answer) {
                                        if (answer) {
                                            document.location = document.location;
                                        }
                                        setTimeout(app.checkForUpdates, timerMS);
                                    });
                                } else {
                                    // File is not newer:  simply setTimeout
                                    setTimeout(app.checkForUpdates, timerMS);
                                }
                            } else {
                                // Value of #hvJsonDate is empty:  fill it and setTimeout
                                hvJsonDate.value = newValue;
                                setTimeout(app.checkForUpdates, timerMS);
                            }
                        }
                    } else {
                        if (console.error) {
                            console.error('checkForUpdates success is false:  ' + response.message + '\n\nFurther processing is suspended.')
                        }
                    }
                }, function (err) {
                    if (console.error) {
                        console.error('checkForUpdates request error:  ' + err.message + '\n\nFurther processing is suspended.')
                    }
                }, function (evt) {
                    // Handle a progress event from the request if the
                    // browser supports XHR2
                });
            });
        };

        /**
        *  Opens a Yes/No confirmation dialog and returns true or false.
        *  The argument is an object:  {'title':'Dialog Title', 'text':'Dialog text'}.
        */
        app.confirmYesNo = function (options) {
            function removeIt() {
                $('#myDialog').dialog('destroy');
                $('#myDialog').remove();
            }
            var defer = $.Deferred();
            var div = $('<div>').attr({ 'id': 'myDialog', 'title': options.title }).css({}).html(options.text).appendTo($('body'));
            var diag = div.dialog({
                autoOpen: false,
                modal: true,
                minWidth: 500,
                //close: removeIt,
                open: function () {
                    $('div.ui-dialog-buttonset button').removeClass('ui-corner-all');
                    $('.ui-widget-overlay').css({ opacity: '.8' });
                    $('.ui-dialog').css('z-index', 1003);
                    $('.ui-widget-overlay').css('z-index', 1002);
                },
                buttons: {
                    'Yes': function () {
                        defer.resolve(true);
                        $(this).dialog('close');
                    },
                    'No': function () {
                        defer.resolve(false);
                        $(this).dialog('close');
                    }
                }
            });

            div.dialog('open');
            return defer.promise();
        }
    </script>
