                        setTimeout(function() {$('#subscribe-form-success').modal('hide');}, 2000);
                    },
                    error: function (data) {
                        console.log('An error occurred.');
                        console.log(data);
                        $("#subscribe-form-result").html(data.responseText);
                    },
                });
            });

            var newslettersubscribeform = $('#newsletter-subscribe-form');
            newslettersubscribeform.submit(function (e) {

                e.preventDefault();

                $.ajax({
                    type: newslettersubscribeform.attr('method'),
                    url: '/brandinsidersummitqsr/newsletter_subscribe/',
                    data: newslettersubscribeform.serialize(),
                    success: function (data) {
                        // $("#newsletter-subscribe-form-fields").collapse('toggle');
                        $("#newsletter-subscribe-form-result").html(data);
                        // $('#newsletter-subscribe-form-success').modal('show');
                        setTimeout(function() {$('#newsletter-subscribe-form-success').modal('hide');}, 2000);
                    },
                    error: function (data) {
                        console.log('An error occurred.');
                        console.log(data);
                        $("#newsletter-subscribe-form-result").html(data.responseText);
                    },
                });
            });

        });
