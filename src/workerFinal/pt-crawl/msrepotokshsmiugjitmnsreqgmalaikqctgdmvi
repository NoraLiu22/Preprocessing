<input type="hidden" name="captcha" value="" id="captcha"/>


<script>
	var captcha_validated = false;
	function Captcha(k) {
		console.log('k=', k)
		document.getElementById('captcha').value = k;
		captcha_validated = k > '';
	}
	function checkCAPTCHA() {
		if (!captcha_validated) ById('err_captcha').innerHTML = "Please validate yourself as a human.";
		return captcha_validated;
	}
</script>




