size();\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t});\n\t\t\t\t\t\tresize();\n\t\t\t\t\t}, 1000);\n\t\t\t\t}\n\t\t\t})();\n\t\t\u003C\u002Fscript\u003E\n\u003C\u002Fdiv\u003E\n\n\n\n\n\t\t\u003Cscript type=\"module\" data-hydrate=\"11hp74x\"\u003E\n\t\timport { start } from \"https:\u002F\u002Fstatic01.nyt.com\u002Fnewsgraphics\u002F2021\u002Fcoronavirus-tracking\u002F_app\u002Fstart-43b9df2f.js\";\n\t\tstart({\n\t\t\ttarget: document.querySelector('[data-hydrate=\"11hp74x\"]').parentNode,\n\t\t\tpaths: {\"base\":\"\u002Finteractive\u002F2021\",\"assets\":\"https:\u002F\u002Fstatic01.nyt.com\u002Fnewsgraphics\u002F2021\u002Fcoronavirus-tracking\"},\n\t\t\tsession: {embedded:true},\n\t\t\troute: false,\n\t\t\tspa: false,\n\t\t\ttrailing_slash: \"never\",\n\t\t\thydrate: {\n\t\t\t\tstatus: 200,\n\t\t\t\terror: null,\n\t\t\t\tnodes: [\n\t\t\t\t\timport(\"https:\u002F\u002Fstatic01.nyt.com\u002Fnewsgraphics\u002F2021\u002Fcoronavirus-tracking\u002F_app\u002Fpages\u002F__layout.svelte-cbf39943.js\"),\n\t\t\t\t\t\timport(\"https:\u002F\u002Fstatic01.nyt.com\u002Fnewsgraphics\u002F2021\u002Fcoronavirus-tracking\u002F_app\u002Fpages\u002Fembeds\u002F_type_-dashboard\u002Findex.html.svelte-5d2dc529.js\")\n\t\t\t\t],\n\t\t\t\tparams: {type:\"article\"},\n\t\t\t\trouteId: \"embeds\u002F[type]-dashboard.html\"\n\t\t\t}\n\t\t});\n\t\u003C\u002Fscript\u003E\u003Cscript type=\"application\u002Fjson\" sveltekit:data-type=\"data\" sveltekit:data-url=\"https:\u002F\u002Fstatic01.nyt.com\u002Fnewsgraphics\u002F2021\u002Fcoronavirus-tracking\u002Fpage\u002Fnavigation.json\"\u003E{\"status\":200,\"statusText\":\"\",\"headers\":{\"content-type\":\"application\u002Fjson\"},\"body\":\"{\\\"sections\\\":[{\\\"icon\\\":\\\"images\u002Fmaps\u002FUSA\u002Fhotspots-state.png\\\",\\\"section\\\":\\\"usa\\\",\\\"primary\\\":[{\\\"name\\\":\\\"Latest Maps and Data\\\",\\\"url\\\":\\\"https:\u002F\u002Fwww.nytimes.com\u002Finteractive\u002F2021\u002Fus\u002Fcovid-cases.html\\\",\\\"rel\\\":\\\"\\\"},{\\\"name\