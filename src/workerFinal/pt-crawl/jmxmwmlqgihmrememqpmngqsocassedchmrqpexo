└─┬─<a href="../nematoida/index.html"><span class="smallcaps">Nematoida</span></a>
  │
  └─<a href="panarthropoda.html#Panarthropoda"><span class="smallcaps">Panarthropoda</span></a>
    ├─<a href="lobopodia.html#Aysheaia"><i>Aysheaia</i></a>
    └─┬─<a href="../tardigrada/tardigrada.html"><span class="smallcaps">Tardigrada</span></a>
      └─┬─<a href="lobopodia.html#Xenusiidae">Xenusiidae</a>
        ├─<a href="../onychophora/index.html"><span class="smallcaps">Onychophora</span></a>
