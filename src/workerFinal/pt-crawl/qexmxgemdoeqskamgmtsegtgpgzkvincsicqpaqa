<script type="text/javascript" src="/_assets/Plugins/datetime-picker/jquery.datetimepicker.js"></script>
<link rel="stylesheet" href="/_assets/Plugins/datetime-picker/jquery.datetimepicker.css" />

<script>

    function recordOptionsToggle(status) {
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Search options toggle', status]);
    };

    $('#main_0_content_3_lstSearchBy input').change(function () {
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Search by', $(this).val()]);
    });

    $('#main_0_content_3_lstWhen input[type=checkbox]').change(function () {
        var checkedStatus;
        if ($(this).prop('checked')) {
            checkedStatus = " checked";
        } else {
            checkedStatus = " unchecked";
        }
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Status', $(this).val() + checkedStatus]);
    });

    $('#main_0_content_3_lstChamber input[type=checkbox]').change(function () {
        var checkedStatus;
        if ($(this).prop('checked')) {
            checkedStatus = " checked";
        } else {
            checkedStatus = " unchecked";
        }
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Chamber', $(this).val() + checkedStatus]);
    });

    $('#main_0_content_3_lstWhat input[type=checkbox]').change(function () {
        var checkedStatus;
        if ($(this).prop('checked')) {
            checkedStatus = " checked";
        } else {
            checkedStatus = " unchecked";
        }
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Result Type', $(this).val() + checkedStatus]);
    });


    $("#lstDatePreset").change(function () {
        var presetVal = this.value;
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Date preset', presetVal]);
        
        if (presetVal == "all") {
            $("#main_0_content_3_filterstartdate").val("");
            $("#main_0_content_3_filterenddate").val("");
        } else if (presetVal == "today") {
            $("#main_0_content_3_filterstartdate").val(moment().format('DD/MM/YYYY'));
            $("#main_0_content_3_filterenddate").val(moment().format('DD/MM/YYYY'));
        } else if (presetVal == "yesterday") {
            $("#main_0_content_3_filterstartdate").val(moment().add(-1, 'days').format('DD/MM/YYYY'));
            $("#main_0_content_3_filterenddate").val(moment().add(-1, 'days').format('DD/MM/YYYY'));
        } else if (presetVal == "thisyear") {
            $("#main_0_content_3_filterstartdate").val(moment(new Date(new Date().getFullYear(), 0, 1)).format('DD/MM/YYYY'));
            $("#main_0_content_3_filterenddate").val(moment(new Date(new Date().getFullYear(), 11, 31)).format('DD/MM/YYYY'));
        } else if (presetVal == "lastyear") {
            $("#main_0_content_3_filterstartdate").val(moment(new Date(new Date().getFullYear() - 1, 0, 1)).format('DD/MM/YYYY'));
            $("#main_0_content_3_filterenddate").val(moment(new Date(new Date().getFullYear() - 1, 11, 31)).format('DD/MM/YYYY'));
        } else if (presetVal == "thisparliament") {
            $("#main_0_content_3_filterstartdate").val(pnHash[currentParliament].DateFrom.format("dd/MM/yyyy"));
            $("#main_0_content_3_filterenddate").val(pnHash[currentParliament].DateTo.format("dd/MM/yyyy"));
        } else if (presetVal == "lastparliament") {
            $("#main_0_content_3_filterstartdate").val(pnHash[currentParliament - 1].DateFrom.format("dd/MM/yyyy"));
            $("#main_0_content_3_filterenddate").val(pnHash[currentParliament - 1].DateTo.format("dd/MM/yyyy"));
        }

        $("#lstParlNum").val("0");
    });

    $("#main_0_content_3_lstSortBy").change(function () {
        var sortVal = this.value;
        var sortString;
        if (sortVal == 0) {
            sortString = "Best match";
        } else if (sortVal == 1) {
            sortString = "Recent first";
        } else if (sortVal == 2) {
            sortString = "Recent last";
        } else if (sortVal == 3) {
            sortString = "A-Z";
        } else {
            sortString = "Z-A";
        }
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Sort by', sortString]);
    });

    $("#main_0_content_3_filterstartdate").datetimepicker({
        format: "d/m/Y",
        timepicker: false,
        scrollInput: false,
        closeOnDateSelect: true,
        onChangeDateTime: function (dp, $input) {
            if ($input.val() != "") {
                _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Start date', $input.val()]);
            }
        },
        onShow: function (ct) {
            this.setOptions({
                maxDate: moment(jQuery("#main_0_content_3_filterenddate").val(), "DD/MM/YYYY").format("YYYY/MM/DD").toString() != "Invalid date" ? moment(jQuery("#main_0_content_3_filterenddate").val(), "DD/MM/YYYY").format("YYYY/MM/DD").toString() : false
            })
        },
    });

    $("#main_0_content_3_filterenddate").datetimepicker({
        format: "d/m/Y",
        timepicker: false,
        scrollInput: false,
        closeOnDateSelect: true,
        onChangeDateTime: function (dp, $input) {
            if ($input.val() != "") {
                _gaq.push(['_trackEvent', 'Committee Search - Widget', 'End date', $input.val()]);
            }
        },
        onShow: function (ct) {
            this.setOptions({
                minDate: moment(jQuery("#main_0_content_3_filterstartdate").val(), "DD/MM/YYYY").format("YYYY/MM/DD").toString() != "Invalid date" ? moment(jQuery("#main_0_content_3_filterstartdate").val(), "DD/MM/YYYY").format("YYYY/MM/DD").toString() : false
            })
        },
    });

    function recordSearchEvent() {
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Search', $("#txtKeyword").val()]);
    };

    var pnu = document.getElementById("lstParlNum");
    var dateFrom = document.getElementById("main_0_content_3_filterstartdate");
    var dateTo = document.getElementById("main_0_content_3_filterenddate");

    var pnData;
    var pnHash = new Array();
    var currentParliament = 0;
    tempuri.org.ParliamentNumbers.GetDates(PNCallBack);

    dateFrom.onchange = function () {
        
        pnu.selectedIndex = 0;
    }

    dateTo.onchange = function () {
        pnu.selectedIndex = 0;
    }

    pnu.onchange = function () {
        var parliamentNumber = this.options[this.selectedIndex].value;
        _gaq.push(['_trackEvent', 'Committee Search - Widget', 'Parl Num', parliamentNumber]);
        if (parliamentNumber == 0) {
            dateFrom.value = "";
            dateTo.value = dateFrom.value;
        }
        else {
            dateFrom.value = pnHash[parliamentNumber].DateFrom.format("dd/MM/yyyy");
            dateTo.value = pnHash[parliamentNumber].DateTo.format("dd/MM/yyyy");
        }

        $("#lstDatePreset").val("all");
    }

    function PNCallBack(WebServiceResult) {
        currentParliament = WebServiceResult[0].ParliamentNumber;
        for (i = 0; i < WebServiceResult.length; i++) {
            var pnObject = WebServiceResult[i];
            pnHash[pnObject.ParliamentNumber] = pnObject;
        }
        pnHash[currentParliament].DateTo = new Date();
    }
</script>


