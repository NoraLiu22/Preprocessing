			<h1 id="firstHeading" class="firstHeading">
				<span dir="auto">Scholarpedia:Authors</span>
			</h1>
			<!-- /firstHeading -->

            
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Scholarpedia</div>
				<!-- /tagline -->
