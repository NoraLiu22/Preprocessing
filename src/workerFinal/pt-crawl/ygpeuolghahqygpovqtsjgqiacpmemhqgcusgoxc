/* Background Gradient Animation */

.grad-anim{
    background: linear-gradient(103.41deg, rgba(255, 255, 255, 0.2) 0.14%, rgba(28, 198, 255, 0.2) 46.87%, rgba(46, 99, 243, 0.2) 99.33%);
    backdrop-filter: blur(60px); 
	/* background: linear-gradient(-45deg, #FFFFFF, #9BF5F5, #FF96FC); */
	background-size: 400% 400%;
	animation: gradient 15s ease infinite;
}



    
    
.grad-anim2{
	/*background: linear-gradient(-45deg, #FFFFFF, #E6F5FF);*/
    background: linear-gradient(40deg,rgba(230, 245, 255, 0.001) 0, rgba(230, 245, 255, 0.001) 100%),url(https://d1zvw2klwdlloe.cloudfront.net/cms-media/prod/02/c7/02c7c593-a82f-458d-a01e-6a482f5d1be0/bg_shapes_bk.png);   
	/*background-size: 400% 400%;*/
	background-size: cover;
	animation: gradient 15s ease infinite;
}
/*
.test{
    background: linear-gradient(40deg,rgba(230, 245, 255, 0.001) 0, rgba(230, 245, 255, 0.001) 100%),url(https://d1zvw2klwdlloe.cloudfront.net/cms-media/prod/02/c7/02c7c593-a82f-458d-a01e-6a482f5d1be0/bg_shapes_bk.png);   
    background-repeat: no-repeat;    
    background-position: center center;    
    background-size: cover;
}
*/

@keyframes gradient {
	0% {
		background-position: 0% 50%;
	}
	50% {
		background-position: 100% 50%;
	}
	100% {
		background-position: 0% 50%;
	}
}

.scale-up-center {
	-webkit-animation: scale-up-center 0.4s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
	        animation: scale-up-center 0.4s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
}

@-webkit-keyframes scale-up-center {
  0% {
    -webkit-transform: scale(0.5);
            transform: scale(0.5);
  }
  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
  }
}
@keyframes scale-up-center {
  0% {
    -webkit-transform: scale(0.5);
            transform: scale(0.5);
  }
  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
  }
}

.reveal{
  position: relative;
  transform: translateY(150px);
  opacity: 0;
  transition: 0.1s all ease;
}

.reveal.active{
  transform: translateY(0);
  opacity: 1;
}





