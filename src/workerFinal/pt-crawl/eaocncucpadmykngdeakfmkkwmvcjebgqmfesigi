                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Curriculum Instruction and Testing" class="ms-sitemapdirectional" href="/TeachingAndLearning/Pages/home.aspx">Teaching And Learning</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/TeachingAndLearning/EducationInnovation/Pages/home.aspx">Education Innovation</a></span><span> &gt; </span><span class="current">Charter Schools</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">Charter Schools</h2>
                
    



				
				<p class="intro">
					Hawai&#39;i&#39;s public charter schools are operated and managed by independent governing boards. They operate under a performance contract with the State Public Charter School Commission. If you&#39;re interested in learning more about Hawaii&#39;s charter schools, please connect with the Charter School Commission.
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

