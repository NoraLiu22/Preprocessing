<script type="text/javascript">
    $(function () {
        $("#search-series").attr('disabled', 'disabled');

        $("#search-series").click(function () {
            window.location.href = "/facts/indicators" + "/" + $("#series-id").val();
        });

        $.ajax({
            url: "/facts/indicators/List",
            type: "post",
            dataType: "json",
            success: function (xmlResponse) {
                var data = $(xmlResponse).map(function () {
                    return {
                        value: (this).Name,
                        id: (this).Id
                    };
                }).get();
                $("#series").autocomplete({
                    source: data,
                    minLength: 0,
                    select: function (event, ui) {
                        $("#series-id").val(ui.item.id);
                        $("#search-series").removeAttr('disabled');
                        //$("#search-series").attr('disabled', ''); //does not work on jQuery 1.6+
                        $("#search-series").focus();
                    }
                });
            }
        });
    });
</script>

