<script type="text/javascript" defer>
	if ($(window).width() < 801) {
		$('#direct_disambiguationDiv h3').click(function() {
			$(this).nextUntil('#direct_disambiguationDiv h3').fadeOut('fast');
			$(this).removeClass('plus-icon').addClass('minus-icon');
			if($(this).next().is(':hidden') == true) {
				$(this).nextUntil('#direct_disambiguationDiv h3').fadeIn('fast');
				$(this).addClass('plus-icon');
			}
		});
		$('#direct_disambiguationDiv h3#direct_searchCriteriaHeader').off();
		$("#direct_disambiguationDiv h3").on("keydown", function (e) {
			var keyCode = e.keyCode || e.which; 
			if (keyCode == 13) { 
				e.preventDefault();
				$(this).nextUntil('#direct_disambiguationDiv h3').fadeOut('fast');
				$(this).removeClass('plus-icon').addClass('minus-icon');
				if($(this).next().is(':hidden') == true) {
					$(this).nextUntil('#direct_disambiguationDiv h3').fadeIn('fast');
					$(this).addClass('plus-icon');
				}
			}
		});
	}

	$(document).ready(function () {
		$.ajax({
			type: 'GET',
			url: 'https://wsj.jobs/jobs/feed/xml',
			dataType: 'xml',
			success: parseXml
		});

		function parseXml(xml) {
			$('#xmlfeed').html('<ul id="content" data-role="listview" data-inset="true"></ul>');

			$(xml).find('job').each(function () {
				var title = $(this).find('title').text();
				//var location = $(this).find('location').text();
				var city = $(this).find('city').text();
				var country = $(this).find('country').text();
				var description = $(this).find('description').text();
				var url = $(this).find('url').text();

				$('#content').append('<li class="job-module"><a href=' + url + '><span><h3 class="h3">' + title + '</h3></span><span class="city">' + city + '</span>, <span class="country">' + country + '</span><span class="description">' + description + '</span></a></li>');

				$('.description').each(function () {
					replaceIt = $(this).text().replace(/\*/g, ' ');
					$(this).text(replaceIt);
					$(this).text(function(index, text) {
						return text.replace('By selecting “Apply” above, you indicate you have read and acknowledge the Dow Jones Applicant Privacy Notice and Dow Jones Cookie Policy, including data transfers as described in the Policy.', '');
						
					});
					$(this).text(function(index, text) {
	
						return text.replace('Job Description:', '');
					});
				});
				if ($('.city').is(':empty')){
					$('.city:empty').parent().contents().filter(function() {
						return this.nodeType == 3;
					}).each(function(){
						this.textContent = this.textContent.replace(', ','');
					});

				}
				$('#xmlfeed span').each(function () {
					if ($(this).text().length > 240) {
						$(this).text($(this).text().substr(0, 237));
						$(this).append('...');

					}
				});

			});
			var $jobs = $(xml).find('job');

			if ($jobs.length == 0) {
				$('#content').html('<div class="nothing-here">There are no jobs currently available.</div>');
				$('#controls').hide();
			}

			$('.job-module:lt(3)').show();

			var size_li = $('.job-module').size();
			var x = 10;
			$('.job-module:lt(' + x + ')').show();
			$('#loadMore').click(function () {
				x = (x + 10 <= size_li) ? x + 10 : size_li;
				$('.job-module:lt(' + x + ')').show();
				$('#showLess').show();
				if (x == size_li) {
					$('#loadMore').hide();
				}
			});
			$('#showLess').click(function () {
				x = (x - 10 < 0) ? 10 : x - 10;
				$('.job-module').not(':lt(' + x + ')').hide();
				$('#loadMore').show();
				$('#showLess').show();
				if (x == 10) {
					$('#showLess').hide();
				}
			});

		}
	});
$(".direct_noJobs").replaceWith('<li class="direct_noJobs">Your search criteria returned no results, please try a different search criteria or please see all our job listings @ <a href="https://dowjones.jobs">https://dowjones.jobs</a>.</li>');

$("#direct_errorContainer").replaceWith('<div class="direct_noJobs direct_error">This job is no longer available, please try a different search or see all our job listings @ <a href="https://dowjones.jobs">https://dowjones.jobs</a>.</div>');

