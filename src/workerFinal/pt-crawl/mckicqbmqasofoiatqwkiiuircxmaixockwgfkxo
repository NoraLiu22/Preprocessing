    <script>
        $(document).ready(function () {
            var coords = "";
            /**
             * If coords is an existing parameter,
             * make sure we persist it across page
             * searches, the back end will ignore invalid
             * values.
             */
            if (coords) {

                $("<input>")
                    .attr({
                        type: "hidden",
                        id: "coords",
                        name: "coords",
                        value: coords
                    })
                    .appendTo("#standardSearch");
            }
        });
    </script>

    

    
<script>
    function applyEvents() {
        gtag("event", "apply_click", {
            event_label: "Apply Click",
            domain: window.location.origin,
        });

        gtag("event", "page_view", {
            page_title: "Apply Click",
            page_location: window.location.origin + "/G/apply-click",
            page_path: "/G/apply-click",
            domain: window.location.origin,
        });
    }

    function applyClick(url) {
        applyEvents();
        var nw = window.open(url, '_blank')
        nw.focus()
    }

    if (document.getElementById("direct_applyButton") !== null) {
        document
            .getElementById("direct_applyButton")
            .addEventListener("click", function () {
                applyEvents();
            });
    }

    if (document.getElementById("direct_applyButtonBottom") !== null) {
        document
            .getElementById("direct_applyButtonBottom")
            .addEventListener("click", function () {
                applyEvents();
            });
    }
</script>


    
    
