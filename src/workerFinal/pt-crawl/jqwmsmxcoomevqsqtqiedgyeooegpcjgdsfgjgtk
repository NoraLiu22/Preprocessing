<script>
    dojo.require('dojo.enabu.main.rest.EnabuBlockProvider');
    dojo.ready(function() {
        var EBPBooksGalleryCommercialVolumes = new dojo.enabu.main.rest.EnabuBlockProvider({ baseUrl: '/', });
        EBPBooksGalleryCommercialVolumes.requestBlockParamsBuilder
            .setBlockPath('sitomulino/BooksGalleryCommercialVolumes')
            .setBlockAuth('none')
            .setBlockBehaviour('only')
            .addBlockCustomParam('section', '1643')
            .addBlockPublisher('EBPBooksGalleryCommercialVolumesEnd', []);
        EBPBooksGalleryCommercialVolumes.requestBlock(EBPBooksGalleryCommercialVolumes.requestBlockParamsBuilder.buildParams());
    });
