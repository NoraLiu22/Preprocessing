<script type="text/javascript">
/**
 *   Funzioni per l'inizializzazione del carousel (ForefrontPanels) 
 */
$(function() {

    // fixing carousel html
	document.querySelector('#forefront-panels > .carousel-inner > .carousel-item' ).classList.add("active")
	document.querySelectorAll('#forefront-panels > .carousel-inner > .carousel-item').forEach(function(elem, index) {
        let images = elem.getElementsByTagName('img')
        // mobile staff
        if (window.innerWidth < 576 && images.length === 2) {
            images[0].remove() // removing desktop img
            images[0].classList.remove('hidden','d-none') // showing mobile img
        }
        document.querySelector('#forefront-panels .carousel-indicators').innerHTML += '<button data-bs-target="#forefront-panels" data-bs-slide-to="' + index + '"></button>'
	});
	document.querySelector('#forefront-panels .carousel-indicators > button' ).classList.add("active")

    // initializing carousel
    var interval = (11 - 6) * 1000;
    var myCarousel = document.getElementById('forefront-panels')
    new bootstrap.Carousel(myCarousel, {interval: interval})
})
</script>

