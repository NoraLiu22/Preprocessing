dojo.provide('shop.instances');
dojo.require('shop.Cart');
dojo.addOnLoad(10, function() {
    if (shop.instances.Cart == null) {
        shop.instances.Cart = new shop.Cart();
        shop.instances.Cart.setRedirectUrl('/');
        shop.instances.Cart.setAddToCartStyleClassChain('customer-type-1');
    }
});
</script>

    
    
