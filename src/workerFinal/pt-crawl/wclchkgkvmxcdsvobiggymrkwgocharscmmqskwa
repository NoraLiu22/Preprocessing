                // Use d3 queue to load topojson and name tsv into Map function
                // ok and US use the same topojson, so we've combined the queue to only load it once
                d3.queue()
                    .defer(d3.json, '/media/assets/public/js/us.topojson')
                    .defer(d3.tsv, '/media/assets/public/js/ok-county-names.tsv')
                    .defer(d3.tsv, '/media/assets/public/js/us-state-names.tsv')
                    .await(function(error, json, okTsv, usTsv) {
                        if (error) throw error;

                        // Map options
                        const okMapOptions = {
                            containerSelector: '#ok-counties-visualized',
                            featureNames: okTsv,
                            colorRange: ['#e8f5f9','#02506d'],
                            colorZero: ['#edefef'],
                            itemCount: itemCountByCounty,
                            projectionName: 'albers',
                            feature: 'counties',
                            tooltipNameAppendage: ' County',
                            searchUrl: 'https://gateway.okhistory.org/search/?explore=true&fq=str_location_county:',
                            searchAppendage: '+County,+OK'
                        };

                        // create map object
                        const okMap = window.Charts.Map(json, okMapOptions);

                        // Check viewport for element and call animate when visible
                        window.Charts.renderInView(okMap);

                        // Scroll event ensures first map is rendered on load
                        const event = document.createEvent('HTMLEvents');
                        event.initEvent('scroll', true, false);
                        window.dispatchEvent(event);

                        // Map options
                        const usMapOptions = {
                            containerSelector: '#us-states-visualized',
                            featureNames: usTsv,
                            colorRange: ['#e8f5f9','#02506d'],
                            colorZero: ['#edefef'],
                            itemCount: itemCountByState,
                            projectionName: 'albersUsa',
                            feature: 'states',
                            searchUrl: 'https://gateway.okhistory.org/search/?explore=true&fq=str_location_state:'
                        };

                        // create map object
                        const usMap = window.Charts.Map(json, usMapOptions);

                        // Check viewport for element and call animate when visible
                        window.Charts.renderInView(usMap);
                    });

                // Use d3 queue to load topojson and name tsv into Map function
                d3.queue()
                    .defer(d3.json, '/media/assets/public/js/world-110m.topojson')
                    .defer(d3.tsv, '/media/assets/public/js/world-country-names.tsv')
                    .await(function(error, json, tsv) {
                        if (error) throw error;

                        // Map options
                        const worldMapOptions = {
                            containerSelector: '#world-countries-visualized',
                            featureNames: tsv,
                            colorRange: ['#e8f5f9','#02506d'],
                            colorZero: ['#edefef'],
                            itemCount: itemCountByCountry,
                            projectionName: 'mercator',
                            feature: 'countries',
                            searchUrl: 'https://gateway.okhistory.org/search/?explore=true&fq=str_location_country:'
                        };

                        // create map object
                        const worldMap = window.Charts.Map(json, worldMapOptions);

                        // Check viewport for element and call animate when visible
                        window.Charts.renderInView(worldMap);
                    });
            } else {
                setTimeout(function() { defer(); }, 50);
            }
        }());
    </script>

    <script type="module">
        import { bindListUpdate, bindListHeaderIconsUpdate } from '/media/assets/public/js/modules/explore_helpers.js?v=20220909';

        (function defer() {
            if ('List' in window) {
                if (document.querySelectorAll('.county-name').length > 0) {
                    const countyList = new List('oklahoma', {
                        valueNames: ['county-name', 'county-count'],
                        page: 10,
                        pagination: {
                            innerWindow: 3,
                            outerWindow: 1
                        },
                    });

                    bindListUpdate(countyList, {
                        visualizedSelector: '#ok-counties-visualized',
                        mapSelector: '.visualization__map--counties',
                        mapHoverClass: 'visualization__map--hover',
                        noResultsSelector: '#ok-no-results',
                        matchingCountSelector: '.matching-count--ok',
                        listCountSelector: '.explore__list-count--ok',
                        valueName: 'county-name',
                    });

                    bindListHeaderIconsUpdate(countyList, '#ok-list thead', '.fa');
                }

                if (document.querySelectorAll('.state-name').length > 0) {
                    const usaList = new List('us', {
                        valueNames: ['state-name', 'state-count'],
                        page: 10,
                        pagination: {
                            innerWindow: 3,
                            outerWindow: 1
                        },
                    });

                    bindListUpdate(usaList,{
                        visualizedSelector: '#us-states-visualized',
                        mapSelector: '.visualization__map--states',
                        mapHoverClass: 'visualization__map--hover',
                        noResultsSelector: '#us-no-results',
                        matchingCountSelector: '.matching-count--us',
                        listCountSelector: '.explore__list-count--us',
                        valueName: 'state-name',
                    });

                    bindListHeaderIconsUpdate(usaList, '#us-list thead', '.fa');
                }

                if (document.querySelectorAll('.country-name').length > 0) {
                    const regionList = new List('world', {
                        valueNames: ['country-name', 'country-count'],
                        page: 20,
                        pagination: {
                            innerWindow: 3,
                            outerWindow: 1
                        },
                    });

                    bindListUpdate(regionList, {
                        visualizedSelector: '#world-countries-visualized',
                        mapSelector: '.visualization__map--countries',
                        mapHoverClass: 'visualization__map--hover',
                        noResultsSelector: '#world-no-results',
                        matchingCountSelector: '.matching-count--world',
                        listCountSelector: '.explore__list-count--world',
                        valueName: 'country-name',
                    });

                    bindListHeaderIconsUpdate(regionList, '#world-list thead', '.fa');
                }
            } else {
                setTimeout(function () { defer(); }, 50);
            }
        }());
    </script>



