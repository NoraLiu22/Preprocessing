            "@type": "NewsArticle",
            "mainEntityOfPage": {
                "@type": "Webpage",
                "url": "https://www.euronews.com/culture/2024/04/29/messi-the-dog-from-anatomy-of-a-fall-is-getting-his-own-tv-show-at-cannes-this-year"
