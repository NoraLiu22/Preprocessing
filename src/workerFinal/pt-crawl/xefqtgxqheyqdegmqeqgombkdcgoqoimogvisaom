        <script>
            function sendEnhancedEvent() {
                
                var ctmElement = document.querySelector(".ctm");
                var enhancedExperience = getComputedStyle(ctmElement, null).getPropertyValue("border-left-style");
                window.dataLayer.push({event:"enhancedExperienceTest", enhancedCustomProp: enhancedExperience});
            }
            if (document.readyState === "loading" || document.readyState === "interactive") {
                window.addEventListener("load", sendEnhancedEvent);
            } else {
                sendEnhancedEvent();
            }
        </script>
