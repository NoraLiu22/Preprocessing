<script>
        (function() {
            let user = {
                    auth: false,
                    hma: false,
                    lic: 'none',
                    name: '',
                    'ret-sub': false,
                    'ret-user': false,
                };
            const docEl = document.documentElement;
            try {
                Object.assign(user, JSON.parse(decodeURIComponent(Object.fromEntries(document.cookie.split('; ').map(x => x.split('='))).gup_lng)))
                if (user.name && user.auth) {
                    docEl.style.setProperty('--gnt-uwn', '\'' + user.name + '\'');
                    docEl.setAttribute('data-u-n', '');
                }
                
            } catch (err) {}
            docEl.setAttribute('data-u-s', user.auth ? user.hma ? 'sub' : 'reg' : 'anon');
            docEl.classList.add('gnt__tn');
