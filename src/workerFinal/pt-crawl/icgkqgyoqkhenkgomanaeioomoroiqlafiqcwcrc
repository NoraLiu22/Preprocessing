    function firePromoAnalytics(e) {
        try {
            var analytics = document.getElementById("pageAnalytics");
            if (analytics) {
                var bpt = window.ga_data.route.basePageType;
                var ssts = window.ga_data.route.ssts.replace('/', ':');
                var analyticString = 'franchise';

                if (bpt === 'homefront') {
                    ssts = 'home'
                }

                var eventString = bpt + ':' + ssts + ':' + e.target.localName + ':' + analyticString;
                analytics.fireEvent(eventString, {}, true);
            } else {
                
            }
        } catch (ex) {
            
        }
    }
