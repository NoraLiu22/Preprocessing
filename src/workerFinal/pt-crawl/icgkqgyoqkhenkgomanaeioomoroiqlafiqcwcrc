<a class="gnt_n_us_dd_a gnt_n_us_dd_a__li" data-u-sr="reg sub" href="https://user.usatoday.com/USAT-GUP/user/logout/?return-url=https:%2F%2Fwww.usatoday.com%2Fhumankind%2F" data-t-l="|H1Nav|account|sign out">Sign Out</a></div><div class=gnt_n_us_dd_ac data-u-sr="anon reg"><div class=gnt_n_us_dd_lbl data-u-sr=anon>Have an existing account?</div><a class="gnt_n_us_dd_ba gnt_n_us_dd_ba__hl" data-u-sr=anon rel=nofollow href="https://login.usatoday.com/USAT-GUP/authenticate?success-url=https:%2F%2Fwww.usatoday.com%2Fhumankind%2F&cancel-url=https:%2F%2Fwww.usatoday.com%2Fhumankind%2F" data-t-l="|H1Nav|account|sign in">Sign In</a><div class=gnt_n_us_dd_lbl>Already have a subscription?</div><a class=gnt_n_us_dd_ba rel=nofollow href=https://cm.usatoday.com/account-activation/ data-t-l="|H1Nav|account|activate account">Activate account</a><div class=gnt_n_us_dd_lbl data-u-sr=anon>Don't have an account?</div><a class="gnt_n_us_dd_ba gnt_n_us_dd_a__li" data-u-sr=anon rel=nofollow href="https://login.usatoday.com/authenticate/?requested-state=create-account&reg_source=CPNAVBAR&reg_medium=ONSITE&reg_campaign=signin&reg_delivery=ux" data-t-l="|H1Nav|account|create account">Create account</a></div></div></div></div></nav></div><script>
            (function() {
                // match Tangent global window obj
                gnt = window.gnt || {};

                // DropDown Containers
                const containers = document.querySelectorAll(('[data-g-r~=nav_ce]'));
    
                for (let i = 0; i < containers.length; i++) {
                    containers[i].querySelector('[data-g-r~=nav_ab]').addEventListener('click', e => {
                        e.preventDefault();
                        containerToggle(containers[i]);
                    });
                }
    
                let offclickHandlerSet = false;
    
                containerToggle = (container) => {
                    for (let i = 0; i < containers.length; i++) {
                        if (containers[i] !== container || (container.hasAttribute('data-g-s') && containers[i] === container)) {
                            collapseContainer(containers[i]);
                            setOffClickHandler();
                        } else {
                            container.setAttribute('data-g-s', 'nav_ao');
                            containers[i].querySelector('[data-g-r~=nav_ab]').setAttribute('aria-expanded', 'true');
                        }
                    }
                }
    
                setOffClickHandler = () => {
                    if (offclickHandlerSet) return;
                    offclickHandlerSet = true;
                    document.addEventListener('click', onDocumentClick);
                }
                collapseContainer = (container) => {
                    container.removeAttribute('data-g-s');
                    container.querySelector('[data-g-r~=nav_ab]').setAttribute('aria-expanded', 'false');
                }
                
                onDocumentClick = (e) => {
                    let offClick = true;
                    for (let i = 0; i < containers.length; i++) {
                        if (containers[i].contains(e.target)) {
                            offClick = false;
                            break;
                        }
                    }
                    if (offClick) {
                        document.removeEventListener('click', onDocumentClick);
                        offclickHandlerSet = false;
                        for (let i = 0; i < containers.length; i++) {
                            collapseContainer(containers[i]);
                        }
                    }
                }

                // Breaking News
                gnt.bn = gnt.bn || {};
                const breakingNewsBar = document.querySelector('.gnt_n_bn') || '';
                const breakingNewsBarId = breakingNewsBar ? breakingNewsBar.dataset.gnt_n_bn_id : '';
    
                if (breakingNewsBar && localStorage.getItem('breakingLastUpdateClosed') === breakingNewsBarId) {
                    gnt.bn.h = true;
                    document.documentElement.classList.add('gnt__nbn');
                }
    
                if (!gnt.bn.h) {
                    const bnClose = document.querySelector('[data-g-r~=base_bnclose]');
                    if (bnClose) {
                        bnClose.addEventListener('click', () => {
                            breakingNewsBar.parentNode.removeChild(breakingNewsBar);
                            document.documentElement.classList.add('gnt__nbn');
                            try {
                                localStorage.setItem('breakingLastUpdateClosed', breakingNewsBarId);
                            } catch (e) {
                                console.error('breakingClose.js - storage failure', e); // debug only
                            }
                        });
                    }
                    
                }
            }());
        </script><header><div class=module-slot id=header><special-franchise-subnav logo-link=https://usatoday.com/humankind/ logo-default=https://www.gannett-cdn.com/gannett-web/subbrands/humankind/logos-and-branding/logo-default.png logo-compact=https://www.gannett-cdn.com/gannett-web/subbrands/humankind/logos-and-branding/logo-default.png logo-desktop-styles="width: 200px;" util-module-path=projects/special/franchise style="min-height: 72px; display: block;" assets='[{ "key": "humankind", "name": "Humankind", "url": "/humankind/", "children": []},{ "key": "militarykind", "name": "Militarykind", "url": "/militarykind/", "children": []},{ "key": "animalkind", "name": "Animalkind", "url": "/animalkind/", "children": []},{ "key": "holidaykind", "name": "Holidaykind", "url": "/holidaykind/", "children": []},{ "key": "sportskind", "name": "Sportskind", "url": "/sportskind/", "children": []},{ "key": "womankind", "name": "Womankind", "url": "/womankind/", "children": []}]'><div slot=sponsor><div class="gnt_x gnt_x__bl"></div></div></special-franchise-subnav></div></header><section class=generic-content-container><div class=module-slot id=pre-content low-priority><div class="gnt_x gnt_x__bl gnt_x__mbl spacer-small"></div></div><div class=module-slot id=content><article id=mainContentSection><custom-style><style>html section.generic-content-container{background:var(--color-page-background)}.content-well{background-color:var(--color-background,#fff);padding:var(--spacer-twentyfour,24px)var(--one-column,8.3333333333%)}promo-story-bucket,.secondary partner-poster{background-color:var(--color-background)}.primary-franchise{margin-bottom var(--spacer-eight, 8px)}.franchise-main-content-container{@apply --flex;flex-flow:column nowrap}special-franchise-hero{display:block}special-franchise-promo-small{display:block;margin-bottom:var(--spacer-twenty,20px)}@media(min-width:960px){.content-well{padding:30px 60px 20px}.franchise-main-content-container{@apply --row;flex-flow:row-rever