        <section id="accordian-wp" style="padding: 150px 0px 100px;">
            <div class="container accordn" style="width: 1000px !important;  max-width: 100%!important;">
                <div class="row">
                    <div class="col-md-12">
                        <div class="accordian-pal">
                            <h3>Course Content</h3>
                            <span><i class="fa fa-angle-down" style="margin-right: 10px;"></i>Expand All</span>
                        </div>
                        <div class="wrap-box-accordian">
                            <button class="accordion">
                                <i class="fas fa-caret-right"></i>Chapter 1 - Cardiac Electrophysiology
                                <div class="right-g-float">
                                    <span> 7 sections</span>

                                </div>
                            </button>
                            <div class="panel">

                                <p>  ●	Cardiac Muscle</p>
                                <p>  ●	Cardiac Chambers</p>
                                <p>  ●	Coronary Artery Anatomy</p>
                                <p>  ●	Cardiac Conduction System</p>
                                <p>  ●	Cardiac Action Potentials</p>
                                <p>  ●	Cardiac Innervation</p>
                                <p>  ●	Cardiac Cycle</p>
                            </div>

                            <button class="accordion">
                                <i class="fas fa-caret-right"></i>Chapter 2 - Electrocardiography
                                <div class="right-g-float">
                                    <span> 12 sections</span>
                            </button>
                            <div class="panel">
                                <p> ●	Electrocardiogram</p>
                                <p> ●	ECG Electrodes</p>
                                <p> ●	Limb Leads</p>
                                <p> ●	Precordial Leads</p>
                                <p> ●	Contiguous Leads</p>
                                <p> ●	ECG Paper</p>
                                <p> ●	ECG Recording</p>
                                <p> ●	ECG Waves</p>
