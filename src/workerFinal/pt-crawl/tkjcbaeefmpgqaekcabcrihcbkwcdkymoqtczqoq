										setTimeout(function() {
											SRCommonWidgetLoader('addWidget', '#sr-widget-ticker', 'us.common.scoreTicker', {
												sport: "mlb",
												allowedSports: "nfl, nba, mlb, nhl, ncaaf, ncaab",
												nextDays: 2,
												dayFlipHoursBefore: 12,
												onItemClick: function(target, obj) {
													const sportIdToName = {
														1: 'soccer',
														2: 'nba',
														3: 'mlb',
														4: 'nhl',
														5: 'nfl',
														6: 'ncaaf',
														7: 'ncaab',
														16: 'nfl'
													};
													if (target === 'match') {
														const srMatchId = (obj.srMatchId) ? obj.srMatchId : obj.matchId;
														const matchID = (typeof obj.matchId === 'string' && obj.matchId.indexOf('sd:match:') === 0)
															? obj.matchId.replace('sd:match:', '')
															: obj.matchId;
														const gameURL = "/game/score/" + sportIdToName[obj.sportId] + "/" + srMatchId + "/" + matchID;
														window.location = gameURL;
													}
												}
											});
										}, 500);
								</script><div class="header-bar"><div class="sc-1op1dd6-0 japmJB js_sticky-top-ad"><div class="sc-1jotqfp-0 sc-1jotqfp-2 jcCEnB dbAYht js_desktop-horizontal-ad"><div is="bulbs-dfp" class="ad-container dfp dfp-slot-TOP_BANNER ad-top-banner collapsed-ad placement-default" data-ad-unit="TOP_BANNER" data-targeting="{&quot;pos&quot;:&quot;top&quot;}"></div><div class="bt-wrapper"><span data-uid="5d1b8a5c7d-384" class="bt-uid-tg" style="display:none !important;text-align:center" data-css-selector="ad-container.ad-top-banner collapsed-ad"></span></div></div></div><div class="sc-1f6xobn-5 cxPhwD js_custom-header-editor-container"><div class="sc-2cjtmb-0 bvJgow hydrated has-background has-curation"><h1 class="sc-19j4917-1 HvWmM"><div class="sc-19j4917-2 kuIhgt"><span class="hidden-title">Seball</span><img src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" alt="Deadspin logo" draggable="auto" srcSet="https://i.kinja-img.com/image/upload/c_fill,h_80,pg_1,q_80,w_80/25d5f0fa942cd687c10e4dc3c23313c9.png 80w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_140/25d5f0fa942cd687c10e4dc3c23313c9.png 140w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_265/25d5f0fa942cd687c10e4dc3c23313c9.png 265w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_340/25d5f0fa942cd687c10e4dc3c23313c9.png 340w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_490/25d5f0fa942cd687c10e4dc3c23313c9.png 490w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_645/25d5f0fa942cd687c10e4dc3c23313c9.png 645w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_740/25d5f0fa942cd687c10e4dc3c23313c9.png 740w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_965/25d5f0fa942cd687c10e4dc3c23313c9.png 965w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_1165/25d5f0fa942cd687c10e4dc3c23313c9.png 1165w, https://i.kinja-img.com/image/upload/c_fit,pg_1,q_60,w_1315/25d5f0fa942cd687c10e4dc3c23313c9.png 1315w, https://i.kinja-img.com/image/