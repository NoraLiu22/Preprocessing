    window.ads_ignore  ={121:true}
}
</script></div></div></header><div class="container global-container-1" tabindex="-1" data-js="global-container-1"><div id="ContentPane2" class="pane" tabindex="-1"><div id="html-091e9c5e82283e58"><!-- html-module (091e9c5e82283e58): no module data found --></div><div class="masthead-meta-cls cpf-masthead" style="--fed-logo:;--hero-color-1:#113551;--hero-color-2:#FFFFFF;--fed-primary-color:#465275;--fed-secondary-a:#2D2D2D;--fed-secondary-b:#383838;--fed-font-family:Montserrat;--fed-secondary-font-family:Source Sans Pro;--fed-hero-overlay-color:#465275;--fed-hero-opacity:0.8;--brand-logo-url:https://img.webmd.com/vim/live/webmd/consumer_assets/site_images/logos/cpf/logo-webmd-site@2x.svg;--brand-link://www.webmd.com;--fed-sponsor-url: ;--fed-sponsor-link: ;--fed-sponsor-content:Supported by;--fed-hero-bg-img-desktop:;--fed-hero-bg-img-mobile:;--fed-theme-color:#113551;--fed-primary-md-color:#E5F0FF;--fed-primary-lt-color:#EEF6F3;--brand-sponsor-url:/webmd/consumer_assets/site_images/sponsored_programs/reckitt-benckiser/mucinex/cold-flu-map-tools/attribution_Logo.svg;undefined:fundedAttr;--image-server://img.lb.wbmdstatic.com/vim/live;"><div class="banner-ad-contianer mobile-flag desktop-tablet-flag"><div class="ad-wrapper ad-container-101"><span></span></div></div><div><a href="#main-container" class="vn-skip-to-main"> Skip to main content </a><nav class="vn-wrapper lite no-stick" style="background-color:#FFFFFF;" aria-label="Primary Site Menu" itemscope itemtype="https://schema.org/SiteNavigationElement"><div class="vn-container hdr-site-1 hdr-site-3 lite"><button class="vn-menu-btn vn-menu lite" style="color:#3557FF;" aria-controls="vn-links" id="vn-menu-toggle" aria-label="Open Menu" aria-expanded="false"><svg xmlns="https://www.w3.org/2000/svg" height="36" viewbox="0 96 960 960" width="36"><path d="M153.333 816q-14.166 0-23.75-9.617Q120 796.766 120 782.55q0-14.216 9.583-23.716 9.584-9.5 23.75-9.5h653.334q14.166 0 23.75 9.617Q840 768.568 840 782.784q0 14.216-9.58