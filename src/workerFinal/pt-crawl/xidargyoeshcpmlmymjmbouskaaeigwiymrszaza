  <script type="text/javascript">		var RhbTodo = RhbTodo || [];
		RhbTodo.push(function(){
			//var colHeader = jQuery('.vereina_image').parents('.collapsible_header').first();
			var colHeader = jQuery('.vereina_image');
			colHeader.on('click', function(){
				var image = jQuery(this).find('img');

				if(image.parents('.collapsible').first().hasClass('is_expanded')){
					image.attr('src', image.attr('data-inactive'));
				} else{
					image.attr('src', image.attr('data-active'));
				}
