<script>
	function showMoreCategories() {
		var targetEl = '.category-wrap',
			triggerEl = '.mobile-faded-effect,.show-more-categories';
		$(triggerEl).on('click', function () {
			$(targetEl).toggleClass('active');
			$('.show-more-categories .text').text(function (i, text) {
				return text === "Show More Types" ? "Show Less Types" : "Show More Types";
			})
			$('.show-more-categories .icon').toggleClass('top');
		});
	}
	showMoreCategories();
</script>

	
