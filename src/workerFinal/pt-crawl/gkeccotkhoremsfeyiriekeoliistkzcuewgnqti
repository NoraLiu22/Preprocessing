<section class="relative w-full group lg:advertisement-filled:my-16 md:advertisement-filled:my-16 sm:advertisement-filled:my-8" data-advertisement="pos_2 desktop pos_2:desktop" data-size="full">
<div class="group-advertisement-empty:hidden z-10 absolute top-0 right-0 bottom-0 left-0 bg-shade-lightest dark:bg-black text-shade-lightest dark:text-black" style="box-shadow: 0 0 0 4px currentColor;">
</div>
<div class="z-10 relative w-full">
<div class="iqdcontainer md:advertisement-filled:overflow-hidden sm:advertisement-filled:overflow-hidden text-center" data-placement="pos_2" data-device="desktop" data-advertisement="pos_2 desktop pos_2:desktop lg md sm"></div>
</div>
</section>
<div class="lg:mb-16 md:mb-16 sm:mb-8"></div>
<section class="bg-white dark:bg-dm-shade-darkest shadow rounded mb-16">
<div data-area="article-teaser-list" class="z-10 w-full">
<header class="sm:pt-16 sm:pb-8">
<div class="z-20 w-full sm:pb-16" style="top: var(--polygon-headerbar-height, 0px);" data-block-el="title">
<div class="lg:flex md:flex sm:flex lg:justify-between md:justify-between sm:flex-col sm:pt-16 md:pt-40 lg:pt-40 lg:px-24 md:px-24" data-area="title-section">
<h2 class="lg:mr-24 md:mr-24 sm:mx-16">
<span class="text-black dark:text-shade-lightest flex justify-between">
<span class="flex lg:justify-start md:justify-start sm:justify-between text-primary-base dark:text-dm-primary-base font-brandUI font-extrabold lg:text-2xl md:text-2xl sm:text-xl leading-tight sm:w-full">
<span>Alle Beiträge
</span>
</span>
</span>
</h2>
</div>
</div>
</header>
<div class="border-separator-b lg:mx-24 md:mx-24 sm:mx-16 lg:mt-24 md:mt-24 h-px"></div>
