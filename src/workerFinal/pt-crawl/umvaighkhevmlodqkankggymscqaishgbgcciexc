<script type="text/javascript">
+function() {
    "use strict";

    // Show ada-toolbar btn after page is loaded
    window.onload = function() {
        document.getElementById("ada-toolbar").style.display = "block";
    };

    var activeModal = null,
        i,
        toggleModal =  function(id) {
            id = (typeof id !== 'undefined') ?  id : null;

            // Close if another modal is visible
            if (activeModal) {
                activeModal.style.display = "none";
            }
            // Select the modal if id is provided
            if (id) {
                activeModal = document.getElementById(id);
                activeModal.style.display = "block";
            }
            // Get the links in the modal and the event handlers
            var link = activeModal.querySelector(".ada_modal--link"),
                closeBtn = activeModal.querySelector(".ada_modal--close"),
                linkHandler = function(e) {
                    if (e.which === 9) {
                        e.preventDefault();
                        closeBtn.focus();
                    }
                },
                closeHandler = function(e) {
                    if (e.which === 9) {
                        e.preventDefault();
                        link.focus();
                    }
                },
                escButtonHandler = function(e) {
                    if (e.which === 27 && activeModal !== null) {
                        e.preventDefault();
                        activeModal.style.display = "none";
                    }
                },
                closeModalHandler = function(e) {
                    if (e.target == activeModal) {
                        e.preventDefault();
                        e.stopPropagation();
                        toggleModal();
                    }
                };

            if (id === null) {
                // remove the event listeners
                activeModal.removeEventListener('click', closeModalHandler);
                link.removeEventListener('keydown', linkHandler);
                closeBtn.removeEventListener('keydown', closeHandler);
                document.removeEventListener('keydown', escButtonHandler);
                activeModal.style.display = "none";
                activeModal = null;
                return;
            }
            // add event listeners for tab, shift + tab and close modal
            activeModal.addEventListener('click', closeModalHandler);
            closeBtn.addEventListener('keydown', closeHandler);
            link.addEventListener('keydown', linkHandler);
            document.addEventListener('keydown', escButtonHandler);
            link.focus();
        };

    // init the modals
    var modals = document.querySelectorAll("[data-action=modal]");
    for (i = 0; i < modals.length; i++) {
        modals[i].addEventListener('click', function(e) {
            e.preventDefault();
            toggleModal(this.getAttribute("data-modal-id"));
        });
    }

    // button for enabling ada mode
    document.querySelector("[data-action=accessible]").addEventListener("click", function() {
        window.location.href = "?ada_mode=1";
    });

    // buttons for closing the modal
    var closeBtns = document.querySelectorAll("[data-action=close]");
    for (i = 0; i < closeBtns.length; i++) {
        closeBtns[i].addEventListener("click", function(e) {
            e.preventDefault();
            toggleModal();
        });
    };

    // remove the ada toolbar
    document.querySelector("[data-action=remove-accessible]").addEventListener("click", function(e) {
        e.preventDefault();
        document.getElementById("ada-toolbar").style.display = "none";
        toggleModal();
    });

    var observer = new MutationObserver(function(mutations) {
        mutations.forEach(function(mutation) {
            if (!mutation.addedNodes) return;

            if (document.body.firstChild.id !== "ada-toolbar") {
                document.body.insertBefore(document.getElementById("ada-toolbar"), document.body.childNodes[0]);
                document.body.insertBefore(document.getElementById("ada-modal-on"), document.body.childNodes[1]);
                document.body.insertBefore(document.getElementById("ada-modal-off"), document.body.childNodes[1]);
            }
        })
    });

    observer.observe(document.body, {
        childList: true
        , subtree: true
        , attributes: false
        , characterData: false
    });
}();
</script>
<script>

    function startCounters() {
        $('.counter').each(function () {
            var $this = $(this),
                countTo = $this.attr('data-count');
            $({
                countNum: 0
            }).animate({
                countNum: countTo
            },
                {
                    duration: 600,
                    easing: 'linear',
                    step: function () {
                        $this.text(Math.round(this.countNum * 10 / 10));
                    },
                    complete: function () {
                        $this.text(this.countNum);
                    }
                });
        });
    }

    var countersTriggered = 0;

    function isCountersInViewport() {
        if ((($("#id_82930dbf-63df-5c2a-950f-3f6b0bfcddd0").offset().top) < window.pageYOffset) && (countersTriggered == 0)) {
            startCounters();
            countersTriggered = 1;
        }
    }

    function checkCurrentViewPort() {
        var screenWidth = window.innerWidth;
        if (screenWidth >= 992) {
            return "desktop";
        }
        else if ((screenWidth <= 991) && (screenWidth >= 768)) {
            return "tablet";
        } else {
            return "mobile";
        }
    }

    function TabsToCarousel() {
        if (checkCurrentViewPort() == "mobile") {
            $("div.inner-container").addClass("carousel");
            $("div.inner-container").attr('data-interval', 'false');

            $("div.tabs-data").removeClass("tab-content");
            $("div.tabs-data").addClass("carousel-inner");

            $("div.tabs-data > div").removeClass("tab-pane");
            $("div.tabs-data > div").removeClass("fade");
            $("div.tabs-data > div").addClass("carousel-item");
        }
        else {
            $("div.inner-container").removeClass("carousel");
            $("div.inner-container").removeAttr("data-interval");

            $("div.tabs-data").addClass("tab-content");
            $("div.tabs-data").removeClass("carousel-inner");

            $("div.tabs-data > div").addClass("tab-pane");
            $("div.tabs-data > div").addClass("fade");
            $("div.tabs-data > div").removeClass("carousel-item");
        }
    }


    function isHeaderSticky() {
        if ($("#sticky-header-start").offset().top < window.pageYOffset + 80) {
            $('#navbar_top').addClass('fixed-top');
        } else {
            if (checkCurrentViewPort() == "desktop") {
                $('#navbar_top').removeClass('fixed-top');
            }
        }
    }

    function addFixedTopHeader() {
        if (checkCurrentViewPort() != "desktop") {
            $('#navbar_top').addClass('fixed-top');
        } else {
            $('#navbar_top').removeClass('fixed-top');
        }

    }

    // * Validation funcs 
    function validateEmail(email) {
        var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        return re.test(email);
    }

    function validateZip(zip) {
        var re = /(^\d{5}$)|(^\d{5}-\d{4}$)/.test(zip);
        return re;
    }

    $(function () {

        $("#audience-arrows a.nav-tab-slide").on("click", function () {
            startCounters();
        });

        $(window).resize(function () {
            TabsToCarousel();
            addFixedTopHeader();
        });

        TabsToCarousel();
        addFixedTopHeader();

        $(".ad-solutions .nav .nav-item.professional a").click(function () {
            $(".ad-solutions-btns button.cbtn").hide();
            $(".ad-solutions-btns button.pbtn").show();
        });
        $(".ad-solutions .nav .nav-item.consumer a").click(function () {
            $(".ad-solutions-btns button.pbtn").hide();
            $(".ad-solutions-btns button.cbtn").show();
        });

        $(window).scroll(function () {
            isCountersInViewport();
            isHeaderSticky();
        });


        $('#collapsibleNavbar > ul > li:not(.dropdown) > a').on('click', function (e) {
            e.preventDefault();
            if (checkCurrentViewPort() != "desktop") {
                $("#collapsibleNavbar").removeClass("show");
                $("#navbar_top button").addClass("collapsed");
            }
            $('html, body').animate(
                {
                    scrollTop: $($(this).attr('href')).offset().top - 224,
                },
                500,
                'linear'
            );


        });


        // *** Form Validation 
        $('#form-contact-us').on('submit', function (e) {
            var haveErr = false;
            var flds = ['first_name', 'last_name', 'company', 'brand-name', 'brand-url', 'phone', 'email', 'email-confirm',
                'city', 'state', 'zip', 'industry', 'audience', '00N50000003Mo8d'];
            // clear errors 
            $('.fld').css({ 'border-color': '#a4a4a4' });
            $('.fld').removeClass('fld-error');
            // check required fields 
            var fld, el;
            for (var i = 0; i < flds.length; i++) {
                fld = flds[i];
                el = $('#' + fld);
                if (el.hasClass('required')) {
                    if (el.val().trim() == "") {
                        el.css({ 'border-color': '#D10000' });
                        el.addClass("fld-error");
                        haveErr = true;
                        console.log(fld + " has error");

                    }
                }
            }
            // check specific fields
            var email = $('#email').val();
            var emailConfirm = $('#email-confirm').val();
            var zip = $('#zip-code').val();

            if (zip && !validateZip(zip)) {
                $('#zip-code').css({ 'border-color': '#D10000' });
                $('#zip-code').addClass("fld-error");
                haveErr = true;
                console.log("zip-code has error");

            }
            if (email && !validateEmail(email)) {
                $('#email').css({ 'border-color': '#D10000' });
                $('#email').addClass("fld-error");
                haveErr = true;
                console.log("email has error");

            }
            if (email && (email != emailConfirm)) {
                $('#email-confirm').css({ 'border-color': '#D10000' });
                $('#email-confirm').addClass("fld-error");
                haveErr = true;
                console.log("email-confirm has error");

            }
            var e;
            if (haveErr) {
                console.log('error!');
                //$('#err-form').append("Please correct the errors and resubmit.<br>")		

                e.preventDefault();
                return false;
            } else {
                console.log('Success!');
                // success 
                $('#btn-submit').prop('disabled', true);
                return true;
            }

        });
    });

</script>
