                        <div class="diagram svg-diagram">
                            <h3>Citations by Year</h3>
                            <div class="diagram ci-diagram" aria-label="citations by year"></div>
                            <style>
                                rect:hover {
                                    fill: steelblue;
                                }
                                .axis path,
                                .axis line {
                                    fill: none;
                                    stroke: #000;
                                    shape-rendering: crispEdges;
                                }
                                .bar {
                                    fill: steelblue;
                                }
                            </style>
                            
    <style>
        rect:hover {
            fill: steelblue;
        }
        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }
        .bar {
            fill: steelblue;
        }
        a:hover .bar, a:focus .bar {
            stroke: black;
            fill-opacity: 60%;
        }
    </style>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".ci-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:2001, frequency:1 },{ year:2003, frequency:1 },{ year:2004, frequency:4 },{ year:2005, frequency:32 },{ year:2006, frequency:54 },{ year:2007, frequency:74 },{ year:2008, frequency:79 },{ year:2009, frequency:124 },{ year:2010, frequency:136 },{ year:2011, frequency:122 },{ year:2012, frequency:142 },{ year:2013, frequency:288 },{ year:2014, frequency:402 },{ year:2015, frequency:511 },{ year:2016, frequency:565 },{ year:2017, frequency:715 },{ year:2018, frequency:739 },{ year:2019, frequency:874 },{ year:2020, frequency:959 },{ year:2021, frequency:1015 },{ year:2022, frequency:1013 },{ year:2023, frequency:805 },{ year:2024, frequency:150 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=rj%3A3201+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                        </div>
                    </div>
                
            
        </article>
    </div>
        </div>
    

