ating Today","tags":{"nodes":[]},"slug":"astronaut-food-nasa-history","hubPageDisplayOverrides":{"articleListTitle":null}},{"id":"cG9zdDoxNjk2MTY=","databaseId":169616,"authorTags":{"nodes":[{"name":"Joe Baur","uri":"/authors/joe-baur/","count":1}]},"categories":{"nodes":[{"uri":"/category/travel/","name":"Travel","slug":"travel","databaseId":23,"parentDatabaseId":null},{"uri":"/category/culture/","name":"Culture","slug":"culture","databaseId":5952,"parentDatabaseId":null},{"uri":"/category/features/","name":"Features","slug":"features","databaseId":5955,"parentDatabaseId":5952},{"uri":"/category/europe/","name":"Europe","slug":"europe","databaseId":5971,"parentDatabaseId":23}]},"primaryCategory":{"uri":"/category/culture/","name":"Culture","slug":"culture","databaseId":5952,"parentDatabaseId":null},"excerpt":"\u003cp\u003eThese history-soaked canteens serve Polish comfort food with a hefty side of nostalgia.\u003c/p\u003e\n","featuredImageAltText":"Milk Bar","featuredImageUrl":"https://www.saveur.com/uploads/2024/05/image_02.jpg?auto=webp","featuredImageListUrl":"https://www.saveur.com/uploads/2024/05/image_02.jpg?auto=webp","featuredImage":{"node":{"altText":"Milk Bar","caption":null,"cropPosition":"","id":"cG9zdDoxNjk2NDg=","sourceUrl":"https://www.saveur.com/uploads/2024/05/image_02.jpg?auto=webp","title":"image_02","mediaDetails":{"height":1094,"width":1944},"mediaAdditionalData":{"mediaSubtitle":null,"mediaCredit":{"mcName":"Raphael Olivier"}}}},"featuredImageVertical":{"featuredImageVertical":null},"date":"2024-05-03T12:25:05","modified":"2024-05-03T12:25:05","showModifiedDate":"","uri":"/culture/best-milk-bars-krakow/","title":"How to Take a Milk Bar Crawl Through Krakow","tags":{"nodes":[]},"slug":"best-milk-bars-krakow","hubPageDisplayOverrides":{"articleListTitle":null}},{"id":"cG9zdDoxNjkwOTQ=","databaseId":169094,"authorTags":{"nodes":[{"name":"Romy Gill","uri":"/authors/romy-gill/","count":22}]},"categories":{"nodes":[{"uri":"/category/techniques/","na