    <td><a href="../chronomedia_years" target="_top" onClick="MM_nbGroup('down','group1','chronomedia','../../chron.gif',1)" onMouseOver="MM_nbGroup('over','chronomedia','','',1)" onMouseOut="MM_nbGroup('out')"><img name="chronomedia" src="../../chron.gif" border="0" alt="" onLoad=""></a></td>
    <td><a href="../../search.htm" target="_top" onClick="MM_nbGroup('down','group1','headblank2','',1)" onMouseOver="MM_nbGroup('over','headblank2','','',1)" onMouseOut="MM_nbGroup('out')"><img name="headblank2" src="../../head_blank.gif" border="0" alt="" onLoad=""></a></td>
    <td><a href="../../quotations/index.htm" target="_top" onClick="MM_nbGroup('down','group1','quotehead','',1)" onMouseOver="MM_nbGroup('over','quotehead','','',1)" onMouseOut="MM_nbGroup('out')"><img name="quotehead" src="../../quote_head.gif" border="0" alt="Quotations department" onLoad=""></a></td>
    <td><a href="../../media/index.htm" target="_top" onClick="MM_nbGroup('down','group1','mediahead','',1)" onMouseOver="MM_nbGroup('over','mediahead','','',1)" onMouseOut="MM_nbGroup('out')"><img name="mediahead" src="../../media_head.gif" border="0" alt="Media department" onLoad=""></a></td>
    <td><a href="../../reference/index.htm" target="_top" onClick="MM_nbGroup('down','group1','refhead','',1)" onMouseOver="MM_nbGroup('over','refhead','','',1)" onMouseOut="MM_nbGroup('out')"><img name="refhead" src="../../ref_head.gif" border="0" alt="Reference department" onLoad=""></a></td>
