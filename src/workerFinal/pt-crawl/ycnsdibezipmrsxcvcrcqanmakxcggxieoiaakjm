<script type="text/plain" class="optanon-category-C0003"  defer='defer' src="https://robbreport.com/wp-content/plugins/pmc-plugins/pmc-recaptcha/assets/build/public.js?ver=0b3f78c206a4efe19201" id="pmc-recaptcha-public-js"></script>
		
<!-- START callback function for onhashchange(when the hash tag in the url gets updated) -->
<script type="text/javascript">
function global_urlhashchanged() {

	/**
	 * Track pageview
	 */
	// Build the current domain name
	var tmp_td = document.domain.split( '.' );
	var tracking_domain = tmp_td[ tmp_td.length - 2 ] + "." + tmp_td[ tmp_td.length - 1 ];

	// Build the current image
	var imgname = location.hash.replace( '#', '' );

	// Track PV in GA
	try {
		ga( 'send', 'pageview', location.pathname + location.hash );

		if ( 'undefined' !== typeof window.dataLayer ) {
			window.dataLayer.push(
				{
					event: 'VirtualPageView',
					'pageview-type': 'virtual_page'
				}
			);
		}
	} catch( err ) {}

	// SADE-103 - removed comscore pageview, this is being loaded via the 'pmc-comscore' plugin
	// PMCVIP-2580. Try PV in Keywee
	try {
		if ( typeof window.snowplowKW == 'function' ) {
			window.snowplowKW( 'trackPageView' );
		}
	} catch ( err ) {
	}
	// Track Quantcast
	try {
		_qevents.push([
			{ qacct:"p-31f3D02tYU8zY" } // PMC P-Code
		]);
	} catch( err ) {}

	/**
	 * Track pageview end
	 */
}
</script>

