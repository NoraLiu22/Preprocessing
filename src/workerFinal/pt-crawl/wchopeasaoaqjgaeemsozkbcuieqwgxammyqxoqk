  if (jQuery(this).attr('src').indexOf("youtu") >= 0){
      jQuery(this).prop('id','ytplayer'+ iframe_count);
      var src= jQuery(this).prop('src');
    //if source !contain ?
    if (!src.includes('?') ){
      src = src + '?enablejsapi=1';
    } else {
      src = src + '&enablejsapi=1';
    }
      jQuery(this).prop('src',src);
      iframe_count++;
  }// end if youtu be
 }); //end iframe each

//console.log( iframe_count );

