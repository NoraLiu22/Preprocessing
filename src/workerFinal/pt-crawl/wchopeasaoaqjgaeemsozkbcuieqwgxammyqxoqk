<script>
  //jQuery('document').ready(function(){
    var bg_color = 'Web Blue - #005491';
    if ('' == 'Yes'){
      //UF Branded Skin checked
      //apply class if text_color contains "UF Blue" or "UF Orange"
      if (bg_color.includes('UF Blue')){
        jQuery('.button_banner_content573183').removeClass('default-style573183');
        jQuery('.bb-button573183').removeClass('default-style573183');
        jQuery('.button_banner_content573183').addClass('uf-branded-blue');
        jQuery('.bb-button573183').addClass('uf-branded-blue');
      } else if (bg_color.includes('UF Orange')){
        jQuery('.button_banner_content573183').removeClass('default-style573183');
        jQuery('.bb-button573183').removeClass('default-style573183');
        jQuery('.button_banner_content573183').addClass('uf-branded-orange');
        jQuery('.bb-button573183').addClass('uf-branded-orange');
      }else if (bg_color.includes('Grey Light') || bg_color.includes('White') ){
        jQuery('.button_banner_content573183').removeClass('default-style573183');
        jQuery('.bb-button573183').removeClass('default-style573183');
        jQuery('.button_banner_content573183').addClass('grey-text');
        jQuery('.bb-button573183').addClass('grey-text');
      }
    } else {
      //UF Branded Skin NOT checked
      if (bg_color.includes('Grey Light') || bg_color.includes('White') ){
        jQuery('.button_banner_content573183').removeClass('default-style573183');
        jQuery('.bb-button573183').removeClass('default-style573183');
        jQuery('.button_banner_content573183').addClass('uf-blue-text');
        jQuery('.bb-button573183').addClass('uf-blue-text');
      }
    }
  
  //replace .blue-text with .uf-blue-text
  jQuery('.blue-text').addClass('uf-blue-text');
  jQuery('.blue-text').removeClass('blue-text');
  //});
</script>
