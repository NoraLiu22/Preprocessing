            <div id="s4-bodyContainer">	                    
                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Resources and Tools" class="ms-sitemapdirectional" href="/ParentsAndStudents/Pages/home.aspx">Parents and Students</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/ParentsAndStudents/EnrollingInSchool/Pages/home.aspx">Enrolling in School</a></span><span> &gt; </span><span class="current">How to Enroll</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">How to Enroll</h2>
                
    



				
				<p class="intro">
					Students enrolling in our public schools must have documentation proving they are residents of the state and if necessary, documents from their previous school and other legal documents. Resources and opt-outs exist for students in exceptional circumstances. The necessary information is detailed here. 
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

