<script id="if-so-js-before" type="text/javascript">
var nonce = "d674c37bab";
    var ajaxurl = "https://www.forbes.com/advisor/wp-admin/admin-ajax.php";
    var ifso_page_url = "https://www.forbes.com/advisor/credit-cards/reviews/american-express-gold-card/";
