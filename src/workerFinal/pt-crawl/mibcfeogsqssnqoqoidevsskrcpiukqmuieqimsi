		<link rel="alternate" href="https://www.forbes.com/advisor/credit-cards/reviews/american-express-gold-card/" hreflang="en-us"  /><link rel="alternate" href="https://www.forbes.com/uk/advisor/credit-cards/reviews/american-express-preferred-rewards-gold-credit-card/" hreflang="en-gb"  /><link rel="alternate" href="https://www.forbes.com/advisor/it/carta-di-credito/american-express-carta-oro-recensione/" hreflang="it-it"  /><link rel="alternate" href="https://www.forbes.com/advisor/ca/credit-cards/reviews/american-express-gold-rewards-card/" hreflang="en-ca"  /><script>
    window.__mpResponseHeaders = (function () {
        let isProduction = true;
        if (isProduction === false) {
            return {
                'x-postal-code': '10001',
                'x-country-code': 'us',
                'x-region': 'ny',
                'x-city-code': 'leesburg',
            };
        }

        const makeRequest = function () {
            const request = new XMLHttpRequest();
            request.open('HEAD', "https:\/\/www.forbes.com\/advisor\/wp-content\/themes\/fadv\/assets\/img\/ZipImage\/1x1.jpg", false);
            request.send(null);
            return request;
        }

        const collectAllResponseHttpHeaders = function (response) {
            const result = {};
            response.getAllResponseHeaders().toLowerCase().split('\n').forEach((header) => {
                let index = header.indexOf(':');
                if(index !== -1){
                    let headerName = header.substring(0, index).trim();
                    let headerValue = header.substring(index + 1).trim();
                    if (!!headerName && !!headerValue) {
                        result[headerName] = headerValue;
                    }
                }
            });
            return result;
        }

        const filterGeoHeaders = function (headers) {
            const filteredHeaders = {};
            for (let headersKey in headers) {
                let hasWhitelistedHeader = ['x-postal-code', 'x-region', 'x-country-code', 'x-city-code'].includes(headersKey);
                if (hasWhitelistedHeader) {
                    filteredHeaders[headersKey] = headers[headersKey];
                }
            }
            return filteredHeaders;
        }

        const response = makeRequest();
        const responseHeaders = collectAllResponseHttpHeaders(response);
        return filterGeoHeaders(responseHeaders);
    })();
</script><ra-title title='American Express Gold Card Review [year]'></ra-title><meta name="format-detection" content="telephone=no" /><meta name="google-site-verification" content="jGqy0685OfScYI_CPpQIY0veB6wppKqi8yiG6FipWBM" /><meta name="fo-verify" content="66cd52e3-8bd5-41b7-a018-a83982c07dc5"><link rel="alternate" type="application/rss+xml" href= "https://www.forbes.com/advisor/feed/"><link href='https://thumbor.forbes.com/thumbor/fit-in/900x510/https://www.forbes.com/advisor/wp-content/uploads/2021/02/47859e30-66f2-11eb-84e8-4d1313fc3187.png' rel='preload' as='image' imagesrcset='https://thumbor.forbes.com/thumbor/fit-in/1290x/https://www.forbes.com/advisor/wp-content/uploads/2021/02/47859e30-66f2-11eb-84e8-4d1313fc3187.png 1290x, https://thumbor.forbes.com/thumbor/fit-in/960x/https://www.forbes.com/advisor/wp-content/uploads/2021/02/47859e30-66f2-11eb-84e8-4d1313fc3187.png 960x, https://thumbor.forbes.com/thumbor/fit-in/600x300/https://www.forbes.com/advisor/wp-content/uploads/2021/02/47859e30-66f2-11eb-84e8-4d1313fc3187.png 600x300, '><link rel="icon" href="https://www.forbes.com/advisor/wp-content/uploads/2017/09/cropped-favicon-32x32.png" sizes="32x32" />
