<script>
    (function handleExternalLinking() {
        window.partnerNames = [];
        var articleBody = '.article-link-wrapper';
        var siteUrl = 'https://www.forbes.com/advisor';

        const stripTrailingSlash = str=>str.charAt(str.length-1)=="/"?str.substr(0,str.length-1):str;

        if (document.querySelector(articleBody)) {
            var elm = document.querySelectorAll(articleBody + ' a');
            
            for (var i = 0; i < elm.length; i++) {
                var $currElm = elm[i];
                var link = $currElm.getAttribute('href');
                if(!link){ continue; }
                    var hasQueryParam = link.indexOf("?"),
                    newRel = $currElm.getAttribute('rel') || '';

                                            const matches = link.match(/^https?\:\/\/([^\/?#]+)(?:[\/?#]|$)/i);
                        
                        if(matches && matches[0]){
                            const partnerURL = stripTrailingSlash(matches && matches[0]);
                            const partnerLinksObj = window.partnerConfig['links'];
                            const bankRateLID = window.partnerConfig['bankrate_lid'];
                            var queryParamURL = new URL(link);

                            if(partnerLinksObj && queryParamURL.href.includes('/click-8860193-')){
                                window.partnerNames.push(partnerLinksObj['/click-8860193-'])  
                            }

                            else if(partnerLinksObj[partnerURL] && partnerURL == 'https://oc.brcclx.com'){
                                if(queryParamURL.searchParams.has("lid")){
                                    var lidValue = queryParamURL.searchParams.get("lid");
                                    if(bankRateLID[lidValue]){
                                        window.partnerNames.push(bankRateLID[lidValue])
                                    }else{
                                        window.partnerNames.push(partnerLinksObj[partnerURL])
                                    }
                                }else{
                                    window.partnerNames.push(partnerLinksObj[partnerURL])
                                }
                            }
                            else if(partnerLinksObj[partnerURL]){
                                window.partnerNames.push(partnerLinksObj[partnerURL])
                            }
                        }
                             
                
                link = hasQueryParam < 0 ? link : link.substring(0, hasQueryParam) ;
                if (link && link.indexOf(siteUrl) == -1 && (link.indexOf("http://") == 0 || link.indexOf("https://") == 0)) {
                    newRel = newRel.indexOf('noopener') < 0 ? newRel + ' noopener': newRel;
                    $currElm.setAttribute('rel', newRel.trim());
                    if(!$currElm.hasAttribute("target")){
                        $currElm.setAttribute('target', '_blank');
                    }
                }
            }
        }
    })();
</script><div class="search-modal" style="display:none">
