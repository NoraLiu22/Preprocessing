        var detectDeviceType = getDeviceTypeInNumber(window.deviceType);
        var dynamicTitle = "Highlights";
        if(detectDeviceType !== 1){
            $('.pc-section h3').on('click', handleTogglePCSection);
        }
        function handleTogglePCSection() {
            var pcSectionEl = $(this).parent();
            pcSectionEl.find('ul').slideToggle();
            pcSectionEl.toggleClass('collapsed');
        }
        $('.highlights .fade-effect, .toggle-highlights').on('click', handleToggleHighlights);
        function handleToggleHighlights() {
            var highlightEl = $('.highlights');
            let hideText = 'Show less';
            let showText = 'Show more'
            if (dynamicTitle === "Highlights") {
                hideText = 'Less Highlights';
                showText = 'More Highlights';
            }
            toggleText = highlightEl.hasClass('closed') ? hideText : showText;
            highlightEl.toggleClass('closed');
            highlightEl.find('span').text(toggleText);
        }
        if(detectDeviceType === 2 || detectDeviceType === 3){
            $('.cons-section').addClass('collapsed');
        }
    });
</script>                                
                                <hr class="sections-separator"/>
                                <div class="cc-review-sections">
                                    
