function cookiePref() {
    const elements = document.getElementsByClassName('tnt-section-cookie-preferences');
    for (const element of elements) {
        element.setAttribute("onclick", 'Osano.cm.showDrawer("osano-cm-dom-info-dialog-open")');
        element.style.cursor = "pointer";
    }
}
cookiePref();
</script>

