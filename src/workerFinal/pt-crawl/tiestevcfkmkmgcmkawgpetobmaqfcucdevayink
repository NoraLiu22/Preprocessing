    <!-- little script to begin focus on page to first element with autofocus class, will work on any page _layout is used -->
<script>
        $(function () {
            $('.autofocus:first').focus();
        });

        //method to check if text is in a proper date format
        //todo: update datepickers to use this and inform users of bad input when appropriate. also consider using
        //moment.js for dates.
        function isValidDate(datestring, format) {
            var isValid = true;

            try {
                $.datepicker.parseDate(format, datestring, null);
            }
            catch (error) {
                isValid = false;
            }

            return isValid;
        }
        //apply a simple 7 or 10 digit phone mask to any input field
        function ApplyPhoneMask(selector, characterCount) {
            var element = $(selector);
            element.inputmask('remove');
            if (characterCount <= 7) {
                element.inputmask({ mask: '999-9999[999]', placeholder: " ", autoUnmask: true, removeMaskOnSubmit: true });
            }
            else {
                element.inputmask({ mask: '999-999-9999', placeholder: " ", autoUnmask: true, removeMaskOnSubmit: true });
            }
        }

        window.addEventListener('load', function () {
            var headerContainer = document.querySelector('.navbar-nerain');
            var contentWrapper = document.querySelector('.content-wrapper');
            var footerContainer = document.querySelector('.footer-container');

            function adjustFooterPosition() {
                var navbarHeight = headerContainer.clientHeight;
                var contentHeight = contentWrapper.clientHeight;
                var viewportHeight = window.innerHeight;
                var footerHeight = footerContainer.clientHeight;

                if (navbarHeight + contentHeight + footerHeight < viewportHeight) {
                    footerContainer.classList.add('sticky-footer');
                } else {
                    footerContainer.classList.remove('sticky-footer');
                }
            }

            // Initial adjustment
            adjustFooterPosition();

            // Re-adjust on window resize, click
            window.addEventListener('resize', adjustFooterPosition);

            // Create a ResizeObserver instance
            var mainContent = document.getElementById("mainContent");
            var resizeObserver = new ResizeObserver(function (entries) {
                for (let entry of entries) {
                    // Check if the height of mainContent has changed
                    if (entry.target === mainContent && entry.contentRect.height !== entry.targetHeight) {
                        adjustFooterPosition();
                        entry.targetHeight = entry.contentRect.height; // Store the new height
                    }
                }
            });

            // Start observing the mainContent element
            resizeObserver.observe(mainContent, { box: 'content-box' });
            mainContent.targetHeight = mainContent.clientHeight; // Store the initial height

        });
        

        $.ajax({
            url: '/NeRain/Home/NavBar',
            success: function (result) {
                $(".loader").html(result);
            }
        });
</script>

    
