	document.querySelector('#coverzoom').addEventListener('click',function(e){
    var cursor = e.target.style.cursor;
    if (cursor=='zoom-in') zoomCover(document.querySelector('.coversquare').style.backgroundImage)},false);
    
  function zoomCover(sFilename) {
  	coverFile = sFilename.substr(sFilename.lastIndexOf('/')+1,sFilename.length-3-sFilename.lastIndexOf('/'));
  	var zoomImg = new Image();
		zoomImg.onload = function(){
  	var height = zoomImg.height;
  	var width = zoomImg.width;
  	zoomWidth = Math.round((window.innerWidth)*.8);
  	zoomHeight = Math.round(zoomWidth/width*height);
  	if (zoomHeight>window.innerHeight*.8) {
	  	zoomHeight = Math.round((window.innerHeight)*.8);
  		zoomWidth = Math.round(zoomHeight/height*width);
  	}
  	if (zoomHeight>height) {
  		zoomHeight=height;
  		zoomWidth=width;
  	}
  	document.querySelector('#zoom_mobile_inner').innerHTML='<img src="https://media.hitparade.ch/cover/big/' + coverFile + '" style="width:'+zoomWidth+'px;height:'+zoomHeight+'px;">';
		}

  	document.querySelector('#zoom_mobile_dark').classList.remove('hidden');
  	document.querySelector('#zoom_mobile').classList.remove('hidden');
  	//document.querySelector('#zoom_mobile_inner').innerHTML='<img src="https://media.hitparade.ch/cover/big/' + coverFile + '" style="width:600px;height:600px;">';
  	zoomImg.src = 'https://media.hitparade.ch/cover/big/' + coverFile;
  }
	
	</script>
	
