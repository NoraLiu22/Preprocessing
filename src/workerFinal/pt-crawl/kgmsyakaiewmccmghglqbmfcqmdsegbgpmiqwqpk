            vipform.submit(function (e) {

                e.preventDefault();

                $.ajax({
                    type: vipform.attr('method'),
                    url: '/events/vip2/',
                    data: vipform.serialize(),
                    success: function (data) {
                        $("#brand-marketer-program-form").collapse('toggle');
                        $("#brand-marketer-program-form-result").html(data);
                        $('#brand-marketer-program-form-success').modal('show');
                        setTimeout(function() {$('#brand-marketer-program-form-success').modal('hide');}, 2000);
                    },
                    error: function (data) {
                        console.log('An error occurred.');
                        console.log(data);
                        $("#brand-marketer-program-form-result").html(data.responseText);
                    },
                });
            });

            var subscribeform = $('#subscribe-form');
