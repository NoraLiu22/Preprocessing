  <div class='article_container'>
    <h1>
      Steroids
    </h1>
    <div class='adwords_in_content'>
<hr>
<script type="text/javascript"><!--
google_ad_client = "ca-pub-5788426211617053";
/* medical discoveries top */
google_ad_slot = "7192559753";
google_ad_width = 728;
