<script>
  $(document).ready(function () {
   $("#messageicon").on("click", function () {
    var epart1 = $('#obstruct').attr('data-madr1');
    var epart2 = $('#obstruct').attr('data-madr2');
    var epart3 = $('#obstruct').attr('data-madr3');
    var adr1 = 'info';
    var adr2 = 'ncsl';
    console.log(adr1);

    if(epart1) {
    $('.cf-send-email input').attr('value', epart1 + '\@' + epart2 + '.' + epart3);
    } else {
     $('.cf-send-email input').attr('value', adr1 + '\@'  + adr2 + '.org');
    }
});
   
  });
