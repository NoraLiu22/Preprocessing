     <div class="row content">
      <div class="twelve columns" id="content-main"> 
       <div class="region region-content">
    <div id="block-system-main" class="block block-system">

    
  <div class="content">
    <div class="view view-timemachine-featured-collections view-id-timemachine_featured_collections view-display-id-page_1 articles top-spotlight view-dom-id-df53c97e4e867e02ad771d509c4855ce">
      
  
  
