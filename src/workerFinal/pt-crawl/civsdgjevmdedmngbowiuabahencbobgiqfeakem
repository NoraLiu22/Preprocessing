                			parent = $(this).parent();
                
                			if ( text.length < 80 ) {
                				parent.html('<h3 class="wish-header-title">' + text + '</h3>');
                			}
                		});
                	}
                
                	var badHeaders = $('.page > div > div.fusion-fullwidth > div > .fusion-layout-column > div > .fusion-title > h2:first-child:last-child p:first-child:last-child');
                
                	if ( badHeaders.length ) {
                		$.each(badHeaders, function() {
                			var text = $(this).text(),
                				parent = $(this).parents('.fusion-title');
                
                			if ( text.length < 80 ) {
                				parent.html('<h3 class="wish-header-title">' + text + '</h3>');
                			}
                		});
                	}
                };
                var webAppHacksNew = new SDG.webAppHacksNew();
            } else {
                if ( $('.wish-hero').length && $('.wish-hero').width() < 500 ) {
                    $('.wish-header-title[style*="max-width: 100%"]').css('width', '100%');
                }
            }
        }, 1600);
    }
    
</script>
