                		$('.wish-header-title').first().css('width', width).css('max-width', '100%');
                	}
                
                	var headlines = $('#main .fusion-fullwidth + .fusion-fullwidth > .fusion-row > .fusion-layout-column > .fusion-column-wrapper > .fusion-text > p:first-child:last-child');
                
