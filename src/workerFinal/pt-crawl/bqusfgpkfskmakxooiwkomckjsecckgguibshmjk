                        <div id="ctl00_UP_TimeLine">
	  
                              <div style="display:none">
                                    <input name="ctl00$TB_FromYear" type="text" id="ctl00_TB_FromYear" />
                                    <input name="ctl00$TB_ToYear" type="text" id="ctl00_TB_ToYear" />
                                    <button id="ctl00_btnApplyTimeLine" type="button" onclick="document.getElementById(&#39;hidepage&#39;).style.display = &#39;block&#39;;"></button> 
