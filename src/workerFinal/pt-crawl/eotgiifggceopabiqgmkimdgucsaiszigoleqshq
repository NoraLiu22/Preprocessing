    <script>
     $(document).ready(function () {
         $('.bar-inner .sub-menu>li>a').matchHeight();
     });
    $(function () {
        $("*").on('keydown', function (event) {
            if (event.altKey && event.shiftKey && event.which == 83) {
                //console.log(event)
                event.preventDefault()
                $("#KeyWord").focus()
            }
        });
    })
</script>
