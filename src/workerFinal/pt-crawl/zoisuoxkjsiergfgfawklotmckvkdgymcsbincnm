                    try {
                        var sSubUrl = 'https://subscriberservices.lee.net/subscriberservices/Content/paymentpagesingle.aspx?Domain=missoulian.com&SubscriberLevel={{level}}&Return=https%3A%2F%2Fmissoulian.com%2Flife-entertainment%2Fnation-world%2Ffood-drink%2F'.replace('{{level}}', window.falconOffer.level);
