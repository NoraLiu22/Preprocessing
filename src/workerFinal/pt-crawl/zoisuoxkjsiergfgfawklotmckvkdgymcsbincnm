                        jQuery(document).ready(function($){
                            function throttleEvvnt(func, wait){
                                var timeout;
                                return function(){
                                    var context = this, args = arguments;
                                    var later = function(){
                                        timeout = null;
                                        func.apply(context, args);
                                    };
                                    clearTimeout(timeout);
                                    timeout = setTimeout(later, wait);
                                }
                            }
                            var evvntLoaded = false;
                            var throttledEvvnt = throttleEvvnt(function(){
                                if(!evvntLoaded){
                                    evvntLoaded = __tnt.elementOnScreen('#evvnt-widget-2070180');
                                    if(evvntLoaded){
                                        var s = document.createElement('script');
                                            s.setAttribute('src', '//discovery.evvnt.com/prd/evvnt_discovery_plugin-latest.min.js');
                                            s.onload = evvntDiscoveryInit;
                                        document.body.appendChild(s);
                                        window.removeEventListener('scroll', throttledEvvnt);
                                    }
                                }   
                            }, 100);
                            window.addEventListener('scroll', throttledEvvnt);
                        });
                    
                </script>
            </div></div></section></div><div id="tncms-block-3111877" class="tncms-block style-title overflow-scroll"><section id="block-3111877" class="block emphasis-h3   " ><div class="clearfix">
    
