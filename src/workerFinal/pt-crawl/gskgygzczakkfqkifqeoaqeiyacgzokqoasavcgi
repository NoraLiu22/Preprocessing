<script>
    if(document.getElementById("buttons") != null){
  document.getElementById("buttons").onclick = function() {showRows('0,1,2');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }
    if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
    }  
  if(document.getElementById("") != null){
  document.getElementById("").onclick = function() {showRows('');};
  }
  
  function showRows(str) {
    var array = str.split(",");
    var x = document.getElementsByClassName("complex-layout");
    for (i = 0; i < x.length; i++) {
      x[i].style.display = "none";
    }
    array.forEach(function(i) {
     if (x[i] != undefined){
      	x[i].style.display = "inline";
     }
    });
  }
  function showAll() {
    var x = document.getElementsByClassName("complex-layout");
    for (i = 0; i < x.length; i++) {
      if (x[i] != undefined){
      	x[i].style.display = "inline";
      }
    }
  }
  
  var ids_array = ['buttons', '', '', 'buttons', '', '','', '', ''];
    
  jQuery(document).ready(function(){
    if ('' != '' && jQuery.inArray( '', ids_array ) != -1 ){
      var default_view = '';
      jQuery('#breadcrumb-navigation > ul.breadcrumbs > li#' + default_view).trigger('click');
    } else {
        showRows('0');
    }
  });
</script>
