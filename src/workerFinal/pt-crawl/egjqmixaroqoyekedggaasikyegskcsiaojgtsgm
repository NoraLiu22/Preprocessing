<div class="carousel">
    <div class="cmsSlickCarousel cmsSlickCarousel-multiple" data-slick="{&quot;fade&quot;:false,&quot;slidesToScroll&quot;:1,&quot;autoplay&quot;:false,&quot;dots&quot;:true,&quot;centerMode&quot;:true,&quot;slidesToShow&quot;:1,&quot;autoplaySpeed&quot;:4000,&quot;responsive&quot;:[{&quot;breakpoint&quot;:576,&quot;settings&quot;:{&quot;slidesToShow&quot;:1,&quot;slidesToScroll&quot;:1,&quot;centerMode&quot;:true}},{&quot;breakpoint&quot;:768,&quot;settings&quot;:{&quot;slidesToShow&quot;:1,&quot;slidesToScroll&quot;:1,&quot;centerMode&quot;:true}},{&quot;breakpoint&quot;:992,&quot;settings&quot;:{&quot;slidesToShow&quot;:3,&quot;slidesToScroll&quot;:3,&quot;centerMode&quot;:false}},{&quot;breakpoint&quot;:1200,&quot;settings&quot;:{&quot;slidesToShow&quot;:3,&quot;slidesToScroll&quot;:3,&quot;centerMode&quot;:false}}]}" data-options="{&quot;captions&quot;:{&quot;576&quot;:false,&quot;768&quot;:true,&quot;992&quot;:false,&quot;1200&quot;:false},&quot;arrowsColor&quot;:{&quot;576&quot;:&quot;#f60&quot;,&quot;768&quot;:&quot;#f60&quot;,&quot;992&quot;:&quot;White&quot;,&quot;1200&quot;:&quot;White&quot;},&quot;arrowsSize&quot;:{&quot;576&quot;:2,&quot;768&quot;:2,&quot;992&quot;:3,&quot;1200&quot;:3}}" data-id="a7fe52eb-7b03-4dc3-850f-c265c1fcd930" >
