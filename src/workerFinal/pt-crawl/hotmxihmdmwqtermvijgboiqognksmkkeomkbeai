<a class="absolute opacity-none pointer-events-none" aria-hidden="true" tabindex="-1" x-effect="if (isOpen) { lastActiveElement = document.activeElement; $focus(); } else if (lastActiveElement) { $focus(lastActiveElement) }" role="none"></a>
<ul>
<li class="mb-24 mx-16" data-logged-in-hidden="true" data-app-hidden>
<a href="https://gruppenkonto.spiegel.de/11freunde/authenticate?requestAccessToken=true&targetUrl=https%3A%2F%2Fwww.11freunde.de%2F" class="w-full leading-normal font-bold bg-primary-base dark:bg-dm-primary-base inline-block hover:bg-primary-dark focus:bg-primary-darker disabled:bg-shade-lighter dark:disabled:bg-shade-darker disabled:text-shade-dark dark:disabled:text-shade-light disabled:cursor-not-allowed text-white dark:text-shade-lightest font-sansUI rounded px-24 py-12 text-base rounded" title="Anmelden" target="_self" data-sara-link="gruppenkonto">
