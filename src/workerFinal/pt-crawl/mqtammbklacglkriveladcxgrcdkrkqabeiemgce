  <div id="main_0_content_0_row1_0_column1_0_content_0_divContent" class="row">
    <div class="columns medium-12">
        
        <h1>Joint Committee on Law Enforcement</h1>
<p class="lead">The Committee monitors and reviews the performance of the ACC and the AFP as well as undertakes parliamentary supervision of Part 2-6 and section 20A of the <em>Proceeds of Crime Act 2002 (relating to unexplained wealth provisions).</em></p>
        
    </div>
</div>
</div>

    
    
    
</div>


<div class="row ">
    

<div class="medium-8 small-12 columns">
  
<script src="/js/vendor/slick/slick.min.js" type="text/javascript"></script>

<script type="text/javascript">

    jQuery(document).ready(function ($) {
        var carouselContainer = $('.js-slider');

        var addPausePlay = function () {
            // Add pause/play button
            var navigationDots = $('.slick-dots');
            if (navigationDots.length) {
                var playPauseBtn = navigationDots.find('.play-pause-btn');
                if (!playPauseBtn.length) {
                    navigationDots.append('<li class="play-pause-container"><a href="#" class="play-pause-btn" aria-label="Pause"><em class="fa fa-fw fa-pause"></em></a></li>');

                    setTimeout(function () {
                        carouselContainer.slick('slickPlay');
                    }, 3000);

                }
            }
        }

        $('.js-slider').on('click', '.play-pause-btn', function (e) {
            e.preventDefault();
            if ($('.play-pause-btn em').hasClass('fa-pause')) {
                $('.play-pause-btn em').removeClass('fa-pause').addClass('fa-play');
                $('.play-pause-btn').attr('aria-label', 'Play');
                $('.js-slider').slick('slickPause');
            }
            else {
                $('.play-pause-btn em').removeClass('fa-play').addClass('fa-pause');
                $('.play-pause-btn').attr('aria-label', 'Pause');
                $('.js-slider').slick('slickPlay');
            }
        });

	    if (carouselContainer.length) {
		    carouselContainer.on('breakpoint', function(event, slick) {
			    addPausePlay();
		    });

		    carouselContainer.not('.slick-initialized').slick({
			    dots: true,
			    speed: 600,
			    arrows: true
		    });

		    addPausePlay();
        }
        $('.js-slider').removeClass('hidden');
    });

</script>

