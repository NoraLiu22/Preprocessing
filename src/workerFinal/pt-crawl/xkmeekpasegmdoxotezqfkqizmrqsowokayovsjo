        margin-left:-35px;
    }
    div#event_title_container{
    	width:70%;
    }
    div.event_title{
    	text-align:left;
    }
  
  div#google-calendar > div.feedsb div.date_day{
    width: 200%;
    margin-left: -35px;
  }

}

@media screen and (min-width: 570px) and (max-width: 1023px){
	div#event_date{
    	margin-left:-25px;
    }
}

  /* apply min-height to equalized divs and reduce bottom margin on small */
  @media screen and (max-width: 39.9375em) {
    div#left, div#right{
      min-height:150px;
    }
