<script>
  jQuery('document').ready(function(){
    jQuery( "li.utlity-menu-link a" ).each(function(){
      //console.log(this,jQuery(this).text());
      if (jQuery(this).text() == ''){
        jQuery(this).remove();
      }
    });
//    if ("" != "Yes"){
    	jQuery('div#utility_menu_dropdown').addClass( 'hide' );
//    }
    jQuery("li.utlity-menu-link:empty").remove();
  });
