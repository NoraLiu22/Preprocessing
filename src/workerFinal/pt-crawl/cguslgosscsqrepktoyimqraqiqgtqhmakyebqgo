    <script type="text/javascript" charset="utf-8"
        src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type="text/javascript" charset="utf-8" src="./js/bootstrap.min.js"></script>
    <script src="./js/owl.carousel.min.js"></script>
    <script src="./js/main.js"></script>

    <script type="text/javascript" charset="utf-8">
        $('#allOcorrencias').empty();
        $('#allColecoes').empty();
        $('#allInstituicoes').empty();
        $('#allConjuntos').empty();
        $('#allEspecies').empty();

        $(document).ready(function () {
            $.ajax({
                url: "https://collectory.sibbr.gov.br/collectory/ws/institution/count"
            })
                .done(function (data) {
                    $('#allInstituicoes').append(data.total.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1."));
                }).fail(function (data) {
                    $('#allInstituicoes').append("0");
                });
        });
        $(document).ready(function () {
            $.ajax({
                url: "https://collectory.sibbr.gov.br/collectory/ws/collection/count"
            })
                .done(function (data) {
                    $('#allColecoes').append(data.total.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1."));
                }).fail(function (data) {
                    $('#allColecoes').append("0");
                });
        });
        $(document).ready(function () {
            $.ajax({
                url: "https://collectory.sibbr.gov.br/collectory/ws/dataResource/count"
            })
                .done(function (data) {
                    $('#allConjuntos').append(data.total.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1."));
                }).fail(function (data) {
                    $('#allConjuntos').append("0");
                });
        });
        $(document).ready(function () {
            $.ajax({
                url: "https://biocache-service.sibbr.gov.br/biocache-service/occurrences"
            })
                .done(function (data) {
                    $('#allOcorrencias').append(data.totalRecords.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1."));
                }).fail(function (data) {
                    $('#allOcorrencias').append("0");
                });
        });

        $(document).ready(function () {
            $.ajax({
                url: "https://bie-webservice.sibbr.gov.br/bie-index/search?q=*&fq=rank:%22species%22&fq=taxonomicStatus:%22accepted%22&fq=idxtype:%22TAXON%22"
            })
                .done(function (data) {
                    $('#allEspecies').append(data.searchResults.totalRecords.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1."));
                }).fail(function (data) {
                    $('#allEspecies').append("0");
                });
        });

        $(function () {
            var includes = $('[data-include]');
            jQuery.each(includes, function () {
                var file = './' + $(this).data('include') + '.html';
                $(this).load(file);
            });
        });



    </script>
</body>

