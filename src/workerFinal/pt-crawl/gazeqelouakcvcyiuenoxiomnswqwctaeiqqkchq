        <script>
            var body = document.body;
            var scrolled = false;
            window.addEventListener('scroll', function () {
                scrolled = true;
            });

            
            if ((window.scrollY || window.pageYOffset) >= 50) {
                body.classList.add('scrolled');
            }

            var timeout = setInterval(function(){
                if (scrolled) {
                    scrolled = false;
                    if ((window.scrollY || window.pageYOffset) >= 50 ) {
                        body.classList.add('scrolled');
                    } else {
                        body.classList.remove('scrolled');
                    }
                }
            }, 50);
        </script>

        
        <script>
            window.csrftoken = "ZOMVA0Kb0lTJ2AzJJykZJTs9xvhwRmg0A4xjDELVCNKUqVJfkBalzipuTDAXb4xu";
        </script>
        


        
        
        
        <script>
            // Load all scripts non-async
            footerScripts.forEach(function(src) {
                var scriptEl = document.createElement('script');
                scriptEl.src = src;
                scriptEl.async = false;
                document.head.appendChild(scriptEl);
            });
        </script>
    



    

    

        <script src="https://cdnjs.cloudflare.com/ajax/libs/camanjs/4.1.2/caman.full.min.js" integrity="sha512-JjFeUD2H//RHt+DjVf1BTuy1X5ZPtMl0svQ3RopX641DWoSilJ89LsFGq4Sw/6BSBfULqUW/CfnVopV5CfvRXA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.21/lodash.min.js" integrity="sha512-WFN04846sdKMIP5LKNphMaWzU7YpMyCU245etK3g/2ARYbPK9Ub18eG+ljU96qKRCWh+quCY7yefSmlkQw1ANQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script>
            // Build image manipulation tool using caman.js
            function runImageManipulator() {
                $(() => {
                    // TODO: See if performance can be improved by:
                    // * Loading smaller res image through AJAX when available
                    // * Using layers for filters, or at least for intensive layers like sharpen
                    if (!($('#current-fileset-img').length > 0)) {
                        return;
                    }

                    const metaId = $('body').data('metaId');
                    const $imgFilters = $('#imgFilters');
                    const $filters = $imgFilters.find('.filter');
                    const $filterInputs = $filters.find('.filter__input');
                    const $filterResets = $filters.find('.filter__reset');
                    const $progressBar = $('#filterProgressBar');
                    const filters = {};
                    let caman;
                    let busy = false;
                    let changed = false;

                    const render = _.throttle(() => {
                        let value;

                        if (busy) {
                            changed = true;
                            return;
                        } else {
                            changed = false;
                        }

                        busy = true;
                        caman.revert(false);

                        const filterKeys = Object.keys(filters);

                        filterKeys.forEach((key) => {
                            value = parseFloat(filters[key], 10);

                            if (value !== 0) {
                                caman[key](value);
                            }
                        });

                        return caman.render(() => {
                            busy = false;
                                sessionStorage.setItem(`${metaId}-filters`, JSON.stringify(filters));
                            if (changed) {
                                return render();
                            } else {
                                return undefined;
                            }
                        });
                    }, 50);

                    // Add available filters to filter object
                    $filterInputs.each((index, element) => {
                        filters[$(element).data('filter')] = $(element).val();
                    });

                    // Assign and init caman object
                    caman = Caman('#current-fileset-img', () => {
                        if (sessionStorage.getItem(`${metaId}-filters`) !== undefined) {
                            const sessionFilters = JSON.parse(sessionStorage.getItem(`${metaId}-filters`));

                            if (sessionFilters != null) {
                                let value;

                                const sessionFilterKeys = Object.keys(sessionFilters);

                                sessionFilterKeys.forEach((key) => {
                                    value = parseFloat(sessionFilters[key], 10);

                                    filters[key] = value;

                                    $(`#filter-${key}`)
                                        .find('.filter__input')
                                        .val(value)
                                        .end()
                                        .find('.filter__value')
                                        .html(value);
                                });
                            }
                            render();
                        }
                    });

                    Caman.Event.listen('processStart', (job) => {
                        // console.log('Start: ', job.name);
                        $progressBar.css('visibility', 'visible');
                    });
                    Caman.Event.listen('processComplete', (job) => {
                        // console.log('Finished: ', job.name);
                    });
                    Caman.Event.listen('renderFinished', (job) => {
                        $progressBar.css('visibility', 'hidden');
                    });

                    // Update image and value element on input change
                    $filterInputs.on('change', function () {
                        const $filterContainer = $(this).closest('.filter');
                        const filter = $(this).data('filter');
                        const value = $(this).val();

                        filters[filter] = value;

                        $filterContainer.find('.filter__value').html(value);

                        render();
                    });

                    // Update numeric values as sliders are moved
                    $filterInputs.on('mousemove', function () {
                        const value = $(this).val();

                        $(this).closest('.filter').find('.filter__value').html(value);
                    });

                    // Bind reset filter buttons
                    $filterResets.on('click', function (e) {
                        const $filterContainer = $(this).closest('.filter');
                        const $filterInput = $filterContainer.find('.filter__input');
                        const filter = $filterInput.data('filter');
                        const value = 0;

                        e.preventDefault();

                        filters[filter] = value;

                        $filterContainer.find('.filter__value').html(value);
                        $filterInput.val(value);
                        render();
                    });

                    // Bind reset all filters
                    $('#resetAllFilters').on('click', () => {
                        $filters.each(function () {
                            const $filterInput = $(this).find('.filter__input');
                            const filter = $filterInput.data('filter');

                            filters[filter] = 0;
                            $filterInput.val(0);
                            $(this).find('.filter__value').html(0);
                        });
                        render();
                    });
                });
            }

            (function defer() {
                if (window.jQuery) {
                    runImageManipulator();
                } else {
                    setTimeout(function() { defer() }, 50);
                }
            }());
        </script>

        

        
        <script src="https://cdnjs.cloudflare.com/ajax/libs/camanjs/4.1.2/caman.full.min.js" defer></script>

        
        

    

    
    
    

    


