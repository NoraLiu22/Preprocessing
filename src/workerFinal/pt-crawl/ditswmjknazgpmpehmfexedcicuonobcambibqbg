<div class="accordion parbase section">
    

    
        <ul class="a11yAccordion  simple" data-testid='accordion-component' data-ishtl="true" id="accordioncontenteachtabtabsmastertab_copybottomContent" data-composer='accordion' data-action='init' data-config='{"id":"#accordioncontenteachtabtabsmastertab_copybottomContent", "firstExpanded":"Expanded", "showOne":"allow-multiple"}'>

            
                <li class="a11yAccordionItem">

                    <div class="a11yAccordionItemHeader">
                        
                        <h2><p><b>Africa and the Middle East</b></p>
</h2>
                    </div>

                    <div class="a11yAccordionHideArea">
                        <div class="bootstrapParentConfigParsys parsys"><div class="text parbase section"><div data-testid='text-component' class="container " data-ishtl="true">
    
    
