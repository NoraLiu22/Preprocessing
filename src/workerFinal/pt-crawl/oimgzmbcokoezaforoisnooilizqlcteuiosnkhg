            // Load all scripts non-async
            footerScripts.forEach(function(src) {
                var scriptEl = document.createElement('script');
                scriptEl.src = src;
                scriptEl.async = false;
                document.head.appendChild(scriptEl);
            });
        </script>
    

    <script type="module">
        import { bindListFiltering } from '/media/assets/public/js/modules/list_helpers.js?v=20220805';

        (function defer() {
            if ('List' in window) {
                const listOptions = {
                    valueNames: ['partner-name','partner-date'],
                    page: 1000
                };

                const partnersList = new List('explore-partners-content', listOptions);

                

                bindListFiltering(partnersList, {
                    noResultsSelector: '#no-results',
                    paginatorSelector: '#titlesPag', 
                    matchingCountSelector: '.matching-count--partners',
                    listCountSelector: '.explore__list-count',
                    listSearchSelector: '#list-search',
                    addAlternatingClasses: {itemParentSelector: '#partner-list'},
                });
            } else {
                setTimeout(function() { defer(); }, 50);
            }
        }());
    </script>



