<div id="ctl25_W7355_DAlbum" style="clear:both;border-top:solid 1px #c0c0c0;padding-top:10px;">
    <h3 id="ctl25_W7355_DAlbumTitle">Country</h3>
    
    <div id="ctl25_W7355_DPhoto" class="floatNear" style="width:210px;display:inline-block;text-align:center">
        <div id="ctl25_W7355_DPhotoEntry" style="text-align:center;margin-top:5px">
            
<div id="PhotoFrameFloatingImageContainer" style="display:none;position:absolute;padding:15px 15px 15px 15px;background:white;border:solid 1px #707070;width:450px;z-index:9999">
    <div id="PhotoFrameFloatingText" style="margin-bottom:10px"></div>
    <div id="PhotoFrameFloatingImage" style="text-align:center"></div>
</div>
<div id="debug"></div>
    <style >
    .PhotoFrame 
    {
        text-align:center;
        vertical-align:middle;
        background:url(/cms/images/shadow.png) no-repeat;
        padding:11px 20px 20px 11px;
        display:inline-block;
        width:200px;
        height:200px;
    }
    </style>
    
    <script>
    
        var MouseOffset = new Object();
        
        MouseOffset.x = 15;
        MouseOffset.y = 15;
    
        var mFloatingImageTimerID = 0;
        
        //===============================================
        //
        //
        //===============================================
        function HideFloatingImageContainer()
        {
            ClearFloatingImageTimer();

            if(mFloatingImageTimerID==0)
                mFloatingImageTimerID = window.setTimeout("HideFloatingImageContainerNow();", 200);
        }

        //===============================================
        //
        //
        //===============================================
        function HideFloatingImageContainerNow()
        {
            ClearFloatingImageTimer();

            document.onmousemove = null;
            document.getElementById("PhotoFrameFloatingImageContainer").style.display="none";
        }

        //===============================================
        //
        //
        //===============================================
        function ClearFloatingImageTimer()
        {
            if(mFloatingImageTimerID!=0)
            {
                window.clearTimeout(mFloatingImageTimerID);
                mFloatingImageTimerID = 0;
            }
        }
        

        //===============================================
        //
        //
        //===============================================
        function ShowFloatingImageContainer(e, src, html)
        {
            ClearFloatingImageTimer();

            var oHtml  = document.getElementById("PhotoFrameFloatingText");
            var oImage = document.getElementById("PhotoFrameFloatingImage");
            
            oImage.innerHTML = "<img border=\"0\" src=\""+src+"\" />";
            oHtml .innerHTML = html;
            oHtml .style.display = html=="" ? "none" : "";

            // Show
            var oContainer = document.getElementById("PhotoFrameFloatingImageContainer");

            followmouse(e);
            
            if(mFloatingImageTimerID==0)
                mFloatingImageTimerID = window.setTimeout("ShowFloatingImageContainerNow();", 500);
            
        }
        
        //===============================================
        //
        //
        //===============================================
        function ShowFloatingImageContainerNow()
        {
            ClearFloatingImageTimer();

            document.onmousemove=followmouse;

            var oContainer = document.getElementById("PhotoFrameFloatingImageContainer");
            oContainer.style.display = "";
        }
        
        
        //===============================================
        //
        //
        //===============================================
        function followmouse(e)
        {
            
            var oContainer = document.getElementById("PhotoFrameFloatingImageContainer");

            if (typeof window.event != "undefined")
            {
                e = window.event;
                e.pageX = e.x;
                e.pageY = e.y;
            }

	        if (typeof e != "undefined")
	        {
                if (typeof e.target != "undefined")
                {
                    e.offsetX    = e.layerX;
                    e.offsetY    = e.layerY;
                    e.srcElement = e.target;
                }

	            var iDocWidth  = document.all
	                           ? document.documentElement.scrollLeft + document.documentElement.clientWidth 
	                           : pageXOffset+window.innerWidth-15;
	                           
	            var iDocHeight = document.all
	                           ? Math.min(document.documentElement.scrollHeight, document.documentElement.clientHeight) 
	                           : Math.min(window.innerHeight)

                
                var iClientLeft   = document.documentElement.scrollLeft;
                var iClientTop    = document.documentElement.scrollTop;
                var iClientRight  = document.documentElement.scrollLeft + iDocWidth  - MouseOffset.x;
                var iClientBottom = document.documentElement.scrollTop  + iDocHeight - MouseOffset.y;
                
                var iX      = e.pageX + MouseOffset.x;
                var iY      = e.pageY + MouseOffset.y;
                var iWidth  = Math.max(oContainer.offsetWidth,  oContainer.clientWidth);
                var iHeight = Math.max(oContainer.offsetHeight, oContainer.clientHeight);

                var iLeft   = e.clientX + document.documentElement.scrollLeft;
                var iTop    = e.clientY + document.documentElement.scrollTop;
                var iRight  = iLeft + iWidth;
                var iBottom = iTop  + iHeight;
                
                if(iRight  > iClientRight)  iX = e.pageX - MouseOffset.x - iWidth;
                if(iBottom > iClientBottom) iY -= (iBottom - iClientBottom);
                
                
                oContainer.style.left = iX +"px";
                oContainer.style.top  = iY +"px";
                
//                document.getElementById("debug").innerHTML = 
//                    "x: " + iX + "y: " + iY +
//                    "w: " + iWidth + "h: " + iHeight;
            }
        }
        
        if(!document.all)
        {
            var oContainer = document.getElementById("PhotoFrameFloatingImageContainer");
            oContainer.parentNode.removeChild(oContainer);
            document.body.appendChild(oContainer, document.body.lastChild);
        }
        
    </script>



