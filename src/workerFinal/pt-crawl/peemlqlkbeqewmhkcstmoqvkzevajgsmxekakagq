              <source srcset="/sites/default/files/styles/homepage_banner/public/2024-02/Trobriand%20Island%20shells%20LEAD%20IMAGE%201600%20compressed.webp?itok=OGRAKGZ9 1x" media="all and (min-width: 1024px) and (max-width: 1199px)" type="image/webp" width="1920" height="1277"/>
