	<script type="text/javascript">
		//***
		//*** Load Header template
		//***
		SiteNameSpace.ajaxCall({
			url: "/includes/template/head.html",
			type: "GET",
			parameters: "",
			dataType: "text",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get head template",
			success: function (results) {
				if (results != null) {
					//***set the site data
					$("#HeaderPane").append(results);
					// console.log("head template loaded.");
					//will filters or head arrive on page first?
					$.each(tagsList, function (i, tag) {
						$('#ulExploreTags').append('<li><a data-LinkType="TagList" href="/tag-list.html?tag=' + tag.value_rts + '">' + tag.value_rts + '</a></li>');
					});
				} else { //no result was found
					// console.log("head template not loaded.");
				}
			}
		}); //end ajaxCall

		//***
		//*** Load FiltersList
		//***
		SiteNameSpace.ajaxCall({
			url: "/JSON/FiltersList.json",
			type: "GET",
			parameters: "",
			dataType: "json",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get FiltersList.json",
			success: function (results) {
				if (results != null) {
					// console.log("filters data loaded.");
					tagsList = results.Tags_rls;
					//will filters or head arrive on page first?
					$.each(tagsList, function (i, tag) {
						$('#ulExploreTags').append('<li><a data-LinkType="TagList" href="/tag-list.html?tag=' + tag.value_rts + '">' + tag.value_rts + '</a></li>');
					});

				} else { //no result was found
					// console.log("filters data not loaded.");
				}
			}
		}); //end ajaxCall

		//***
		//*** Load Achivement Highlights template
		//***
		SiteNameSpace.ajaxCall({
			url: "/includes/template/AchievementsHighlightsList.html",
			type: "GET",
			parameters: "",
			dataType: "text",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get AchievementtsHighlightsList template",
			success: function (results) {
				if (results != null) {
					//***set the site data
					$("#AchievementHighlightsPane").append(results);
					// console.log("AchievementtsHighlightsList template loaded.");
				} else { //no result was found
					// console.log("AchievementtsHighlightsList template not loaded.");
				}
			}
		}); //end ajaxCall

		//***
		//*** Load Footer template
		//***
		SiteNameSpace.ajaxCall({
			url: "/includes/template/foot.html",
			type: "GET",
			parameters: "",
			dataType: "text",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get foot template",
			success: function (results) {
				if (results != null) {
					//***set the site data
					$("#FooterPane").append(results);
					// console.log("foot template loaded.");
				} else { //no result was found
					// console.log("foot template not loaded.");
				}
			}
		}); //end ajaxCall



		SiteNameSpace.WaitForAjax(SiteNameSpace.SetupGAListeners);







	</script>
	<script id="tagsItem-template" type="text/x-tagsTemplate">
		<a data-LinkType="ContentTag" title="@title" class="listing__tag-item listing__tag-link" href="@url">@text</a>
		<span class="listing__tag-item listing__tag-seperator">|</span>
	</script>

	<script id="related-template" type="text/x-relatedTemplate">
		<tr><td><a data-LinkType="SimilarlyTagged" href="@url">@text</a></td></tr>
	</script>



	<script id="programs-template" type="text/x-programsTemplate">
		<div class="sidebar__item">
				<div class="sidebar__link"><a href="@url">@Name</a></div>
				<div class="sidebar__copy">@LeadParagraph</div>
		</div>
	</script>


	<script id="attachements-template" type="text/x-attachementsTemplate">
		<li class="icon-link__item"><a href='/attachments/@FileName_rst' id="aDownloadable" target="_blank"><span class="icon-link__download-icon"></span>DOWNLOAD @Title_rst</a></li>
	</script>

	<script id="breadCrumb-template" type="text/x-breadCrumbTemplate">
		<span class="breadcrumb__item breadcrumb__seperator"></span>
		<span class="breadcrumb__item ">
				<a data-LinkType="Breadcrumb" href="@URL_RST" class="breadcrumb__item breadcrumb__link">@Text_rst</a>
		</span>
	</script>

	<script type="text/javascript">
		// console.log("Resource start json processing.");
        if (window.location.href.split("?").length > 1) { window.history.pushState({}, document.title, window.location.pathname) };

		var tagsList = "";

		//***
		//*** load json
		//***
		var pathname = ""
		pathname = window.location.pathname;
		pathname = (pathname.endsWith(".html")) ? "/JSON" + pathname.replace(".html", ".json") : "/JSON" + pathname + ".json";
		// console.log("pathname = " + pathname);
		SiteNameSpace.ajaxCall({
			url: pathname,
			type: "GET",
			parameters: "",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get " + pathname,
			success: function (results) {

				if (results != null) {
					//***set the site data

					var staff = results;
					// console.log("Boo = " + staff.Name_rst);

					//***
					//*** merge into tempate onto page
					//***

					//*** merge meta into tempate onto page
					if (staff.Metadata_Title_rst.length > 0) $("#Head").append("<meta name=\"Title\" content=\"" + staff.Metadata_Title_rst + "\" >");
					if (staff.Metadata_Description_rst.length > 0) $("#Head").append("<meta name=\"Description\" content=\"" + staff.Metadata_Description_rst + "\" >");
					if (staff.Keywords_rst.length > 0) $("#Head").append("<meta name=\"Keywords\" content=\"" + staff.Keywords_rst + "\" >");
					$("#Head").append("<meta name=\"TypeOfContent\" content=\"People\" >");

					//*** BreadCrumb list in top main column
					for (var i = 0; i < staff.BreadCrumbs_rls.length; i++) {
						var relatedItem = $('#breadCrumb-template').text();
						relatedItem = relatedItem.replace("@Text_rst", staff.BreadCrumbs_rls[i].bcname_rst);
						relatedItem = relatedItem.replace("@URL_RST", staff.BreadCrumbs_rls[i].bcurl_rst);
						$("#lblBreadCrumbs").append(relatedItem);
					}

					//*** merge main content into tempate onto page
					$("#pageTitle").text(staff.Metadata_Title_rst);
					$("#h1_detail__header").text(staff.Metadata_Title_rst);
					if (staff.OfficeRoles_rls.length > 0) {
						$("#aStaffOffice").text(staff.OfficeRoles_rls[0].OfficeName_rst + " (" + staff.OfficeRoles_rls[0].OfficeAbbr_rst + ")");
						$("#aStaffOffice").attr("href", "/about-us/offices/" + staff.OfficeRoles_rls[0].OfficeAbbr_rst + ".html");
					}

					$("#h2_detail__subtitle2").text(staff.Title_rst);
					$("#div_detail__body").html(staff.Bio_rst);

					$("#aContactStaff").html("<span class=\"icon-link__email-icon\"></span>Contact " + staff.Metadata_Title_rst);
					$("#aContactStaff").attr("href", "https://contact.darpa.mil/contact?people=" + staff.URL_rst.replace(".html", ""));


					//	Let's send the page view to Google Analytics
					gtag('event', 'page_view', {
						page_title: staff.Metadata_Title_rst
					})


					//*** merge main image into tempate onto page
					var staffImage = staff.Image_rob;
					if (staffImage !== null) {
						var imageString = "<img src=\"" + staffImage.URL_rst + "\" id=\"img_detail__image\" alt=\"" + staffImage.AlternateText_rst + "\">";
						$("#div_detail_header__thumbnail").append(imageString);
						$("#Head").append("<meta name=\"image\" content=\"/DDM_Gallery/" + staffImage.Thumbnail_rst + "\" >");

					};

					//*** merge tags list into tempate onto page
					for (var i = 0; i < staff.Tags_rls.length; i++) {
						var tagsItem = $('#tagsItem-template').text();
						tagsItem = tagsItem.replace("@title", staff.Tags_rls[i].shortName_rst);
						tagsItem = tagsItem.replace("@url", staff.Tags_rls[i].url_rst);
						tagsItem = tagsItem.replace("@text", staff.Tags_rls[i].name_rst);
						$("#divTagsList").append(tagsItem);
						$("#Head").append("<meta name=\"Topic\" content=\"" + staff.Tags_rls[i].name_rst + "\" >");
					}


					//*** merge related list into tempate onto page
					for (var i = 0; i < staff.RelatedContent_rls.length; i++) {
						var relatedItem = $('#related-template').text();
						relatedItem = relatedItem.replace("@url", staff.RelatedContent_rls[i].url_rst);
						relatedItem = relatedItem.replace("@text", staff.RelatedContent_rls[i].content_name_rst);
						$("#myRelatedContent_dlItems").append(relatedItem);
					}




					//*** program list in main content column
					for (var i = 0; i < staff.Programs_rls.length; i++) {
						$("#mySideBarType2").Visible = true;
						var programItem = $("#programs-template").text();
						programItem = programItem.replace("@url", staff.Programs_rls[i].Program_URL_rst)
						programItem = programItem.replace("@Name", staff.Programs_rls[i].Program_Name_rst)
						programItem = programItem.replace("@LeadParagraph", staff.Programs_rls[i].Program_LeadParagraph_rst)
						$("#mySideBarType2").append(programItem);
						$("#mySideBarType2").visible = true;
					}
					if (!staff.Programs_rls.length) {
						$("#mySideBarType2").hide();
						$("#content__wrapper").removeClass("sidebar-second");
					}

					//***	Secondary Body

					if (staff.SecondaryBody_rst.trim() != "") {
						$("#mySideBarType3").Visible = true;
						$("#mySideBarType3").html(staff.SecondaryBody_rst);
					}

					//*** Attachements list in right content column
					for (var i = 0; i < staff.Attachments_rls.length; i++) {
						var attachmentItem = $('#attachements-template').text();
						attachmentItem = attachmentItem.replace("@Filename_rst", staff.Attachments_rls[i].Filename_rst);
						attachmentItem = attachmentItem.replace("@Title_rst", staff.Attachments_rls[i].Title_rst);
						$("#AttachementsBelow").append(attachmentItem);
					}



					//*** right column visiblity tweak
					//If mySideBarType5.Visible = False And mySideBarType3.Visible = False Then
					//UpdateContentWrapperDiv()  ' hide the side bar
					//End If

				}

			},
			error: function (xhr) {
				location = "/not-found.html";
			}






		}); //end ajaxCall
	</script>

</body>
