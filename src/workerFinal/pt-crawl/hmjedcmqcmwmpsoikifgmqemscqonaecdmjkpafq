<script>

    $(document).ready(function () {
        if ($.inArray('selNoticeCountry', UNGM.autocompletes, 0) === -1) {
            UNGM.autocompletes.push('selNoticeCountry');
        }
        $('#selNoticeCountry').selectToAutocomplete({
            'copy-attributes-to-text-field': true // This attribute is needed to disable the autocomplete
        });

        // Linking the label with the visible input instead of the invisible select
        var inputId = 'selNoticeCountry-input';
        $('#selNoticeCountry').next('input').attr("id", inputId);
        $("label[for='selNoticeCountry']").attr("for", inputId);
    });
</script>

