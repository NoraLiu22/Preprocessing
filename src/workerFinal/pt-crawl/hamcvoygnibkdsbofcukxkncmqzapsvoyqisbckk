        <script type="application/javascript">
            $.getJSON('assets/sw/versions.json', function(data) {
                var sorted = data.sort(function(a, b) {
                    return a[1] > b[1] ? -1 : a[2] > b[2] ? -1 : 1;
                });
                var latest = sorted[0];
                $('#download-latest-date').text("Latest Release Date: " + cnri.getFormattedDate(latest[1]) + ".");
            });

        </script>

