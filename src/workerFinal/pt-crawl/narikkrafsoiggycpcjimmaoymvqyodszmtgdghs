<script type="text/javascript" src="http://images.china.cn/images1/resource/scripts/jquery/jquery-3.6.0.min.js"></script>
<script type="text/javascript">
    (function(window, $) {
        $(function() {
            lun(".carousel", ".carousel .img", ".carousel .img li", ".carousel .num", ".carousel .num li", ".carousel .btn_r", ".carousel .btn_l", 900, "false");
        })

        $('.div3 .content ul li').on('mouseenter', function() {
            $(this).css({
                right: 0,
                background: '#0956c5'
            });
        });
        $('.div3 .content ul li').on('mouseleave', function() {
            $(this).css({
                right: '-13%',
                background: '#cb1715'
            });
        });
        if ((window.navigator.mimeTypes[40] || !window.navigator.mimeTypes.length)) {
            $('.div3 .content ul li').off('mouseenter').off('mouseleave');
            $('.div3 .content ul li').hover(function() {
                $(this).css('background', '#0956c5');
            }, function() {
                $(this).css('background', '#cb1715');
            })
        }

        function lun(a, b, c, d, e, f, g, w, p) {
            var i = 1;
            var clone = $(c).first().clone();
            var clone1 = $(c).last().clone();
            $(b).append(clone);
            $(b).prepend(clone1);
            $(b).stop().animate({
                left: -1 * w
            }, 500);
            var size = $(c).length;
            for (var j = 0; j < size - 2; j++) {
                $(d).append("<li></li>");
            }
            $(c).eq(1).addClass("show");
            $(e).first().addClass("on");
            if (p == "true") {
                var t = setInterval(function() {
                    i++;
                    move();
                }, 2000);
                $(a).hover(function() {
                    clearInterval(t);
                }, function() {
                    t = setInterval(function() {
                        i++;
                        move();
                    }, 2000);
                });
            }
            $(e).hover(function() {
                var index = $(this).index();
                i = index + 1;
                $(b).stop().animate({
                    left: -i * w
                }, 500);
                $(c).eq(i).addClass("show").siblings().removeClass("show");
                $(this).addClass("on").siblings().removeClass("on");
            });

            $(f).click(function() {
                i++;
                move();
            })

            $(g).click(function() {
                i--;
                move();
            })

            function move() {
                if (i == size - 1) {
                    $(b).css({
                        left: 0
                    });
                    i = 1;
                }

                if (i == 0) {
                    $(b).css({
                        left: -(size - 1) * w
                    });
                    i = size - 2;
                }
                $(b).stop().animate({
                    left: -i * w
                }, 500);
                if (i == size - 2) {
                    $(c).eq(0).addClass("show").siblings().removeClass("show");
                    $(e).eq(i - 1).addClass("on").siblings().removeClass("on");
                } else {
                    $(c).eq(i).addClass("show").siblings().removeClass("show");
                    $(e).eq(i - 1).addClass("on").siblings().removeClass("on");
                }
            }
        }

        $('.play').each(function(i) {
            $(this).click(function() {
                if ($('audio')[i].paused) {
                    $('audio').each(function(i) {
                        $('audio')[i].pause();
                        $('.play').find('img').attr('src', 'http://images.china.cn/images1/en/governanceofchina2022/play.png');
                    });
                    $('audio')[i].play();
                    $(this).find('img').attr('src', 'http://images.china.cn/images1/en/governanceofchina2022/stop.png');
                } else {
                    $('audio')[i].pause();
                    $(this).find('img').attr('src', 'http://images.china.cn/images1/en/governanceofchina2022/play.png');
                }
            })
        })
    })(window, jQuery)
</script>

