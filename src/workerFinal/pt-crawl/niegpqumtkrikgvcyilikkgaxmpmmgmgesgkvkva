<script type="text/javascript">
var sPath = window.location.pathname;
var parent = window.location.pathname.split('/');
var sid = 543;

if (sPath == '/weather/' || parent[1] == 'weather' || parent[1] == 'closings') {
sid = 308;
}
else if (sPath == '/beoshow/' || parent[1] == 'beoshow') {
sid = 513;
}
else if (sPath == '/podcasts/' || parent[1] == 'podcasts') {
sid = 544;
}


;!function(){
window.PianoESPConfig = {
