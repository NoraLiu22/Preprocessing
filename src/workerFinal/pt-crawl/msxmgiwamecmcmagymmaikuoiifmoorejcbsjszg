    <script>
        $('#menubtn').click(function(){
            var _m=$('#nav_m');
            if(_m.is(':visible')){
                _m.slideUp();
                $(this).removeClass('active');
            }else{
                if($('#search_m').is(':visible')){
                    $('#searchbtn').trigger('click');
                    setTimeout(function(){
                        _m.slideDown();
                        $('#menubtn').addClass('active');
                    },200)
                }else{
                    _m.slideDown();
                    $(this).addClass('active');
                }
            }
        });
        $('#searchbtn').click(function(){
            var _m=$('#search_m');
            if(_m.is(':visible')){
                _m.slideUp();
                $(this).removeClass('active');
            }else{
                if($('#nav_m').is(':visible')){
                    $('#menubtn').trigger('click');
                    setTimeout(function(){
                        _m.slideDown();
                        $('#searchbtn').addClass('active');
                    },200)
                }else{
                    _m.slideDown();
                    $(this).addClass('active');
                }
            }
        });



        $('#rightBtn a').each(function(){

            $(this).hoverIntent({

                over:function(){
                    $(this).find('div').stop(false,false).animate({scrollTop:'35px'})
                },
                out:function(){
                    $(this).find('div').stop(false,false).animate({scrollTop:'0px'})
                }
            })
        });

        $('#rightBtn a.b1').tooltip({
            container:'body',
            html:true,
            placement:'left',
            trigger:'manual',
            html:'true',
            title:'<input type="text" id="search_input">'
        });


        var search_words = "";
        //if(search_words == ''){
        //    $('#rightBtn a.b1').tooltip('show');
        //}

        $('#rightBtn a.b1').tooltip('show');

        if(search_words !=''){
            $("#search_input").val(search_words)
        }
        $('#rightBtn a.b1').click(function(){
            var q = $("#search_input").val();
            if(q!=''&&q.length>0){
                q = q.trim();
            }
            window.location.href = "/en/p/paper_search/?q="+q;
        });


        function footerPosition(){
            $(".footer").removeClass("fixed-bottom");
            var contentHeight = document.body.scrollHeight,//网页正文全文高度
                winHeight = window.innerHeight;//可视窗口高度，不包括浏览器顶部工具栏
            if(!(contentHeight > winHeight)){
                //当网页正文高度小于可视窗口高度时，为footer添加类fixed-bottom
                $(".footer").addClass("fixed-bottom");
            } else {
                $(".footer").removeClass("fixed-bottom");
            }
        }
        footerPosition();
        $(window).resize(footerPosition);

    </script>

