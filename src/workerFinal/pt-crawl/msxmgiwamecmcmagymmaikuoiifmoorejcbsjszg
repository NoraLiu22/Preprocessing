        myChart.setOption(option);
        myChart.showLoading();
        $.get("/en/p/getstatistics/").done(function (data) {
            myChart.hideLoading();
            myChart.setOption({

                xAxis: [
                    {
                        type: 'category',
                        boundaryGap: false,
                        axisLine: {onZero: false},
                        data: data['category']
                    }
                ],
                yAxis: [
                    {
                        // max: 500000,
                        min: get_y_min(data["view_count"][0]),
                        // interval: 100000,
                    },
                    {
                        min: get_y_min(data["download_count"][0]),
                        // max: 9000,
                        // interval: 2000,
                    }
                ],
                series: [
                    {
                        name: view_count,
                        type: 'line',
                        animation: false,
                        areaStyle: {
                            normal: {}
                        },
                        lineStyle: {
                            normal: {
                                width: 1
                            }
                        },
                        data: data['view_count']
                    },
                    {
                        name: download_count,
                        type: 'line',
                        yAxisIndex: 1,
                        animation: false,
                        areaStyle: {
                            normal: {}
                        },
                        lineStyle: {
                            normal: {
                                width: 1
                            }
                        },
                        data: data['download_count']
                    }
                ]
            });
        });

        function get_y_min(num) {
            // 获取y轴上的最小显示数字
            var num1 = parseInt(num*0.8);
            var tmp1  = Math.pow(10,num1.toString().length-1);
            num1 = Math.floor(num1/tmp1)*tmp1;
            return num1;
        }
