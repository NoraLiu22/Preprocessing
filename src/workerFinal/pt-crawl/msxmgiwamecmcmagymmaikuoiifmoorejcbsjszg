    <script>
        var size = window.getComputedStyle(document.body, ':after').getPropertyValue('content');
        var myChart;
        
            var view_count = 'views';
            var download_count = 'downloads';
        
        if (size.indexOf("Lplus") != -1) {
            myChart = echarts.init(document.getElementById('stat'));
        } else {
            myChart = echarts.init(document.getElementById('stat_phone'));
        }

        option = {
            title: {
                x: 'center',
                align: 'right'
            },
            grid: {
                bottom: 30,
                left: 65,
                right: 50
            },
            toolbox: {
                feature: {
                    saveAsImage: {}
                }
            },
            tooltip: {
                trigger: 'axis',
                axisPointer: {
                    type: 'cross',
                    animation: false,
                    label: {
                        backgroundColor: '#0d6001'
                    }
                }
            },
            color: ['#133e89', '#a2f000'],//图例图标文字颜色
            legend: {
                data: [view_count, download_count],
                x: 'left',
                left: 10,
                right: 10,
                textStyle: {
                    color: "#cee9fd"//图例名称文字颜色
                }
            },

            xAxis: [
                {
                    type: 'category',
