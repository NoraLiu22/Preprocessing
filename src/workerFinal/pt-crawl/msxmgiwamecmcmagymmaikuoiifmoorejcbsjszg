                        inside: true
                    }
                }
            ],
            yAxis: [

                {
                    name: view_count,
                    splitLine: {
                        show: true,
                        lineStyle: {
                            color: '#c1e3fc',
                            width: 1
                        }
                    },
                    type: 'value',
                    // max: 500000,
                    // min: 100000,
                    // interval: 100000,
                    nameTextStyle: {
                        color: "#cee9fd"//Y轴名称文字颜色
                    },
                    axisLabel: {
                        show: true,
                        textStyle: {
                            color: '#cee9fd'
                        }
                    },
                    axisLine: {
                        lineStyle: {
                            color: "#cee9fd"
                        }
                    },
                    axisTick: {
                        inside: true
                    }

                },
                {
