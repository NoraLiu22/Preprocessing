    padding: 30px;
    border: 2px solid #a01b27;
    margin-top: 4px;
}

@media (max-width: 600px) {
  .definition-popup {
    width:300px;
  }
}

.definition-popup .close {
    font-size:22px;
    cursor: pointer;
    position: absolute;
    right:10px;
    top:5px;
}
.definition {
  display: none;
}</style>
<div class="definition-popup">
	<div class="close">X</div>
	<div class="content">&nbsp;</div>
</div>


  
