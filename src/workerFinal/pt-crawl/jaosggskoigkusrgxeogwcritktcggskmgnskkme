    "url(/images/default-source/about-us/ambassadors-and-artists/banners/hugh-jackman-banner.jpg?Status=Temp&sfvrsn=2)",];
    var i = 0;
    var replaceBgImg = setInterval(bgImgReplacer, 3500);

    function bgImgReplacer() {
        if (i != bgImgs.length) {
            pageBanner.css({
                transition: "background-image 1s ease",
                background: bgImgs[i],
                backgroundSize: "cover",
                backgroundPosition: "center center"
            });
            i++;
        } else {
            pageBanner.css({
                transition: "background-image 1s ease",
                background: bgImgs[i],
                backgroundSize: "cover",
                backgroundPosition: "center center"
            });
            i = 0;
        }
        
    }
});

