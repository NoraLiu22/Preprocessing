var imgPre3=new Array();
for (i=1;i<4;i++){
    imgPre3[i]=new Image();
    imgPre3[i].src=imgUrl3[i];
}

function setTransition3(){
   if (document.all){
      imgUrl3rotator.filters.revealTrans.Transition=Math.floor(Math.random()*20);
      imgUrl3rotator.filters.revealTrans.apply();
   }
}
function playTransition3(){
   if (document.all)
      imgUrl3rotator.filters.revealTrans.play()
}

function nextAd3(){
   if(adNum3<imgUrl3.length-1)adNum3++ ;
      else adNum3=1;
   setTransition3();
   document.images.imgUrl3rotator.src=imgUrl3[adNum3];
   playTransition3();
   theTimer=setTimeout("nextAd3()", 6000);
}
function jump2url3(){
   jumpUrl=imgLink3[adNum3];
   jumpTarget='_blank';
   if (jumpUrl != ''){
      if (jumpTarget != '')window.open(jumpUrl,jumpTarget);
      else location.href=jumpUrl;
   }
}
function displayStatusMsg3() { 
   status=imgLink3[adNum3];
   document.returnValue = true;
}
