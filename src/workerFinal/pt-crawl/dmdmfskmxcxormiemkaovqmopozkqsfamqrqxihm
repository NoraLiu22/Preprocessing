                     <div class="content-section">
                        				
                        <!-- ou:migration_comment ORIGINAL TMPL CONTENT BEGINS HERE -->
                           <h2>Livestock and Forage Links</h2>
                           
                           <div id="accordion-d17e515" class="accordion">
                              <div class="card">
                                 <div class="card-header" id="heading-1">
                                    <h3 class="mb-0"><button class="btn btn-link accordion-trigger" data-toggle="collapse" data-target="#collapse-d17e515-1" aria-expanded="true" aria-controls="collapse-d17e515-1">Agricultural Organizations&nbsp;and Associations<span class="fas fa-plus"></span><span class="fas fa-minus"></span></button></h3>
                                 </div>
                                 <div id="collapse-d17e515-1" class="collapse show" aria-labelledby="heading-1" data-parent="#accordion-d17e515">
                                    <div class="card-body">
                                       
                                       <ul>
                                          
                                          <li><a href="http://www.fb.org/" target="_blank" rel="noopener">American Farm Bureau</a></li>
                                          
                                          <li><a href="http://www.afia.org" target="_blank" rel="noopener">American Feed Industry Association</a></li>
                                          
                                          <li><a href="http://www.afgc.org/" target="_blank" rel="noopener">American Forage and Grassland Council</a></li>
                                          
                                          <li><a href="https://www.arba.net/" target="_blank" rel="noopener">American Rabbit Breeders Association</a></li>
                                          
                                          <li><a href="http://www.arbeef.org/" target="_blank" rel="noopener">Arkansas Cattlemen's Association</a></li>
                                          
                                          <li><a href="http://www.arfb.com/" target="_blank" rel="noopener">Arkansas Farm Bureau</a></li>
                                          
