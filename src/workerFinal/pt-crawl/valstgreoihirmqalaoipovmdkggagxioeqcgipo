        function transView(){
          var x = (flag) ? 'initial-scale=1' : 'width=1200';
          var  relax = (flag) ? 'none' : 'block';
          vp.setAttribute('content', x);
          document.getElementById("mobileView").style.display=relax;

          flag^=1;
        };
