    <script type="text/javascript" >
      jQuery(document).ready(function($) {
        $('#small-news li:first-child').find('h3').trigger('click');

        $('a.full_bio').on('click', function(e) {
          e.preventDefault();
          var $elem = $(this).closest('.row').find('div.full_bio');
          $elem.toggle(1000);
          return false;
        });
        $('a.short_bio').on('click', function(e) {
          e.preventDefault();
          var $elem = $(this).closest('.row').find('div.short_bio');
          $elem.toggle(1000);
          return false;
        });
        
        
      });  //end Document ready
    </script>
  
