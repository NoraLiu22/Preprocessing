ctions"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl01":{"NavigateAfterClick":true,"Value":"Learn"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl01_ctl00":{"NavigateAfterClick":true,"Value":"Chapter Quiz"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl01_ctl01":{"NavigateAfterClick":true,"Value":"Listening Quiz (Multiple Choice)"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl01_ctl02":{"NavigateAfterClick":true,"Value":"Listening Quiz (Short Answer)"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl01_ctl03":{"NavigateAfterClick":true,"Value":"Flashcards"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl02":{"NavigateAfterClick":true,"Value":"Connect"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl02_ctl00":{"NavigateAfterClick":true,"Value":"Listening Styles Quiz"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl02_ctl01":{"NavigateAfterClick":true,"Value":"Composer Biographies"},"ctl00_ChapterNavArea_RadTabStrip1_ctl24_ctl02_ctl02":{"NavigateAfterClick":true,"Value":"Glossary"}});</script>
