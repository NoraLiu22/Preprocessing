<script>
  function validateSearch() {
    $('#form_species_search').addClass('was-validated');
    if ($('#form_species_search')[0].checkValidity() === false) {
      $('#form_species_search :submit').prop('disabled', true)
    }
    else {
      $('#form_species_search :submit').prop('disabled', false)
    }
  }
  $(document).ready(function(){
    
    $(':text').blur(function(){
        var val = $(this).val();
        $(this).val($.trim(val));
    });

    $('#form_species_search :text').focus(function(){
      validateSearch()
    });
    $('#form_species_search :text').blur(function(){
      validateSearch()
    });
    $('#form_species_search :text').on('input', function(){
      validateSearch()
    });
    $('#form_species_search').submit(function(e){
      e.preventDefault();
      $('#form_species_search').addClass('was-validated');
      if ($('#form_species_search')[0].checkValidity() === true) {
        $.post("/species/ajax/", $("#form_species_search").serialize(), function(data){
          $('#species_search_result table').bootstrapTable('load', data.rows);
            if (data.rows.length == 0 || data.rows.length > 1000) {
                $('#toolbar button').prop('disabled', true).css('cursor', 'not-allowed');
                $('#disablebutton').removeClass('d-none');
            }
            else {
                $('#toolbar button').prop('disabled', false).css('cursor', 'pointer');
                $('#disablebutton').addClass('d-none');
            }
          $('#species_search_result').removeClass('d-none');
          $('html,body').animate({
              scrollTop: $('#species_search_result').offset().top,
          }, 1000);
        });
      }
    });
  });
  function latinFormatter(value) {
    return '<i>' + value + '</i>'
  }
  function spdetailFormatter(value, row) {
    var link = "/species/detail/" + '?genus='+row.genus+'&species='+value
    return '<a target="_blank" href="' + link +'"><i>' + value + '</i> <i class="bi-link"></i></a>'
  }
  function othernameFormatter(index, row) {
    return '<span class="text-muted">Other Names: ' + row.othername + '</span>'
  }
  $('#toolbar button').click(function(){
    var refseq_acs = $('#species_search_result table').bootstrapTable('getData').map(x=>x.refseq_ac);
    if (refseq_acs.length > 0) {
        $('#toolbar .spinner-border').removeClass('d-none');
        $.ajax({
            url: "/species/table/download/",
            data: {ids: refseq_acs, select_type: $('#toolbar select').val()},
            type: 'post',
            xhr: function(){
                var xhr = new XMLHttpRequest();
                xhr.responseType= 'blob'
                return xhr;
            },
            success: function(data){
                $('#toolbar .spinner-border').addClass('d-none');
                const myBlob = new Blob([data]);
                saveAs(myBlob, "MT_files.zip");
            },
        });
    }
  });
  window.icons = {
    columns: 'bi-card-list',
    export: 'bi-cloud-download',
    detailOpen: 'bi-node-plus',
    detailClose: 'bi-node-minus',
  }
</script>

