        <table data-toggle="table" data-search="true" data-pagination="true" 
          data-side-pagination="client" data-search-highlight="true" data-toolbar="#toolbar" 
          data-detail-view="true" data-icons-prefix="bi" data-icons="icons"
          data-detail-formatter="othernameFormatter" data-show-columns="true"
          data-show-export="true" data-export-types="['txt', 'xlsx']" data-export-data-type="all">
            <thead>
              <tr>
                <th data-sortable="true" data-field="refseq_ac" data-visible="false">RefSeq ID</th>
                <th data-sortable="true" data-field="order">Order</th>
                <th data-sortable="true" data-field="family">Family</th>
                <th data-sortable="true" data-field="genus" data-formatter="latinFormatter">Genus</th>
                <th data-sortable="true" data-field="species" data-formatter="spdetailFormatter">Species</th>
