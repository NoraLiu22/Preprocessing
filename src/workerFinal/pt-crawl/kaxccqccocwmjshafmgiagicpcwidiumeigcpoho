<div class="card bg-light">
    <div class="card-body">
      <p>
        <a href="/blast/help/#blast_help_query" class="badge badge-info">
            <i class="bi-link"></i> Help
        </a>
        <a href="/blast/advanced/" class="badge badge-primary"><i class="bi-link"></i> Advanced Search</a>
      </p>
      <h3 class="card-subtitle text-muted">Enter a sequence in <strong>FASTA format</strong> or <strong>Plain Text</strong>: </h3>
      <form action="/blast/ajax/simple/" method="post" id="form_blast" novalidate>

        <textarea class="form-control" id="blast" name="blast" rows="10" style="font-family: monospace;" required></textarea>

        <div class="form-group">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="blastdb" id="blastdb1" value="complete" checked>
                <label class="form-check-label" for="blastdb1">Against Complete mtDNA</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="blastdb" id="blastdb2" value="all">
                <label class="form-check-label" for="blastdb2">Against Complete + Partial mtDNA</label>
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>

      <div class="text-center">
        <div class="spinner-border text-success d-none" role="status">
      </div>
      </div>
      <div id="blast_result" class="d-none">
        <p></p>
        <h3>Sequence Search Result: </h3>
        <pre class="border rounded bg-light" style="font-size: 60%;"></pre>
      </div>
    </div>
</div>


<script>
  function validateBLAST() {
    $('#form_blast').addClass('was-validated');
    if ($('#form_blast')[0].checkValidity() === false) {
      $('#form_blast :submit').prop('disabled', true)
    }
    else {
      $('#form_blast :submit').prop('disabled', false)
    }
  }

  $(document).ready(function(){

    $('#form_blast textarea').focus(function(){
      validateBLAST()
    });
    $('#form_blast textarea').blur(function(){
      validateBLAST()
    });
    $('#form_blast textarea').on('input', function(){
      validateBLAST()
    });

    $('#form_blast').submit(function(e){
      e.preventDefault();
      $('#form_blast').addClass('was-validated');
      if ($('#form_blast')[0].checkValidity() === true) {
        $('.spinner-border').removeClass('d-none');
        $.post("/blast/ajax/simple/", $("#form_blast").serialize(), function(data){
          $('#blast_result pre').html(data);
          $('.spinner-border').addClass('d-none');
          $('#blast_result').removeClass('d-none');
          $('.fishname-link').each(function(){
            var url = $(this).attr('href');
            $(this).attr('href', "/species/detail/" + url).attr('target', '_blank');
          });
          $('html,body').animate({
              scrollTop: $('#blast_result').offset().top,
          }, 1000);
        });
      }
    });
  });
  
</script>

