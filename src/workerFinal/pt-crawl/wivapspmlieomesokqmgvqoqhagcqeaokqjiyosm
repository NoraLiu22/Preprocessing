		<SCRIPT TYPE="text/javascript">
			var a=0;
			function ShowBox(){
				a=1;
				obj = document.getElementById("coversearch");
				obj.style.display = 'inline-block';
			}
			//same way as above but reversed
			function HideBox(){
				if(a==1){
					a=0;
					obj = document.getElementById("coversearch");
					obj.style.display = 'none';
				}
			}
			
			//READ MORE FUNCTION - PRODUCT DETAIL PAGE
			js(".desc-tab-content .button").click(function() {
				var descContent = js('.desc-tab-content');
				var readMore = js('.read-more');
				var readBtn = js('.read-more .button');
				if(!js(descContent).hasClass('desc-show')) {
					descContent.addClass('desc-show');
					readMore.children('.button').text('Read Less');
					readMore.css('background','none');
				} else {
					descContent.removeClass('desc-show');
					readMore.css('background','linear-gradient(to bottom, #ffffff00, white)');
					readBtn.text('Read More');
				}
				//$(this).parent().hide();
				return false;    
			});
				
		 </SCRIPT>

 


