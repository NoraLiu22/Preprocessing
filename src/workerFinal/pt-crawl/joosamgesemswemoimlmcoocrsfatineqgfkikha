// make footer api call that returns country
$(document).ready(function(){
    $.ajax({
    url: 'https://www.nationalgeographic.com/proxy/footer',
    async: false,
    type: 'GET',
    dataType: "json",
    }).done(function (data) {
    var paFootCountry = data.country;
    //set country cookie for bootstrap.js to read and configure OneTrust geo
    document.cookie = 'country=' + data.country + '; path=/;';
    //make dynamic footer call based on country returned
    $.ajax({
    url: 'https://assets-cdn.nationalgeographic.com/natgeo/static/geofooterconfigs/' + paFootCountry + '.json',
    async: false,
    type: 'GET',
    dataType: "json",
    }).done(function (footer) {
    var footerHTML='';
    if(footer && footer.length){
    footer.forEach(function(link, i){
    var styleAttr = '';
    if(link.style){ 
    styleAttr='class="'+link.style+'" ';
    }
        footerHTML+=`<p class="footmouse">`;
        footerHTML+=`<a `;
        footerHTML+=styleAttr;
        footerHTML+=`href="`;
        footerHTML+=link.url;
        footerHTML+=`">`;
        footerHTML+=link.title;
        footerHTML+=`</a>`;
        footerHTML+=`</p>`;
    });
    }
    console.log(footerHTML)
    //not sure what the real footer is on the page, but here is an example
    document.getElementById('paDynFooter').innerHTML = footerHTML;
    //after this footer is dynamically injected, you can now load bootstrap.js and all other vendor code
    });
    });

    
    // DATALAYER CONFIGURATION

    // search the URL for specific variables
    var url = window.location.href
    var queryParams = new URLSearchParams(window.location.search);
    var pageid = queryParams.get('cds_page_id');
    var marketing = queryParams.get('cmpid');
    console.log("Referring URL: " + url);
    console.log("Paramters: " + queryParams);
    console.log("Page ID: " + pageid);
    console.log("Marketing: " + marketing);
    
    
    if(pageid == "226717") { // eCare login page ID
        var logstatus = "logged out";
    } else {
        var logstatus = "logged in";
    }
    
    console.log("Login Status: " + logstatus);
    
    if(marketing != null) { // grab marketing tags from URL
        var mktg = marketing;
    } else {
        var mktg = 'not applicable'
    }
    // search the URL for page ID
    
    var language = navigator.languages && navigator.languages[0] || // Chrome / Firefox
                   navigator.language || // All browsers
                   navigator.userLanguage; // IE <= 10
    
    var pgname = window.location.pathname.split("/").slice(-1);
    var newpgname = pgname.toString().slice(6);
    var nwpgname = newpgname.toString().slice(0, -4);
    
    console.log("Language: " + language);
    console.log("Page Name: " + nwpgname);

    switch (nwpgname) {
