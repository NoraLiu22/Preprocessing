<script>
window.addEventListener('load',function(){   
var id      = '';
                                var price   = 0;
                                var page    ='';
                                
                                if(window.location.pathname =='/')
                                {
                                                var page ='home';
                                                
                                }
                                else if(window.location.pathname =='/cart')
                                {
                                                var page  = 'cart';
                                                var id    = new Array();
                                                var price = new Array();
                                                jQuery('.sku').each(function(){
                                                id.push(jQuery(this).text().split('#').reverse()[0]);
                                                });
                                                price = jQuery('.cartsummary .price:last').text().replace(/[^0-9.]/g,'');
                                                price = parseFloat(price);
                                                
                                }  
                                else if(jQuery('.category.mainContentWell').length > 0)
                                {
                                                var page ='category';
                                                
                                }
                                else if(jQuery('.product.mainContentWell').length > 0)
                                {
                                                var page  = 'product';
                                                var id    = new Array();
                                                id              = jQuery('.sku').text().split('#').reverse()[0];
                                                
                                                var price = jQuery('.pricing .basePrice').text().replace(/[^0-9.]/g,'');
                                                if(price  =='' || price ==undefined || price ==NaN)
                                                {
                                                price = 0;
                                                }
                                                price     = parseFloat(price);
                                
                                }
                                else if(window.location.href.indexOf('/success')!=-1)
                                {
                                                var page ='conversion';
                                                
                                }
                                else 
                                {
                                                page ='other';
                                                
                                }
                                
                                var google_tag_params = { 
                                ecomm_prodid: id,
                                ecomm_pagetype: page,
                                ecomm_totalvalue: price  
                                };
    
     try {
        ga('set', 'dimension1', google_tag_params.ecomm_prodid.toString());
    } catch (e) {}
    try {
        ga('set', 'dimension2', google_tag_params.ecomm_pagetype.toString()); 
    } catch (e) {}
    try {
        ga('set', 'dimension3', google_tag_params.ecomm_totalvalue.toString()); 
    } catch (e) {}
    ga('send', 'event', 'page', 'visit', google_tag_params.ecomm_pagetype.toString(), {
        'nonInteraction': 1
    });
});
</script>

