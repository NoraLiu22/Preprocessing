    (function defer() {
      if ('List' in window) {
        const collectionList = new List('collection-list', { valueNames: ['collection-name','collection-count']});
        const partnerList = new List('partner-list', { valueNames: ['partner-name','partner-count']});
        const decadeList = new List('decade-list', { valueNames: ['decade-name','decade-count']});
        const yearList = new List('year-list', { valueNames: ['year-name','year-count']});
      } else {
        setTimeout(function() { defer() }, 50);
      }
    }());
  </script>


