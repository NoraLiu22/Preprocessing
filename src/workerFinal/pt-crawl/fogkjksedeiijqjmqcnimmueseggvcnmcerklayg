<script>jQuery(function($){
    // Delete the session storage items
    delete sessionStorage["searchUrl"];
    delete sessionStorage["searchResults"];

    // Putting the search URL in session storage
    // Lets the user click back to the search results
    // when they are on the viewer page
    sessionStorage.setItem("searchUrl", window.location.href);

