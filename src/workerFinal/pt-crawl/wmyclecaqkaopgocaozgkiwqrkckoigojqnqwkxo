le species that prefer a particular condition could indicate that cultural practices should be altered.","version":"1.0.0","ipn":"ENH1370","publishedAt":"2023-05-09T00:00:00.000Z","modifiedAt":"2023-05-09T09:13:50.000Z","revisedAt":"2022-11-08T00:00:00.000Z","language":"en_US","isPinned":null,"audience":[],"contacts":[{"dln":"EP634","contactEppn":"marblesc@ufl.edu","isLead":false,"email":"marblesc@ufl.edu","fname":"Stephen ","lname":"Marble","prefName":"Chris","ufid":"04557766"}],"versions":[{"dln":"EP634","oid":"Dnktx8chhb","number":"1.0.0","status":"Released","publishedAt":"2023-05-09T00:00:00.000Z","revisedAt":"2022-11-08T00:00:00.000Z"}]}],"url":"https://edis.ifas.ufl.edu","criticalIssueMap":{"1":{"id":1,"description":"1. Agricultural and Horticultural Enterprises"},"2":{"id":2,"description":"2. Water Quality, Quantity, and Supply"},"3":{"id":3,"description":"3. Natural Resources and Environmental Quality"},"4":{"id":4,"description":"Families and Communities"},"5":{"id":5,"description":"Nutrition, Health and Food Safety"},"6":{"id":6,"description":"7. 4-H Youth Development"},"7":{"id":7,"description":"Other"},"8":{"id":8,"description":"4. Sustainable Communities"},"9":{"id":9,"description":"5. Individuals and Families"},"10":{"id":10,"description":"6. Community Resource Development"},"11":{"id":11,"description":"Organizational Goals"}},"expertTypeMap":{"1":{"id":1,"description":"external","ufAffiliated":null},"2":{"id":2,"description":"student","ufAffiliated":true},"3":{"id":3,"description":"Specialist/SSA/RSA","ufAffiliated":true},"4":{"id":4,"description":"County agent","ufAffiliated":true},"5":{"id":5,"description":"postdoc","ufAffiliated":true},"6":{"id":6,"description":"staff","ufAffiliated":true}},"sideBarWidgets":["broader-topics","units","experts","collections","links"],"pageBodyWidgets":["title","description","pinned-publications","pinned-associations","narrower-topics","associations","publications","blog-posts"],"topicTree":{"id":1088,"typeId":7,"heade