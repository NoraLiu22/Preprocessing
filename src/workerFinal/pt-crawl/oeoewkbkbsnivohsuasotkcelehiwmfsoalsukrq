              <source srcset="/sites/default/files/styles/aspect_3_2_854x569_1_3_scale_and_crop_/public/feature_image/X-4-4.jpg?h=af752bc2&amp;itok=HtwkIdG9 1x" media="all and (min-width: 48em)" type="image/jpeg"/>
