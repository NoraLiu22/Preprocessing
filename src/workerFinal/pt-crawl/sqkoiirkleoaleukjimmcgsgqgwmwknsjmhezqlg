 <div id="events-highlights" class="row" ><!-- large-12 columns -->
 <!-- LEFT -->
    <div id="events_and_news_container_left" class="medium-6 large-6 small-12 columns events_and_news_container left"  > <!-- data-equalizer-watch -->
      <div id="left"  >
        <h2 class="news_header">Event Calendar</h2>
     
        <!-- left content - Events Calendar 19441 ** -->
        <div id="calendar"><!-- navigation object : Google Calendar JSON - Related Content Branch --><!-- Google Calendar Feed JSON - text/f6v3 -->
<div id="google-calendar"><div style="height:calc(3 * 100px);"></div></div>
<script>
/* use a function for the exact format desired... */
function ISODateString(d){
 function pad(n){return n<10 ? '0'+n : n}
 return d.getUTCFullYear()+'-'
      + pad(d.getUTCMonth()+1)+'-'
      + pad(d.getUTCDate())+'T'
      + pad(d.getUTCHours())+':'
      + pad(d.getUTCMinutes())+':'
      + pad(d.getUTCSeconds())+'Z'}

var d = new Date();
//alert(ISODateString(d)); // prints something like 2009-09-28T19:03:12Z

var maxResults  = "3";
var calGroup    = "q4nlav1ptfdl0ihd8658lc4n44@group.calendar.google.com";
var calendarUrl = "https://www.googleapis.com/calendar/v3/calendars/" + calGroup + "/events?key=AIzaSyClbI-VxJ01Mxli6YMmoq0D0JVBNXLHjtw&orderBy=startTime&singleEvents=true&timeMin=" + d.toISOString() + "&maxResults=" + maxResults + "&callback=?";
var eventLinks = "true";

function buildCalendar(url){
    var calContent  = "";
	var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
    $.getJSON(url, function (data){
        var entries = data.items;//feed.entry;
        if(entries.length == 0){
        	calContent += "<p>No Events Available</p>";
        	$("#google-calendar").html(calContent);
            return;
        }
        $.each(entries, function(i, loc){
            var title       = loc.summary;//title.$t;
          	var description = "";
            if(loc.description){
            	description = loc.description;//content.$t;
              	description = description.split(" ").slice(0, 20).toString().replace(/,/g, " ") + "...";
          	}
          var timezone = loc.start.timeZone;
          if (timezone == undefined){ timezone = 'America/New_York'; }
			if(loc.start.date)
				var date = loc.start.date;//content.$t;
			else
				var date = loc.start.dateTime;
				
			date = date.substring(0, 10);			
			date = months[date.substring(5,7) - 1] + " " + date.substring(8,10) + ", " + date.substring(0,4);
            var link        = loc.htmlLink;
            
            calContent += "<div class='feedsb'>";
            if(eventLinks == "false"){
              calContent += "<h4>" + date;
              calContent += "<h5>" + title + "</h5>";
              calContent += "<p>" + description + "</p>";
            }
            else{
              calContent += "<h4><a href='" + link + "&ctz=America/New_York' target='_blank'>" + date + "</a>";
              calContent += "<h5>" + title + "</h5>";
              calContent += "<p>" + description + "</p>";
             }
            calContent += "</div>";
        })
        $("#google-calendar").html(calContent);
    })
};

$(document).ready(function(){
	buildCalendar(calendarUrl);
});
</script><div id="left_after_calendar"></div></div> 

     <a href="https://calendar.google.com/calendar/embed?src=q4nlav1ptfdl0ihd8658lc4n44%40group.calendar.google.com&amp;ctz=America%2FNew_York"><button class="uf-dotted-blue-border-button button large hollow" style="margin-top: 1rem;font-size: 1rem!important;text-transform: uppercase;color: #0021a5;padding: 1rem 1rem 1rem 1rem; border: 3px solid #0021a5;float:right;margin-right:25px;margin-bottom:0;" type="submit">View Full Calendar</button></a>
    </div>
    </div>
