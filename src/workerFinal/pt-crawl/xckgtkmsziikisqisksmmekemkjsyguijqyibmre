  <div class="views-exposed-form needs-validation block block--exposedformadvanced-searchpage-1" data-drupal-selector="views-exposed-form-advanced-search-page-1">

  
            <h2>Advanced Search</h2>
        

            
<form action="/advancedsearch" method="get" id="views-exposed-form-advanced-search-page-1" accept-charset="UTF-8" class="form">
  
<div class="js-form-item form-item form-type-textfield js-form-type-textfield form-item-field-authors-value js-form-item-field-authors-value form-group">
      <label class="form-label" for="edit-field-authors-value">Author (contains)</label>
  
  
  
  
  
