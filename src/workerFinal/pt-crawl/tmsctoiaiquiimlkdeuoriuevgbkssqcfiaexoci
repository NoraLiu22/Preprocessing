                    <div class="diagram svg-diagram">
                        <h3>Publications by Year</h3>
                        <div class="diagram se-diagram" aria-label="publications by year"></div>
                        
    <style>
        rect:hover {
            fill: steelblue;
        }
        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }
        .bar {
            fill: steelblue;
        }
        a:hover .bar, a:focus .bar {
            stroke: black;
            fill-opacity: 60%;
        }
    </style>
    
        <script src="/static/js/d3.v3.min.js"></script>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".se-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1956, frequency:32 },{ year:1957, frequency:50 },{ year:1958, frequency:54 },{ year:1959, frequency:69 },{ year:1960, frequency:124 },{ year:1961, frequency:82 },{ year:1962, frequency:106 },{ year:1963, frequency:105 },{ year:1964, frequency:111 },{ year:1965, frequency:84 },{ year:1966, frequency:40 },{ year:1967, frequency:107 },{ year:1968, frequency:126 },{ year:1969, frequency:152 },{ year:1970, frequency:165 },{ year:1971, frequency:172 },{ year:1972, frequency:192 },{ year:1973, frequency:163 },{ year:1974, frequency:166 },{ year:1975, frequency:195 },{ year:1976, frequency:179 },{ year:1977, frequency:147 },{ year:1978, frequency:162 },{ year:1979, frequency:141 },{ year:1980, frequency:167 },{ year:1981, frequency:189 },{ year:1982, frequency:201 },{ year:1983, frequency:196 },{ year:1984, frequency:184 },{ year:1985, frequency:164 },{ year:1986, frequency:182 },{ year:1987, frequency:182 },{ year:1988, frequency:191 },{ year:1989, frequency:176 },{ year:1990, frequency:207 },{ year:1991, frequency:247 },{ year:1992, frequency:223 },{ year:1993, frequency:239 },{ year:1994, frequency:231 },{ year:1995, frequency:303 },{ year:1996, frequency:294 },{ year:1997, frequency:290 },{ year:1998, frequency:322 },{ year:1999, frequency:327 },{ year:2000, frequency:320 },{ year:2001, frequency:384 },{ year:2002, frequency:399 },{ year:2003, frequency:382 },{ year:2004, frequency:380 },{ year:2005, frequency:401 },{ year:2006, frequency:446 },{ year:2007, frequency:503 },{ year:2008, frequency:452 },{ year:2009, frequency:455 },{ year:2010, frequency:533 },{ year:2011, frequency:572 },{ year:2012, frequency:561 },{ year:2013, frequency:689 },{ year:2014, frequency:199 },{ year:2015, frequency:353 },{ year:2016, frequency:598 },{ year:2017, frequency:707 },{ year:2018, frequency:760 },{ year:2019, frequency:876 },{ year:2020, frequency:887 },{ year:2021, frequency:1143 },{ year:2022, frequency:975 },{ year:2023, frequency:679 },{ year:2024, frequency:199 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=se%3A129+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Publication" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

