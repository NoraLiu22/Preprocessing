            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1956, frequency:1 },{ year:1957, frequency:6 },{ year:1958, frequency:3 },{ year:1959, frequency:9 },{ year:1960, frequency:41 },{ year:1961, frequency:33 },{ year:1962, frequency:28 },{ year:1963, frequency:34 },{ year:1964, frequency:22 },{ year:1965, frequency:21 },{ year:1966, frequency:29 },{ year:1967, frequency:32 },{ year:1968, frequency:62 },{ year:1969, frequency:60 },{ year:1970, frequency:88 },{ year:1971, frequency:87 },{ year:1972, frequency:105 },{ year:1973, frequency:115 },{ year:1974, frequency:141 },{ year:1975, frequency:135 },{ year:1976, frequency:138 },{ year:1977, frequency:150 },{ year:1978, frequency:178 },{ year:1979, frequency:187 },{ year:1980, frequency:160 },{ year:1981, frequency:354 },{ year:1982, frequency:410 },{ year:1983, frequency:409 },{ year:1984, frequency:386 },{ year:1985, frequency:401 },{ year:1986, frequency:412 },{ year:1987, frequency:443 },{ year:1988, frequency:455 },{ year:1989, frequency:538 },{ year:1990, frequency:554 },{ year:1991, frequency:739 },{ year:1992, frequency:685 },{ year:1993, frequency:534 },{ year:1994, frequency:739 },{ year:1995, frequency:645 },{ year:1996, frequency:786 },{ year:1997, frequency:523 },{ year:1998, frequency:559 },{ year:1999, frequency:724 },{ year:2000, frequency:677 },{ year:2001, frequency:866 },{ year:2002, frequency:884 },{ year:2003, frequency:747 },{ year:2004, frequency:1014 },{ year:2005, frequency:1319 },{ year:2006, frequency:1108 },{ year:2007, frequency:1690 },{ year:2008, frequency:2177 },{ year:2009, frequency:2325 },{ year:2010, frequency:2324 },{ year:2011, frequency:2095 },{ year:2012, frequency:1849 },{ year:2013, frequency:1733 },{ year:2014, frequency:1534 },{ year:2015, frequency:1740 },{ year:2016, frequency:2095 },{ year:2017, frequency:2300 },{ year:2018, frequency:2495 },{ year:2019, frequency:2648 },{ year:2020, frequency:2738 },{ year:2021, frequency:3047 },{ year:2022, frequency:2876 },{ year:2023, frequency:2441 },{ year:2024, frequency:666 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=rj%3A129+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                        </div>
                    </div>
                
            
        </article>
    </div>
        </div>
    

                    <div class="clearfix"></div>
                </div>
            </div>

