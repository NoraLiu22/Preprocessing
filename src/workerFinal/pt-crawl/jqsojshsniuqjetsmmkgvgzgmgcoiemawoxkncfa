    //set show/hide for legend
    //use phantom_legend to keep rest of Annotation Box content evenly spaced
    $_toggle_legend.click(function() {
        if ($_phantom_legend.height() > 0)
            $_phantom_legend.height($_legend_items.outerHeight());
