        document.addEventListener("DOMContentLoaded", function() {
        const teams = [
    { name: "Professor C. Lee Giles", role: "PI and Project Director", image: "/resources/img/teams/ProfessorCLeeGiles.jpg", link: "http://clgiles.ist.psu.edu/", text: "Lee Giles cocreated CiteSeer in 1997 with Steve Lawrence and Kurt Bollacker while they were at the NEC Research Institute (now NEC Labs) in Princeton, New Jersey, USA.  Now at the Pennsylvania State University, Dr. Giles continues the tradition of the CiteSeer project." },
    { name: "Dr. Jian Wu", role: "Assistant Professor at Old Dominion University", image: "/resources/img/teams/DrJianWu.jpg", link: "http://fanchyna.wix.com/jianwu", text: "Jian Wu is improving the CiteSeerx crawler and investigating our crawling policies and also maintains the CiteSeerx database, index and repository."},
    { name: "Abhishek Talesara", role: "Graduate Student at Penn State University", image: "/resources/img/teams/abhishektalesara.jpg", link: "https://www.linkedin.com/in/abhishek-talesara-2aa21114a/", text: "Abhishek is responsible for improving the quality of the metadata of the CiteSeerX data"},
    { name: "Jaideep Kukkadapu", role: "Graduate Student at Penn State University", image: "/resources/img/teams/jaideepkukkadapu.jpg", link: "https://www.linkedin.com/in/jaideepkukkadapu/", text: "Jaideep is responsible for full-stack development and specializes in optimizing Elasticsearch nodes, as well as integrating the Learning to Rank (LTR) algorithm into the CiteSeerX search engine."},
    { name: "Surya Shanmugaraja", role: "Graduate Student at Penn State University", image: "/resources/img/teams/suryashanmugaraja.jpg", link: "https://www.linkedin.com/in/suryasr/", text: "Surya is responsible for full-stack development, primarily the Ingestion of the academic documents as well as the building and maintenance of the static webpage for CiteSeerX."},
    //{ name: "", role: "", image: "", link: "", text: ""},

    // Add more team members as needed
];


const teamListDiv = document.getElementById("teamList");

teams.forEach(team => {
            const groupDiv = document.createElement("div");
            groupDiv.classList.add("dropdown");
            groupDiv.classList.add("profile-dropdown");
    const profileLink = document.createElement("a");
    profileLink.href = team.link;
    profileLink.classList.add("profile-link");
    profileLink.target = "_blank";

    const profileDiv = document.createElement("div");
    profileDiv.classList.add("profile");

    const profileContentDiv = document.createElement("div");
    profileContentDiv.classList.add("profile-content");

    const imageCropperDiv = document.createElement("div");
    imageCropperDiv.classList.add("image-cropper");

    const image = document.createElement("img");
    image.src = team.image;
    image.alt = "img";
    image.style.width = "80px";

    const nameHeader = document.createElement("h3");
    nameHeader.innerHTML = `<strong>${team.name}</strong>`;

    const roleHeader = document.createElement("h4");
    roleHeader.innerHTML = team.role;

    const profileTextDiv = document.createElement("div");
    profileTextDiv.classList.add("profile-text");
    profileTextDiv.classList.add("dropdown-content");
    profileTextDiv.classList.add("ptext");
    profileTextDiv.innerHTML = team.text;

profileDiv.addEventListener('mouseover', function() {
        profileTextDiv.style.display = 'block';
    });

    profileDiv.addEventListener('mouseout', function() {
        profileTextDiv.style.display = 'none';
    });
    imageCropperDiv.appendChild(image);
    profileContentDiv.appendChild(imageCropperDiv);
    profileContentDiv.appendChild(nameHeader);
    profileContentDiv.appendChild(roleHeader);
    profileDiv.appendChild(profileContentDiv);
    profileLink.appendChild(profileDiv);
    profileLink.appendChild(profileTextDiv);
    groupDiv.appendChild(profileLink);

    teamListDiv.appendChild(groupDiv);
        });
//	});


