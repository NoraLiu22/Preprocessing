<script type='text/javascript' src='https://www.forbes.com/advisor/wp-content/themes/fadv/assets/dist/generic-widget-2021.min.js?ver=1712735530_05052024' id='cc_widget_credit_card_2021_script-js'></script>
<script id="toggle-tooltip-script-js-after" type="text/javascript">
window.toggleTooltip = () => {
	const device_type = getDeviceTypeInNumber(window.deviceType);
	const cardWraps = document.querySelectorAll('.card-rating');

	const showTooltip = (toolTipWrap) => {
		toolTipWrap.classList.add("show");
	}

	const hideTooltip = (toolTipWrap) => {
		toolTipWrap.classList.remove("show");
	}

	if (device_type === 1) {
		cardWraps.forEach((cardWrap) => {
			const toolTipWrap = cardWrap.querySelector('.card-rating-tooltip');
			if(toolTipWrap !== null){
				cardWrap.addEventListener("mouseenter", () => showTooltip(toolTipWrap));
				cardWrap.addEventListener("mouseleave", () => hideTooltip(toolTipWrap));
			}
		});

	} else {
		if (!window.tooltipMobileEvent) {
			window.tooltipMobileEvent = true;
			document.body.addEventListener("click", function (e) {
				const ratingComponent = e.target.closest('.card-rating');
				const cardRatingToolTips = document.querySelectorAll('.card-rating-tooltip');

				const removeTooltip = () => {
					cardRatingToolTips.forEach(hideTooltip);
				}
				if (ratingComponent != null && ratingComponent.classList.contains('card-rating')) {
					const ratingTooltip = ratingComponent.querySelectorAll('.card-rating-tooltip');
					if (ratingTooltip.length > 0 && !ratingTooltip[0].classList.contains("show")) {
						removeTooltip();
						showTooltip(ratingTooltip[0]);
					} else {
						removeTooltip();
					}
				} else {
					removeTooltip();
				}
			});
		}
	}
}
document.addEventListener('DOMContentLoaded', () => {
	setTimeout(() => {
		toggleTooltip();
	}, 1000);
})
</script>
<script src="https://www.forbes.com/advisor/wp-content/themes/fadv/assets/dist/js/v3.min.js?v=1693809975"></script>
<script src="https://www.forbes.com/advisor/wp-content/themes/fadv/assets/dist/js/tid.min.js?v=1693809975"></script>

<script type="text/javascript">
    (function () {
        const COOKIE_NAME = "fadvfpuid";
        const DATA_LAYER = window.dataLayer || [];
        const COOKIE_EXPIRATION_DAYS = 400; //Set to maximum number of days
        const MIN_COOKIE_LENGTH_FOR_USER_ID = 30;

        async function initializeTracking() {
            const userCookie = getCookie(COOKIE_NAME);
            
            if (userCookie && userCookie.length > MIN_COOKIE_LENGTH_FOR_USER_ID) {
                initializeWithExistingUser(userCookie);
            } else {
                try {
                    const newVisitorId = await generateVisitorId();
                    setCookieAndInitialize(newVisitorId);
                } catch (error) {
                    console.error("Error generating user ID:", error);
                }
            }
        }

        async function generateVisitorId() {
            const fp = await FingerprintJS.load();
            const { visitorId } = await fp.get();
            return "FA" + visitorId;
        }

        function initializeWithExistingUser(userId) {
            window.visitorId = userId;
            enableTrackingWithTid(userId);
        }

        function setCookieAndInitialize(visitorId) {
            setCookie(COOKIE_NAME, visitorId, COOKIE_EXPIRATION_DAYS, getTransferLinkDomain());
            window.visitorId = visitorId;
            enableTrackingWithTid(visitorId);
        }

        function enableTrackingWithTid(fpid) {
            DATA_LAYER.push({ "fpid": fpid });
            
            if (typeof window._initWTid === 'function') {
                window._initWTid();
            }
        }

        function setCookie(e, o, t, i) {
            const n = new Date();
            n.setTime(n.getTime() + 24 * t * 60 * 60 * 1e3);
            const r = "expires=" + n.toUTCString();
            document.cookie = e + "=" + encodeURIComponent(o) + ";" + r + ";path=/;domain=." + i;
        }

        function getTransferLinkDomain() {
            const e = new URL(window.location).hostname.split(".");
            return e
                .slice(0)
                .slice(-(4 === e.length ? 3 : 2))
                .join(".");
        }

        function getCookie(e) {
            let o = e + "=",
                t = decodeURIComponent(document.cookie).split(";");
            for (let e = 0; e < t.length; e++) {
                let i = t[e];
                for (; " " == i.charAt(0); ) i = i.substring(1);
                if (0 == i.indexOf(o)) return i.substring(o.length, i.length);
            }
            return "";
        }

        initializeTracking();
    })();
