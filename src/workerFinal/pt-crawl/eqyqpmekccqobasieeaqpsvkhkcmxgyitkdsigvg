    (()=>{
            if(window.location.hash == "#data-studies"){
            
            var prContentWrapper = document.querySelector('.pr-container');

            // Manupulate PR Content text Parent Wrap
            var prContentItem = document.querySelectorAll("[data-studies = 'true']");
            prContentItem.forEach((item)=>{
                var nexSibling = item.nextElementSibling;
                if(nexSibling.tagName == 'HR'){
                    nexSibling.remove();
                }
                prContentWrapper.appendChild(item);
                if(!(item.classList.contains('hide-section-title'))){
                    var newSibling = document.createElement('hr');
                    prContentWrapper.appendChild(newSibling);
                }
            })
            
            //Append PR Title 
            var prTitleWrapTag = document.querySelector(".pr-container template");

            if(prTitleWrapTag){
                var prTitleTag = prTitleWrapTag.content.cloneNode(true);
                prContentWrapper.insertBefore(prTitleTag, prContentWrapper.children[0]);
            }
        
        }
    })();
