	  	// number of images on top and bottom
	  	var top_length=64;
	  	var bot_length=91; 
	  	
	  	// total number of images in the directory.  Might change this to a php call
	  	var tot_images=98;
	  	
	  	function fadeMeIn(i) {
	  		//rotate through the top and bottom images and fade them in
			if ( i < bot_length ){
				jQuery("#b_slot_" + bot_fadein[i]).find("img.front_tns").css('display','block').fadeTo(1000,1); // first the bottom
		  		if ( i < top_length ){
					jQuery("#t_slot_" + top_fadein[i]).find("img.front_tns").css('display','block').fadeTo(1000,1); // then the top
		  		}
		  		setTimeout(function(){fadeMeIn(++i);},20); // wait
		  	}
		  	setTimeout(function(){jQuery("#block01 div embed").css('display','block').fadeTo(3000,1);})
	  	}	
	  	
		$(document).ready(function(){
		  	$("#findme").css("display","none");
		  	$("#block01 div img").fadeIn(0);
			highlightFrontLink();
		});
		function highlightFrontLink () {
	  		dimMe($("#block00"),$("#block00 div img"));
	  		//dimMe($("#block01"),$("#block01 div img"));
	  		dimMe($("#block02"),$("#block02 div img"));
	  		//dimMe($("#block03"),$("#block03 div img"));
	  		//dimMe($("#block13"),$("#block03 div img"));
	  		dimMe($("#block04"),$("#block04 div img"));
	  		dimMe($("#block05"),$("#block05 div img"));
	  		//dimMe($("#block06"),$("#block06 div img"));
	  		dimMe($("#block07"),$("#block07 div img"));
	  		dimMe($("#block08"),$("#block08 div img"));
	  		//dimMe($("#block09"),$("#block09 div img"));
	  		dimMe($("#block10"),$("#block10 div img"));
	  		dimMe($("#block11"),$("#block11 div img"));
	  		dimMe($("#block12"),$("#block12 div img"));
	  	}
	  	function dimMe(me,my_img){
	  		me.bind("mouseenter",
		  	function(){
		  		$("#findme").stop(true);
		  		$(".front_features:not(.nofade) div img").css("display","none");
		  		my_img.wait(250).fadeIn(500);
		  		//$(".coml2010dimmer").stop(true,true);
		  		me.css("zIndex","300");
		  		$("#findme").css("display","block");
		  		$("#findme").fadeTo(250, .8);
		  	});	
		  	me.wait(250).bind("mouseleave",
		  	function(){
		  		//$(".coml2010dimmer").stop(true,true);
		  		//my_img.wait(250).fadeOut(1500);
		  		$(".front_features:not(.nofade) div img").css("display","none");
		  		//$("#coml2010dimmer").fadeOut(250);
		  		$("#findme").fadeTo(250,0);
		  		$("#findme").css("display","none");
		  		me.css("zIndex","0");
	  		});
	  	}
	  	
	  	$.fn.wait = function(time, type) {
	        time = time || 1000;
	        type = type || "fx";
	        return this.queue(type, function() {
	            var self = this;
	            setTimeout(function() {
	                $(self).dequeue();
	            }, time);
	        });
	    };
    	$.fn.clearQueue = function( type ) {
			return this.queue( type || "fx", [] );
		}
	  	function addLoadEvent(func) {
		  var oldonload = window.onload;
		  if (typeof window.onload != 'function') {
		    window.onload = func;
		  } else {
		    window.onload = function() {
		      if (oldonload) {
		        oldonload();
		      }
		      func();
		    }
		  }
		}
		addLoadEvent(function(){fadeMeIn(0);});
	</script>
