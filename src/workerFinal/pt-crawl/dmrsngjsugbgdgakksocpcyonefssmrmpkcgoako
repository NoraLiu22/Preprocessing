  <section id="featured-section" class="paragraph paragraph--text paragraph-container paragraph-129901">
  <div >
    
      <style type="text/css">
.resources-background{
background-color: #F5F0EA;
}
.resources-container{
display: flex;
display: -ms-flex;
flex-flow: column;
