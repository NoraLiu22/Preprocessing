          <tr>
            <td width="560" height="96%" valign="top" align="left" style="padding-left: 10">
            <div class="PageHeading">
              <!-- #BeginEditable "PageHeading" -->
              <div style="margin-top:0px; padding-top:0px">
              </div>
              <!-- #EndEditable --></div>
            <!-- #BeginEditable "Content" -->
            <!-- start of air alert automation script -->
            <script type="text/javascript" src="Scripts/jquery-1.9.1.min.js"></script>
           
            <script>
            	$(document).ready(function(){
            		
            		var divMission = document.getElementById("divMission");
            		var divAirAlert = document.getElementById("divAirAlert");
            		var imgAirAlert = document.getElementById("imgAirAlert");
            		
            		divMission.style.display = "none";
            		divAirAlert.style.display = "none";
            		
            		var m_names = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");
            		
            		$.ajax({
            			url: "RAANWebServicesPortal/RAANWebServices.asmx/AirAlert_CheckForAirCurrentAirAlert",
            			contentType: "application/json; charset=utf-8",
            			dataType: "json",
            			data: "{}",
            			type: "POST",
            			success: function (response) {
            				//alert("success:" + response.d);
            				
            				if (response.d !== null)
            				{
            				
            					
            					var startDate = response.d[1];
            					var endDate = response.d[2]
            					
            					var today = new Date();
            					
            				
            					var dd = today.getDate();
            					var mm = today.getMonth();
            					var mm = m_names[mm];        					
            				
            					
            					var yyyy = today.getFullYear();
            					
            					today = mm+' '+dd+', '+yyyy;
            										
            					var spStartDate = document.getElementById("spStartDate");
            					var spEndDate = document.getElementById("spEndDate");
            					var spEffectDate = document.getElementById("spEffectDate");
            					
            					var MultiDaysAirAlert = document.getElementById("MultiDaysAirAlert");
		            			var OneDayAirAlert = document.getElementById("OneDayAirAlert");
            					
            					var startDateParts = startDate.split("/");
            					
            					var startDateMonth = startDateParts[0]-1;
            					
            					startDateMonth = m_names[startDateMonth];
            					startDate = startDateMonth+' ' + startDateParts[1]+', '+startDateParts[2];
            					
            					
            					var endDateParts = endDate.split("/");
            					
            					var endDateMonth = endDateParts[0]-1;
            					
            					endDateMonth = m_names[endDateMonth];
            					endDate = endDateMonth+' ' + endDateParts[1]+', '+endDateParts[2];

            					spStartDate.innerHTML = startDate;
            					
            					//spStartDate.style.fontWeight = "bold";
            					
            					spEndDate.innerHTML = endDate;
            					
            					//spEndDate.style.fontWeight = "bold";
            					
								spEffectDate.innerHTML = today;
								
								//spEffectDate.style.fontWeight = "bold";
								
								//compare start date and end date to see if an one day air alert is called or multiple days air alert is called
		            					
		            			var startDateObj = new Date(startDateParts[2], startDateParts[1], startDateParts[0]);
		            					
		            			var endDateObj = new Date(endDateParts[2], endDateParts[1], endDateParts[0]);
		            					
		            				if (startDateObj == endDateObj)//one day air alert;
		            				{
		            						OneDayAirAlert.style.display = "block";
		            						MultiDaysAirAlert.style.display ="none";
		            						spEffectDate.innerHTML = startDate;
		            					
		            				}
		            					
		            					
		            				if (endDateObj > startDateObj) // multiple days air alert
		            				{
		            						OneDayAirAlert.style.display = "none";
		            						MultiDaysAirAlert.style.display ="block";
		            							
		            					
		            				}
										
            					
            					//alert(startDate + ' ' + endDate);
            					//show air alert banner;
            					imgAirAlert.src="images/Banners/AirAlertBanner_final.2lines-01.png"; //change air alert banner for Air Alert that is effective.
            					imgAirAlert.style.width = '571';
            					divMission.style.display = "none";
            					divAirAlert.style.display ="block";
            					return true;
            				}
            				
            				else
            				
            				{
            					//alert("No Data: " + response.d);
            					//hide alert banner //show mission statement
            					
            					//divMission.style.display="block";
            					//divAirAlert.style.display = "none";
            					//return true;
            					
            					  					
            					 //check for pending air alert
            					
            					$.ajax({
		            			url: "RAANWebServicesPortal/RAANWebServices.asmx/AirAlert_CheckForAirPendingAirAlert",
		            			contentType: "application/json; charset=utf-8",
		            			dataType: "json",
		            			data: "{}",
		            			type: "POST",
		            			success: function (response) {
            					//alert("success:" + response.d);
            				
	            				if (response.d !== null)
	            				{
	            				
            					
            					var startDate = response.d[1];
            					var endDate = response.d[2]
            					
            					var today = new Date();
            					
            				
            					var dd = today.getDate();
            					var mm = today.getMonth();
            					var mm = m_names[mm];        					
            				
            					
            					var yyyy = today.getFullYear();
            					
            					today = mm+' '+dd+', '+yyyy;
            										
            					var spStartDate = document.getElementById("spStartDate");
            					var spEndDate = document.getElementById("spEndDate");
            					var spEffectDate = document.getElementById("spEffectDate");
            					
            					var MultiDaysAirAlert = document.getElementById("MultiDaysAirAlert");
		            			var OneDayAirAlert = document.getElementById("OneDayAirAlert");
            					
            					var startDateParts = startDate.split("/");
            					
            					var startDateMonth = startDateParts[0]-1;
            					
            					startDateMonth = m_names[startDateMonth];
            					startDate = startDateMonth+' ' + startDateParts[1]+', '+startDateParts[2];
            					
            					
            					var endDateParts = endDate.split("/");
            					
            					var endDateMonth = endDateParts[0]-1;
            					
            					endDateMonth = m_names[endDateMonth];
            					endDate = endDateMonth+' ' + endDateParts[1]+', '+endDateParts[2];

            					spStartDate.innerHTML = startDate;
            					
            					//spStartDate.style.fontWeight = "bold";
            					
            					spEndDate.innerHTML = endDate;
            					
            					//spEndDate.style.fontWeight = "bold";
            					
								spEffectDate.innerHTML = today;
								
								//spEffectDate.style.fontWeight = "bold";
								
								//compare start date and end date to see if an one day air alert is called or multiple days air alert is called
								
								var startDateObj = new Date(startDateParts[2], startDateParts[1], startDateParts[0]);
		            					
		            			var endDateObj = new Date(endDateParts[2], endDateParts[1], endDateParts[0]);
		            					
		            			if (startDateObj == endDateObj)//one day air alert;
		            				{
		            						OneDayAirAlert.style.display = "block";
		            						MultiDaysAirAlert.style.display ="none";
		            						spEffectDate.innerHTML = startDate;
		            					
		            				}
		            					
		            					
		            			if (endDateObj > startDateObj) // multiple days air alert
		            				{
		            						OneDayAirAlert.style.display = "none";
		            						MultiDaysAirAlert.style.display ="block";
		            							
		            					
		            				}
										
            					
            					//alert(startDate + ' ' + endDate);
            					//show air alert banner;
            					divMission.style.display = "none";
            					//divEffectDate.style.display = "none"; //Don't display Effective Date for pending Air Alert.
            					imgAirAlert.src="images/Banners/AirAlertBanner_VAD.jpg"; //change air alert banner for pending Air Alert
            					
            					divAirAlert.style.display ="block";
            					return true;
            				}
            				
            				else
            				
            				{
        
            					//alert("No Data: " + response.d);
            					//hide alert banner //show mission statement
            					
            					divMission.style.display="block";
            					divAirAlert.style.display = "none";
            					return true;
	            			}            			
	            			},
	            			error: function (response) {
	            				//alert("error:" + response.responseText);
	            					divMission.style.display="block";
	            					divAirAlert.style.display = "none";
	            					return true;
	            			},
	            			failure: function (response) {
	            				//alert("failure:" + response.responseText);
	            					divMission.style.display="block";
	            					divAirAlert.style.display = "none";
	            					return true;
	            				
	            			}
	            		
	            		
	            		});
	            		
            			}
            			
            			},
            			error: function (response) {
            				//alert("error:" + response.responseText);
            					divMission.style.display="block";
            					divAirAlert.style.display = "none";
            					return true;
            			},
            			failure: function (response) {
            				//alert("failure:" + response.responseText);
            					divMission.style.display="block";
            					divAirAlert.style.display = "none";
            					return true;
            				
            			}
            		
            		
            		});
            	
            	
            	})
            
            
            </script>
            
            <!-- end of air alert automation script -->

