    <script>
        function closeModal() {
            $('#ipadModal').css('display', 'none');
            $('.modalInnerContent').css('display', 'none');
        }

        $(function () {
            if (window.innerWidth > 599) {
                $('#start').fadeIn(100);
            }

            // Screen resize or orientation change
            $(window).resize(function () {
                if (window.innerWidth > 599 && window.innerWidth < 900) {
                    $('.mobileMore').show();
                    $('section').hide();
                    $('#start').fadeIn(100);
                } else if (window.innerWidth < 600) {
                    $('.mobileMore').hide();
                    $('section').show();
                    $('#mobileGlobe>img').attr('src', 'https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/index-pages/erf/globe-mobile.svg');
                    closeModal();
                } else {
                    closeModal();
                }
            });

            // Bubble clicks
            $('.bubble').click(function () {
                if (window.innerWidth > 599 && window.innerWidth < 769) {
                    // iPad portrait mode
                    $('#start').fadeIn(1000);
                    $('.closeModal').on('click', closeModal);
                    var showID = $(this).attr('id');
                    $('#ipadModal').css({
                        'display': 'flex'
                    });
                    if (showID === 'employee') {
                        $('.modalInnerContent').hide();
                        $('#employeeModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'talent') {
                        $('.modalInnerContent').hide();
                        $('#talentModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'customer') {
                        $('.modalInnerContent').hide();
                        $('#customerModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'investor') {
                        $('.modalInnerContent').hide();
                        $('#investorModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'risk') {
                        $('.modalInnerContent').hide();
                        $('#riskModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'government') {
                        $('.modalInnerContent').hide();
                        $('#governmentModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'tech') {
                        $('.modalInnerContent').hide();
                        $('#techModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else if (showID === 'insurance') {
                        $('.modalInnerContent').hide();
                        $('#insuranceModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    } else {
                        $('.modalInnerContent').hide();
                        $('#supplyModal').fadeIn(750);
                        $(this).css("background-color", "#ffe600");
                        $(this).find('span').css("color", "#2e2e38");
                        $(this).siblings().css("background-color", "#2e2e38");
                        $(this).siblings().find('span').css("color", "#fafafa");
                    }

                } else if (window.innerWidth > 768) {
                    // iPad Landscape and desktops
                    // $('#start').show();
                    var panelID = ($(this).attr('id') + 'Text');
                    // console.log(panelID);
                    $('section').hide();
                    $('#' + panelID).fadeIn(1000);
                    $(this).css("background-color", "#ffe600");
                    $(this).find('span').css("color", "#2e2e38");
                    $(this).siblings().css("background-color", "#2e2e38");
                    $(this).siblings().find('span').css("color", "#fafafa");
                    $('#globe>img').attr('src',
                        'https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/index-pages/erf/globe-grey.svg');
                    $('#globe>img').addClass('clickable');
                    $(document).on('click', "img.clickable", function () {
                        // console.log('clicked');
                        $('section').hide();
                        $('.bubble').css("background-color", "#2e2e38");
                        $('.bubble').find('span').css("color", "#fafafa");
                        $('#start').show();
                        $('.imgGlobe').attr('src',
                            'https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/index-pages/erf/globe.svg'
                        );
                        $('.imgGlobe').removeClass('clickable');
                    });
                }
            });

            $('.boxTitle').click(function () {
                if (window.innerWidth < 600) {
                    $(this).next().toggle(250);
                    $(this).parent().toggleClass('opened');
                }
            });
            $(".readMore").click(function () {
                $header = $(this);
                $content = $header.prev();
                $content.slideToggle(100, function () {
                    $header.toggleClass('readLess');
                    $header.text(function () {
                        return $content.is(":visible") ? "Read Less" :
                            "Read More";
                    });
                });
            });
        });
    </script> 
   </div>
  </div> 
