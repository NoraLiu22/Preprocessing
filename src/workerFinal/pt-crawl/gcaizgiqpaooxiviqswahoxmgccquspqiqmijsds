<script>
    $('[data-fancybox="shows"]').fancybox({
    infobar: false,
    type: 'inline',
    autoFocus: false,
    btnTpl: {
        // Arrows
        arrowLeft:
        '<button data-fancybox-prev class="fancybox-button fancybox-button--arrow_left rotate-180" title="">' +
        '<div><img class="br-100" alt="" src="data:image/png;base64,"/></div>' +
        "</button>",
        arrowRight:
        '<button data-fancybox-next class="fancybox-button fancybox-button--arrow_right" title="">' +
        '<div><img class="br-100" alt="" src="data:image/png;base64,"/></div>' +
        "</button>"
    },
    afterLoad: function afterLoad(instance, current) {
        let title = current.src.substr(16); //remove "#hidden-content-"
        ga('send', 'event', 'podcast.series', 'click', title);
        document.querySelector(".fancybox-container audio").stop();
    }
    });

</script>

<!-- Add GA to all audio trailer clicks and subscribe -->
<script>
    var allAudioElements = document.getElementsByTagName("audio");

    for (audio of allAudioElements) {
        audio.addEventListener('play', function() {
            let title = this.dataset.name;
            ga('send', 'event', 'podcast.trailer', 'play', title);
        });
    }

    var allAppleLinks = document.getElementsByClassName("apple-button");

    for (link1 of allAppleLinks) {
        link1.addEventListener('click', function() {
            let title = this.dataset.name;
            ga('send', 'event', 'podcast.outbound', 'apple', title);
            return true;
        });
    }

    var allSpotifyLinks = document.getElementsByClassName("spotify-button");

    for (link2 of allSpotifyLinks) {
        link2.addEventListener('click', function() {
            let title = this.dataset.name;
            ga('send', 'event', 'podcast.outbound', 'spotify', title);
            return true;
        });
    }   

    var allXimalayaLinks = document.getElementsByClassName("ximalaya-button");

    for (link3 of allXimalayaLinks) {
        link3.addEventListener('click', function() {
            let title = this.dataset.name;
            ga('send', 'event', 'podcast.outbound', 'ximalaya', title);
            return true;
        });
    }   

</script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/animejs/3.2.1/anime.min.js"></script>

<script>
    // example: https://codepen.io/anand2/pen/vYyXKQz
    const wave1 = "M-16.6251 309.365C277.403 309.365 225.193 692.624 524.717 692.624C824.241 692.624 835.232 2.89404 1060.56 2.89404C1285.89 2.89404 1387.57 423.327 1585.42 423.327C1783.27 423.327 1764.03 195.249 2115.77 195.249",
      wave2 = "M-16.625 379.365C277.403 379.365 225.193 2.62384 524.717 2.6239C824.241 2.62397 835.232 612.894 1060.56 612.894C1285.89 612.894 1387.57 283.327 1585.42 283.327C1783.27 283.327 1825.66 825.249 2115.77 825.249",
      wave3 = "M-16.625 326.365C277.403 326.365 225.193 489.624 524.717 489.624C824.241 489.624 835.232 109.894 1060.56 109.894C1285.89 109.894 1387.57 400.327 1585.42 400.327C1783.27 400.327 1825.66 2.24902 2115.77 2.24902",
      wave4 = "M-16.625 469.365C277.403 469.365 235.193 2.62384 534.717 2.6239C834.241 2.62397 835.232 362.894 1060.56 362.894C1285.89 362.894 1387.57 103.327 1585.42 103.327C1783.27 103.327 1825.66 565.249 2115.77 565.249";

      var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
      var wave = document.getElementById("wave"); 
    if(width < 560){
        wave.style.stroke="#ffffff50";
        wave.style.strokeWidth="15";
    }else{
        wave.style.stroke="#ffffff36";
        wave.style.strokeWidth="9";
    }
    
      anime({
    targets: '.wave-top > path',
    easing: 'linear',
    duration: 25000,
    loop: true,
    d: [
        { value: [wave1, wave2] },
        { value: wave3 },
        { value: wave4 },
        { value: wave1 },
    ],
    });
    </script>

