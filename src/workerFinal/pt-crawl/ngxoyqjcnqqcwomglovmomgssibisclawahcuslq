<section class="longform-article__banner">
    <div class="longform-banner__body">
        <h4 class="longform-banner__kicker">
            <a href="/toah" class="longform-banner__top-level-link">
                Heilbrunn Timeline of Art History
            </a>
            <a href="/toah/essays" class="longform-banner__second-level-link">
                Essays
            </a>
        </h4>
        <h1 class="longform-banner__heading">Marcel Duchamp (1887–1968)</h1>
    </div>
</section>

    <section class="artwork-carousel">
        <div class="the-artworks la-musette la-musette--offset la-musette--no-scrollbar js-la-musette">
                <div class="the-artwork__wrapper">
                    <a href="/art/collection/search/488364" class="the-artwork__link" tabindex="0">
                        <img class="the-artwork" alt="Here, This Is Stieglitz Here" src="https://collectionapi.metmuseum.org/api/collection/v1/iiif/488364/1011091/restricted" loading="lazy" />
                    </a>
                    <div class="the-artwork__meta">
                        <h4 class="the-artwork__title">
                            <a href="/art/collection/search/488364">
                                Here, This Is Stieglitz Here
                            </a>
                        </h4>
                        <p class="the-artwork__creator">
                            Francis Picabia
                        </p>
                    </div>
                </div>
                <div class="the-artwork__wrapper">
                    <a href="/art/collection/search/271420" class="the-artwork__link" tabindex="0">
                        <img class="the-artwork" alt="Dust Breeding" src="https://collectionapi.metmuseum.org/api/collection/v1/iiif/271420/597734/restricted" loading="lazy" />
                    </a>
                    <div class="the-artwork__meta">
