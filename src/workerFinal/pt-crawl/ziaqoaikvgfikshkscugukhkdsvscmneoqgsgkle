                                                    <script>
                                                        function Listele() {

                                                            Forum.submit();
                                                        }
                                                    
                                                        function sayfala(Adet1, Sayfa) {
                                                            Forum.ListeSayfa.value = Sayfa;
                                                            Forum.Adet.value = Adet1;
                                                            Forum.submit();
                                                        }
