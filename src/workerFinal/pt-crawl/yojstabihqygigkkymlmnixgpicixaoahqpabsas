function onarrow(imgName, statText, style)
{
	if(valid) document[imgName].src = eval('arrow' + style + 'on.src');
	window.status = statText;
	return true;
}

function offarrow(imgName, style)
{
	if(valid) document[imgName].src = eval('arrow' + style + 'off.src');
	window.status = "";
	return true;
}
