        (function defer() {
            if ('List' in window) {
                const typeList = new List('type-list', { valueNames: ['type-name','type-count']});
                const partnerList = new List('partner-list', { valueNames: ['partner-name','partner-count']});
                const languageList = new List('language-list', { valueNames: ['language-name','language-count']});
                const decadeList = new List('decade-list', { valueNames: ['decade-name','decade-count']});
