                    <section class="generic-widget-wrapper" id="genericProductWidget2021" data-generic-widget="root" data-mktp-container='{"componentTitle":"Workday","componentType":"Generic Product"}' >
<div class="exclude-eds cc-wrapper " data-mktp-product='{"itemId":1055566,"itemName":"Workday","itemBrand":"","itemCategory":"Business","itemCategory2":"","itemCategory3":"","itemCategory4":"","price":0,"quantity":1,"affiliation":"","coupon":"","secondaryCallout":"","starRatings":"4.3","initialCost":0,"subscriptionCost":0}'>
