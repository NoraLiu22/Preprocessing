                            const compliancePixelImage = new Image(0, 0);
                            let cpURL = encodeURIComponent(window.location.href);
                            compliancePixelImage.src = `https://lgforbes.akamaized.net/rtblog.php?requrl=${cpURL}&cid=8CUX59211&crid=611817330`;
                        })();
