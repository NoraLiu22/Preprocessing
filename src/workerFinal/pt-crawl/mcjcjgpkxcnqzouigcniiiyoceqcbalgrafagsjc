		<link rel="alternate" href="https://www.forbes.com/advisor/business/software/best-human-resource-management-systems/" hreflang="en-us"  /><link rel="alternate" href="https://www.forbes.com/advisor/in/business/software/best-human-resource-management-systems/" hreflang="en-in"  /><link rel="alternate" href="https://www.forbes.com/uk/advisor/business/software/best-hr-software/" hreflang="en-gb"  /><link rel="alternate" href="https://www.forbes.com/advisor/au/business/software/best-human-resource-management-systems/" hreflang="en-au"  /><script>
    window.__mpResponseHeaders = (function () {
        let isProduction = true;
        if (isProduction === false) {
            return {
                'x-postal-code': '10001',
                'x-country-code': 'us',
                'x-region': 'ny',
                'x-city-code': 'leesburg',
            };
        }

        const makeRequest = function () {
            const request = new XMLHttpRequest();
            request.open('HEAD', "https:\/\/www.forbes.com\/advisor\/wp-content\/themes\/fadv\/assets\/img\/ZipImage\/1x1.jpg", false);
            request.send(null);
            return request;
        }

        const collectAllResponseHttpHeaders = function (response) {
            const result = {};
            response.getAllResponseHeaders().toLowerCase().split('\n').forEach((header) => {
                let index = header.indexOf(':');
                if(index !== -1){
                    let headerName = header.substring(0, index).trim();
                    let headerValue = header.substring(index + 1).trim();
                    if (!!headerName && !!headerValue) {
                        result[headerName] = headerValue;
                    }
                }
            });
            return result;
        }

        const filterGeoHeaders = function (headers) {
            const filteredHeaders = {};
            for (let headersKey in headers) {
                let hasWhitelistedHeader = ['x-postal-code', 'x-region', 'x-country-code', 'x-city-code'].includes(headersKey);
                if (hasWhitelistedHeader) {
                    filteredHeaders[headersKey] = headers[headersKey];
                }
            }
            return filteredHeaders;
        }

        const response = makeRequest();
        const responseHeaders = collectAllResponseHttpHeaders(response);
        return filterGeoHeaders(responseHeaders);
    })();
