  <div class="node__content">
    

  <section id="paragraph-541" class="component-contact-info paragraph--type-contact-info paragraph--view-mode-default paragraph-component" data-entity-id="541" data-entity-type="paragraph">

    <div class="component-contact-info__content grid-row">
      <div class="contact-image grid-col-auto">
        
  <div class="blazy blazy--field blazy--field-image blazy--field-image--default field field--name-field-image field--type-entity-reference field--label-visually_hidden" data-blazy="">
    <h2 class="field__label visually-hidden">Image</h2>
              <div class="field__item">



      <figure>
                <div data-b-token="b-8111fb6a25e" class="media media--blazy">
        <picture>
