	<script>
		jQuery(document).ready(function($){
			if($('body').hasClass('single-conditions')) {
				$('.select-conditions').addClass('active');
			} else if($('body').hasClass('single-term'))  {
				$('.select-terms').addClass('active');
			}
			var jumpLtr = $('.page-single-condition .side-list .ltr-group ul li a.current').text().charAt(0);
			var current = $('.page-single-condition .side-list .ltr-group ul li a.current').text();
			$(".mobile-select option:contains(" + current + ")").attr('selected', 'selected');
			var jumpLtrPos = $('.ltr-group[data-ltr="' + jumpLtr + '"]').position();
			$('.side-list').animate({ scrollTop: jumpLtrPos.top - 25}, 0);
		});
	</script>
