    	<script>
		$(function() {
			$('.faq-question').off('click').on('click', function() {
				var parentFaqs = $(this).parents('.faqs');
				var faqAnswer = parentFaqs.find('.faq-answer');
				if (parentFaqs.hasClass('clicked')) {
					parentFaqs.removeClass('clicked');
				} else {
					parentFaqs.addClass('clicked');
				}
				faqAnswer.slideToggle();
			});
		})
