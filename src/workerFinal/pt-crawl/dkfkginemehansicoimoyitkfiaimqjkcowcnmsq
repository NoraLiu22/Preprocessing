        function lazyLoadBackgroundImage() {
            const observer = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.style.backgroundImage = `url(${entry.target.dataset.bgSrc})`;
                        observer.unobserve(entry.target);
                    }
                });
            }, {
                rootMargin: "500px",
            });
            const element = document.querySelectorAll('.lazy-load-bg');
            element.forEach((item) => {
                observer.observe(item);
            })
        }
        document.addEventListener('DOMContentLoaded', () => {
            lazyLoadBackgroundImage();
        })
    </script>
<script>
    (function(){
        var PerformanceApi = (function(){
            "use strict";

            var data = {};

            function isEmptyObject(obj) {
                for (var prop in obj) {
                    if (obj.hasOwnProperty(prop)) {
                        return false;
                    }
                }
                return true;
            }

            function isStringSet(str) {
                return str !== undefined && str !== "" && str !== null;
            }

            function getTimeDifference(endTime, startTime) {
                if (endTime === 0 || !(isStringSet(startTime) && isStringSet(endTime))) {
                    return 0;
                }
                return parseInt(endTime - startTime);
            }

            function isPerformanceApiSupported() {
                return window.performance && typeof window.performance.getEntriesByType == 'function';
            }

            function setNavigationPerformanceData() {
                var navEntry = performance.getEntriesByType("navigation")[0];
                if (navEntry == undefined) {
                    return;
                }
                /**
                 * Connection and response time Related fields
                 */
                //Time just before resource is looked for in application cache/fetched from server
                data.fetch_st = parseInt(navEntry.fetchStart);
                data.fetch_t = getTimeDifference(navEntry.responseEnd, navEntry.fetchStart);

                //Time just before domain name lookup
                data.dns_st = parseInt(navEntry.domainLookupStart);
                //Domain Name lookup time
                data.dns_t = getTimeDifference(navEntry.domainLookupEnd, navEntry.domainLookupStart);

                //Time just before initiating connection to the server
                data.conn_st = parseInt(navEntry.connectStart);

                //Time taken to establish connection to the server
                data.conn_t = getTimeDifference(navEntry.connectEnd, navEntry.connectStart);

                //Time taken to establish secure connection to the server including SSL handshake
                data.ssl_t = getTimeDifference(navEntry.connectEnd, navEntry.secureConnectionStart);

                //Time just before request is sent for the resource
                data.req_st = parseInt(navEntry.requestStart);

                //Time when first byte of response is received.
                data.resp_st = parseInt(navEntry.responseStart);

                // Response time only (download): Time between first and last byte of response received.
                data.download_t = getTimeDifference(navEntry.responseEnd, navEntry.responseStart);

                // Request plus response time (network only)
                data.total_t = getTimeDifference(navEntry.responseEnd, navEntry.requestStart);

                // Time to First Byte (TTFB)
                data.ttfb_t = getTimeDifference(navEntry.responseStart, navEntry.requestStart);

                /**
                 * DOM Rendering fields
                 */
                //Time just before current document readiness is set to "interactive"
                data.d_ia = parseInt(navEntry.domInteractive);

                //Time just before DOMContentLoaded event is fired
                data.d_cl_st = parseInt(navEntry.domContentLoadedEventStart);

                //Time taken to complete DOMContentLoaded event
                data.d_cl_t = getTimeDifference(navEntry.domContentLoadedEventEnd, navEntry.domContentLoadedEventStart);

                //Time just before current document readiness is set to "complete"
                data.d_comp = parseInt(navEntry.domComplete);

                /**
                 * Load event and total duration
                 */
                //Time just before Load event is fired
                data.load_st = parseInt(navEntry.loadEventStart);

                //Time taken to complete Load event
                data.load_t = navEntry.loadEventEnd - navEntry.loadEventStart;

                //Load Event End - Start Time
                data.duration_t = parseInt(navEntry.duration);

                /**
                 * Request type details
                 */
                // Navigation type. Possible values: "navigation", "reload", "back_forward", "prerender"
                data.type = navEntry.type;

                data.redirect_cnt = navEntry.redirectCount;

                /**
                 * Payload sizes
                 */
                // Size in octets of response (header + body)
                data.trf_size = navEntry.transferSize;

                // Size in octets of response body before removing any applied content-codings
                data.e_body_s = navEntry.encodedBodySize;

                // Size in octets of response body after removing any applied content-codings
                data.d_body_s = navEntry.decodedBodySize;

                // Size in octets of response header
                data.header_s = getTimeDifference(navEntry.transferSize, navEntry.encodedBodySize);

                //http protocol version
                data.nhp = navEntry.nextHopProtocol;
            }

            function setPaintPerformanceData() {
                var firstPaintEntry = performance.getEntriesByName("first-paint")[0];
                var firstContentfulPaintEntry = performance.getEntriesByName("first-contentful-paint")[0];
                if (firstPaintEntry) {
                    data.first_paint = parseInt(firstPaintEntry.startTime);
                }
                if (firstContentfulPaintEntry) {
                    data.first_cf_paint = parseInt(firstContentfulPaintEntry.startTime);
                }
            }

            function executeNavigationPaintMonitoring(callback) {
                setTimeout(function () {
                    setNavigationPerformanceData();
                    setPaintPerformanceData();
                    data.logid = 'kfk';
                    data.evtid = 'adv_perf';
                    data.requrl = encodeURIComponent(window.location.href);
                    if (typeof callback == "function" && !isEmptyObject(data)) {
                        callback(data);
                    }
                }, 0);// setTimeout done to allow for load event to end and get loadEventEnd time.
            }

            function init(callback) {
                if (isPerformanceApiSupported()) {
                    window.onload = function () {
                        executeNavigationPaintMonitoring(callback);
                    };
                }
            }

            return {
                init: init
            };
        })();

        PerformanceApi.init(function(data){
            var el = document.createElement('img');
            var params = Object.keys(data).map(key => `${key}=${data[key]}`).join('&');
            el.src = `https://lg3.media.net/log?${params}`;
            el.style = 'height:0;width:0;visibility:hidden;';
            document.body.appendChild(el);
        });
    })();
</script><div class="overlay"></div>
