	<div id="MSOZoneCell_WebPartctl00_SPWebPartManager1_g_4200161b_4407_455a_ab67_40b884ac3780" class="s4-wpcell-plain ms-webpartzone-cell ms-webpart-cell-vertical ms-fullWidth ">
		<div class="ms-webpart-chrome ms-webpart-chrome-vertical ms-webpart-chrome-fullWidth ">
			<div WebPartID="4200161b-4407-455a-ab67-40b884ac3780" HasPers="false" id="WebPartctl00_SPWebPartManager1_g_4200161b_4407_455a_ab67_40b884ac3780" width="100%" class="ms-WPBody " allowDelete="false" allowExport="false" style="" ><div id="ctl00_SPWebPartManager1_g_4200161b_4407_455a_ab67_40b884ac3780">
				


<script type="text/javascript">var finderKeyword = "";var finderExtended = "";</script>
<script type="text/javascript">$(document).ready(function () { window.fr1 = $('#fr1').SumoSelect({ okCancelInMulti:true });window.fr2 = $('#fr2').SumoSelect({ csvDispCount: 10 });window.fr3 = $('#fr3').SumoSelect({ csvDispCount: 10 });window.fr4 = $('#fr4').SumoSelect({ csvDispCount: 10 });initializeFinderFilter(); });</script>

<script type="text/javascript">
    function setFinderKeyword() {
        if (finderKeyword != "") {
            $("#HIDOEFinderKeyword").val(decodeURIComponent(finderKeyword));
        }
    }

    var currentUrl = document.URL;

    if (currentUrl.indexOf("#k=") > -1) {
        var urlArr = currentUrl.split("#");
        currentUrl = urlArr[0];
    }

    function submitFinderKeyword() {

        var newFinderKeyword = $("#HIDOEFinderKeyword").val().trim();
        
        if (newFinderKeyword != "") {
            newFinderKeyword = "fk=" + encodeURIComponent(newFinderKeyword);

            if (finderKeyword != "") {
                var oldFinderKeyword = "fk=" + encodeURIComponent(finderKeyword);
                currentUrl = currentUrl.replace(oldFinderKeyword, newFinderKeyword);
            }
            else {
                if (currentUrl.indexOf("?") > -1) {
                    currentUrl = currentUrl + "&" + newFinderKeyword;
                }
                else {
                    currentUrl = currentUrl + "?" + newFinderKeyword;
                }
            }

            window.location.assign(currentUrl + "#k=" + getKeyWordFilter());
        }
    }
       

    $(document).ready(function () {
        $(document).click(function (event) {
            if (!$(event.target).hasClass("HIDOESelectorElement")) {
                if (window.fr1 != null)
                    $('#fr1')[0].sumo.hideOpts();
                if (window.fr2 != null)
                    $('#fr2')[0].sumo.hideOpts();
                if (window.fr3 != null)
                    $('#fr3')[0].sumo.hideOpts();
                if (window.fr4 != null)
                    $('#fr4')[0].sumo.hideOpts();
            }
        });
    });

    function initializeFinderFilter() {
        setFinderKeyword();

        if (finderExtended == "" || finderExtended == "fx=true") {
            $("#finderWebPartFilters").show();
            $("#filterShowHide").text("Hide Filters");
        }
        else {
            $("#finderWebPartFilters").hide();
            $("#filterShowHide").text("Show Filters");
        }
    }

    function showHideFinderFilter() {
        if ($("#filterShowHide").text() == "Show Filters") {
            finderExtended = "fx=true";
            $("#finderWebPartFilters").show("slow");
            $("#filterShowHide").text("Hide Filters");
        }
        else {
            finderExtended = "fx=false";
            $("#finderWebPartFilters").hide("slow");
            $("#filterShowHide").text("Show Filters");
        }
    }

    function buildNewUrl(oldParam, newParam) {
        var newUrl = "";

        if (oldParam != "") {
            newUrl = currentUrl.replace(oldParam, newParam);
        }
        else {
            if (currentUrl.indexOf("?") > -1) {
                newUrl = currentUrl + "&" + newParam;
            }
            else {
                newUrl = currentUrl + "?" + newParam;
            }
        }

        newUrl = newUrl.replace("&&", "&");

        return newUrl;
    }

    function clearFilter() {
        var cleanUrl = [location.protocol, '//', location.host, location.pathname].join('');
        if (finderExtended != "") {
            cleanUrl = cleanUrl + "?" + finderExtended;
        }

        window.location.assign(cleanUrl);
    }

    function removeSelectedFinderFilter(removeUrl) {
        if (finderExtended != "") {
            if (removeUrl.indexOf(finderExtended) < 0) {
                if (removeUrl.indexOf("?") > -1) {
                    removeUrl = removeUrl + "&" + finderExtended;
                }
                else {
                    removeUrl = removeUrl + "?" + finderExtended;
                }
            }
        }

        removeUrl = removeUrl.replace("&&", "&");

        window.location.assign(removeUrl + "#k=" + getKeywordFilterRemoval(removeUrl));
    }

    function singleSelectedFinderFilter(filterId, oldFilterValue, newFilterValue) {
        var redirectUrl = currentUrl;

        if (newFilterValue == "all") {
            if (oldFilterValue != "") {
                redirectUrl = redirectUrl.replace(oldFilterValue, "");
            }
        }
        else {
            newFilterValue = filterId + "=" + newFilterValue;
            redirectUrl = buildNewUrl(oldFilterValue, newFilterValue);
        }

        if (redirectUrl.indexOf("fx=true") < 0) {
            if (redirectUrl.indexOf("?") > -1) {
                redirectUrl = redirectUrl + "&fx=true";
            }
            else {
                redirectUrl = redirectUrl + "?fx=true";
            }
        }

        redirectUrl = redirectUrl.replace("&&", "&");

        window.location.assign(redirectUrl + "#k=" + getKeyWordFilter());
    }

    function getKeyWordFilter() {
        var keywordFilter = "";

        var fr1 = $("#fr1").val();
        var fr1Ref = $("#fr1").attr("refiner");

        var fr2 = $("#fr2").val();
        var fr2Ref = $("#fr2").attr("refiner");

        var fr3 = $("#fr3").val();
        var fr3Ref = $("#fr3").attr("refiner");

        var fr4 = $("#fr4").val();
        var fr4Ref = $("#fr4").attr("refiner");

        keywordFilter = getFilterToken(fr1, fr1Ref) + getFilterToken(fr2, fr2Ref) + getFilterToken(fr3, fr3Ref) + getFilterToken(fr4, fr4Ref);

        return encodeURIComponent($("#HIDOEFinderKeyword").val().trim() + keywordFilter);
    }

    function getKeywordFilterRemoval(removalUrl) {
        var keywordFilter = "";
        var urlTokens = removalUrl.split('?');
        var fr1 = "all";
        var fr1Ref = $("#fr1").attr("refiner");
        var fr2 = "all";
        var fr2Ref = $("#fr2").attr("refiner");
        var fr3 = "all";
        var fr3Ref = $("#fr3").attr("refiner");
        var fr4 = "all";
        var fr4Ref = $("#fr4").attr("refiner");

        if (urlTokens.length > 1) {
            var querystring = urlTokens[1];

            if (querystring.length > 1) {
                var params = querystring.split('&');

                if (params.length > 0) {
                    for (i = 0; i < params.length; i++) {
                        if (params[i].indexOf("fr1") > -1) {
                            fr1 = $("#fr1").val();
                        }
                        if (params[i].indexOf("fr2") > -1) {
                            fr2 = $("#fr2").val();
                        }
                        if (params[i].indexOf("fr3") > -1) {
                            fr3 = $("#fr3").val();
                        }
                        if (params[i].indexOf("fr4") > -1) {
                            fr4 = $("#fr4").val();
                        }
                    }

                    keywordFilter = getFilterToken(fr1, fr1Ref) + getFilterToken(fr2, fr2Ref) + getFilterToken(fr3, fr3Ref) + getFilterToken(fr4, fr4Ref);
                }
            }
        }

        return encodeURIComponent($("#HIDOEFinderKeyword").val().trim() + keywordFilter);
    }

    function getFilterToken(fr, frRef) {
        var frFilterToken = "";

        if ($.isArray(fr)) {
            $.each(fr, function (index, value) {
                if (value != "all") {
                    if (fr.length > 2) {
                        frFilterToken = frFilterToken + "(" + frRef + ":" + value + ")";
                    }
                    else {
                        frFilterToken = frFilterToken + frRef + ":" + value;
                    }

                    if (index != fr.length - 1) {
                        frFilterToken = frFilterToken + " OR ";
                    }
                }
            });

            if (fr.length > 2) {
                frFilterToken = " (" + frFilterToken + ")";
            }
            else {
                frFilterToken = " " + frFilterToken;
            }
        }
        else {
            if (fr != "all" && fr != null) {
                return " " + frRef + ":" + fr;
            }
        }

        return frFilterToken;
    }
</script>

