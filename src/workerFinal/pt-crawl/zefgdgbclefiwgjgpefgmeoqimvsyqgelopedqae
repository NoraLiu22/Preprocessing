                if (window.jQuery && typeof($.fn.popover) != 'undefined' ) {
                  var $mapsPop = $("#ark-context-jump-map");
                  $mapsPop.popover({
                    container: '#overview-content',
                    trigger: "manual",
                    placement: "bottom",
                    html: "true",
                    title: "Recognize a Place? <i id='close-been-here' class='fa fa-close pull-right'></i>",
                    content: "If you can locate it, you can help " +
                    "<a style='text-decoration: underline;' id='mapHelpPopoverLink' href='#coverage-map-help'>improve our maps!</a>"
                  });
                  $mapsPop.popover('show');

                  $("#close-been-here").on('click', function() {
                    $mapsPop.popover('destroy');
                  });
                  ;
                } else {
                  setTimeout(function() { defer() }, 50);
                }
            }());
            
          
        
