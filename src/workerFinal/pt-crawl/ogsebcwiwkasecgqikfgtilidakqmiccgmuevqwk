<body text="#000000" bgcolor="#FFFFFF" link="#00CC00" vlink="#FF0000" alink="#33FF33" background="http://www.naturalworlds.org/wolf/images/slate_2.jpg" nosave oncontextmenu="return false" onselectstart="return false" ondragstart="return false">
