$(function(){
	$('.partGroupTitle + ul.partMemberList').each(function() {
		const parentCat = $(this).closest('.level2').prevAll('.level1').find('.partTitle .part').text();
		$(this).find('.commonPart').text(parentCat)
	});
	if(document.URL.match("/2022")) {
		$('.catNav').hide();
	}
});
</script>
</section>
