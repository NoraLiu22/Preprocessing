    $(document).ready(function ()
    {



                    $.ajax({
                async: true,
                cache: true,
                type: "GET",
                url: "/Home/PopupContent",
                success: function (data) {
                    if (data.length == 0) {
                        console.log("olmadı");
                    }
                    else {
                        $("#popup").empty();
                        var large = ' <div class="modal" id="myModal" role="dialog" data-backdrop="true"> ' + ' <div class= "modal-dialog"> ' + ' <div class="modal-content"> ' + ' <div class="modal-header text-right"> ' +
                            ' <i class="fa fa-times fa-1x" data-dismiss="modal"> ' + ' </i> ' + ' </div> ' + ' <div class= "modal-body" id = "modal-content"> ' + data[0].icerik + ' </div> ' +
                            ' </div> ' + ' </div > ';
                        $("#popup").append(large);
                        $("#myModal").modal();
                    }


                }
            });
    })
</script>

