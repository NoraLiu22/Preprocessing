<script>
// rotating image code

var imagePath = "/images/subpage/banner/"; //I left this as root/images/subpage.  Smart? maybe. -J Woodard 20-Dec-2005
																					 //Addendum, 13-Jan-2005.  The only 'not-smart' thing about this is that it WILL NOT work on your local machine.
																					 //Using the "Preview in [Safari, Firefox, IE]" feature in Dreamweaver will show you broken images when it tries
																					 //to cycle, since it's looking for the images at the root level of your hard drive.
																					 //It WILL however, allow this template to be applied to pages at any level of hierarchy on the site.
var start = Math.floor(7 * Math.random());
var mySlideList_left = [ imagePath + 'left01.gif', imagePath + 'left02.gif', imagePath + 'left03.gif', imagePath + 'left04.gif', imagePath + 'left05.gif', imagePath + 'left06.gif', imagePath + 'left07.gif']; 
var mySlideShow_left = new SlideShow(mySlideList_left, 'slide_left', 6000, "mySlideShow_left", start); 

var mySlideList_mid = [imagePath + 'mid01.gif', imagePath + 'mid02.gif', imagePath + 'mid03.gif', imagePath + 'mid04.gif', imagePath + 'mid05.gif', imagePath + 'mid06.gif', imagePath + 'mid07.gif']; 
var mySlideShow_mid = new SlideShow(mySlideList_mid, 'slide_mid', 6000, "mySlideShow_mid", start); 

var mySlideList_right = [imagePath + 'right01.gif', imagePath + 'right02.gif', imagePath + 'right03.gif', imagePath + 'right04.gif', imagePath + 'right05.gif', imagePath + 'right06.gif', imagePath + 'right07.gif']; 
var mySlideShow_right = new SlideShow(mySlideList_right, 'slide_right', 6000, "mySlideShow_right", start); 

function SlideShow(slideList, image, speed, name, start) {
	this.slideList = slideList; 
	this.image = image; 
	this.speed = speed; 
	this.name = name; 
	this.current = start; 
	this.timer = 0; 
} 

SlideShow.prototype.play = SlideShow_play; 

function SlideShow_play() { 
	with(this) { 
		if(current++ == slideList.length-1) current = 0;
		switchImage(image, slideList[current]); 
		clearTimeout(timer); 
		timer = setTimeout(name+'.play()', speed); 
	}  
} 

function switchImage(imgName, imgSrc) {
  	if (document.images)  {
    	if (imgSrc != "none") { 
			document.images[imgName].src = imgSrc; 
		} 
	} 
} 

function addLoadEvent(func) {
  var oldonload = window.onload;
  if (typeof window.onload != 'function') {
    window.onload = func;
  } else {
    window.onload = function() {
      oldonload();
      func();
    }
  }
}
</script>
