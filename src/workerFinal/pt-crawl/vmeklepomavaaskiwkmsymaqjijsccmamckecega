        // data
        
            // all placenames
            window.Maps.mapData.placeNameList = [
              
                {
                  value: "United States \u002D Washington D.C."
                }
              
            ];
            // placenames with geo-coded locations
            window.Maps.mapData.geoPlaceNames = [
              
              {
                p: 'United States - Washington D.C.',
                lat: '38.91706',
                lng: '-77.00025'
              }
              
            ];
        
        // placepoints
        
            window.Maps.mapData.placePointList = [[38.895848, -77.055683]];
        
        // placeboxes
        

        
            (function defer() {
                if ((window.jQuery) && ('Charts' in window) && ('initMap' in window.Maps)) {
                  // Bind the initMap function to scroll and resize to render when in view
                  const $map = $('#map');
                  window.Charts.renderInView($map, window.Maps.initMap);
                } else {
                  setTimeout(function () { defer(); }, 50);
                }
            }());
    </script>

        
        
    



</body>
</html>
