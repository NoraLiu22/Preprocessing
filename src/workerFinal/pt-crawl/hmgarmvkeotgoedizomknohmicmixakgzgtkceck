<source data-srcset="https://cdn.prod.www.spiegel.de/images/ed3e4d78-3e15-490f-81ff-3b910d1cf27c_w488_r1.778_fpx47_fpy81.webp 488w, https://cdn.prod.www.spiegel.de/images/ed3e4d78-3e15-490f-81ff-3b910d1cf27c_w288_r1.778_fpx47_fpy81.webp 288w" data-sizes="(max-width: 487px) 100vw, (min-width: 488px) and (max-width: 719px) 488px, (min-width: 720px) and (max-width: 1019px) 488px, (min-width: 1020px) 288px" type="image/webp">
