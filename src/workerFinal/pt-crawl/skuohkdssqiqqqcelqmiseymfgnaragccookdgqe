        require(['foundation/main'], function() {
            require(['blogs/main']);
            require(['https://static01.nyt.com/projects/blogs/js/tracking.js']);
            // Blacklist some trackers imported from nyt5 legacy libraries
            $(document).ajaxSend(function(event, jqXHR, settings) {
                if (settings.url.indexOf && settings.url.indexOf('amazon-adsystem') !== -1) {
                    jqXHR.abort();
                }
