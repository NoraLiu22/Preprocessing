                var divsize = document.querySelector('#emp-8c008').parentNode.clientWidth;
                var AdButler = AdButler || {}; AdButler.ads = AdButler.ads || [];
                var abkw = window.abkw || '';
                console.log("emp-8c008-size-"+divsize);
                if(divsize >= 728){
                    var plc551849 = window.plc551849 || 0;
                    document.write('<'+'div id="placement_551849_'+plc551849+'"></'+'div>');
