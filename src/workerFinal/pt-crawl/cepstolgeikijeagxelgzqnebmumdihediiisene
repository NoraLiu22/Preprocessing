    data-show-emergency-copy=""
    data-emergency-text=""
    data-emergency-text-icon=""
    data-emergency-text-cta=""
    data-show-emergency-cta-btn=""
    data-emergency-text-cta="" 
    data-seo-alt-tag="Two First Nations girls smiling, hanging from a tree"
    data-seo-alt-description="Two First Nations girls smiling in the outback"
    data-gradient-opacity="60"
    data-show-secondary-btn=""
    data-secondry-btn-text="" 
    data-secondry-btn-icon=""
