    function sliderPhoto() {

        var activeindex = $("#sliderdiv .active").attr("id");
        var sliderHtml = "";
        var tarih = "";
        var sliderImages = "";
        var captionclass = "caption-solust";

        $("#carouseldiv").html("");

        $.each(jsModel, function (i, item) {

            tarih = moment(item.CreateDate).format("YYYY/MM/DD")
            if (item.EnmSliderTitleLocation == 2) {
                captionclass = "caption-sagust";
            }
            else if (item.EnmSliderTitleLocation == 4) {
                captionclass = "caption-sagalt";
            }
            else if (item.EnmSliderTitleLocation == 1) {
                captionclass = "caption-solust";
            }
            else if (item.EnmSliderTitleLocation == 3) {
                captionclass = "caption-solalt";
            }
            else if (item.EnmSliderTitleLocation == 5) {
                captionclass = "caption-line";
            }

             if (activeindex != undefined) {
                 if (activeindex == "item-" + i) {
                     sliderHtml += '<div class="carousel-item active" id="item-' + i + '">';
                 }
                 else {
                     sliderHtml += '<div class="carousel-item" id="item-' + i + '">';
                 }
             }
             else {
                 if (i == 0) {
                     sliderHtml += '<div class="carousel-item active" id="item-' + i + '">';
                 }
                 else {
                     sliderHtml += '<div class="carousel-item" id="item-' + i + '">';
                 }
             }

             $.each(item.Photos, function (i, photo) {
                if (pagewidth < 615 && photo.EnmImageType == 2) {
                    sliderImages = 'https://cdnuploads.aa.com.tr/uploads/ayrimcilikhatti/Contents/' + tarih + '/thumbs_m_c_' + photo.ImageName;
                    return false
                }
                else if (pagewidth > 615 && photo.EnmImageType == 4) {
                    sliderImages = 'https://cdnuploads.aa.com.tr/uploads/ayrimcilikhatti/Contents/' + tarih + '/thumbs_b_c_' + photo.ImageName;
                    return false
                }
            });

            sliderHtml += '<a href="https://www.aa.com.tr/tr/ayrimcilikhatti/' + item.CategoryRoute + '/' + item.Route + '/' + item.ID + '">'+
                '<img src="' + sliderImages+'" class="d-block w-100"></a>' +
                '<a href="https://www.aa.com.tr/tr/ayrimcilikhatti/' + item.CategoryRoute + '/' + item.Route + '/' + item.ID + '"><div class="carousel-caption '+ captionclass+'">' +
                '<h3>' + item.Title + '</h3>' +
                '</div></a></div>';
         });

        if (sliderHtml != $("#sliderdiv").html()) {
            $("#sliderdiv").html(sliderHtml);
        }

    }

</script>
