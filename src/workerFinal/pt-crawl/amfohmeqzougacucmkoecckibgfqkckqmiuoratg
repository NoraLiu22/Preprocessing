<script>

async function loadingIcon() {
	var x= document.getElementById("KGVia").value;
	var patt = /^.{5,60}$/;
	var valid = patt.test(x);
	if (valid==true){
	document.getElementById("buttonHide").style.display = "none";
	document.getElementById("loaderIcon").style.display = "block";
	document.getElementById("spacer").style.padding = "70px 30px 20px 30px";
	document.getElementById("spacer").style.overflow="hidden";
	}
};

</script>


