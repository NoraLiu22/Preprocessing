    <script type="text/javascript"> 
    ( function($) { 
    $(document).ready(function(){
        var myPlaylist = new jPlayerPlaylist({
            jPlayer: "#jquery_jplayer_1",
            cssSelectorAncestor: "#jp_container_1"
        },
        loadPodcast("https://www.ttbook.org/podcast/shows/rss.xml"), {
            playlistOptions: {
                autoPlay: false
            },
            cssSelectorAncestor: "#jp_container_1",
            swfPath: "./",
