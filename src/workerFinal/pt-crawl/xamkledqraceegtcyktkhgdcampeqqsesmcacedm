	<script>
		var trackOtherPackages = document.querySelectorAll('.other-packages')
		for (var i = 0; i < trackOtherPackages.length; i++) {
			if (trackOtherPackages[i].classList.contains('other-packages')) {
				trackOtherPackages[i].setAttribute("href", "https://subscribe.theage.com.au/other-packages" + "?promote_channel=" + promote_channel)
			}
		}

		var trackOtherSubscriptions = document.querySelectorAll('.other-subscriptions')
		for (var i = 0; i < trackOtherSubscriptions.length; i++) {
			if (trackOtherSubscriptions[i].classList.contains('other-subscriptions')) {
				trackOtherSubscriptions[i].setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_webbasic" + "&promote_channel=" + promote_channel)
			}
		}

		var cardPosition = document.querySelector(".card-position")
		if (cardPosition.classList.contains("weekly-view")) {
			var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
			if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
				trackButtonWeekday.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldigwkend" + PROMOTE_CHANNEL)
			}
			var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
			if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
				trackButtonWeekend.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_webbasic" + PROMOTE_CHANNEL)
			}
			var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
			if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
				trackButtonUltimate.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldig" + PROMOTE_CHANNEL)
			}
		}

		document.querySelector('.visual-status').addEventListener('click', function () {
			// toggle weekly
			if (cardPosition.classList.contains('annual-view')) {
				cardPosition.classList.remove("annual-view")
				cardPosition.classList.add("weekly-view")

				var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
				if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
					trackButtonWeekday.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldigwkend" + PROMOTE_CHANNEL)
				}
				var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
				if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
					trackButtonWeekend.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_webbasic" + PROMOTE_CHANNEL)
				}
				var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
				if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
					trackButtonUltimate.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldig" + PROMOTE_CHANNEL)
				}

				var subscribeCtaWeekly = document.querySelectorAll('.subscribe-now')
				for (var i = 0; i < subscribeCtaWeekly.length; i++) {
					if (subscribeCtaWeekly[i].classList.contains('subscribe-now')) {
						subscribeCtaWeekly[i].innerHTML = 'SUBSCRIBE NOW'
					}
				}

				var headlineWeekly = document.querySelector('.page-headline')
				headlineWeekly.innerHTML = 'Get a well-balanced perspective.'

				var subheadlineWeekly = document.querySelector('.page-subheadline')
				subheadlineWeekly.innerHTML = 'Join The Age from $4.50 per week and support news you can trust.'

				var termsWeekly = document.querySelector('.terms')
				termsWeekly.innerHTML = 'Subscription will automatically renew unless the subscriber changes or cancels their subscription in My Account or by calling customer service on 13 66 66. Home delivery (if included) is available in selected areas only. Please allow 10 days for delivery to commence. For full subscription terms and conditions visit theage.com.au/conditions. <a href="https://www.theage.com.au/digital-subscription-terms" target="_blank">theage.com.au/conditions</a>.'

			} else {
				// toggle annual
				cardPosition.classList.add("annual-view")
				cardPosition.classList.remove("weekly-view")
				var trackButtonWeekday = document.querySelector('.card-content .subscribe-weekday')
				if (trackButtonWeekday.classList.contains('subscribe-weekday')) {
					trackButtonWeekday.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldigwkend_12m" + PROMOTE_CHANNEL)
				}
				var trackButtonWeekend = document.querySelector('.card-content .subscribe-weekend')
				if (trackButtonWeekend.classList.contains('subscribe-weekend')) {
					trackButtonWeekend.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_webbasic_12m" + PROMOTE_CHANNEL)
				}
				var trackButtonUltimate = document.querySelector('.card-content .subscribe-ultimate')
				if (trackButtonUltimate.classList.contains('subscribe-ultimate')) {
					trackButtonUltimate.setAttribute("href", "https://www.theage.com.au/subscription/account/?bundle_key=age_alldig_12m" + PROMOTE_CHANNEL)
				}

				var subscribeCtaAnnual = document.querySelectorAll('.subscribe-now')
				for (var i = 0; i < subscribeCtaAnnual.length; i++) {
					if (subscribeCtaAnnual[i].classList.contains('subscribe-now')) {
						subscribeCtaAnnual[i].innerHTML = 'SUBSCRIBE NOW'
					}
				}

				var headlineAnnual = document.querySelector('.page-headline')
				headlineAnnual.innerHTML = 'Get a well-balanced perspective.'

				var subheadlineAnnual = document.querySelector('.page-subheadline')
				subheadlineAnnual.innerHTML = 'Subscribe and save 10% on your annual package.*'

				var termsAnnual = document.querySelector('.terms')
				termsAnnual.innerHTML = '*Discount is 10% off the annual subscription price for the first 12 months on selected packages only. Offer is available to new subscribers only. At the end of the promotional period the subscription rate will increase to the regular package rate, billed annually. This offer is subject to change. Subscriptions automatically renew unless the subscriber changes or cancels their subscription in My Account or by calling customer service on 13 66 66. For full subscription terms and conditions visit <a href="https://www.theage.com.au/digital-subscription-terms" target="_blank">theage.com.au/conditions</a>.'

			}
		})
	</script>

