	<script>
		var deviceType
		var md = new MobileDetect(window.navigator.userAgent)
		var isMobile = md.phone() || navigator.userAgent.match(/iPhone|iPad|iPod/i)
		var isTablet = md.tablet()
		if (isMobile) {
			deviceType = 'mobile'
		} else if (isTablet) {
			deviceType = 'tablet'
		} else {
			deviceType = 'desktop'
		}
	</script>

