window.onload = function() {if (document.getElementById){if (document.getElementById('PageX')){scrollIt();}}}
window.onscroll = function() {if (document.getElementById){if (document.getElementById('PageX')){setCoords();}}}
function scrollIt() {window.scrollTo(document.getElementById('PageX').value, document.getElementById('PageY').value);}
function setCoords() {var myPageX; var myPageY;if (document.all){myPageX = document.body.scrollLeft;myPageY = document.body.scrollTop;}else{myPageX = window.pageXOffset; myPageY = window.pageYOffset;}document.getElementById('PageX').value = myPageX; document.getElementById('PageY').value = myPageY;}</script>
