<script>
    $('documenet').ready(function () {
        if (sessionStorage.majorFilter) {
            console.log("Pulling stored value of: " + sessionStorage.majorFilter);
            $('#mjrFilter').val(sessionStorage.majorFilter);
            mjrFilter($('#mjrFilter'));
        }
        $('#mjrFilter').keyup(function () {
            console.log("storing: " + this.value);
            sessionStorage.majorFilter = this.value;
            mjrFilter($(this));
        });
		
		document.getElementById("mjrFilter").addEventListener("search", function(event) {
		mjrFilter($("#mjrFilter"));  
		});
		
    });

    mjrFilter = function (elem) {
        var debugLog = false;
        var filter = elem.val().trim().toUpperCase();
        if (debugLog) console.log("filtering for " + filter);

        //Move options that don't match from main to slush
        $('#Main_ddlMajor option').each(function () {
            var obj = $(this);
            if (filter != "" && obj.val() != "0" && !obj.html().toUpperCase().includes(filter)) {
                if (debugLog) console.log(obj.html() + " does not contain ''" + filter + "'' so we will move it to slush.");
                obj.remove().appendTo('#slush');
            }
        });

        //move options that do match from slush to main
        $('#slush option').each(function () {
            var obj = $(this);
            if (filter == "" || obj.html().toUpperCase().includes(filter)) {
                if (debugLog) console.log(obj.html() + " does contain ''" + filter + "'' so we will move it to slush.");
                obj.remove().appendTo('#Main_ddlMajor');
            }
        });

        //re-sort main
        var options = $('#Main_ddlMajor option');
		var originalSelectionValue = $('#Main_ddlMajor').val();
        var arr = options.map(function (_, o) {
            return {
                t: $(o).text(),
                v: o.value
            };
        }).get();
        arr.sort(function (o1, o2) {
            return o1.t.toUpperCase() > o2.t.toUpperCase() ? 1 : o1.t.toUpperCase() < o2.t.toUpperCase() ? -1 : 0;
        });
        options.each(function (i, o) {
            o.value = arr[i].v;
            $(o).text(arr[i].t);
			$(o).attr("selected", arr[i].v == originalSelectionValue ? "selected" : null);
        });

        //select the default
        if (filter == "") $('#Main_ddlMajor').val("0");

    };  
</script>

<div class="portal-main-content">

    <div class="row">
        <div class="columns large-12">
            <div id="pageTitle">
               
	            <h1>Graduation MAPs</h1>
                 <!---->
	            <p>Graduation MAPs (Major Academic Plans) show a semester-by-semester plan for taking all classes needed to earn your degree. This is a new service for WSU students, and we are currently adding new MAPs every week. If your major is not listed, check directly with the academic department.</p>
	            <h2>New students</h2>
	            <p>Find your MAP below using the default Catalog Year. Also, review <a href="http://weber.edu/orientation" target="_blank">New Student Orientation</a> for more information and to meet your academic advisor.</p>
	            <h2>Current students</h2>	
                
                    <p><a href="http://apps.weber.edu/gradmaps/default.aspx?cas=true" id="Main_lbLogin">Log in</a> to your portal and be sure to choose the correct Catalog Year for your major, which is the year in which you were admitted or declared your major. If you have questions, schedule an appointment with your <a href="#" target="_blank">academic advisor</a>.</p>
                
	            
            </div>
        </div>
    </div>
