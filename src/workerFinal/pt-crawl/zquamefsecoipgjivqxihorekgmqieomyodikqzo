			<div class="article-summary photo-article-summary">
				<h2 class="article-heading">
				    Joint Venture Education Forum celebrates 20th year of education partnerships with Hawaii’s military community
				</h2>
                
    22-Aug-2019



				
				<p class="intro">
					The Joint Venture Education Forum (JVEF), a cooperative partnership between Hawaii’s military community, HIDOE and other community organizations, highlighted success stories of Hawaii’s military students and their families at its 20th Annual Meeting today.
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

