            <i class="ob-icons ob-icons-recherche"></i>
                        <form action="https://search.openedition.org/results" method="get">
                                    <div class="ob-dropdown-search">
                        <select name="searchdomain" class="ob-dropdown-search-select">
                            <option  disabled="disabled">Rechercher dans...</option>
                            <option value="openedition" selected="selected">Tout OpenEdition</option>
                            <option value="catalogue-journals">Catalogue des revues</option>
                            <option value="catalogue-notebooks">Catalogue des carnets</option>
                        </select>
                    </div>
                                <input type="search" name="q" placeholder="Expression ou mot-cl&eacute;" >
                <button type="submit" class="ob-buttons-search">Rechercher</button>
            </form>
            <script>
            (function( $ ) {
                'use strict';
                $(function() {
                $('select[name="searchdomain"]').change( function() {
                    var value = $(this).val();
                    var form_action = "";
                    if (value == "catalogue-notebooks") {
                    form_action = "https://www.openedition.org/catalogue-notebooks";
                    } else if ( value == "openedition") {
                    form_action = "https://search.openedition.org/results";
                    }else {
                    form_action = "https://www.openedition.org/catalogue-journals";
                    }
                    if (form_action != "") {
                    $(this).closest('form').attr('action', form_action);
                    }
                });
                });
            })(jQuery);
            </script>
