<script language="javascript">
    window.onload = customFocusElement('year');

    function focusOnYear() {
        customFocusElement('year');
    }

    function yearValidation() {
        var year = document.getElementById("year").value;
        var text = /^[0-9]+$/;
        if (year != 0) {
            if ((year != "") && (!text.test(year))) {
                alert("                                   World Neuropterida Metakey\n                                                    Warning!\n\n               Enter a 4-digit year between 1500 and the current year");
                return false;
            }

            if (year.length != 4) {
                alert("                                   World Neuropterida Metakey\n                                                    Warning!\n\n               Enter a 4-digit year between 1500 and the current year");
                return false;
            }
            var current_year = new Date().getFullYear();
            if ((year < 1500) || (year > current_year)) {
                alert("                                   World Neuropterida Metakey\n                                                    Warning!\n\n               Enter a 4-digit year between 1500 and the current year");
                return false;
            }
            return true;
        }
    }
</script>

