(function()
{
  var offset = pageYOffset;
  var y = 0;
  window.addEventListener("scroll", function()
  {
    var iph = element("iph");
    var max = iph.offsetHeight * 1.1;
    y = Math.min(Math.max(y - (pageYOffset - offset), -max), 0);
    iph.style.top = y + "px";
    iph.classes.toggle("shadow", y > -max && iph.parentNode.getBoundingClientRect().top < 0);
    offset = pageYOffset;
  }, false);
