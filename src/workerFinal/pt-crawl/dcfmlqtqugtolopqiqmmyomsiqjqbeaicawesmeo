<script type="pmdelayedscript" data-cfasync="false" data-no-optimize="1" data-no-defer="1" data-no-minify="1">
    const advisorCollapseItems = document.querySelectorAll(".advisor-collapse-header");

    if (advisorCollapseItems.length) {
        advisorCollapseItems.forEach(function (header) {
            header.onclick = function (e) {
                e.preventDefault();
                this.parentNode.classList.toggle("active");
            };
        });
    }
