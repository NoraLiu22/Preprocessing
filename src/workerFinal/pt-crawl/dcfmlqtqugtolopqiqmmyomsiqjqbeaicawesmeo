							usePageSlug = '1',
							useDevice = '1',
							uniqueId = '24279'
						$("#widget-wrapper-24279_2 a").each(function() {
							var ahref = this.href || '';
							var attribution = usePageSlug.length ? pageSlug + '-' : '';
							attribution += uniqueId ? uniqueId.replace(/\s+/g, '-').toLowerCase() : '';
