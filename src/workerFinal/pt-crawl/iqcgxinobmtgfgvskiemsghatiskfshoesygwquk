<script type="text/javascript">
    require([
        'jquery'
    ], function ($) {
        'use strict';
        $(document).on('click touch', '#event .event-tab-title div', function () {
            if (!$(this).closest('.event-tab-title').hasClass('active')) {
                $("#event .event-tab-title").removeClass('active');
                $(this).closest('.event-tab-title').addClass('active');
                $("#event .event-tab-container").attr('hidden', 'hidden');
                let id = $(this).data('href');
                $("#content" + id).removeAttr('hidden');
            } else {
                console.log('tab is active');
            }
        })
    });</script>
