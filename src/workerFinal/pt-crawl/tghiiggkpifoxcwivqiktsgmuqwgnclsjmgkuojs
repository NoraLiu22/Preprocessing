    function changeHam() {
        window.scrollTo(0, 0);
        let hamNav = document.getElementById("ham-menu-img")
        let flexContainerDiv = document.getElementById("flex-navigation-container");
        let flexContainerNavDiv = document.getElementById("flex-navigation-container");
        let chatContainer = document.querySelector(".fatv18");
        let x = hamNav.src.split("/"), t = x.length-1, y = x[t];

        let ua = window.navigator.userAgent;
        let iOS = !!ua.match(/iPad/i) || !!ua.match(/iPhone/i);
        let webkit = !!ua.match(/WebKit/i);
        let iOSSafari = iOS && webkit && !ua.match(/CriOS/i);
        let hamMenu = document.querySelector(".ham-menu");

        if (y == 'menu.svg') {
            flexContainerDiv.style.display = "flex"
            hamNav.src='/img/recruitment-2021/x.svg'
        }

        if (y == 'x.svg') {
            flexContainerDiv.style.display = "none"
            hamNav.src='/img/recruitment-2021/menu.svg'
        }

        if (iOSSafari && y == 'menu.svg') {
            hamMenu.style.paddingBottom = "50px";
            flexContainerNavDiv.style.height = "87vh";

            if (chatContainer) {
                chatContainer.style.display = "flex";
                chatContainer.style.bottom = "92vh";
                chatContainer.style.right = "10px";
            }
        } 

        if (iOSSafari && y == 'x.svg') {
            hamMenu.style.paddingBottom = "0px";
            flexContainerNavDiv.style.height = "92vh";

            if (chatContainer) {
                chatContainer.style.display = "flex";
                chatContainer.style.bottom = "110px";
                chatContainer.style.right = "10px";
            }
        } 
    }

    let min_inner_height = false;
    let max_inner_height = false;
    let passiveIfSupported = false;
    
    try {
        window.addEventListener("test", null, Object.defineProperty({}, "passive", { 
            get: function () { 
                passiveIfSupported = { 
                    passive: true 
                }; 
            } 
        }));
    } catch (err) {}

    document.addEventListener('scroll', function (e) {
        let win_inner_h = window.innerHeight;
        if (/iPad|iPhone|iPod/.test(navigator.userAgent)) {
            if (min_inner_height === false || win_inner_h < min_inner_height) {
                min_inner_height = win_inner_h;
            }
            if ((max_inner_height === false || win_inner_h > max_inner_height) && win_inner_h > min_inner_height) {
                max_inner_height = win_inner_h;
            }
            if (max_inner_height !== false && max_inner_height == win_inner_h) {
                addElementClass(document.body, 'safari-toolbars-hidden');
            } else {
                removeElementClass(document.body, 'safari-toolbars-hidden');
            }
        }
    }, passiveIfSupported);

    document.querySelector('.search-glass').addEventListener('click', e => {
        const menuLinks = document.querySelectorAll(".flexDiv");  

        for (i = 0; i < menuLinks.length-1; i++) {
            if (menuLinks[i].style.display == 'flex' || menuLinks[i].style.display == "") {
                menuLinks[i].style.display = 'none';
            }
            else if (menuLinks[i].style.display == 'none') {
                menuLinks[i].style.display = 'flex';
            } 
        }

        const topNavSearch = document.querySelector('.top-nav-search');
        const topNavSearchIcon = document.querySelector('.uf-search-display');
        if (topNavSearch.style.display == 'none' || topNavSearch.style.display == "") {
            topNavSearch.style.display = 'block';
            topNavSearchIcon.style.position = "fixed";
            topNavSearchIcon.style.right = "0vw";
        } 
        else if (!topNavSearch.style.display == 'none' || !topNavSearch.style.display == "") {
            topNavSearch.style.display = 'none';
            topNavSearchIcon.style.position = "";
            topNavSearchIcon.style.right = "";
        }
        
        const inputSearch = document.querySelector(".site-search-input");
        inputSearch.focus();
    })

    document.querySelector('.search-glass').addEventListener('keydown', e => {
        if (event.keyCode == 13) {
            event.preventDefault();         
            const menuLinks = document.querySelectorAll(".flexDiv");  

            for (i = 0; i < menuLinks.length-1; i++) {
                if (menuLinks[i].style.display == 'flex' || menuLinks[i].style.display == "") {
                    menuLinks[i].style.display = 'none';
                }
                else if (menuLinks[i].style.display == 'none') {
                    menuLinks[i].style.display = 'flex';
                } 
            }

            const topNavSearch = document.querySelector('.top-nav-search');
            if (topNavSearch.style.display == 'none' || topNavSearch.style.display == "") {
                topNavSearch.style.display = 'block';
            } 
            else if (!topNavSearch.style.display == 'none' || !topNavSearch.style.display == "") {
                topNavSearch.style.display = 'none';
            }
            
            const inputSearch = document.querySelector(".site-search-input");
            inputSearch.focus();
        }
    })

