      <script>
        const galleryImages = document.querySelectorAll(".gallery__img");
        let imageSource;
        let currentImageSource;
        let firstGalleryImg;
        let lastGalleryImg;
        let i;
        
        galleryImages.forEach((img, i) => {
            i = galleryImages.length-1;
            img.addEventListener("click", (e) => {
                firstGalleryImg = galleryImages[0].src;
                firstGalleryImg = firstGalleryImg.replace('_t','');
                lastGalleryImg = galleryImages[i].src;
                lastGalleryImg = lastGalleryImg.replace('_t','');
                imageSource = e.target.src;
                currentImageSource = imageSource;
                imageSource = imageSource.replace('_t','');
                imageModal(imageSource);
            });
        });

        let imageModal = (src) => {
            let backImg;

            const backBtn = document.createElement("img");
            backBtn.setAttribute("class", "backGalleryButton");
            backBtn.setAttribute("src", "./img/chevron-left-wt.svg");

            let forwardBtnClass;
            let galleryCount = 0;
            let forwardImg;
            let forwardBtn = document.createElement("img");
            forwardBtn.setAttribute("class", "forwardGalleryButton");
            forwardBtn.setAttribute("src", "./img/chevron-right-wt.svg");

            const modal = document.createElement("div");
            modal.setAttribute("class", "modal"); 
            document.querySelector(".campusGallerySection").append(modal);
            const newImage = document.createElement("img");
            newImage.setAttribute("src", src);

            const closeBtn = document.createElement("img");
            closeBtn.setAttribute("class", "modalClose");
            closeBtn.setAttribute("src", "./img/x-circle.svg");

            if (src == lastGalleryImg) {
                forwardBtn.className += ' hideG';
            }
            if (src == firstGalleryImg) {
                backBtn.className += ' hideG';
            }
            
            closeBtn.onclick = () => {
                modal.remove();
            };
            
            forwardBtn.onclick = () => {
              backBtn.classList.remove("hideG");
              galleryImages.forEach(function(child, index) {
                if (child.src == currentImageSource) {
                  if ((index + 2) + (galleryCount) == galleryImages.length) {
                    forwardBtnClass = document.querySelector('.forwardGalleryButton');
                    forwardBtnClass.className += ' hideG';
                  }
                  if (galleryCount + index + 2 > galleryImages.length) {
                    galleryCount = galleryCount - 1;
                  }
                  if (galleryCount < galleryImages.length - 1) {
                    galleryCount = galleryCount + 1;
                  }
                  forwardImg = galleryImages[galleryCount + index].src.replace('_t','');
                  newImage.setAttribute("src", forwardImg);
                }
              });

            };

            backBtn.onclick = () => {
                  forwardBtn.classList.remove("hideG");
                  galleryImages.forEach(function(child, index) {
                    if (child.src == currentImageSource) {
                      if (galleryCount + index < 2) {
                        bacBtnClass = document.querySelector('.backGalleryButton');
                        bacBtnClass.className += ' hideG';
                      }
                      if (galleryCount + index > 0) {
                        galleryCount = galleryCount - 1;
                      }
                      backImg = galleryImages[galleryCount + index].src.replace('_t','');
                      newImage.setAttribute("src", backImg);
                    }
                  });
            };

            modal.append(newImage, closeBtn, forwardBtn, backBtn);
        };

      </script>

