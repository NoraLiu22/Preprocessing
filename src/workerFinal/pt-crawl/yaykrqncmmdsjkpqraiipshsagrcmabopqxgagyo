<script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.17/d3.min.js"></script>
<script type="text/javascript">
    (function defer() {
      if ('d3' in window && window.jQuery && 'tooltip' in jQuery.fn) {
        
          colorArray = ["#F9F2EB", "#F4E5D8", "#EFD9C4", "#EACCB1", "#E5BF9D", "#E0B38A", "#DBA677", "#D69963", "#D18D50", "#CC803C", "#C77329", "#C26716"];
        

        function makeUnique(value, index, self) {
            return self.indexOf(value) === index;
        }

        const counts = [50,48,151,203,354,395,787,844,724,1010,1070,1056,1401,1472,1546,1651,1625,1644,1645,1673,1520,1624,1713,1807,1813,2068,2076,2111,2227,2082,1847,1955,1840,1943,1954,2289,2310,2221,2162,2310,2739,3022,2995,2861,3117,3115,3055,2970,3033,3095,3109,3088,3020,3028,3262,3333,3263,3144,2869,3271,2285,2463,3711,8219,9715,10261,9872,4234,4004,3869,3785,3368,3605,3775,3719,3283,3548,3492,2948,2552,2637,3356,4360,4953,4407,5124,5536,5083,4754,4402,4460,5167,5230,4895,4823,5005,4951,6844,6887,7525,9010,11766,10771,9871,9586,9647,11389,12113,11484,11772,12715,13533,13464,13780,12189,10559,10434,11119,9873,8026,5584,5269,5199,3717,2834,2829,2558,3094,3935,3836,1507,455,414,129,215,116,100,83,94,107,108,83,76,57,43,42,46,44,2,28,8,9,10,3,1,2,1,1,1,2,1,1,1];
        //counts = counts.filter( makeUnique ).sort(function(a, b){return a - b});

        const range_max = d3.max(counts);
        const range_min = d3.min(counts);
        // If all values are the same, set the range to 0-max or else no years will get shaded.

        let range;
	      if (range_min === range_max){
          range = [0, range_max]
	      } else {
          range = [range_min, range_max]
        }

        const color = d3.scale.quantize()
                              .domain(range)
                              .range(colorArray);

        const fontsize = d3.scale.quantize()
                                 .domain([d3.min(counts), d3.max(counts)])
                                 .range([14,15,16,17,18,19,20,22,23,24,25,26,28,29,30,31,32,33]);

        const dataTable = d3.select("#year-buttons");

        dataTable.selectAll('a.count:not(.disabled)')
                 .data(counts)
                 .style('background-color', function(d) { return color(d)})
                 .style('font-size', function(d) { return fontsize(d) + 'px'});

        // Set the empty dates to have the smallest font
        dataTable.selectAll('a.count.disabled')
                 .style('font-size', '14px');


        } else {
          setTimeout(function() { defer(); }, 50);
        }
    }());
</script>  


