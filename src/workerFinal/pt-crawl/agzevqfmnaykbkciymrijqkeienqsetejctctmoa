<source data-srcset="https://cdn.prod.www.spiegel.de/images/1ce328fc-0001-0004-0000-000000595097_w488_r1.778_fpx30.37_fpy49.98.webp 488w, https://cdn.prod.www.spiegel.de/images/1ce328fc-0001-0004-0000-000000595097_w288_r1.778_fpx30.37_fpy49.98.webp 288w" data-sizes="(max-width: 487px) 100vw, (min-width: 488px) and (max-width: 719px) 488px, (min-width: 720px) and (max-width: 1019px) 488px, (min-width: 1020px) 288px" type="image/webp">
