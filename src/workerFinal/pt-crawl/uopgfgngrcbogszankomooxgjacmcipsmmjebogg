            function handleBtnMore() {
                var value = $("input[name='PageNumber']").val();
                value++;
                $("input[name='PageNumber']").val(value);
            }

            function OnSuccess(data, enableMasonry) {
                if (data.length === 0) {
                    $(".btn-more-container").hide();
                } else {
                    if (enableMasonry === 'True') {
                        if (window.readyState === "loading") {
                            // Calls the function during the 'DOMContentLoaded' event, after the HTML document has been completely loaded
                            window.addEventListener("DOMContentLoaded",
                                function () {
                                    setTimeout(function () { RefreshMasonry(document.querySelector('.list-news .grid')); }, 500);
                                });
                        } else {
                            // Calls the function directly in cases where the widget is rendered dynamically after 'DOMContentLoaded' has occurred
                            setTimeout(function () { RefreshMasonry(document.querySelector('.list-news .grid')); }, 500);
                        }
                    }
                }
            }
        </script>

