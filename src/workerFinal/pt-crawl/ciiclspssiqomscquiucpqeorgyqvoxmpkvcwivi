<script>
    var options = {
        valueNames: ['title', 'snippet', 'content', 'date', 'year', { name: 'timestamp', attr: 'data-timestamp' }],
        page: 6,
        pagination: true
    };

    var mediaList = new List('media-list', options);

    var currentSearch = "fullarticle";

    $('#dateselector').change(function () {
        var selection = this.value;
        _gaq.push(['_trackEvent', 'Joint PO Statements', 'Date selector', selection]);
        if (selection !== "*") {
            mediaList.filter(function (item) {
                return (item.values().year == selection);
            });
        } else {
            mediaList.filter();
        }
    });

    $('#searchby').change(function () {
        var selection = this.value;
        currentSearch = this.value;
        _gaq.push(['_trackEvent', 'Joint PO Statements', 'Search by', selection]);
        if (selection == "title") {
            mediaList.search($('#keywords').val(), ['title']);
        } else {
            mediaList.filter();
            mediaList.search($('#keywords').val());
        }
    });

    $('#sortby').change(function () {
        var selection = this.value;
        _gaq.push(['_trackEvent', 'Joint PO Statements', 'Sort by', selection]);
        if (selection == "mostrecent") {
            mediaList.sort('timestamp', { order: "desc" });
        }
        else if (selection == "recentlast") {
            mediaList.sort('timestamp', { order: "asc" });
        }
        else if (selection == "titleaz") {
            mediaList.sort('title', { order: "asc" });
        }
        else if (selection == "titleza") {
            mediaList.sort('title', { order: "desc" });
        }
    });

    $('#keywords').keyup(function () {
        var searchBy = $('#searchby').val();
        _gaq.push(['_trackEvent', 'Joint PO Statements', 'Keywords', searchBy]);
        if (searchBy == "title") {
            mediaList.search($('#keywords').val(), ['title']);
        } else {
            mediaList.filter();
            mediaList.search($('#keywords').val());
        }
    });

    mediaList.on('searchComplete', function (e) {
        if (e.matchingItems.length == 0) {
            _gaq.push(['_trackEvent', 'Joint PO Statements', 'No results', $('#searchby').val()]);
            $('.not-found').show();
        } else {
            $('.not-found').hide();
        }
    });
</script>
