<script type="text/javascript">
  const nlm_caseDefelctionResultsobserver = new MutationObserver(function (mutations_list) {
    mutations_list.forEach(function (mutation) {
      mutation.addedNodes.forEach(function (added_node) {
        
        if (added_node.className == 'case-deflection-loading pull-left' || added_node.className == 'panel panel-default content-panel') {


          let className = added_node.className;
          let addedNode = added_node;
          if (className == 'panel panel-default content-panel') {
            className = "results pull-left"
            addedNode = document.getElementById("EntityFormPanel").querySelectorAll('.results').item(0);
          }


          //if(added_node.id == 'case-deflection-topics') {
          //debugger;
          if (className != null) {
            const text = document.getElementById("deflection-results").querySelectorAll('.' + className.split(' '[0]))
            //debugger;
            for (const el of text) {
              document.getElementById("deflection-results").removeChild(el);
            }
            document.getElementById("deflection-results").append(addedNode);
          }

          //observer.disconnect();
        }

        if(added_node.className == "results pull-left")
        {
          //Wait 500 ms and then check if empty, if so then clear and remove
          setTimeout((node) => {
            if(node.childNodes.length == 1)
            {
              document.getElementById("deflection-results").replaceChildren();
              node.remove();
            }
          }, 2000, added_node);

          
        }

      });
    });
  });

  nlm_caseDefelctionResultsobserver.observe(document.querySelector("#EntityFormPanel"), { subtree: true, childList: true });
</script>


