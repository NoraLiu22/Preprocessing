     data-tooltips="{&quot;tooltips&quot;:[],&quot;dash_seen&quot;:null}">
    
    <div id="name-section" >
        <h2 class="trackTitle">
            Avalanche
            
        </h2>

        <h3 style="margin:0px;">by 
        <span> 
          <a href="https://jennyowenyoungs.bandcamp.com">Jenny Owen Youngs</a>
          </span>
        
        </h3>

    

    
    </div>
    <div class="middleColumn">
        
        

        <div id="tralbumArt">
        
            <a class="popupImage" href="https://f4.bcbits.com/img/a3922739091_10.jpg">
                <img src="https://f4.bcbits.com/img/a3922739091_16.jpg">
            </a>
        
        </div>

        
