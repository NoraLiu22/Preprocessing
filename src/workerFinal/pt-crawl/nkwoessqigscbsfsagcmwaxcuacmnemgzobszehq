            //var datetime = currentdate.getDate() + "/"
              //          + (currentdate.getMonth()+1)  + "/" 
             //        + currentdate.getFullYear() + " @ "  
             //           + currentdate.getHours() + ":"  
             //           + currentdate.getMinutes() + ":" 
             //           + currentdate.getSeconds();
            var d = new Date();
            var days = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];
            var months = ["January","February","March","April","May","June","July","August","September","October","November","December",];
            document.getElementById("current-time").innerHTML = days[d.getDay()]+", "+months[d.getMonth()]+" "+d.getDate()+", "+d.getFullYear();
        });
        $(function(){
        if ( $('#site-top-nav .website-login .list-inline > li > a.login-btn.user-control-link').length ) {
              $('#site-top-nav .website-login .list-inline > li > a.login-btn.user-control-link').html($('#site-top-nav .website-login .list-inline > li > a.login-btn.user-control-link').html().replace('Log In','Website Log In'));
        }
        if ( $('.website-login-mobile .not-logged-in .list-inline > li > a.login-btn.user-control-link').length ) {
              $('.website-login-mobile .not-logged-in .list-inline > li > a.login-btn.user-control-link').html($('.website-login-mobile .not-logged-in .list-inline > li > a.login-btn.user-control-link').html().replace('Log In','Website Log In'));
        }
      });
        
    </script>
    

