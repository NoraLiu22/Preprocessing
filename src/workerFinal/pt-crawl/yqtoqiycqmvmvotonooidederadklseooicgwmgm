            <div data-field="field-text-wysiwyg"><script>
const contentLoadedCallback = function () {
  const body = document.querySelector('body');
  const lang = document.querySelector('html').getAttribute('lang');
  body.setAttribute('data-event-category', `HRW homepage click, ${lang}`);

  const addEventAction = function (wrapperSelector, actionName, elementSelector = 'a, button', setNameFromElementText = false) {
    document.querySelectorAll(wrapperSelector).forEach((wrapper) => {
      wrapper.querySelectorAll(elementSelector).forEach((element) => {
        element.setAttribute('data-event-action', actionName);
        if (setNameFromElementText) {
          element.setAttribute('data-event-name', element.innerText);
        }
      });
    });
  };

  addEventAction('#block-donationcalloutbanner', 'Donate, top image banner');
  addEventAction('#block-donationcalloutheaderbar', 'Donate, top text banner');
  addEventAction('.header-bar__cta', 'Donate, navbar button');
  addEventAction('.translations-bar', 'Translation bar', 'a', true);
  addEventAction('.search-input', 'Search input', 'input');
  addEventAction('.billboard', 'Billboard');
  addEventAction('.billboard__related', 'Billboard related link');
  addEventAction('.content-cards-block', 'Content cards');
  addEventAction('.views-element-container .media-list', 'Latest news');
  addEventAction('.views-element-container .block-header', 'See more (news) link');
  addEventAction('.container-large .donation__main', 'Donate, below latest news', 'a, button', true);

  addEventAction('.video-playlist .block-header', 'Watch more (videos) link');
  addEventAction('.video-playlist .tabs-vertical__main', 'Video play');
  addEventAction('.video-playlist .tabs-vertical__main', 'Video text description', 'summary');
  addEventAction('.video-playlist .tabs-vertical__list', 'Video select');
  addEventAction('.mb-8 > .media-list .block-header', 'More reports link');
  addEventAction('.mb-8 > .media-list .media-list__content', 'Reports');
  addEventAction('.most-viewed', 'Most viewed');
  addEventAction('.news-block', 'News by Region/Topic');
  addEventAction('.news-block__tabslist', 'Region/Topic tabs', 'li', true);
  addEventAction('#block-donationfooterblock', 'Donate, footer block');
  addEventAction('.footer__signup', 'Footer newsletter signup');
  addEventAction('.footer__social', 'Footer social links', 'a', true);
  addEventAction('.footer-nav', 'Footer navigation', 'a', true);


  // Special handling for promo blocks
  document.querySelectorAll('.promo').forEach((promo) => {
    let title = 'Promo box: ' + promo.querySelector('h3').innerText;
    promo.querySelectorAll('a, button').forEach((element) => {
      element.setAttribute('data-event-action', title);
    });
  });

  // Vue header menus are not quite ready at document ready
  window.setTimeout(function () {
    addEventAction('.trending__list', 'Trending', 'a', true);
    addEventAction('.nav-desktop__primary', 'Main navigation', 'a, button', true);
    addEventAction('.header-bar', 'Mobile navigation toggle', '.header-bar__toggle[aria-expanded="false"]');
    addEventAction('.nav-mobile__list', 'Mobile navigation', 'a, button', true);
  }, 1000);
};

if (
  document.readyState === "complete" ||
  (document.readyState !== "loading" && !document.documentElement.doScroll)
) {
  contentLoadedCallback();
} else {
  document.addEventListener("DOMContentLoaded", contentLoadedCallback);
}
</script></div>
      
