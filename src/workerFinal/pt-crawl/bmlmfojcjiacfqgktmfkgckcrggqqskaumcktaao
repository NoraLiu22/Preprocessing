<script language=javascript>

function toggle_box(s,h){
if (h) {
		ById(s+'_source').style.display='none';
		ById(s+'_img_toggle').src='//img.tfd.com/toggle/b-down.gif';
	}
	else {
		ById(s+'_source').style.display='';
		ById(s+'_img_toggle').src='//img.tfd.com/toggle/b-up.gif';
	}
}
function box_render() {
	var code = '<!--dictionary lookup box by TheFreeDictionary.com-->\n' +
		'<style>#dictionarybox TD, INPUT, SELECT {font-size:~3;}</style>\n' +
		'<form action="https://www.thefreedictionary.com/_/partner.aspx" method="get" target="_blank" name="dictionary"\n' +
		'style="display:inline;margin:0">\n' +
		'<table id="dictionarybox" cellspacing="0" cellpadding="3" style="border:1px ~C solid;font-family:~2;width:~6;background-color:~8;color:~9"><tr>\n' +
		'<td bgcolor="~A" style="border-bottom:1px ~C solid"><img src="https://img.tfd.com/Help.gif" width="25" height="25"/></td>\n' +
		'<td bgcolor="~A" style="border-bottom:1px ~C solid" colspan="2" nowrap="yes"><a\n' +
		'style="text-decoration:none;color:~B" href="https://www.thefreedictionary.com" rel="nofollow"><div style="font-size:~5"><b>Online Reference</b></div>\n' +
		'<div style="font-size:~4">Dictionary, Encyclopedia &amp; more</div></a></td></tr>\n' +
		'<tr><td align="center">Word:<br/>\n' +
		'<img id=dictionarybox_img_toggle src="https://img.tfd.com/toggle/b-~1.gif" style="cursor:hand;cursor:pointer"\n' +
		'onclick="var s=document.getElementById(\'dictionarybox_source\').style;this.src=\'//img.tfd.com/toggle/b-\'+(s.display?\'up.gif\':\'down.gif\');s.display=(s.display?\'\':\'none\')"/></td>\n' +
		'<td colspan="2"><input name="Word" value="" size="26"/></td></tr>\n' +
		'<tr id="dictionarybox_source"~S><td align=right>Look in:</td><td colspan=2 style="font-size:~4;text-align:left" id="boxsource_td"><style>#boxsource_td A {color:~9;text-decoration:none}</style>\n' +
		'<!--[if IE]><style>#boxsource_td INPUT {height:12pt}</style><![endif]-->\n' +
		'<input type="radio" name="Set" value="www" checked="yes"/>Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="freethesaurus"/>Thesaurus<br/>\n' +
		'<input type="radio" name="Set" value="medical-dictionary"/>Medical Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="legal-dictionary"/>Legal Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="financial-dictionary"/>Financial Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="acronyms"/>Acronyms<br/>\n' +
		'<input type="radio" name="Set" value="idioms"/>Idioms<br/>\n' +
		'<input type="radio" name="Set" value="encyclopedia2"/>Encyclopedia<br/>\n' +
		'<input type="radio" name="Set" value="encyclopedia"/>Wikipedia<br/>\n' +
		'<input type="radio" name="Set" value="periodicals"/>Periodicals<br/>\n' +
		'<input type="radio" name="Set" value="literature"/>Literature\n' +
		'<div style="cursor:hand;cursor:pointer"\nonclick="var s=document.getElementById(\'dictionarybox_ol\').style;document.getElementById(\'dictionarybox_ol_img\').src=\'//img.tfd.com/toggle/b-\'+(s.display?\'up.gif\':\'down.gif\');s.display=(s.display?\'\':\'none\')"><img \n' +
		'id=dictionarybox_ol_img src="https://img.tfd.com/toggle/b-~oli.gif"/>Other languages:</div><div id="dictionarybox_ol"~ol>\n' +
		'<input type="radio" name="Set" value="es"/>Spanish Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="de"/>German Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="fr"/>French Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="it"/>Italian Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="zh"/>Chinese Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="pt"/>Portuguese Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="nl"/>Dutch Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="no"/>Norwegian Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="el"/>Greek Dictionary<br/>\n' +
		'<input type="radio" name="Set" value="ru"/>Russian Dictionary</div>\n' +
		'</td></tr>\n' +
		'<tr><td align="right">by:</td>\n' +
		'<td><select name="mode"><option value="w">Word</option><option value="s">Starts with</option>\n<option value="e">Ends with</option><option value="d">Mentions</option></select></td>\n' +
		'<td align=right><input type="submit" name="submit" value="Look it up"/></td></tr></table></form>\n' +
		'<!--end of dictionary lookup box-->';


	var box_font='Arial';
	var box_font_size1='10pt';
	var box_font_size2='8pt';
	var box_font_size3='12pt';
	var box_width='230px';
	var f=document.customize_box;

	if (f.box_type[2].checked) {
		toggle_box('dictionarybox',1);
		toggle_box('OtherLanguages',1);
		code=code.replace('~1', 'down');
		code=code.replace('~S', ' style="display:none"');
		code=code.replace('~oli', 'down');
		code=code.replace('~ol', ' style="display:none"');
	}
	else if (f.box_type[1].checked){
		toggle_box('dictionarybox',0);
		toggle_box('OtherLanguages',1);
		code=code.replace('~1', 'up');
		code=code.replace('~S', '');
		code=code.replace('~oli', 'down');
		code=code.replace('~ol', ' style="display:none"');
	}
	else {
		toggle_box('dictionarybox',0);
		toggle_box('OtherLanguages',0);
		code=code.replace('~1', 'up');
		code=code.replace('~S', '');
		code=code.replace('~oli', 'up');
		code=code.replace('~ol', '');
	}

	if (f.box_open[0].checked) {
		code=code.replace(/_blank/g, '_top');
	}

	if (f.fsize[0].checked) {
		box_font_size1='8pt';box_font_size2='7pt';box_font_size3='10pt';box_width='195px';
	}
	else if (f.fsize[2].checked) {
		box_font_size1='12pt';box_font_size2='10pt';box_font_size3='14pt';box_width='265px'
	}
	else if (f.fsize[3].checked) {
		box_font_size1='';box_font_size2='smaller';box_font_size3='larger';box_width='230px';
	}

	box_font=f.sFont.options[f.sFont.selectedIndex].value;
	code=code.replace(/~2/g, box_font);
	Style('boxtable').fontFamily=box_font;

	code=code.replace(/~3/g, box_font_size1);
	code=code.replace(/~4/g, box_font_size2);
	code=code.replace(/~5/g, box_font_size3);
	var box_tags1=['box_td1','box_td2','box_td3','box_word','box_mode','box_submit'];
	for (var i in box_tags1)
		ById(box_tags1[i]).style.fontSize=box_font_size1;
	Style('boxtitle').fontSize=box_font_size3;
	Style('boxtitle2').fontSize=box_font_size2;
	Style('boxsource_td').fontSize=box_font_size2;
	code=code.replace(/~6/, box_width);
	Style('boxtable').width=box_width;

	code=code.replace(/~7/, box_width);
	Style('boxtable').width=box_width;

	code=code.replace(/~8/g, f.Background.value);
	Style('boxtable').backgroundColor = f.Background.value;

	code=code.replace(/~9/g, f.TextColor.value);
	Style('boxtable').color=f.TextColor.value;
	//for (var i=1;i<=20;i++)
	//	Style('ba'+i).color=f.TextColor.value;

	code=code.replace(/~A/g, f.TitleBackground.value);
	Style('btdt1').backgroundColor = f.TitleBackground.value;
	Style('btdt2').backgroundColor = f.TitleBackground.value;

	code=code.replace(/~B/g, f.TitleTextColor.value);
	Style('bta').color=f.TitleTextColor.value;

	code=code.replace(/~C/g, f.BorderColor.value);
	Style('boxtable').border='1px '+f.BorderColor.value+' solid';
	Style('btdt1').borderBottom='1px '+f.BorderColor.value+' solid';
	Style('btdt2').borderBottom='1px '+f.BorderColor.value+' solid';


	f.code.value=code;
}
box_render();
</script>

	<!-- Dictionary Lookup End -->

