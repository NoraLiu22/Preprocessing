				<div id=tfd_hm_a>
				<a  id=tfd_hm_a1 href="#" onclick="ById('frmHM').src='//'+window.location.hostname+'/_/WoD/hangmanjs.aspx?f=2&r='+Math.random();return false">New&nbsp;Game</a>
				<a  id=tfd_hm_a2 href="#" onclick="javascript:top.location='/'+encodeURIComponent(tfd_hm_word.toLowerCase());return false">Learn&nbsp;the&nbsp;word</a>
				<iframe id="frmHM" src="" style="width:1px;height:1px;position:absolute" frameborder=0></iframe>
				</div>
				<script type="text/javascript">
				var tfd_hm_sh_key = '';
				function tfd_gshare(div, key, mg) {
				    if (!document.getElementById(div) || !key || key.length <= 3) return;
				    var u = escape('http:/'+'/www.thefreedictionary.com/_/sh.aspx?t=' + key);
				    var fb = '<a target="_blank" href="http:/' + '/www.facebook.com/share.php?u=' + u + '" title="Share on Facebook"><span style="width:16px;height:16px;display:block;background-image:url(//img.tfd.com/m/share.png)"></span></a>';
				    var tw = '<a href="https:/' + '/twitter.com/share?url=' + u + '&text=' + escape(mg) + '" target="_blank"><span style="width:16px;height:16px;display:block;background-image:url(//img.tfd.com/m/share.png);background-position:-16px 0px"></span></a>'
				    document.getElementById(div).innerHTML = '<span style="border:dashed 1px orange;margin-right:5px;padding:3px;margin:auto;display:inline-block">' + fb + tw + '</span>';
				}

				var tfd_hm_word='';
				var tfd_hm_guess='';
				function tfd_hm_update(){ 
				var frm=document.tfd_hm_form;
				if (frm.input.disabled) return; 
				var c='';
				for (var i=0;i<frm.input.value.length;i++) {
					c=frm.input.value.substr(i,1).toUpperCase();
					if (tfd_hm_guess.indexOf(c)<0) {
						tfd_hm_guess+=c;
						if (tfd_hm_word.indexOf(c)<0) frm.wrong.value+=c
						break;
					}
				}
				frm.input.value=c;
				for (var i=0;i<frm.wrong.value.length;i++)
					ById('tfd_hm_td'+i).style.backgroundColor='transparent';
				frm.word.value='';
				var gamelost=ById('tfd_hm_td9').style.backgroundColor=='transparent';
				for (var i=0;i<tfd_hm_word.length;i++)
				frm.word.value+=(tfd_hm_guess.indexOf(tfd_hm_word.substr(i,1))>=0 || gamelost?tfd_hm_word.substr(i,1):'_');
				if (frm.word.value.indexOf('_')<0) {
					if (gamelost){
						ById('tfd_hm_span').style.backgroundPosition='0 -220px';
						document.getElementById('tfd_hm_result').innerHTML='Sorry, you lost.';
						tfd_gshare('hmShare', 'h' + tfd_hm_sh_key + '0', "I couldn't figure out this word in Hangman. Can you?");
					}
					else {
						ById('tfd_hm_span').style.backgroundPosition='0 0';
						document.getElementById('tfd_hm_result').innerHTML='You won!';
						for (var i=0;i<=9;i++)
							ById('tfd_hm_td'+i).style.backgroundColor='transparent';
						var l = frm.wrong.value.length; for (var i = 0, x = ''; i < tfd_hm_word.length; i++) { if (x.indexOf(tfd_hm_word.charAt(i)) < 0) x += tfd_hm_word.charAt(i) }
						tfd_gshare('hmShare', 'h' + tfd_hm_sh_key + String.fromCharCode(65 + l), "I beat Hangman in " + (l + x.length) + " guesses! Can you beat me?");
                    }
					frm.input.disabled=true;
					ById('tfd_hm_a').style.visibility='';
				}}
				var tfd_hm_cnt=0;
				function tfd_hm_init(c){
				    if (!c && document.tfd_hm_form.input.disabled) return; 
				ById('tfd_hm_span').style.backgroundPosition='0 -110px';
				if (c || tfd_hm_cnt==1)
					for (var i=0;i<=9;i++)
					ById('tfd_hm_td'+i).style.backgroundColor=(Style('t_hm').backgroundColor?Style('t_hm').backgroundColor:'white');
				tfd_hm_cnt+=1;
				}
				function tfd_hm_refresh() {
				if (ById('hmShare')) ById('hmShare').innerHTML = '';
				tfd_hm_guess='';
				ById('tfd_hm_a').style.visibility='hidden';
				if (tfd_hm_cnt) tfd_hm_init(1);
				tfd_hm_cnt+=1;
				var frm=document.tfd_hm_form;
				frm.input.disabled=false;
				document.getElementById('tfd_hm_result').innerHTML='';
				frm.input.value='';
				frm.wrong.value='';
				tfd_hm_guess='';
				tfd_hm_update();
				}
				if (document.cookie.indexOf('h=1')>=0)
				document.write('<scr'+'ipt type="text/javascript" src="/_/WoD/hangmanjs.aspx?f=1&shr=1&r='+Math.random()+'"></scr'+'ipt>');
				else { 
				document.cookie='h=1';
				
				tfd_hm_word='distill';
				tfd_hm_sh_key = 'czgKVzghCqPPk1Im';
				window.setTimeout('tfd_hm_refresh()',100);
				}
				</script></form>
		</div>
	<div id=a_hm><a id=al_15 href="https://www.thefreedictionary.com/lookup.htm">Hangman</a> provided by <a id=al_16 href="https://www.thefreedictionary.com/">TheFreeDictionary.com</a></div>
	</div>
