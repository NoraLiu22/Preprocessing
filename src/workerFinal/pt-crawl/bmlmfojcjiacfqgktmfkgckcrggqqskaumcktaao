<script type="text/javascript">
function img_render() {
	var content=['wod','quote','aod','history','birthday','holiday'];
	var contName=['Word of the Day','Quote of the Day','Article of the Day','This Day in History','Today\'s Birthday','Today\'s Holiday'];
	var f=document.form_img;
	var code='';

	for (var i in content) {
		var e=content[i];
		var width=Number(f.sWidth.value);
		var radio=eval('f.img_c_'+e);
		var format=0;
		for (j=0;j<radio.length;j++)
				if (radio[j].checked) format=radio[j].value;
		if (format!='null') {
			//code+='<!--'+contName[i]+' by TheFreeDictionary.com-->\n';
			code+='<div style="font-weight:bold;font-size:larger;margin-bottom:2px">'+contName[i]+'</div>\n';
			if (!f.hideborder.checked && f.BorderColor.value) code+='<div style="border:1px '+f.BorderColor.value+' solid;padding:3px;width:'+(width+6)+'px">\n';
			code+='<a href="https://www.thefreedictionary.com/_/WoD/png-click.aspx?t='+e+'" target="_'+(f.img_open1.checked?'top':'blank')+'">';
			code+='<img border="0" src="https://www.thefreedictionary.com/_/!'+e+'!'+width+'!'+format+'.png" width="'+width+'" /></a>\n';
			if (!f.hideborder.checked && f.BorderColor.value) code+='</div>\n';
			code+='<div style="font-size:smaller"><a href="https://www.thefreedictionary.com/lookup.htm" rel="nofollow">'+contName[i]+'</a>\n'+
			'provided by <a href="https://www.thefreedictionary.com/" rel="nofollow">TheFreeDictionary.com</a></div><br clear="all"/>\n';
			code+='\n\n'//'<!-- End of '+contName[i]+'-->\n\n';
		}
	}
	f.code.value=code;
	ById('imgPreview').innerHTML='<span style="font-family:Arial, Helvetica, sans-serif"><span class="titl">Preview:</span> <a href="javascript:void(0)" class="info-tip tooltip-right" data-tooltip="Here is a live example of the content you will get by using the code provided">?</a></span><div style="height: 700px; overflow-y: scroll; margin-top: 5px; padding: 5px; border: 1px solid rgb(210, 210, 210);">'+code +'</div>';

}
img_render();
</script>

