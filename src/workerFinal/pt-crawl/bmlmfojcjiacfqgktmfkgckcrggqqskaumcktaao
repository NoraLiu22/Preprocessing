	var wm_code = '<!--WordHub by TheFreeDictionary.com-->\n'+
		'<div style="width: ~w; position: relative; background-color: ~2; padding: 4px">\n'+
		'<div style="font: bold ~b ~f; color: ~t">WordHub</div>\n'+
		'<style type="text/css">\n'+
		'#t_wm {width: 100%;border: 1px ~1 solid;background-color: ~2;font: normal ~n ~f;color: ~c}\n'+
		'#t_wm H3 {margin-top: 0px;font: bold ~b ~f;color: ~c}\n'+
		'#t_wm A { color: ~3 }\n'+
		'</style>\n'+
		'<div id="t_wm"> Please wait...</div>\n'+
		'<div style="font: normal ~s ~f; color: ~c">\n'+
		'<a style="color: ~al" href="https://wordhub.com" rel="nofollow">WordHub</a> provided by <a style="color: ~al" href="https://www.thefreedictionary.com/" rel="nofollow">TheFreeDictionary.com</a>\n'+
		'</div>\n'+
		'</div>\n'+
		'<scr'+'ipt type="text/javascript" async src="https://img.tfd.com/daily/wordmaker-top.js" charset="UTF-8"></scr'+'ipt>\n'+
		'<!--end of WordHub-->\n';

	var content=['wod','aod','gr','iod','dh','tb','th','q','wt','gq','bee','ms','hm','wm','mm'];
	var f=document.form_iod;
	var code='';
	var font_size1='10pt';
	var font_size2='8pt';
	var font_size3='12pt';
	var d_width=f.sWidth.value;

	if (f.fsize[0].checked) {
		font_size1='8pt';font_size2='7pt';font_size3='10pt';//width='195px';
	}
	else if (f.fsize[2].checked) {
		font_size1='12pt';font_size2='10pt';font_size3='14pt';//width='265px'
	}
	else if (f.fsize[3].checked) {
		font_size1='';font_size2='smaller';font_size3='larger';//width='230px';
	};

	for (var i in content) {
		var e=content[i];
		if (eval('f.'+e).checked) {
			Style('div_'+e).display='';
			if (code) code += '\n<br/>\n';
			code += eval(e+'_code');
			if (f.hideborder.checked)
				Style('t_'+e).border='none';
			else
				Style('t_'+e).border='1px '+f.BorderColor.value+' solid';
			Style('t_'+e).backgroundColor=f.Background.value;
			Style('a_'+e).fontSize=font_size2;
			Style('tl_'+e).fontSize=font_size3;
			if (ById('h3_'+e)) {
				Style('h3_'+e).fontSize=font_size3;
				Style('h3_'+e).color=f.TextColor.value;
			};
			Style('a_'+e).color=f.TextColor.value;
			Style('tl_'+e).color=f.TitleColor.value;
		}
		else
			Style('div_'+e).display='none';
	};
	Style('tfd_hm_input').backgroundColor=f.Background.value;
	var d_font=f.sFont.options[f.sFont.selectedIndex].value;
	code=code.replace(/~f/g, d_font);
	Style('dPreview').fontFamily=d_font;
	Style('dPreview').width=(d_width=='100%' || d_width==''?'300px':d_width);

	Style('t_hm').height=(d_width>='350px'?'120px':'240px');

	var hm_text=['word','wrong','input','result'];
	for (var i in hm_text)
		Style('tfd_hm_'+hm_text[i]).color=f.TextColor.value;
	Style('t_hm').color=f.TextColor.value;
	Style('t_hm').fontSize=font_size1;
	Style('tfd_hm_a1').color=f.LinkColor.value;
	Style('tfd_hm_a2').color=f.LinkColor.value;
	Style('tfd_hm_input').border='1px '+f.TextColor.value+' solid';
	Style('t_bee').color=f.TextColor.value;
	Style('t_bee').fontSize=font_size1;
	Style('t_wm').color=f.TextColor.value;
	Style('t_wm').fontSize=font_size1;


	code=code.replace(/~x2/g, f.Background.value.replace('#','x'));
	code=code.replace(/~xc/g, f.TextColor.value.replace('#','x'));
	code=code.replace(/~x3/g, f.LinkColor.value.replace('#','x'));

	code=code.replace(/~hmh/g, Style('t_hm').height);

	code=code.replace(/~s/g, font_size2);
	code=code.replace(/~n/g, font_size1);
	code=code.replace(/~b/g, font_size3);
	for (i=1;i<=40;i++) {
		if (!ById('td_i'+i)) break;
		Style('td_i'+i).fontSize=font_size1;
		Style('td_i'+i).color=f.TextColor.value;
	}
	code=code.replace(/~c/g, f.TextColor.value);
	code=code.replace(/~t/g, f.TitleColor.value);
	code=code.replace(/~2/g, f.Background.value);
	Style('dPreview').backgroundColor=f.Background.value;

	for (i=1;i<=40;i++)
		if (ById('a_'+i))
			Style('a_'+i).color=f.LinkColor.value;
		else
			continue;



	for (i=1;i<=40;i++)
		if (ById('al_'+i))
			Style('al_'+i).color=f.AttLinkColor.value;
		else
			continue;

	code=code.replace(/~3/g, f.LinkColor.value);
	code=code.replace(/~al/g, f.AttLinkColor.value);
	if (f.d_open[1].checked) // new window
		code=code.replace(/target=_top/g, '').replace(/-top.js/g, '.js').replace(/~hmtarget/, ',_blank');
	else
		code=code.replace(/~hmtarget/, '').replace(/target=\"_blank\"/g, "");
		
	code=code.replace(/~w/g, d_width);
	if (f.hideborder.checked)
		code=code.replace(/border:1px ~1 solid/g, 'border:none');
	else
		code=code.replace(/~1/g, f.BorderColor.value);
	f.code.value=code;

};
d_render();
window.setTimeout('box_render();d_render()',100);
</script>

