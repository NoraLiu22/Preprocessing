<SCRIPT type="text/javascript">
function ById(id){return document.getElementById(id)};
function ToggleCodeCust(id) {
	var cust = ById(id + '_customize');
	if (cust.style.display !== 'block') {
		cust.style.display = 'block';
		ById(id + '_code').style.display = 'none';
	} else {
		cust.style.display = 'none';
		ById(id + '_code').style.display = 'block';
	}
}
var tabIds = ['21', '22', '23'];
function hasClass(el,cls){return el&&el.className?el.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)')):!1};
function addClass(el,cls){if(el&&!hasClass(el,cls))el.className+=' '+cls};
function removeClass(el,cls){if(el&&hasClass(el,cls)){el.className=lib.trim(el.className.replace(new RegExp('(\\s|^)'+cls+'(\\s|$)'),' '))}};
function selectTab(id) {
	if (hasClass(ById(id),'psactive')) return;
	for (var i = 0; i < tabIds.length; i++) {
		removeClass(ById('t'+tabIds[i]), 'psactive');
		ById('daily'+tabIds[i]+'_content').style.display = 'none';
	}
	addClass(ById('t'+id), 'psactive');
	ById('daily'+id+'_content').style.display = 'block';
}
function hashChange() {
	var hash = window.location.hash.substr(1);
	if (hash === '21' || hash === '22' || hash === '23') {
		selectTab(hash);
	}
}
if ('onhashchange' in window) {
	if (window.addEventListener) {
		window.addEventListener("hashchange", hashChange, false);
	}
	else if (window.attachEvent) {
		window.attachEvent("onhashchange", hashChange);
	}
}
var PassColor, SetColor;
function ColorPicker(color, callback){
	PassColor=color;
	SetColor=callback;
	top.open('colors.html', 'colorwindow','width=500,height=280');
}
function Style(name) {
	if (ById(name)==null) alert(name);
	return ById(name).style;
}
function inverseColor(c){
	var res='#';
	for (var i=1;i<6;i=i+2)
		res+=inverseColor2(c.substr(i, 2));
	return res;
}
function inverseColor2(c){
	c=0xFF-eval('0x'+c);
	if (c < 0x10)
		c= '0' +c.toString(16);
	else
		c= c.toString(16);
	return c;
}
focus();
</SCRIPT>

