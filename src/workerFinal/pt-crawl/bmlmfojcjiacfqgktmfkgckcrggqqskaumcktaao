<script type="text/javascript">

function toggle_box12(s,h){
if (h) {
		ById(s+'_source12').style.display='none';
	}
	else {
		ById(s+'_source12').style.display='';
	}
}
function box_render12() {
	var code = '<!--Word Finder lookup box by TheFreeDictionary.com-->\n' +
		'<style>#dictionarybox12 TD, INPUT, SELECT {font-size:~3;}</style>\n' +
		'<form action="https://www.thefreedictionary.com/_/partner.aspx" method="get" target="_blank" name="wordfinder"\n' +
		'style="display:inline;margin:0">\n' +
		'<input type="hidden" name="LookupSrc" value="wf">\n'+
		'<table id="dictionarybox12" cellspacing="0" cellpadding="3" style="border:1px ~C solid;font-family:~2;width:~6;background-color:~8;color:~9"><tr>\n' +
		'<td bgcolor="~A" style="border-bottom:1px ~C solid; text-align: center"><img src="https://img.tfd.com/Help.gif" width="25" height="25"/></td>\n' +
		'<td bgcolor="~A" style="border-bottom:1px ~C solid" colspan="2" nowrap="yes"><a\n' +
		'style="text-decoration:none;color:~B" href="https://www.thefreedictionary.com/Word-Finder.htm" rel="nofollow"><div style="font-size:~5"><b>Word Finder</b></div>\n' +
		'<div style="font-size:~4">Scrabble Word Finder and<br/>Words With Friends cheat dictionary</div></a></td></tr>\n' +
		'<tr><td align="center">Word:</td>\n' +
		'<td colspan="2"><input name="Word" value="" size="26"/></td></tr>\n' +
		'<tr id="dictionarybox_source12"~S><td colspan=3 style="font-size:~4;text-align:left" id="boxsource_td12"><style>#boxsource_td12 A {color:~9;text-decoration:none}</style>\n' +
		'<!--[if IE]><style>#boxsource_td12 INPUT {height:12pt}</style><![endif]-->\n' +
		'<table style="margin: 0 auto 0.5em auto; ~ol"><tr>\n' +
		'	<td><input type="radio" name="mode" id="opt112" value="s" checked><label for=opt112>Starts With</label></td>\n' +
		'	<td><input type="radio" name="mode" id="opt113" value="e"><label for=opt113>Ends With</label></td>\n' +
		'</tr><tr>\n' +
		'	<td><input type="radio" name="mode" id="opt114" value="c"><label for=opt114>Contains</label></td>\n' +
		'	<td><input type="radio" name="mode" id="opt115" value="u"><label for=opt115>Unscramble</label></td>\n' +
		'</tr></table>\n' +
		'</td></tr>\n' +
		'<tr><td colspan="3" align=center style="text-align: center">\n' +
		'<input id=box_submit12 style="font-size: 10pt" type=button onclick="this.form.submit()" value="Look it up"></td></tr></table></form>\n' +
		'<!--end of Word Finder lookup box-->';


	var box_font='Arial';
	var box_font_size1='10pt';
	var box_font_size2='8pt';
	var box_font_size3='12pt';
	var box_width='230px';
	var f=document.customize_box12;

	if (f.box_type12[1].checked) {
		toggle_box12('dictionarybox', 1);
		code=code.replace('~1', 'up');
		code=code.replace('~S', '');
		code=code.replace('~oli', 'down');
		code=code.replace('~ol', 'display:none;');
	} else {
		toggle_box12('dictionarybox', 0);
		code=code.replace('~1', 'up');
		code=code.replace('~S', '');
		code=code.replace('~oli', 'up');
		code=code.replace('~ol', '');
	}

	if (f.box_open12[0].checked) {
		code=code.replace(/_blank/g, '_top');
	}

	if (f.fsize12[0].checked) {
		box_font_size1='8pt';box_font_size2='7pt';box_font_size3='10pt';box_width='195px';
	}
	else if (f.fsize12[2].checked) {
		box_font_size1='12pt';box_font_size2='10pt';box_font_size3='14pt';box_width = '265px';
	}
	else if (f.fsize12[3].checked) {
		box_font_size1='';box_font_size2='smaller';box_font_size3='larger';box_width='230px';
	}

	box_font=f.sFont12.options[f.sFont12.selectedIndex].value;
	code=code.replace(/~2/g, box_font);
	Style('boxtable12').fontFamily=box_font;

	code=code.replace(/~3/g, box_font_size1);
	code=code.replace(/~4/g, box_font_size2);
	code=code.replace(/~5/g, box_font_size3);
	var box_tags1=['box_td112','box_word12','box_submit12'];
	for (var i in box_tags1)
		ById(box_tags1[i]).style.fontSize=box_font_size1;
	Style('boxtitle12').fontSize=box_font_size3;
	Style('boxtitle212').fontSize=box_font_size2;
	Style('boxsource_td12').fontSize=box_font_size2;
	code=code.replace(/~6/, box_width);
	Style('boxtable12').width=box_width;

	code=code.replace(/~7/, box_width);
	Style('boxtable12').width=box_width;

	code=code.replace(/~8/g, f.Background12.value);
	Style('boxtable12').backgroundColor = f.Background12.value;

	code=code.replace(/~9/g, f.TextColor12.value);
	Style('boxtable12').color=f.TextColor12.value;


	code=code.replace(/~A/g, f.TitleBackground12.value);
	Style('btdt112').backgroundColor = f.TitleBackground12.value;
	Style('btdt212').backgroundColor = f.TitleBackground12.value;

	code=code.replace(/~B/g, f.TitleTextColor12.value);
	Style('bta12').color=f.TitleTextColor12.value;

	code=code.replace(/~C/g, f.BorderColor12.value);
	Style('boxtable12').border='1px '+f.BorderColor12.value+' solid';
	Style('btdt112').borderBottom='1px '+f.BorderColor12.value+' solid';
	Style('btdt212').borderBottom='1px '+f.BorderColor12.value+' solid';


	f.code12.value=code;
}
box_render12();
	</script>

