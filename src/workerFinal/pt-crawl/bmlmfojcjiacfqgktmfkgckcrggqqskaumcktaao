<script type="text/javascript">
	var tfd_mw_left_selected = 0;
	var tfd_mw_answers = [null, null, null, null, null];

	function tfd_mw_click_a(b) {
		ById('tfd_mw_left').style.visibility = 'hidden';
		ById('tfd_mw_right').style.visibility = '';
		for (var i=0; i<5; i++) 
			ById('tfd_a' + i).style.border = '1px black solid';
		ById(b.id).style.border = '2px red solid';
		tfd_mw_left_selected = b.id.substr(5, 1);
	}

	function tfd_mw_click_b(b) {
		ById('tfd_mw_left').style.visibility = '';
		ById('tfd_mw_right').style.visibility = 'hidden';
		var match = b.id.substr(5, 1);
		for (var i=0; i<5; i++) {
			if (tfd_mw_answers[i] == match) tfd_mw_answers[i] = null;
			ById('tfd_b' + i).style.backgroundColor = 'white';
		}
		tfd_mw_answers[tfd_mw_left_selected] = match;
		for (var i=0; i<5; i++) 
			if (tfd_mw_answers[i] != null) ById('tfd_b' + tfd_mw_answers[i]).style.backgroundColor = ById('tfd_a' + i).style.backgroundColor;
	}

	function tfd_mw_clear() {
		tfd_mw_answers = [null, null, null, null, null];
		for (var i=0; i<5; i++) 
			ById('tfd_b' + i).style.backgroundColor = 'white';
	}
	
	// Mismatch
	
	var tfd_mm_left_selected = 0;
	var tfd_mm_answers = [null, null, null, null, null];

	function tfd_mm_click_a(b) {
		ById('tfd_mm_left').style.visibility = 'hidden';
		ById('tfd_mm_right').style.visibility = '';
		for (var i=0; i<5; i++) 
			ById('tfd_mm_a' + i).style.border = '1px #777 solid';
		ById(b.id).style.border = '2px red solid';
		tfd_mm_left_selected = +b.id.substr(8, 1);
	}

	function tfd_mm_click_b(b) {
		ById('tfd_mm_left').style.visibility = '';
		ById('tfd_mm_right').style.visibility = 'hidden';
		var match = b.id.substr(8, 1);
		for (var i=0; i<5; i++) {
			if (tfd_mm_answers[i] == match) tfd_mm_answers[i] = null;
			ById('tfd_mm_b' + i).style.backgroundColor = '#777';
		}
		tfd_mm_answers[tfd_mm_left_selected] = match;
		for (var i=0; i<5; i++) 
			if (tfd_mm_answers[i] != null) ById('tfd_mm_b' + tfd_mm_answers[i]).style.backgroundColor = ById('tfd_mm_a' + i).style.backgroundColor;
	}

	function tfd_mm_clear() {
		tfd_mm_answers = [null, null, null, null, null];
		for (var i in tfd_mm_answers)
			ById('tfd_mm_b' + i).style.backgroundColor = '#777';
	}
	
	
</script>
