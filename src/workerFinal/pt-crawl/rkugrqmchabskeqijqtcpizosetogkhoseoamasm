        style="position:relative; height:100%; width:100%;">







    
    <img alt=""
        class='iconic '
        

        
            

                
                    
                        
                        
                            src="https://www.loc.gov/static/managed-content/uploads/sites/6/2024/01/Official-Kagan35.jpg#h=2240&amp;w=1450"
                        

                    
                

            

        


    
        onerror="this.onerror=null;this.src='/static/images/original-format/web-page.svg'"
    

    
