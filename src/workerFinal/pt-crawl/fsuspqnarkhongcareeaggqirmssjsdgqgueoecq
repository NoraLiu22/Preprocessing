      <article id="result-2" 
               class="row  result  result--list  result--even">
        <div class="result__preview  col-xs-5 col-sm-4  col-md-3">
          <a class="result__preview--link  link  link--internal" 
             aria-hidden="true" 
             tabindex="-1"
             id="result-img-2"
             data-ga-cat="Search Result Selected (image)"
             data-ga-action="Tribhuvan University and its Educational Activities in Nepal [2/24]"
             href="/ark:/67531/metadc331162/">

            
            <img data-src="/ark:/67531/metadc331162/small/" 
                 src="/ark:/67531/metadc331162/thumbnail/"
                 alt="Thesis or Dissertation: Tribhuvan University and its Educational Activities in Nepal"
                 class="result__img  lazyload"
                 aria-hidden="true" />
          </a>
          
            
              <span class="label label-default"
                    style="padding:4px; font-size:60%; color:#333;">
