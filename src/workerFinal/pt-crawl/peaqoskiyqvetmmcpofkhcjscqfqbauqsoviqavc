    <script>
        // Load recaptcha API if it hasn't been loaded because of DNT
        if (window.doNotTrack == "1" || navigator.doNotTrack == "yes" || navigator.doNotTrack == "1") {
            const scriptEl = document.createElement('script');
            scriptEl.src = "https://www.google.com/recaptcha/api.js";
            scriptEl.async = true;
            document.head.appendChild(scriptEl);
        }
    </script>

    <script>
        var recaptchaVerifyCallback = function(response) {
            jQuery('#feedback-form-submit').removeAttr('disabled');
        };

        var recaptchaExpiredCallback = function() {
            jQuery('#feedback-form-submit').attr('disabled', 'disabled');
        };

        function onSubmit(token) {
          const emailInput = document.getElementById('email-address');
          const emailWarning = document.getElementById('email-warning');
          const emailRegex = /^[\w-]+(\.[\w-]+)*@([\w-]+\.)+[a-zA-Z]{2,7}$/;
          const categorySelection = document.getElementById('feedback-category');
          const categoryWarning = document.getElementById('category-warning');

          if (!emailRegex.test(emailInput.value)) {
            event.preventDefault();
            emailWarning.style.display = 'block';
          } else {
            emailWarning.style.display = 'none';
          }

          if (categorySelection.value == '') {
            event.preventDefault();
            categoryWarning.style.display = 'block';
          } else {
            categoryWarning.style.display = 'none';
          }

          if (emailWarning.style.display == 'none' && categoryWarning.style.display == 'none') {
            document.getElementById('feedback-form').submit();
          }
        }
    </script>

    <script>
        (function defer() {
            if (window.jQuery) {

                var $feedbackCategory = $("#feedback-category"),
                    $correctionButtonsWrapper = $("#correction-buttons"),
                    $correctionButtons = $correctionButtonsWrapper.find(".btn-correction-problem"),
                    $userFeedback = $("#user-feedback");

                $feedbackCategory.on('change', function(){
                    var selected = $(this).find('option:selected').val();
                    console.log(selected);
                    if (selected == 'Correction') {
                        $correctionButtonsWrapper.removeClass('hidden').addClass('show');
                        //$techButtonsWrapper.addClass('hidden');
                    } else {
                        $correctionButtonsWrapper.removeClass('show').addClass('hidden');
                    }
                });

                $correctionButtons.on('click', function(){
                    var contents = $userFeedback.val(),
                        buttonText = $(this).text();

                    $userFeedback.val(contents + '\n\nTechnical Issue: ' + buttonText);
                });

            } else {
                setTimeout(function() { defer() }, 50);
            }
        }());
    </script>



