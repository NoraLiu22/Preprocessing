              <div  class="col-sm-12">
          <div class="mb-3 block block-elemental block-elemental-component-blockbutton">
  
    
      <div class="component component--button component--view-default">
  

  <div class="field field--field-link field--visually-hidden">
    <div class="field__label visually-hidden">Link</div>
                  <div class="d-inline-block"><a href="/permits/permit-exemptions" class="btn btn-primary">See if you are exempt from needing a hunting permit</a></div>
            </div>
</div>

