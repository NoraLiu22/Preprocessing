function playprev(){
    if(currslid==0){
        currslid = (n.length-1);
    }
    else{
        currslid = currslid-1;
    };
    setfoc(currslid);
    playit();
}
function stop(){
  clearTimeout(slidint);
}
function playit(){
    slidint = setTimeout(playnext,5000);
}
function stopit(){
    clearTimeout(slidint);
    }
window.onload = function(){
    playit();
}
