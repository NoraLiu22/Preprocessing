    <script type="text/javascript">
        function _addPageLoadingDiv() {
            $('body').append("<div class='pageloading'>Loading&#8230;</div>");
        };
        $(function () {
            $('#SearchForm').submit(function (event) {
                _addPageLoadingDiv();
                window.location = "/Profiles/Faculty/ListByDepartmentName?DepartmentName=All Faculty&RankStatus=Regular&SearchTerm=" + $('#SearchTextBox').val();
                event.preventDefault();
            });
            $('.dropdown-item').click(function () {
                _addPageLoadingDiv();
            });

            $('.FacultyProfileLink').click(function () {
                _addPageLoadingDiv();
            });

            $(".box").click(function () {
                _addPageLoadingDiv();
            });

            $(".autoClean").on("keyup change", function () {
                //Regex for <(any a-z character)  <!  </  <?  
                var regexBracket = new RegExp(/(<)([a-zA-Z?!/])/g);
                //Regex for &#
                var regexAndHash = new RegExp(/(&#)/g);

                if (regexBracket.test($(this).val()) || regexAndHash.test($(this).val())) {
                    // Get Cursor Position
                    var cursorStart = this.selectionStart
                    var cursorEnd = this.selectionEnd;
                    cleanInput = $(this).val();
                    cleanInput = cleanInput.replace(regexBracket, "$1 $2").replace(regexAndHash, "& #");
                    $(this).val('').val(cleanInput);
                    this.setSelectionRange(cursorStart + 1, cursorEnd + 1);
                }
            });

        });
    </script>
    
    <script src="https://cdn.jsdelivr.net/clipboard.js/1.5.3/clipboard.min.js"></script>
    <script src="/Profiles/Scripts/_profile.js"></script>
;
    

</body>
