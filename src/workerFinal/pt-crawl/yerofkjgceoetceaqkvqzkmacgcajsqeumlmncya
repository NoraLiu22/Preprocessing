		function toggleMobileMenu() {
			var menu = document.getElementById('MobileMenu');
			var wrapper = document.getElementById('wrapper');
			menu.style.display = (menu.offsetWidth === 0 && menu.offsetHeight === 0) ? 'block' : 'none';
