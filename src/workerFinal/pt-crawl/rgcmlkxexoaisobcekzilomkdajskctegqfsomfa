            function headerflow() {
        var textwidth = $('.title-header-name > div').width();
        if (textwidth > $(window).width() - 87) {
            $(".title-header-main").addClass("title-header-main-flow");
            $(".title-header-name").addClass("title-header-name-flow");
        }
    }
