<!-- Start Iframe component -->
<div class="clearfix">	
	 <script>
$(document).ready(function() {
	var max = 255;
    $("#ms-conf-search-textbox").keyup(function(event) {
		if (event.keyCode === 13) {
			doSubmitMSConfSearch()
		}
		var currentCount = this.value.length;
		var remainingCount = max - currentCount;
		$("#char-count-remaining").text(remainingCount);
		if(currentCount >= max) {
            e.preventDefault();
        }
	});
});

function doSubmitMSConfSearch(){
	var currenturl = window.location.href;
	var virtualparam = "";
	if ($("#virtualflag").prop("checked") == true){
		virtualparam = "&virtualflag=Y&virtualConfReadOnly=Y";
	}
	else {
		virtualparam = "&virtualflag=B&virtualConfReadOnly=N";
	}

	if(currenturl.includes("www.ieee.org")){
		window.location.href = "https://conferences.ieee.org/conferences_events/conferences/search?q=" + $("#ms-conf-search-textbox").val() + virtualparam;
	} else {
		var newurl = "https://conferencesqa.ieee.org/conferences_events/conferences/search?q=" + $("#ms-conf-search-textbox").val() + virtualparam;
		//console.log(newurl);
		window.location.href = newurl;
	}
}
</script>  
    <div id='icx-div'>
    	 <div style="padding: 20px 20px 30px 20px; background-color: #069; position: relative; margin-bottom: 0px; border-radius: 10px;">
	<input id="ms-conf-search-textbox" autocomplete="off" class="form-control" maxlength="255" name="search" placeholder="Search for IEEE conferences by title, keywords, location or sponsors" type="text" typeaheadoptionfield="eventTitle" typeaheadoptionslimit="10" style="width: 93%; height: 45px;float: left; border-radius: 0px;">
	<button aria-label="Search" style="right: 20px; bottom: 20px; background-color: #f90; outline: none; border: 0; margin-left: -4px; cursor: pointer; transition: all .3s ease-in-out; padding: 0; height: 45px; width: 45px;" onclick="doSubmitMSConfSearch();">
		<i class="fa fa-search" placement="bottom" style="color: #fff; font-size: 22px; height: 45px; width: 45px; line-height: 45px;"></i>
		<span style="height: 15px; width: 20px; background-color: #069; position: absolute; top: 14px; right: 15px; -webkit-transform: rotate(45deg); transform: rotate(45deg);"></span>
	</button>
	<span style="color: #fff;bottom: 0; left: 20px;font-size: 14px;float: left;"><input type="checkbox" id="virtualflag" style="width: 12px; height: 12px;"><label for="virtualflag" style="margin-left: 5px;vertical-align:middle;font-weight: 400;">Search virtual events</label></span>
	<span style="color: #fff;bottom: 0; right: 65px;font-size: 12px;opacity: .5; float: right;">(<span id="char-count-remaining">255</span> characters left)</span>
</div>  
