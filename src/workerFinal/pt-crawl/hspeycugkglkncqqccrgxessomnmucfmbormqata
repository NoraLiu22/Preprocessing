<script>
"use strict";
/**
 * @func swapView
 * @desc switch to collation view
 */
function swapView() {
  var verse = ("0" + (document.form.books.selectedIndex + 1)).slice(-2) + ("00" + (document.form.chapters.selectedIndex + 1)).slice(-3) + ("00" + (document.form.verses.selectedIndex + 1)).slice(-3);
  document.getElementById("swap").setAttribute("href",`../apparatus/index.htm?v=${verse}`);  // add link for switching to collation
  }
</script>
