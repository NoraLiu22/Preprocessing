	<!-- Artboard: 600 -->
	<div
		id="g-net-sticker-prices-600"
		class="g-artboard"
		style="min-width: 600px"
		data-aspect-ratio="1.357"
		data-min-width="600"
	>
		<div style="padding: 0 0 73.7163% 0"></div>
		<img
			id="g-net-sticker-prices-600-img"
			class="g-net-sticker-prices-img g-aiImg"
			alt=""
			data-src="https://static01.nytimes.com/newsgraphics/2023-07-21-college-economic-diversity/74c641ea-4123-4ad9-a2ca-9f936ab7d131/_assets/net-sticker-prices-600.png"
