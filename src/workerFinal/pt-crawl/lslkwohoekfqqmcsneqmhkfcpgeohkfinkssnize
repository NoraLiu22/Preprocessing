  <script id="als-svc">
    (function () {
      if (!window.NYTD || !window.NYTD.Abra) {
        return;
      }
      var alsVariant = window.NYTD.Abra('als_toggle');
      if (alsVariant != null && alsVariant.match(/(1_block)/)) {
        window.NYTD.Abra.reportExposure('als_toggle');
