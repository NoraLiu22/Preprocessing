      <div>
                    <div class="highlighted"></div>
      
            
            
            
              <a id="main-content" tabindex="-1"></a>
              <!-- googleon: index -->
    <div class="region region-content p-0 m-0 content">
    <div data-drupal-messages-fallback class="hidden"></div>          
    <div class="row bs-3col-bricked">
  

    <div class="col-sm-12 bs-region bs-region--top">
    <div class="block-region-top"><section class="views-element-container block block-views block-views-blockfeatured-news-list-featured-news clearfix">
  
    

      <div><div class="view view-featured-news-list view-id-featured_news_list view-display-id-featured_news js-view-dom-id-b4a7806ebba33ff4a0d41ea379f596f7e3360afb93c1ad7cce5074f10c86842d">
  
    
      
      <div class="view-content">
          <div class="views-view-grid horizontal cols-3 clearfix col">
            <div class="row nomargin views-row clearfix row-1">
                  <div class="col-sm-4 no-padding-left views-col col-1">
