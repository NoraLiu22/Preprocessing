<script type="text/javascript"><!--
    jQuery(document).ready(function($) {
        if ($('#lodel-container').length == 1) {
            $('#ob-header, #ob-search, #ob-breadcrumb').remove();
        }
        $('.ob-authdata').hide();
    if (document.getElementById('authdata')) {
        document.getElementById('authdata').remove();
    }
