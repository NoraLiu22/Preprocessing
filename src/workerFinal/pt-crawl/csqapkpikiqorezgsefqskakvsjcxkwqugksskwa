    // FAQ script
    // We can have multiple instance of this FAQ widget - prevents multiple iterations
    // Initialise a single variable
    $(window).on('load', function () {
        if (!window.faqSectionInitialised) {
            window.faqSectionInitialised = true;

            window.faqs =
            {
                "@context": "https://schema.org",
                "@type": "FAQPage",
                mainEntity: []
            };
        }

        window.faqs.mainEntity.push(
            {
  "@type": "Question",
  "name": "Where will your funds go?",
  "acceptedAnswer": {
    "@type": "Answer",
    "text": "The funds you donate will support the Syria T\u0026uuml;rkiye Earthquake Appeal. Should we receive more contributions than needed for an emergency appeal, we will use the excess in other emergency-related activities."
  }
},
{
  "@type": "Question",
  "name": "Is my donation tax deductible?",
