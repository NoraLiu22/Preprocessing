                                                function menuSelectGo(month_obj) {
                                                    var selected_month_val = month_obj.options[month_obj.selectedIndex].value;
                                                    
                                                    var location = window.location;
                                                    var path = location.pathname;
                                                    var query_string = '?pub_code=ffp&&container=p20110819100700000&&date=';
                                                    //var redirect_url = '?pub_code=ffp&&container=p20110819100700000&&date=';
                                                    var redirect_url = path + query_string;
                                                    
                                                    redirect_url = redirect_url + selected_month_val;
                                                    window.location.href = redirect_url;

                                                }

                                                function genLinkUrl( query_str) {
                                                    var location = window.location;
                                                    var path = location.pathname;
                                                    var link = path + query_str;
                                                
                                                    window.location.href = link;
                                                    
                                                }

                                            </script>
