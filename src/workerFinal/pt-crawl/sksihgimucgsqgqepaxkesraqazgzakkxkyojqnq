<script>
  //jQuery('document').ready(function(){
    var bg_color = 'UF Orange - #f36812 ';
    if ('' == 'Yes'){
      //UF Branded Skin checked
      //apply class if text_color contains "UF Blue" or "UF Orange"
      if (bg_color.includes('UF Blue')){
        jQuery('.button_banner_content1056254').removeClass('default-style1056254');
        jQuery('.bb-button1056254').removeClass('default-style1056254');
        jQuery('.button_banner_content1056254').addClass('uf-branded-blue');
        jQuery('.bb-button1056254').addClass('uf-branded-blue');
      } else if (bg_color.includes('UF Orange')){
        jQuery('.button_banner_content1056254').removeClass('default-style1056254');
        jQuery('.bb-button1056254').removeClass('default-style1056254');
        jQuery('.button_banner_content1056254').addClass('uf-branded-orange');
        jQuery('.bb-button1056254').addClass('uf-branded-orange');
      }else if (bg_color.includes('Grey Light') || bg_color.includes('White') ){
        jQuery('.button_banner_content1056254').removeClass('default-style1056254');
        jQuery('.bb-button1056254').removeClass('default-style1056254');
        jQuery('.button_banner_content1056254').addClass('grey-text');
        jQuery('.bb-button1056254').addClass('grey-text');
      }
    } else {
      //UF Branded Skin NOT checked
      if (bg_color.includes('Grey Light') || bg_color.includes('White') ){
        jQuery('.button_banner_content1056254').removeClass('default-style1056254');
        jQuery('.bb-button1056254').removeClass('default-style1056254');
        jQuery('.button_banner_content1056254').addClass('uf-blue-text');
        jQuery('.bb-button1056254').addClass('uf-blue-text');
      }
    }
  
  //replace .blue-text with .uf-blue-text
  jQuery('.blue-text').addClass('uf-blue-text');
  jQuery('.blue-text').removeClass('blue-text');
  //});
</script>
