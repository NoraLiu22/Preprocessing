<article id="article_32438" class="editorial full article">
    

    
        

<script type="text/javascript">
var gusto = window.gusto || {};
gusto.article = {
    'title': 'The 10 greatest ballet dancers of the 20th century',
    'publish_epoch': '1634662496',
    'slug': 'greatest-ballet-dancers',
    'author': 'Rob Weinberg',
    'category': 'Ballet'
