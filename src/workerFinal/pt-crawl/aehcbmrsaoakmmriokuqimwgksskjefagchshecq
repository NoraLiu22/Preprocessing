									var e = document.currentScript.parentNode;
									if (e.classList.contains('dtads-location')) {
										if (!e.classList.contains('dtads-' + window.DTScenario.getExperience())) {
											e.parentNode.removeChild(e);
										}
									}
								}
							}());
						</script>
