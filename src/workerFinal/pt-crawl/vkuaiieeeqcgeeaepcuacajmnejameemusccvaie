const skipCmp = false;
const forceCmp = false;
const baseUrl = "https://coupons.fortune.com";

            window.cmpStatus = new Promise(resolve => {
                window.cmpResolve = resolve;
            });
            loadCmp();

            async function loadCmp() {
                try {
                    const isCmpRequired = await fetchIsCmpRequired();

                    if (!isCmpRequired) {
                        window.cmpResolve('not-required');
                        return;
                    }

                    const script = document.createElement('script');
                    script.setAttribute('src', cmpScriptUrl);
                    script.setAttribute('onload', 'window.cmpResolve("success");');
                    script.setAttribute('onerror', 'window.cmpResolve("failed");');
                    document.head.append(script);
                } catch {
                    window.cmpResolve('failed');
                }
            }

            async function fetchIsCmpRequired() {
                if (skipCmp || new URL(window.location.href).searchParams.has('skip_cmp')) return false;
                if (forceCmp || new URL(window.location.href).searchParams.has('force_cmp')) return true;

                const response = await fetch(`${baseUrl}/api/cmp`);
                const data = await response.text();
                return data === '1';
            }
