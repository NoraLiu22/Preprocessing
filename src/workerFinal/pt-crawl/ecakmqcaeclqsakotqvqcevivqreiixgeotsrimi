   <div class="box component section default-style col-xs-12 col-lg-3"> 
     
    <div class="component-content"> 
     <div class="paragraphSystem content"> 
      <div class="image component section default-style col-xs-12 col-sm-6 col-md-4 col-lg-11 col-lg-offset-0 image_copy_117011784"> 
       <div class="component-content left"> 
        <div class="analytics-image-tracking"></div> 
