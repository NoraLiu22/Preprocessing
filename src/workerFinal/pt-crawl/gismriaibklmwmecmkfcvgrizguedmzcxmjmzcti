    <div class="g-header-container"></div>
  


  
  <script id="nytg-dynamic-script_top">
  	(function() {
  		// move header to new location
  		var container = document.querySelector(".g-header-container");
  		var header = document.querySelector(".interactive-header");
      if ( container && header ) {
        container.appendChild(header);
        document.querySelector("#app").classList.add("has-top-cover");
      }
  	})(); 
  </script>
  

  


 

