  (function() {
    window.NYTG = window.NYTG || {};
    window.NYTG.ASSETS = "https://static01.nytimes.com/newsgraphics/2022-08-16-orion-spacecraft/95117ed2c04bf9aa5798245544a0de9998c15287/_assets/".slice(0, -1);
