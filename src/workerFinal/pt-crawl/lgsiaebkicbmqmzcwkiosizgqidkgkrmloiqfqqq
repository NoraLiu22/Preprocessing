}
#nav-content p {
    border: 2px solid #003b5c;
    border-radius: 5px;
    background-color: #003b5c;
    color: white;
    padding: 20px;
    margin-bottom: 5px;
}
#nav-content ul {
    margin: 0px;
    width: 100%;
    list-style: none;
}
#nav-content ul a {
    text-decoration: none;
}
#nav-content ul li {
    background-color: #003b5c;
    width: 100%;
    color: white;
    margin-bottom: 5px;
    padding: 10px 20px;
    border: 2px solid #003b5c;
    border-radius: 5px;
    transition: all .2s ease-out;
}
#nav-content ul a:hover li {
    background-color: white;
    color: #003b5c;
    border: 2px solid #003b5c;
}
#nav-content ul a.selectednav li {
    background-color: white;
    color: #003b5c;
    border: 2px solid #003b5c;
    border-radius: 5px;
    transition: 0s;
}
/* END OF NAV BLOCK */

/* CONTENT BLOCKS */
.page-content {
    max-width: calc(100% - 330px);
    width: calc(100% - 330px);
    position: relative;
    left: 330px;
}

article.visible h4 {
    padding-top: 0px;
    padding-bottom: 0px;
    margin-top: 20px;
}

/* END OF CONTENT BLOCKS */

/* DROP DOWNS */
         .dropDownSection {
		/*border: 1px solid #d8d8d8;*/
		border-bottom: 0px solid #d8d8d8;
		padding: 0;
		margin-bottom: 10px;
	}
	
	.dropDownSection summary {
		border: 2px solid #003b5c;
		border-radius: 5px;
		padding: 16px;
		display: block;
		cursor: pointer;
		position: relative;
	}

	.dropDownSection summary span {
		display: inline-block;
		padding-right: 20px;
	}
	
	.dropDownSection summary::-webkit-details-marker {
		display: none;
	}
	
	.dropDownSection summary::after {
		position: absolute;
		right: 20px;
		top: 5px;
		display: inline-block;
		transition: 0.2s; /* Speed of animation for the arrow*/
		content: '\203A'; /* Chevron icon aka the arrow*/
		font-size: 20pt;
		color: #003b5c;
	}
	
	.dropDownSection[open] summary::after {
		transform: rotate(90deg); /* how many degrees the arrow rotates */
	}
	
	.dropDownSection p {
		margin: 20px;
	}
	
	.dropDownSection ul {
		padding-left: 40px;
	}
	
	.dropDownSection h4 {
		margin-left: 20px;
	}
	
	#dropDownSectionHelp h4 {
		margin: 0;
	}
	
	.dropDownTable {
		margin-top: 0;
		margin-bottom: 0;
		border: 0;
		width: 100%;
	}

	#help {
    		border: 2px solid #f1f1f1;
    		background-color:#f1f1f1;
    		border-radius: 5px;
	}

	#help[open] {
    		padding-bottom: 8px;
	}

	#help summary {
		border: 0;
	}

	#help summary h4 {
		margin: 0;
		padding-right: 20px;
	}

