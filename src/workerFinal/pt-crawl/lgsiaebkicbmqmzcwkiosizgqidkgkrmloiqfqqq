                            }
                        }

                        return false;
                    });

                    $("#txtGenericSearchTermMobile").keypress(function (e) {
                        if (e.keyCode === 13) {
                            $("#btnGenericSearchMobile").click();
                            return false;
                        }
                    });
                });
            </script>
