		    jQuery(document).ready(function ($) {
		        //if page is for shipping, and cart contains any items from Products category, hide Media Mail Shipping Option
		        if (document.URL.indexOf("checkoutshipping") != -1) {
		            if ($("span[class='shoppingcartproductsku']:contains('PROD')").length > 0) {
		                //Hide Media Mail
		                $("label:contains('Media Mail')").parent().parent().hide();
		            }
		            if ($("span[class='shoppingcartproductsku']:contains('PROD0001')").length > 0 ||
						$("span[class='shoppingcartproductsku']:contains('PROD0002')").length > 0
					) {
		                //Hide Media Mail
		                $("label:contains('Priority Mail')").parent().parent().hide();
		            }
		        }

				$("#accordian h3").click(function () {
					//slide up all the link lists
					$("#accordian ul ul").slideUp();
					$("#accordian h3").css('background-color', '#1c0c32');
					//slide down the link list below the h3 clicked - only if its closed
					if (!$(this).next().is(":visible")) {
						$(this).next().slideDown();
						$(this).css('background-color', '#461d7c');
					}
				});
			
				$(".navbarToggle").click(function () {
					$("#accordian").toggle();
				});
			
				$(".searchToggle").click(function () {
					$(".cartBarToggle").slideUp("fast");
					$('.active').removeClass('active');
					$(".searchBarToggle").slideToggle("fast");
					$(this).toggleClass("active");
					return false;
				});
				$(".cartToggle").click(function () {
					$(".searchBarToggle").slideUp("fast");
					$('.active').removeClass('active');
					$(".cartBarToggle").slideToggle("fast");
					$(this).toggleClass("active");
					return false;
				});
			
