<div class="tabs-vertical" data-tabs-vertical-id="1510497511">

  <div class="tabs-vertical__main">
                <div
        class="tabs-vertical__main-item hidden"
        id="tabs-main--1510497511-1"
        role="tabpanel"
        aria-expanded="false"
        aria-labelledby="tabs-tab--1510497511-1"
