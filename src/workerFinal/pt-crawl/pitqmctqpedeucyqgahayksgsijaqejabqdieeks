<script>
    var pagewidth = 0;
    $(document).ready(function () {
        pagewidth = $(window).width();
        mansetscreen();
        $(window).resize(function () {
            mansetscreen();
        });
    });

    function mansetscreen() {
        if (pagewidth < 680) {
            $("#mansetmobildiv").show();
            $("#mansetdiv").hide();

        }
        else if (pagewidth > 680) {
            $("#mansetmobildiv").hide();
            $("#mansetdiv").show();
        }
    }
</script>
