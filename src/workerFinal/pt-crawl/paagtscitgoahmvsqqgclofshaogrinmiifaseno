<script>
    $(document).ready(function () {
        $("#carousel-example-generic").swiperight(function () {
            $(this).carousel('prev');
            $("#carousel-example-generic").removeClass('isPaused');
            $("#carousel-example-generic").addClass('isPlaying');
        });
        $("#carousel-example-generic").swipeleft(function () {
            $(this).carousel('next');
            $("#carousel-example-generic").removeClass('isPaused');
            $("#carousel-example-generic").addClass('isPlaying');
        });
        $('#carousel-example-generic').carousel({
            interval: 5000,
            pause: "false"
        });
    });
    $('#carousel-example-generic').mousemove(function () {
        $("#carousel-example-generic").carousel('pause');
    }).blur(function () {
        $("#carousel-example-generic").removeClass('isPaused');
        $("#carousel-example-generic").addClass('isPlaying');
    });
    $('#carousel-example-generic').on("tap", function () {
        $("#carousel-example-generic").carousel('pause');
    }).blur(function () {
        $("#carousel-example-generic").removeClass('isPaused');
        $("#carousel-example-generic").addClass('isPlaying');
    });
</script>
