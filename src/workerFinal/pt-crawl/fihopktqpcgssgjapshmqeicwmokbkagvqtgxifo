<a data-beam-element-clicked-no-delay data-type="LINK" data-location="Footer" data-position="footer" data-text="tiktok" aria-label="TikTok" class="Icon Icon--sm hover:border-b-0" href="https://www.tiktok.com/@bankrate/" rel="noopener" target="_blank" title="TikTok logo">
<svg class="hover:text-blue" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24" xml:space="preserve" focusable="false" fill="currentColor"><path d="M19.9 5.7c-2.5-.2-3.3-2.3-3.5-2.9-.1-.3-.4-.5-.7-.5H13c-.4 0-.8.3-.8.8v12.1c0 1.1-.9 2-1.9 2-1.1 0-2-.9-2-2 0-1 .8-1.8 1.7-1.9.4 0 .7-.4.7-.7V9.5c0-.2-.1-.4-.2-.5-.1-.1-.3-.2-.5-.2-3.6 0-6.6 3-6.6 6.6 0 3.6 3 6.6 6.6 6.6 3.6 0 6.6-3 6.6-6.6V10c.9.4 2.2.8 3.5.4.3-.1.6-.4.6-.7V6.4c-.1-.4-.4-.7-.8-.7zM19.1 9c-1.4.1-2.8-.9-2.8-1-.3-.1-.6-.1-.8 0-.2.1-.4.4-.4.7v6.5c0 2.8-2.3 5.1-5.1 5.1S4.9 18 4.9 15.2c0-2.5 1.9-4.7 4.3-5v1.6c-1.4.4-2.4 1.8-2.4 3.3 0 1.9 1.5 3.5 3.5 3.5 1.9 0 3.4-1.5 3.4-3.5V3.8h1.4c.6 1.4 1.9 2.9 3.9 3.3V9z" class="icon-base"></path><title>Tiktok</title></svg></a>
