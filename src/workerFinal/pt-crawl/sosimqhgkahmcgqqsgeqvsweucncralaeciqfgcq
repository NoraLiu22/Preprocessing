        <script>
            var body = document.body;
            var scrolled = false;
            window.addEventListener('scroll', function () {
                scrolled = true;
            });

            
            if ((window.scrollY || window.pageYOffset) >= 50) {
                body.classList.add('scrolled');
            }

            var timeout = setInterval(function(){
                if (scrolled) {
                    scrolled = false;
                    if ((window.scrollY || window.pageYOffset) >= 50 ) {
                        body.classList.add('scrolled');
                    } else {
                        body.classList.remove('scrolled');
                    }
                }
            }, 50);
        </script>

        
        <script>
            window.csrftoken = "h7BDO12clryHKtVT65n4RTHlZDasvpVa8uonImyrgcZYfllJZAk6q6nLs2HOj41V";
        </script>
        


        
        
        
        <script>
            // Load all scripts non-async
            footerScripts.forEach(function(src) {
                var scriptEl = document.createElement('script');
                scriptEl.src = src;
                scriptEl.async = false;
                document.head.appendChild(scriptEl);
            });
        </script>
    
        
    
    

    

        <script type="text/javascript">
            (function defer() {
                if ('List' in window) {
                

                    const listOptions = {
                        valueNames: ['facet-name','facet-count'],
                        page: 1500
                    };

                


                
                    
                

                
                    
                

                
                    
                        const titlesFacetList = new List('facet-list-serialseries-titles', listOptions);
                        titlesFacetList.sort('facet-count', { order: "desc" });
                    
                

                
                    
                

                
                    
                        const regionsFacetList = new List('facet-list-world-regions', listOptions);
                        regionsFacetList.sort('facet-name', { order: "asc" });
                    
                

                
                    
                        const countriesFacetList = new List('facet-list-countries', listOptions);
                        countriesFacetList.sort('facet-name', { order: "asc" });
                    
                

                
                    
                        const statesFacetList = new List('facet-list-us-states', listOptions);
                        statesFacetList.sort('facet-name', { order: "asc" });
                    
                

                
                    
                        const countiesFacetList = new List('facet-list-counties', listOptions);
                        countiesFacetList.sort('facet-name', { order: "asc" });
                    
                

                
                    
                        const decadesFacetList = new List('facet-list-decades', listOptions);
                        decadesFacetList.sort('facet-name', { order: "desc" });
                    
                

                
                    
                        const yearsFacetList = new List('facet-list-years', listOptions);
                        yearsFacetList.sort('facet-name', { order: "desc" });
                    
                

                
                    
                        const monthsFacetList =  new List('facet-list-months', listOptions);
                    
                

                
                    
                

                

                

                


                
                } else {
                    setTimeout(function() { defer(); }, 50);
                }
            }());
        </script>

    



