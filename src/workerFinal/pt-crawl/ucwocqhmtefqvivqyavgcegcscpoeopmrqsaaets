            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1983, frequency:1 },{ year:1984, frequency:9 },{ year:1985, frequency:22 },{ year:1986, frequency:44 },{ year:1987, frequency:49 },{ year:1988, frequency:51 },{ year:1989, frequency:85 },{ year:1990, frequency:100 },{ year:1991, frequency:113 },{ year:1992, frequency:126 },{ year:1993, frequency:99 },{ year:1994, frequency:122 },{ year:1995, frequency:116 },{ year:1996, frequency:107 },{ year:1997, frequency:193 },{ year:1998, frequency:181 },{ year:1999, frequency:153 },{ year:2000, frequency:136 },{ year:2001, frequency:189 },{ year:2002, frequency:237 },{ year:2003, frequency:234 },{ year:2004, frequency:245 },{ year:2005, frequency:333 },{ year:2006, frequency:410 },{ year:2007, frequency:467 },{ year:2008, frequency:497 },{ year:2009, frequency:610 },{ year:2010, frequency:683 },{ year:2011, frequency:720 },{ year:2012, frequency:809 },{ year:2013, frequency:751 },{ year:2014, frequency:1081 },{ year:2015, frequency:1325 },{ year:2016, frequency:1460 },{ year:2017, frequency:1607 },{ year:2018, frequency:1719 },{ year:2019, frequency:1912 },{ year:2020, frequency:2164 },{ year:2021, frequency:2273 },{ year:2022, frequency:2387 },{ year:2023, frequency:2467 },{ year:2024, frequency:764 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=rj%3A1008+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                        </div>
                    </div>
                
            
        </article>
    </div>
        </div>
    

                    <div class="clearfix"></div>
                </div>
            </div>

