<script type="text/javascript">
    // Click on Year Link in Year List on the subscribe page
    $('#ddlSelectIssueYears').change(function () {
        window.location.replace('/spp/subscribe/SingleIssue' + '/' + this.value);
    });

    function buyIssue(urlFormat, baseUrl, custType, rateCode, year) {
        var $itemNumber = $('#ddlSelectIssue').val();
        var $url = urlFormat.replace('{0}', baseUrl).replace('{1}', $itemNumber).replace('{2}', custType).replace('{3}', rateCode).replace('{4}', year);
        window.location.replace($url);
    }

    // Click on Year Link in Year List on the subscribe page
    $('#ddlSelectBackIssue').change(function () {
        window.location.replace('/spp/subscribe/BackIssues' + '/0/' + this.value);
    });

</script>
 
