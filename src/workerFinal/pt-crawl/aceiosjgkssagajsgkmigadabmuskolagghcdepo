<script>
    // APPLY
    let arrowOpen = false;
    const openMulti = () => {
        const selectWrap1 = document.querySelector(".selectWrapper");
        const containerArrow = document.querySelector(".apply-arrow");
        arrowOpen ? containerArrow.style.transform = "rotate(0deg)" : containerArrow.style.transform = "rotate(180deg)";
        arrowOpen = !arrowOpen;
        if (document.querySelector(".selectWrapper").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper").style.opacity = 0;
            document.querySelector(".selectWrapper").style.pointerEvents = "none";
            selectWrap1.classList.remove('navLinkOpen');
            resetAllMenus();
        } else {
            document.querySelector(".selectWrapper").style.opacity = 1;
            document.querySelector(".selectWrapper").style.pointerEvents = "all";
            selectWrap1.classList.add('navLinkOpen');
        }

        const selectWrap2 = document.querySelector(".selectWrapper2");
        if (document.querySelector(".selectWrapper2").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper2").style.opacity = 0;
            document.querySelector(".selectWrapper2").style.pointerEvents = "none";
            selectWrap2.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        const selectWrap3 = document.querySelector(".selectWrapper3");
        if (document.querySelector(".selectWrapper3").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper3").style.opacity = 0;
            document.querySelector(".selectWrapper3").style.pointerEvents = "none";
            selectWrap3.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        const selectWrap4 = document.querySelector(".selectWrapper4");
        if (document.querySelector(".selectWrapper4").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper4").style.opacity = 0;
            document.querySelector(".selectWrapper4").style.pointerEvents = "none";
            selectWrap4.classList.remove('navLinkOpen');
            resetAllMenus();
        }
        const selectWrap5 = document.querySelector(".selectWrapper5");
        if (document.querySelector(".selectWrapper5").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper5").style.opacity = 0;
            document.querySelector(".selectWrapper5").style.pointerEvents = "none";
            selectWrap4.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        let applyArrow = document.getElementById("apply-button-img")
        if (applyArrow) {
            let x = applyArrow.src.split("/"), t = x.length-1, y = x[t];
            if (y == 'chevron-down.svg') {
                applyArrow.src='/img/recruitment-2021/chevron-up.svg'
            }

            if (y == 'chevron-up.svg') {
                applyArrow.src='/img/recruitment-2021/chevron-down.svg'
            } 
        }

    }
    const nextMenu = (e) => {
        let isOnMobile = window.innerWidth < 500;
        if (isOnMobile) {
            window.scrollTo(0, 0);
        }
        if (event instanceof MouseEvent || event instanceof KeyboardEvent && (event.keyCode === 13 || event.keyCode === 32)) {
            menuIndex = eval(event.target.parentNode.id.slice(-1));
            document.querySelectorAll(".multiSelect")[e].style.transform =
            "translateX(-100%)";
            document.querySelectorAll(".multiSelect")[e].style.clipPath =
            "polygon(100% 0, 100% 0, 100% 100%, 100% 100%)";
            document.querySelectorAll(".multiSelect")[e + 1].style.transform =
            "translateX(0)";
            document.querySelectorAll(".multiSelect")[e + 1].style.clipPath =
            "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
        }
    }
    const prevMenu = (e) => {
        let isOnMobile = window.innerWidth < 500;
        if (isOnMobile) {
            window.scrollTo(0, 0);
        }
        if (event instanceof MouseEvent || event instanceof KeyboardEvent && (event.keyCode === 13 || event.keyCode === 32)) {
            menuIndex = eval(event.target.parentNode.id.slice(-1));
            document.querySelectorAll(".multiSelect")[e].style.transform =
            "translateX(100%)";
            document.querySelectorAll(".multiSelect")[e].style.clipPath =
            "polygon(0 0, 0 0, 0 100%, 0% 100%)";
            if (e == 4) {
                document.querySelectorAll(".multiSelect")[e - 4].style.transform =
                "translateX(0)";
                document.querySelectorAll(".multiSelect")[e - 4].style.clipPath =
                "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
            } else if (e != 3 && e != 4) {
                document.querySelectorAll(".multiSelect")[e - 1].style.transform =
                "translateX(0)";
                document.querySelectorAll(".multiSelect")[e - 1].style.clipPath =
                "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
            } else {
                document.querySelectorAll(".multiSelect")[e - 3].style.transform =
                "translateX(0)";
                document.querySelectorAll(".multiSelect")[e - 3].style.clipPath =
                "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
            }
        }
    }
    const resetAllMenus = () => {
        const containerArrow = document.querySelector(".apply-arrow");
        containerArrow.style.transform = "rotate(0deg)";
        arrowOpen = false;

        setTimeout(() => {
        let x = document.getElementsByClassName("multiSelect");
        let i;
        for (i = 1; i < x.length; i++) {
            x[i].style.transform = "translateX(100%)";
            x[i].style.clipPath = "polygon(0 0, 0 0, 0 100%, 0% 100%)";
        }
        document.querySelectorAll(".multiSelect")[0].style.transform =
            "translateX(0)";
        document.querySelectorAll(".multiSelect")[0].style.clipPath =
            "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
        }, 300);
    }

    // ACADEMICS
    let arrowOpen2 = false;
    const openMulti2 = () => {
        const containerArrow2 = document.querySelector(".academics-arrow");
        arrowOpen2 ? containerArrow2.style.transform = "rotate(0deg)" : containerArrow2.style.transform = "rotate(180deg)";
        arrowOpen2 = !arrowOpen2;
        const selectWrap2 = document.querySelector(".selectWrapper2");
        if (document.querySelector(".selectWrapper2").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper2").style.opacity = 0;
            document.querySelector(".selectWrapper2").style.pointerEvents = "none";
            selectWrap2.classList.remove('navLinkOpen');
            resetAllMenus();
        } else {
            document.querySelector(".selectWrapper2").style.opacity = 1;
            document.querySelector(".selectWrapper2").style.pointerEvents = "all";
            selectWrap2.classList.add('navLinkOpen');
        }

        const selectWrap1 = document.querySelector(".selectWrapper");
        if (document.querySelector(".selectWrapper").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper").style.opacity = 0;
            document.querySelector(".selectWrapper").style.pointerEvents = "none";
            selectWrap1.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        const selectWrap3 = document.querySelector(".selectWrapper3");
        if (document.querySelector(".selectWrapper3").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper3").style.opacity = 0;
            document.querySelector(".selectWrapper3").style.pointerEvents = "none";
            selectWrap3.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        const selectWrap4 = document.querySelector(".selectWrapper4");
        if (document.querySelector(".selectWrapper4").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper4").style.opacity = 0;
            document.querySelector(".selectWrapper4").style.pointerEvents = "none";
            selectWrap4.classList.remove('navLinkOpen');
            resetAllMenus();
        }
        const selectWrap5 = document.querySelector(".selectWrapper5");
        if (document.querySelector(".selectWrapper5").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper5").style.opacity = 0;
            document.querySelector(".selectWrapper5").style.pointerEvents = "none";
            selectWrap4.classList.remove('navLinkOpen');
            resetAllMenus();
        }

        let academicsArrow = document.getElementById("academics-button-img")
        let x = academicsArrow.src.split("/"), t = x.length-1, y = x[t];

        if (y == 'chevron-down.svg') {
            academicsArrow.src='/img/recruitment-2021/chevron-up.svg'
        }

        if (y == 'chevron-up.svg') {
            academicsArrow.src='/img/recruitment-2021/chevron-down.svg'
        } 
    }
    const nextMenu2 = (e) => {
        if (event instanceof MouseEvent || event instanceof KeyboardEvent && (event.keyCode === 13 || event.keyCode === 32)) {
            menuIndex = eval(event.target.parentNode.id.slice(-1));
            document.querySelectorAll(".multiSelect2")[menuIndex].style.transform =
            "translateX(-100%)";
            document.querySelectorAll(".multiSelect2")[menuIndex].style.clipPath =
            "polygon(100% 0, 100% 0, 100% 100%, 100% 100%)";
            document.querySelectorAll(".multiSelect")[menuIndex + 1].style.transform =
            "translateX(0)";
            document.querySelectorAll(".multiSelect2")[menuIndex + 1].style.clipPath =
            "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
        }
    }
    const prevMenu2 = (e) => {
        let isOnMobile = window.innerWidth < 500;
        if (isOnMobile) {
            window.scrollTo(0, 0);
        }
        if (event instanceof MouseEvent || event instanceof KeyboardEvent && (event.keyCode === 13 || event.keyCode === 32)) {
            menuIndex = eval(event.target.parentNode.id.slice(-1));
            document.querySelectorAll(".multiSelect2")[menuIndex].style.transform =
            "translateX(100%)";
            document.querySelectorAll(".multiSelect2")[menuIndex].style.clipPath =
            "polygon(0 0, 0 0, 0 100%, 0% 100%)";
            document.querySelectorAll(".multiSelect2")[menuIndex - 1].style.transform =
            "translateX(0)";
            document.querySelectorAll(".multiSelect2")[menuIndex - 1].style.clipPath =
            "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
        }
    }
    const resetAllMenus2 = () => {
        const containerArrow2 = document.querySelector(".academics-arrow");
        containerArrow2.style.transform = "rotate(0deg)";
        arrowOpen2 = false;
        setTimeout(() => {
        let x = document.getElementsByClassName("multiSelect2");
        let i;
        for (i = 1; i < x.length; i++) {
            x[i].style.transform = "translateX(100%)";
            x[i].style.clipPath = "polygon(0 0, 0 0, 0 100%, 0% 100%)";
        }
        document.querySelectorAll(".multiSelect2")[0].style.transform =
            "translateX(0)";
        document.querySelectorAll(".multiSelect2")[0].style.clipPath =
            "polygon(0 0, 100% 0, 100% 100%, 0% 100%)";
        }, 300);
    }

    // Cost and Aid
    let arrowOpen3 = false;
    const openMulti3 = () => {
        const containerArrow3 = document.querySelector(".cost-and-aid-arrow");
        arrowOpen3 ? containerArrow3.style.transform = "rotate(0deg)" : containerArrow3.style.transform = "rotate(180deg)";
        arrowOpen3 = !arrowOpen3;
        const selectWrap3 = document.querySelector(".selectWrapper3");
        if (document.querySelector(".selectWrapper3").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper3").style.opacity = 0;
            document.querySelector(".selectWrapper3").style.pointerEvents = "none";
            selectWrap3.classList.remove('navLinkOpen');
            resetAllMenus();
        } else {
            document.querySelector(".selectWrapper3").style.opacity = 1;
            document.querySelector(".selectWrapper3").style.pointerEvents = "all";
            selectWrap3.classList.add('navLinkOpen');
        }

        const selectWrap1 = document.querySelector(".selectWrapper");
        if (document.querySelector(".selectWrapper").style.pointerEvents == "all") {
            document.querySelector(".selectWrapper").style.opacity = 0;
            document.querySelector(".selectWrapper").style.pointerEvents = "none";
            selectWrap1.classList.remove('navLinkOpen');
