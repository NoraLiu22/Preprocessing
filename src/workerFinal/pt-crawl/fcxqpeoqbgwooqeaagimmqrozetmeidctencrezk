              <source srcset="/sites/default/files/styles/paragraph_spotlight_banner_small/public/media/Documents/OSE/ose_banner.jpg.webp?itok=99QFQpDa 1x" media="all and (min-width: 610px)" type="image/webp" width="441" height="441"/>
