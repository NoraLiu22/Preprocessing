var webData = {"page":[]};


function generateUUID() {
	// Generate UUID with crypto API
	if (typeof crypto !== 'undefined') {
		return ([1e7]+-1e3+-4e3+-8e3+-1e11).replace(/[018]/g, c =>
			(c ^ crypto.getRandomValues(new Uint8Array(1))[0] & 15 >> c / 4).toString(16)
		);
	}

	// If Crypto API not available, fallback. Not as reliable.
	return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
    var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
    return v.toString(16);
  });
}

function getCookieVal(offset) {
	var endstr = document.cookie.indexOf (";", offset);
	if (endstr == -1) {
		endstr = document.cookie.length;
	}
	return unescape(document.cookie.substring(offset, endstr));
}

function getCookie(name) {
	var arg = name + "=";
	var alen = arg.length;
	var clen = document.cookie.length;
	var i = 0;
	while (i < clen) {
		var j = i + alen;
		if (document.cookie.substring(i, j) == arg) {
			return getCookieVal(j);
		}

		i = document.cookie.indexOf(" ", i) + 1;
		if (i == 0) break;
	}
	return "";
}

function hcPermutiveSetCookie(name, data, expiration) {
	if (!expiration) {
		expiration = new Date();
		// 1 year.
		expiration.setTime(expiration.getTime() + (5*365*24*60*60*1000));
	}

	if (typeof data !== 'string') {
		data = JSON.stringify(data);
	}

	document.cookie = name + "=" + data + "; expires=" + expiration.toUTCString() + "; path=/";
}

function getPermutiveDefaultUserID () {
	var cookieID = "hcpermutive_uuid";
	var cookieData = getCookie(cookieID);
	if (!cookieData) {
		cookieData = generateUUID();
		hcPermutiveSetCookie(cookieID, cookieData);
		return cookieData;
	}

	return cookieData;
}

// permutive identify - Default
var userDefaultIdentity = getPermutiveDefaultUserID();
if (userDefaultIdentity !== null && typeof userDefaultIdentity !== 'undefined') {
	window.permutive.identify([{
		id: userDefaultIdentity,
		tag: "default",
		priority: 1,
	}]);

}


