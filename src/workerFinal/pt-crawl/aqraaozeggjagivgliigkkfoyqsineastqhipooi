    window.scroll(() => {
      let threshold = 50;
      document.getElementsByClassName("side-nav-freshman").innerHTML = window.scrollTop();
      if (window.scrollTop() >= threshold)
      document.getElementsByClassName("side-nav-freshman").classList.add("position-fixed");
      else
      document.getElementsByClassName("side-nav-freshman").classList.remove("position-fixed");
      let check = document.getElementsByClassName("side-nav-freshman").height() - document.getElementById("side-link-menu-desktop").height()-21;
      if (window.scrollTop() >= check)
      document.getElementById("side-link-menu-desktop").classList.add("bottom-space");
      else
      document.getElementById("side-link-menu-desktop").classList.remove("bottom-space");
      });
