<script>
    $(document).ready(function() {

        $('.iframe-btn').on('click', function() {    
            $('#iframe1').attr('src', '');
            $('#iframe2').attr('src', '');
            setTimeout(function() {
                $('#iframe1').attr('src', 'https://www.youtube.com/embed/Wx-l1jVRPCw');
                $('#iframe2').attr('src', 'https://www.youtube.com/embed/pwZSK_NuEKI');
            }, 100);
        });

        (function () {
            $(".carousel-item").width($(".navigation").width());
            $(".carousel-item").height($("#basic").height());
            var $frame  = $('#basic');
            var $slidee = $frame.children('ul').eq(0);
            var $wrap   = $frame.parent();

            // Call Sly on frame
            var options = {
                horizontal: 1,
                itemNav: 'basic',
                smart: 1,
                activateOn: 'click',
                mouseDragging: 0,
                touchDragging: 0,
                releaseSwing: 1,
                startAt: 0,
                scrollBar: $wrap.find('.scrollbar'),
                scrollBy: 1,
                pagesBar: $wrap.find('.pages'),
                activatePageOn: 'click',
                speed: 500,
                elasticBounds: 1,
                dragHandle: 1,
                dynamicHandle: 1,
                clickBar: 1,
                cycleBy: 'pages',
                cycleInterval: 5000,

                // Buttons
                prev: $wrap.find('.right-arrow'),
                next: $wrap.find('.left-arrow')
            };

            var frame = new Sly($frame, options);

            frame.init();
            $(window).resize(function (e) {
                $(".carousel-item").width($(".navigation").width());
                $(".carousel-item").height($("#basic").height());
                frame.reload();
            });
        }());
    });


</script>

