	link.href = '/surveys/international/assets/css/navbar.css';
	head.appendChild(link);

	var bootstapLink = document.createElement("link");

	bootstapLink.type = "text/css";
	bootstapLink.rel = "stylesheet";
	bootstapLink.href = '/surveys/international/assets/bootstrap-3.4.1/css/bootstrap.min.css';
	head.appendChild(bootstapLink);

    iap_navbar('iap');
</script>



