(function($) {
	$( '#archive-years' ).change(function(){
		$( '.archive-months-of-year' ).hide().prop( "disabled", true ).prop( "hidden", true );
		var v = $(this).val();
		$( '.in-year-'+v ).show().prop( "disabled", false ).prop( "hidden", false );
		
		var m = $('#archive-months').val();
		if (null === m) $('#archive-months option:first-child').prop('selected', true);
	});
	$( '#archive-button' ).click(function(){
		window.location.href = 
			'https://www.amny.com/' +
			$('#archive-years').val()+'/' +
			$('#archive-months').val()+'/' +
			$('#archive-cat').val()+'/';
	});
})( jQuery );
