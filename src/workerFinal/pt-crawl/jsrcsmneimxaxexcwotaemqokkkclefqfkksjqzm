<script src="https://code.jquery.com/jquery-3.6.3.min.js" integrity="sha256-pvPw+upLPUjgMXY0G+8O0xUf+/Im1MZjXxxgOcBQBXU=" crossorigin="anonymous"></script>
<script>
jQuery(function($) {
var BREAK_POINT = 768,
FIXED_WIDTH = 1280;
var $viewport = $('meta[name="viewport"]');
var onResize = function() {
var windowWidth = $(window).width();
if (windowWidth <= FIXED_WIDTH && windowWidth >= BREAK_POINT) {
$viewport.attr('content', 'width=' + FIXED_WIDTH);
} else {
$viewport.attr('content', 'width=device-width');
}
}
onResize();
$(window).on('resize', onResize);
});
</script>
<script src="/js/app.js?rev=202403181825"></script>
<script src="/libs/Chart.min.js"></script>
<script>
jQuery(function($) { // variables
var labelFontColor = '#FFF';
var labelFontColorGray = '#333';
var labelFontStyle = '300';
var labelFontFamily = "'Roboto Condensed', 'Helvetica Neue', 'Helvetica', 'Arial', sans-serif";
var labelTextAlignCenter = 'center';
var labelTextAlignRight = 'right';
var labelTextBaseline = 'middle';
var labelTextPadding = 0;
var titleFontColor = '#333';
var titleFontStyle = 'bold';
// fontsize : PC
var titleFontSize = 14;
var labelFontSizeLarge = 20;
var labelFontSizeSmall = 15;
var labelFontSizeRight = 25;
// fontsize : SP
if ($(window).width() < 767) {
titleFontSize = 12;
labelFontSizeLarge = 11;
labelFontSizeSmall = 11;
labelFontSizeRight = 16;
}
// IE11対策用にいれていたcanvasのwidthを削除して、SP時にレスポンシブが機能するようにする
if ($(window).width() < 768) {
$('canvas').removeAttr('width');
}
// ラベル表示 フォントサイズ 1カラム用 : large
var dataLabelPluginLarge = {
afterDatasetsDraw: function(chart) {
var ctx = chart.ctx;
chart.data.datasets.forEach(function(dataset, series) {
var meta = chart.getDatasetMeta(series);
if (!meta.hidden) {
meta.data.forEach(function(element, elements) {
var dataString = dataset.data[elements].toString();
ctx.fillStyle = labelFontColor;
var fontSize = labelFontSizeLarge;
var fontStyle = labelFontStyle;
var fontFamily = labelFontFamily;
ctx.font = Chart.helpers.fontString(fontSize, fontStyle, fontFamily);
ctx.textAlign = labelTextAlignCenter;
ctx.textBaseline = labelTextBaseline;
var padding = labelTextPadding;
var position = element.tooltipPosition();
var barWidth = element._model.x - element._model.base;
var centerX = element._model.base + barWidth / 2;
ctx.fillText(dataString, centerX, position.y + 2);
});
}
});
}
}
// ラベル表示 フォントサイズ 2カラム用 : small 
var dataLabelPluginSmall = {
afterDatasetsDraw: function(chart) {
var ctx = chart.ctx;
chart.data.datasets.forEach(function(dataset, series) {
var meta = chart.getDatasetMeta(series);
if (!meta.hidden) {
meta.data.forEach(function(element, elements) {
var dataString = dataset.data[elements].toString();
ctx.fillStyle = labelFontColor;
var fontSize = labelFontSizeSmall;
var fontStyle = labelFontStyle;
var fontFamily = labelFontFamily;
ctx.font = Chart.helpers.fontString(fontSize, fontStyle, fontFamily);
ctx.textAlign = labelTextAlignCenter;
ctx.textBaseline = labelTextBaseline;
var padding = labelTextPadding;
var position = element.tooltipPosition();
var barWidth = element._model.x - element._model.base;
var centerX = element._model.base + barWidth / 2;
ctx.fillText(dataString, centerX, position.y + 2);
});
}
});
}
}
// ラベル表示 方向 縦 : vertical
var dataLabelPluginVertical = {
afterDatasetsDraw: function(chart) {
var ctx = chart.ctx;
chart.data.datasets.forEach(function(dataset, series) {
var meta = chart.getDatasetMeta(series);
if (!meta.hidden) {
meta.data.forEach(function(element, elements) {
var dataString = dataset.data[elements].toString();
ctx.fillStyle = labelFontColorGray;
var fontSize = labelFontSizeRight;
var fontStyle = labelFontStyle;
var fontFamily = labelFontFamily;
ctx.font = Chart.helpers.fontString(fontSize, fontStyle, fontFamily);
ctx.textAlign = labelTextAlignCenter;
ctx.textBaseline = labelTextBaseline;
var padding = labelTextPadding;
var position = element.tooltipPosition();
var barWidth = element._model.x - element._model.base;
ctx.fillText(dataString, position.x, position.y - 15);
});
}
});
}
} // chart01
var chart01 = function() {
var targetWrapperId = 'chart01__wrapper';
var targetCanvasId = 'chart01';
var ctx = document.getElementById(targetCanvasId).getContext('2d');
var legendCount = 8;
window.myBar = new Chart(ctx, {
type: 'horizontalBar',
data: {
labels: false,
datasets: [{
label: 'Top Management',
backgroundColor: '#0086D9',
borderWidth: 0,
data: [12.0]
}, {
label: 'Corporate Planning / Marketing',
backgroundColor: '#006BAD',
borderWidth: 0,
data: [10.4]
}, {
label: 'IT',
backgroundColor: '#CCCCCC',
borderWidth: 0,
data: [5.8]
}, {
label: 'Sales',
backgroundColor: '#BFBFBF',
borderWidth: 0,
data: [21.8]
}, {
label: 'Professional',
backgroundColor: '#999999',
borderWidth: 0,
data: [8.0]
}, {
label: 'R&D',
backgroundColor: '#7F7F7F',
borderWidth: 0,
data: [4.6]
}, {
label: 'Manufacturing',
backgroundColor: '#595959',
borderWidth: 0,
data: [3.7]
}, {
label: 'Others',
backgroundColor: '#494949',
borderWidth: 0,
data: [33.7]
}]
},
options: {
animation: false,
responsive: true,
title: {
display: false
},
hover: {
mode: null
},
tooltips: {
enabled: false
},
legend: {
display: false
},
legendCallback: function(chart) {
var text = [];
text.push('<ul class="re-p-chart-legend">');
for (var i = 0; i < legendCount; i++) { // 凡例項目数
text.push('<li class="re-p-chart-legend__item"><span style="color:' + chart.data.datasets[i].backgroundColor + '">●<\/span> ');
if (chart.data.datasets[i].label) text.push(chart.data.datasets[i].label);
text.push('<\/li>');
}
text.push('<\/ul>');
return text.join('');
},
scales: {
xAxes: [{
display: false,
stacked: true,
gridLines: {
display: false
}
}],
yAxes: [{
display: false,
stacked: true,
ticks: {
fontSize: titleFontSize,
fontColor: titleFontColor,
fontStyle: titleFontStyle
},
gridLines: {
display: false
}
}]
}
},
plugins: [dataLabelPluginLarge] // large
});
document.getElementById(targetWrapperId).innerHTML = myBar.generateLegend();
};
chart01(); // chart02
