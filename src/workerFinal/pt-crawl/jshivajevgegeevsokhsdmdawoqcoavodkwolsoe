<script>
    $(window).scroll(function() {
        let scroll = $(window).scrollTop();
        
        if ($(".smart-banner-wrapper").find(".smartbanner").length > 0){ 
            if (scroll < 60) {
                $(".headerTop").css("top", `-${scroll}px`);
            } else if (scroll > 60) {
                $(".headerTop").css("top", "-60px");
            }
        }
    });
</script>

