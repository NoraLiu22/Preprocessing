                <div class="input-group">
                    <div class="dollar-sign">$</div>
                    <input v-el:custom-amount v-on:input="limitToNumberWithSetLength" v-on:keyup="validateCustomAmount" type="text" class="form-control" v-model="customAmount" v-bind:id="'customAmount_' + donationWidgetIndex" v-bind:min="donationProduct.MinimumDonationAmount" pattern="\d*" v-bind:maxlength="maximumLength" v-on:keypress="submitCustomAmountOnEnterKeypress" />
                </div>
            </span>
        </button>
    </div>
    <p v-if="customAmountError" class="text-danger center-block">{{customAmountError}}</p>
    <div class="Donation-widget__recurrence text-right" v-if="donationProduct.AllowRecurringDonation">
        <span>I would like to donate:</span>
        <div class="btn-group">
            <button v-on:click.prevent="monthlyDonation = false" type="button" class="btn" v-bind:class="!monthlyDonation ? 'btn-primary active' : 'btn-default'">Once</button>
            <button v-on:click.prevent="monthlyDonation = true" type="button" class="btn" v-bind:class="monthlyDonation ? 'btn-primary active' : 'btn-default'">Monthly</button>
        </div>
    </div>
    <div class="text-right form-group">
        <button type="submit" class="Donation-widget__submit btn btn-lg btn-primary" v-on:click.prevent="submitDonation" :disabled="isBusy || isCompleted">
          <span v-if="isCompleted">
            <span class="fa-stack check">
              <i class="fa fa-circle fa-stack-2x"></i>
              <i class="fa fa-check fa-stack-1x text-primary"></i>
            </span>&nbsp;Added
          </span>
          <span v-else>Donate now</span>
          <i v-if="isBusy" class="fa fa-spinner fa-spin"></i>
        </button>
        <div class="alert alert-danger text-left" v-if="genericError"><p>Sorry, something has gone wrong while processing. Please try again later.</p></div>
    </div>
</div>

    </div>
</div>
