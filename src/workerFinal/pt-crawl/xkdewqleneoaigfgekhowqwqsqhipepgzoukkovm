$(".togglestory3").click( function (){
  $(".contentstory3").slideToggle();
    if($(this).hasClass("show")){
     	$(this).text("Read less");
		
    }
    else{
      	$(this).text("Read more");
      	
    } 
  return false;
  });

</script>

