<script type="text/javascript">

    $(document).ready(function () {
        emptyPictureFrame();
        //emptyDescriptionFrame();
    });

    function emptyPictureFrame() {

        $("#imageContainer").each(function (index) {
            if ($(this).text().trim().slice(0, 6) === 'Bilder') {
                var divtagBilder = $(this);
                var LiTagsBilder = divtagBilder.parent('div').find('li');
                var noPhotos = LiTagsBilder.length - 1;

                if (noPhotos === 0) {
                    var addText = $("<p></p>").text("Vi har enda ingen bilder av denne");
                    divtagBilder.siblings().remove();
                    divtagBilder.parent('div').append(addText);

                    return false;
                }
            }
        })
    }

    function emptyDescriptionFrame() {

        $("#descriptionContainer").each(function (index) {
            if ($(this).text().trim().slice(0, 11) === 'Beskrivelse') {
                var divTagDescription = $(this);
                var tagDescription = divTagDescription.parent('div').find('p');

                if (tagDescription.text().length === 0) {
                    var addText = $("<p></p>").text("Vi har enda ingen beskrivelse av denne");
                    divTagDescription.parent('div').append(addText);
                }
            }
        })
    }
</script>





