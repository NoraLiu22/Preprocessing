    <script>
        (function() {
            Array.prototype.forEach.call(document.querySelectorAll('ul.leftsidemenu'), function(leftsidemenu) {
                leftsidemenu.addEventListener('click', function(ev) {
                   if (ev.target.nodeName === "BUTTON" && ev.target.classList.contains('toggle')) {

                       //ev.stopPropagation();
                       ev.preventDefault();
                       var button = ev.target;
                       if (button.classList.contains('glyphicon-menu-up')) {
                           button.classList.remove('glyphicon-menu-up');
                       } else {
                           button.classList.add('glyphicon-menu-up');
                       }
                       var tmp = button.getAttribute('aria-label');
                       button.setAttribute('aria-label', button.dataset.ariaLabel);
                       button.dataset.ariaLabel = tmp;
                   };
                });
            });
        })();
    </script>




