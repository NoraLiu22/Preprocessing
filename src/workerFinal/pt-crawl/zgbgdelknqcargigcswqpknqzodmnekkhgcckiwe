                        <script>
                /**
                 * The ZoomControl adds +/- button for the map
                 *
                 */
                function ZoomControl(controlDiv, map) {
                    // Creating divs & styles for custom zoom control
                    // controlDiv.style.padding = '5px';
                    controlDiv.classList.add('customMapControl');

                    // Set CSS for the control wrapper
                    let controlWrapper = document.createElement('div');
                    controlWrapper.classList.add('customMapControlBlock');
                    controlDiv.appendChild(controlWrapper);

                    // Set CSS for the zoomIn
                    let zoomInButton = document.createElement('div');
                    //Todo not work hovered cursor
                    zoomInButton.classList.add('customMapPlus', 'hovered');
                    controlWrapper.appendChild(zoomInButton);

                    // Set CSS for the zoomOut
                    let zoomOutButton = document.createElement('div');
                    //Todo not work hovered cursor
                    zoomOutButton.classList.add('customMapMinus', 'hovered');
                    controlWrapper.appendChild(zoomOutButton);

                    // Setup the click event listener - zoomIn
                    google.maps.event.addDomListener(zoomInButton, 'click', function () {
                        map.setZoom(map.getZoom() + 1);
                    });

                    // Setup the click event listener - zoomOut
                    google.maps.event.addDomListener(zoomOutButton, 'click', function () {
                        map.setZoom(map.getZoom() - 1);
                    });
                }

                //site coordinates https://www.gps-coordinates.net/
                let locationArray = [
                    {
                        lat: -21.912239908329493,
                        lng: 35.4453867215969,
                        locationName: 'Bazaruto Island',
                        icon: 'https://kisawasanctuary.com/wp-content/themes/my-theme/assets/img/icons/marker-icon.svg'
                    },
                ];
                let map, markers;
                let mapZoom,
                    mapLat,
                    mapLng;

                if (!window.matchMedia("(max-width: 767px)").matches) {
                    mapZoom = 10;
                    mapLat = -21.912239908329493;
                    mapLng = 35.4453867215969;
                } else {
                    mapZoom = 5;
                    mapLat = -21.912239908329493;
                    mapLng = 35.4453867215969;
                }

                function initMap() {
                    let centerMap = {
                        lat: mapLat,
                        lng: mapLng
                    };

                    let mapOptions = {
                        zoom: mapZoom,
                        center: centerMap,
                        disableDefaultUI: true,
                        styles: [    {
        "featureType": "administrative",
        "elementType": "all",
        "stylers": [
            {
                "saturation": "-100"
            }
        ]
    },
    {
        "featureType": "administrative.province",
        "elementType": "all",
        "stylers": [
            {
                "visibility": "off"
            }
        ]
    },
    {
        "featureType": "landscape",
        "elementType": "all",
        "stylers": [
            {
                "saturation": -100
            },
            {
                "lightness": 65
            },
            {
                "visibility": "on"
            }
        ]
    },
    {
        "featureType": "poi",
        "elementType": "all",
        "stylers": [
            {
                "saturation": -100
            },
            {
                "lightness": "50"
            },
            {
                "visibility": "simplified"
            }
        ]
    },
    {
        "featureType": "road",
        "elementType": "all",
        "stylers": [
            {
                "saturation": "-100"
            }
        ]
    },
    {
        "featureType": "road.highway",
        "elementType": "all",
        "stylers": [
            {
                "visibility": "simplified"
            }
        ]
    },
    {
        "featureType": "road.highway",
        "elementType": "labels",
        "stylers": [
            {
                "visibility": "off"
            }
        ]
    },
    {
        "featureType": "road.arterial",
        "elementType": "all",
        "stylers": [
            {
                "lightness": "30"
            }
        ]
    },
    {
        "featureType": "road.local",
        "elementType": "all",
        "stylers": [
            {
                "lightness": "40"
            }
        ]
    },
    {
        "featureType": "transit",
        "elementType": "all",
        "stylers": [
            {
                "saturation": -100
            },
            {
                "visibility": "simplified"
            }
        ]
    },
    {
        "featureType": "water",
        "elementType": "geometry",
        "stylers": [
            {
                "lightness": -25
            },
            {
                "saturation": -97
            },
            {
                "color": "#c8e5e8"
            }
        ]
    },
    {
        "featureType": "water",
        "elementType": "labels",
        "stylers": [
            {
                "lightness": -25
            },
            {
                "saturation": -100
            }
        ]
    }
]
                    };

                    let map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);
                    // console.log(document.getElementById('map_canvas'));

                    locationArray.forEach(function (item, index, array) {
                        // console.log(item);
                        let marker = new google.maps.Marker({
                            position: {lat: item['lat'], lng: item['lng']},
                            map: map,
                            title: item['locationName'],
                            icon: item['icon'],
                        });
                        markers1 = marker;
                    });
                    // Create the DIV to hold the control and call the ZoomControl() constructor
                    // passing in this DIV.
                    let zoomControlDiv = document.createElement('div');
                    let zoomControl = new ZoomControl(zoomControlDiv, map);

                    zoomControlDiv.index = 1;
                    map.controls[google.maps.ControlPosition.TOP_LEFT].push(zoomControlDiv);
                }
            </script>
        
