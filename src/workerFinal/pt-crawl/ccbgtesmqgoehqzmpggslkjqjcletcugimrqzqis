    <script>
        $(".txtb input").on("focus", function () {
            $(this).addClass("focusanp");
        });
        $(".txtb input").on("blur", function () {
            if ($(this).val() == "")
                $(this).removeClass("focusanp");
        });


        $('.wrap-news-dt').click(function (e) {

            if ($(this).find('.wrap-section-kp').hasClass('show')) {
                $(this).find('.wrap-section-kp').removeClass('show');
                menuOpen = "No";
            } else {
                $(this).find('.wrap-section-kp').addClass('show');
                menuOpen = "Yes";
            }

        });

        $(document).mouseup(function (e) {
            var container = $(".wrap-news-dt");

            // if the target of the click isn't the container nor a descendant of the container
            if (!container.is(e.target) && container.has(e.target).length === 0) {
                $('.wrap-section-kp').removeClass('show');
            }
        });
    </script>
