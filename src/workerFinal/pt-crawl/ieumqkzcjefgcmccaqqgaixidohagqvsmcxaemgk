					function HideWN(){
						var i
						for (i=1; i<6; i++)
						{
							var t = document.getElementById('WNSHOW'+i);
							t.style.display='none';
						};
					};
					function ShowWN(n){
						HideWN();
						document.getElementById('WNSHOW'+n).style.display='inline';
					};
    </script>
