<script>
    var pageIndex = 1, pageSize = 20, recordCount = 0, movieType = $("#OptionDate option:selected").val();

    // select change 事件
    function selctChange(type) {
        movieType = type;
        pageIndex=1
        getNewList()
    }

    getNewList()

    function getNewList() {
        $Common.Popup.LoadingShow('WrapInfo');//Loading显示
        $Common.Ajax.PostAPI("BoxOffice_GetHisbestInland", "t="+movieType+"&pindex="+pageIndex+"&size="+pageSize, function (data) {
            $Common.Popup.LoadingHide('WrapInfo');//Loading关闭
            if (data && data["Table"] && data["Table"].length > 0) {
                $Common.Data.InitTemplate("TP_TableList_JS", { list: data["Table"], rank: pageIndex , size:pageSize , movieType:movieType }, function (html) {
                    $("#TP_TableList").html(html);
                });
                if (pageIndex == 1) {
                    recordCount = Number(data["Table1"][0]['TotalCount']);
                    $Common.Data.SetPaging("TableList_Paging", pageIndex, pageSize, recordCount, function (indexNum) {
                        $(document).scrollTop(0);
                        pageIndex = indexNum;
                        getNewList();//获取数据-表格
                    });
                }
            }else $("#TP_TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        }, function (msg) {
            $Common.Popup.LoadingHide('WrapInfo');//Loading关闭
            $("#TP_TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        });
    }


</script> contentType 9 text/html url 77 https://www.endata.com.cn:443/BoxOffice/BO/History/Movie/Alltimedomestic.html responseCode 3 200 