                        <div id="DredgeNo4NationalHistoricSite" class="well hidden">
                            <div class="row">
                                    <div class="col-sm-4">
                                            <a href="https://parks.canada.ca/lhn-nhs/yt/klondike/culture/lhn-nhs-drague4-dredge4">
                                                <img class="img-responsive" alt="" src="//pcweb2.azureedge.net/-/media/WET4/proximite-nearby/yt/yt-klondike-lhn-nhs-640x360.jpg?modified=20221125212558" loading="lazy">
