<div id="locationSearch" class="col-xs-10 col-xs-offset-1 col-sm-offset-0 col-sm-12 thumbnail" style="padding-left:15px;">
    <h2 class="h2 mrgn-bttm-lg">
        Find a Parks Canada location
    </h2>
    <div class="nojs-show">
        <p><a href="#noTarget" class="wb-lbx wb-init wb-lbx-inited">Find a Parks Canada location</a></p>
    </div>
        <!-- GeomapStart -->
        <script>
            $(document).ready(function () {
                if ($("#siteNameSearch").length == 1) {
                    var $document = wb.doc,
                        searchByNameInput = $("#searchByName");

                    searchByNameInput.trigger("wb-update.wb-datalist");
                    $document.on("change", "#searchByName", function (event) {
                        var locationTargetValue = event.target.value.replace(/[^a-z0-9+]+/gi, '');
                        var locationTarget = $('#siteSearch li[locationtarget="' + locationTargetValue + '"]');
                        if (locationTarget.length) {
                            var provSelect = $('#siteSearch').find('select').first();
                            $.when($(provSelect).val($(locationTarget).attr('province')).change()).done(function () {
                                var itemSelect = $('#siteSearch').find('select').eq(1);
                                $(itemSelect).val(event.target.value).change()
                            });
                            return;
                        }
                        //default map canada
                        $('#location_map_reset').click();
                    });
                }
                if ($("#siteSearch").length == 1) {
                    $("body").on("change", "#siteSearch select", function () {
                        var form = $(this).closest('form');
                        var provinceItem = form.find('select').filter(':visible:first');
                        var provinceChange = $(this).attr("id") == provinceItem.attr("id");
                        var province = $(provinceItem).find(":selected").val();
                        if (province != null)
                            province = province.trim();

                        var location = form.find('select').filter(':visible:last').find(":selected").val();
                        if (location != null)
                            location = location.trim();

                        if (province == null || province == '') {
                            //no selection default map canada
                            $('#location_map_reset').click();
                        } else if (!provinceChange && location != '' && location != province) {
                            //if location change - map location
                            var locationTargetValue = location.replace(/[^a-z0-9+]+/gi, '');
                            var locationTarget = $('.locationTable tr[locationtarget="' + locationTargetValue + '"]');
                            if (locationTarget.length && $(locationTarget).find('a[data-map="location_map"]').length) {
                                $(locationTarget).find('a[data-map="location_map"]').click();
                            } else {
                                //if can't find location target - map province
                                $("#gm_province option").filter(function () {
                                    //may want to use $.trim in here
                                    return $(this).text() == province;
                                }).prop('selected', true);
                                $('#location_map_submit').click();
                            }
                        } else {
                            //if province change - map province
                            $("#gm_province option").filter(function () {
                                //may want to use $.trim in here
                                return $(this).text() == province;
                            }).prop('selected', true);
                            $('#location_map_submit').click();
                        }
                    });
                }
            });
        </script>
        <label class="mapLabel" for="location_map">Search on map</label>
