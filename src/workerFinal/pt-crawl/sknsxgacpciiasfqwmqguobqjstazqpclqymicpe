                                                <tr locationTarget="AkamiUapishkKakKasuakMealyMountainsNationalParkReserve" data-geometry="POINT (-58.348138 53.572076)" data-type="wkt">
                                                    <td>1</td>
                                                    <td>Akami-Uapishkᵁ-KakKasuak-Mealy Mountains National Park Reserve</td>
                                                        <td><a href="https://parks.canada.ca/pn-np/nl/mealy">Akami-Uapishkᵁ-KakKasuak-Mealy Mountains National Park Reserve</a></td>
                                                    <td>-58.348138 53.572076 </td>
                                                </tr>
                                                <tr locationTarget="AulavikNationalPark" data-geometry="POINT (-119.5956 73.727)" data-type="wkt">
                                                    <td>1</td>
