				(function() {
					var els = document.querySelectorAll('.dtc-secure-email');

					els.forEach(function(el) {
						el.addEventListener('click', function() {
							var address = window.DTUtil.DecodeBase64(el.getAttribute('data-addr') || '');
							address = address.replace(/[^a-z0-9@\.-_\s]/gi, ''); // remove garbage characters for FF
							document.location = 'mailto:' + address;
						});
					});
				}());
			</script>
