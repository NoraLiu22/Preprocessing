<script type="text/javascript">
    $(function() {
        var wi = $(window).width();
        $(".pop").first().removeClass("pop").addClass("pop-hover");
        $(".off").first().removeClass("off").addClass("on");

        $('div[id^="faqspan"]').click(function(e) {
            var link = $(this).children(".hr-tlt").children("a");
            //alert($(link).length);
            if ($(link).length == 0) {
                var i = $(this).attr("id").substring(7);
                $(".on").first().removeClass("on").addClass("off");
                if ($(this).parent().hasClass("pop")) {
                    $(".pop-hover").removeClass("pop-hover").addClass("pop");
                    $(this).parent().removeClass("pop").addClass("pop-hover");
                    var $div = $("#faqdiv" + i);
                    $div.removeClass("off").addClass("on");
                } else {
                    $(this).parent().removeClass("pop-hover").addClass("pop");
                }
            }
        });
    });
</script>
                                
                                
                                
