                    <div class="diagram svg-diagram">
                        <h3>Citations by Year</h3>
                        <div class="diagram ci-diagram" aria-label="citations by year"></div>
                        
    <style>
        rect:hover {
            fill: steelblue;
        }
        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }
        .bar {
            fill: steelblue;
        }
        a:hover .bar, a:focus .bar {
            stroke: black;
            fill-opacity: 60%;
        }
    </style>
    
    <script>
        var margin = { top: 20, right: 0, bottom: 30, left: 50 };
        var width = 930 - margin.left - margin.right;
        var height = 150 - margin.top - margin.bottom;
        var svg = d3.selectAll(".ci-diagram").append("svg")
            .attr("width", width + margin.left + margin.right)
            .attr("height", height + margin.top + margin.bottom)
            .append("g")
            .attr("transform", "translate(" + margin.left + "," + margin.top + ")");
        var data = [{ year:1951, frequency:1 },{ year:1952, frequency:2 },{ year:1953, frequency:1 },{ year:1954, frequency:4 },{ year:1955, frequency:2 },{ year:1956, frequency:5 },{ year:1957, frequency:1 },{ year:1958, frequency:4 },{ year:1959, frequency:6 },{ year:1960, frequency:1 },{ year:1961, frequency:2 },{ year:1962, frequency:6 },{ year:1963, frequency:3 },{ year:1964, frequency:6 },{ year:1965, frequency:7 },{ year:1966, frequency:6 },{ year:1967, frequency:8 },{ year:1968, frequency:6 },{ year:1969, frequency:6 },{ year:1970, frequency:19 },{ year:1971, frequency:19 },{ year:1972, frequency:33 },{ year:1973, frequency:42 },{ year:1974, frequency:35 },{ year:1975, frequency:39 },{ year:1976, frequency:50 },{ year:1977, frequency:37 },{ year:1978, frequency:62 },{ year:1979, frequency:56 },{ year:1980, frequency:68 },{ year:1981, frequency:55 },{ year:1982, frequency:95 },{ year:1983, frequency:74 },{ year:1984, frequency:77 },{ year:1985, frequency:118 },{ year:1986, frequency:116 },{ year:1987, frequency:117 },{ year:1988, frequency:127 },{ year:1989, frequency:114 },{ year:1990, frequency:140 },{ year:1991, frequency:138 },{ year:1992, frequency:139 },{ year:1993, frequency:126 },{ year:1994, frequency:109 },{ year:1995, frequency:112 },{ year:1996, frequency:165 },{ year:1997, frequency:138 },{ year:1998, frequency:139 },{ year:1999, frequency:125 },{ year:2000, frequency:112 },{ year:2001, frequency:127 },{ year:2002, frequency:160 },{ year:2003, frequency:127 },{ year:2004, frequency:154 },{ year:2005, frequency:156 },{ year:2006, frequency:187 },{ year:2007, frequency:191 },{ year:2008, frequency:204 },{ year:2009, frequency:221 },{ year:2010, frequency:177 },{ year:2011, frequency:207 },{ year:2012, frequency:211 },{ year:2013, frequency:178 },{ year:2014, frequency:222 },{ year:2015, frequency:206 },{ year:2016, frequency:218 },{ year:2017, frequency:186 },{ year:2018, frequency:239 },{ year:2019, frequency:241 },{ year:2020, frequency:265 },{ year:2021, frequency:308 },{ year:2022, frequency:265 },{ year:2023, frequency:223 },{ year:2024, frequency:61 }];

        data.forEach(function(d) {
            d.year = +d.year;
            d.frequency = +d.frequency;
        });

        var xScale = d3.scale.ordinal()
            .rangeRoundBands([0, width], .1);
        var yScale = d3.scale.linear()
            .range([height, 0]);

        // fill if less than 20 years span
        var zero = d3.max(
            [
                20 -
                (
                    d3.max(
                        data,
                        d => d.year
                    )
                    + 1
                    - d3.min(
                        data,
                        d => d.year
                    )
                ),
                0
            ]
        );

        // generate range from min to max
        xScale.domain(
            d3.range(
                d3.min(
                    data,
                    d => d.year
                ),
                d3.max(
                    data,
                    d => d.year
                ) + 1 + zero
            )
        );
        yScale.domain(
            [
                0,
                d3.max(
                    data,
                    d => d.frequency
                )
            ]
        );

        var xAxis = d3.svg.axis()
            .scale(xScale)
            .tickValues(
                // show ticks every 5 years for range up to 50 years, every 10
                // years up to 100 years, and every 20 years above that
                xScale.domain().filter(
                    function(d, i) {
                        let ticks;
                        if (xScale.domain().length <= 50) {
                            ticks = 5;
                        }
                        else if (xScale.domain().length <= 100) {
                            ticks = 10;
                        }
                        else {
                            ticks = 20;
                        }

                        return !(d % ticks);
                    }
                )
            )
            .tickSize(6,3,0) // removes end tick
            .orient("bottom");
        var yAxis = d3.svg.axis()
            .scale(yScale)
            .ticks(3)  // number of ticks
            .tickFormat(d3.format("d")) // only integers
            .tickSize(6,3,0) // removes end tick
            .orient("left");
        svg.selectAll(".bar")
            .data(data)
            .enter()
            .append("a")
            .attr(
                "xlink:href",
                d => "/?q=ra%3Arao.calyampudi-radhakrishna+py%3A" + d.year
            )
            .attr(
                "title",
                d => d.frequency + " Citation" + (d.frequency > 1 ? "s" : "") + " in " + d.year
            )
            .append("rect")
            .attr("class", "bar")
            .attr("x", d => xScale(d.year))
            .attr("width", xScale.rangeBand())
            .attr("y", d => yScale(d.frequency))
            .attr("height", d => height - yScale(d.frequency));
        svg.append("g")
            .attr("class", "x axis")
            .attr("transform", "translate(0," + height + ")")
            .call(xAxis);
        svg.append("g")
            .attr("class", "y axis")
            .call(yAxis)
            .append("text")
            .attr("transform", "rotate(-90)")
            .attr("y", 6)
            .attr("dy", ".75em")
            .style("text-anchor", "end")
            .text("");
    </script>

                    </div>
                </div>
            
                    
                        
                    
                        
                    
                        
