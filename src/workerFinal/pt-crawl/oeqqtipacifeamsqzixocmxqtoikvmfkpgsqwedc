<script type="text/javascript">
		/* <![CDATA[ */
		var jsC
		if(getCookie('Csc'))
			jsc=getCookie('Csc');
		else jsc = 0;
		var lid_38155_q1_autobox,lid_38155_q2_autobox,lid_38155_q3_autobox;						
		if(jsc==0)
		{	
			document.getElementById("evf").style.display = "";
			lid_38155_q1_autobox=new AutoSuggestion('lid_38155_input1','/pages/mod/v4/archive/SearchSuggestion.asp',100,1,20,50,1000,'static','en','lid_38155_cID','cID','0',"submitForm('lid_38155_formFilterEventSearch');");		
		}		
		if(jsc==1)
		{		
			document.getElementById("searchLink").style.display = "none";
			document.getElementById("ntf").style.display = "";
			lid_38155_q2_autobox=new AutoSuggestion('lid_38155_input2','/pages/mod/v4/archive/SearchSuggestion.asp',100,1,20,50,1000,'static','en','lid_38155_tID','tID','1',"submitForm('lid_38155_formFilterTeamSearch');");
		}
		if(jsc==2)
		{	
			document.getElementById("pf").style.display = "";
			lid_38155_q3_autobox=new AutoSuggestion('lid_38155_input3','/pages/mod/v4/archive/SearchSuggestion.asp',250,2,20,50,1000,'dynamic','en','lid_38155_pID','pID','2',"SetGBCookie();submitForm('lid_38155_formFilterPlayerSearch');");
		}

		function showSearchBox()
		{
			if(document.getElementById("sb").style.display == "none")
			{
				document.getElementById("sb").style.display = "";
				if (jsc == 0 )
				{
					document.getElementById("ev").className = "option-selected";
					document.getElementById("nt").className = "option";
					document.getElementById("sp").className = "option";			
				}
				if (jsc == 1 )
				{
					document.getElementById("ev").className = "option";
					document.getElementById("nt").className = "option-selected";
					document.getElementById("sp").className = "option";	
				}
				if (jsc == 2 )
				{
					document.getElementById("ev").className = "option";
					document.getElementById("nt").className = "option";
					document.getElementById("sp").className = "option-selected";			
				}
			}
			else 
			{
				document.getElementById("sb").style.display = "none";
			}
				
		}
		
		function hideSearchBox(selected)
		{

			document.getElementById("sb").style.display = "none";
			jsc=Number(selected);

			if(jsc==0)
			{
				document.getElementById("evf").style.display = "";
				document.getElementById("ntf").style.display = "none";
				document.getElementById("pf").style.display = "none";		
				document.getElementById("searchLink").style.display = "";						
				if(!lid_38155_q1_autobox){lid_38155_q1_autobox=new AutoSuggestion('lid_38155_input1','/pages/mod/v4/archive/SearchSuggestion.asp',100,1,20,50,1000,'static','en','lid_38155_cID','cID','0',"submitForm('lid_38155_formFilterEventSearch');")};					
			}
			if(jsc==1)
			{
				document.getElementById("evf").style.display = "none";
				document.getElementById("ntf").style.display = "";
				document.getElementById("pf").style.display = "none";		
				document.getElementById("searchLink").style.display = "none";
				if(!lid_38155_q2_autobox){lid_38155_q2_autobox=new AutoSuggestion('lid_38155_input2','/pages/mod/v4/archive/SearchSuggestion.asp',100,1,20,50,1000,'static','en','lid_38155_tID','tID','1',"submitForm('lid_38155_formFilterTeamSearch');")};
			}
			if(jsc==2)
			{
				document.getElementById("evf").style.display = "none";
				document.getElementById("ntf").style.display = "none";
				document.getElementById("pf").style.display = "";			
				document.getElementById("searchLink").style.display = "";				
				if(!lid_38155_q3_autobox){lid_38155_q3_autobox=new AutoSuggestion('lid_38155_input3','/pages/mod/v4/archive/SearchSuggestion.asp',250,2,20,50,1000,'dynamic','en','lid_38155_pID','pID','2',"submitForm('lid_38155_formFilterPlayerSearch');")};
			}
			setCookie('Csc',selected,'','/');
		}
		
		function SubmitSelected()
		{
			if(jsc==0)
				submitForm('lid_38155_formFilterEventSearch');
			if(jsc==2)
				submitForm('lid_38155_formFilterPlayerSearch');
		}

		function SetGBCookie()
		{
			if(jsc==2)
				setCookie('CBtoSRP',0,'','/');
		}
		
