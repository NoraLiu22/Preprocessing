    <script type="text/javascript">
        var labelType, useGradients, nativeTextSupport, animate;

        (function () {
            var ua = navigator.userAgent,
            iStuff = ua.match(/iPhone/i) || ua.match(/iPad/i),
            typeOfCanvas = typeof HTMLCanvasElement,
            nativeCanvasSupport = (typeOfCanvas == 'object' || typeOfCanvas == 'function'),
            textSupport = nativeCanvasSupport
            && (typeof document.createElement('canvas').getContext('2d').fillText == 'function');
            //I'm setting this based on the fact that ExCanvas provides text support for IE
            //and that as of today iPhone/iPad current text support is lame
            labelType = (!nativeCanvasSupport || (textSupport && !iStuff)) ? 'Native' : 'HTML';
            nativeTextSupport = labelType == 'Native';
            useGradients = nativeCanvasSupport;
            animate = !(iStuff || !nativeCanvasSupport);
        })();

        $(function () {
            var json;
            $("#infovis").width(getWidth(350));
            $("#infovis").height(getHeight(600));
            var sizeSeries = "";
            var colorSeries= "";

            var indicators = window.location.hash.split(":");
            if(indicators.length > 0 && sizeSeries == "")
            {   
                sizeSeries = indicators[0].replace("#","");
                colorSeries = indicators[1];
            }

            if (sizeSeries != "") {
                getTreemapData(sizeSeries + "," + colorSeries);
                $("select[name=listSeries] option[value=" + sizeSeries + "]").attr("selected", true);
                $("select[name=listColor] option[value=" + colorSeries + "]").attr("selected", true);
            }

            $("#listSeries").change(function () {
                var seriesCode = $(this).val();
                var colorSeriesCode = $("#listColor").val();
                if (seriesCode != "" && colorSeriesCode != "") {
                    getTreemapData(seriesCode + "," + colorSeriesCode);
                    window.location.hash = seriesCode + ":" + colorSeriesCode;
                } else {
                    return false;
                }
            });

            $("#listColor").change(function () {
                var seriesCode = $(this).val();
                var colorSeriesCode = $("#listSeries").val();
                if (seriesCode != "" && colorSeriesCode != "") {
                    getTreemapData(colorSeriesCode + "," + seriesCode);
                    window.location.hash = colorSeriesCode + ":" + seriesCode;
                } else {
                    return false;
                }
            });


            function updateHash() {
                var cs = [];
                $('input:checkbox:checked').each(function (i) {
                    cs.push($(this).attr("id"));
                });
                if (cs.length > 0) {
                    window.location.hash = "country=" + cs.join(":");
                //$("#permalink").attr("href", window.location.href);
                }
            }

            //url: "/visualization/treemap/GetData/" + seriesCode,
            function getTreemapData(seriesCode) {
                $.ajax({
                    url: "/facts/visualizations/treemap/GetData/" + seriesCode,
                    type: "post",
                    dataType: "json",
                    success: function (dataValue) {
                        plot(dataValue);
                        /*
                        //alert(dataValue[0].Country.ShortName);
                        //$("#countryRequested").html(str);
                        //$("#countryRequested").html(dataValue.Country.Id);
                        //$("#seriesRequested").html(dataValue.Series.Id);
                        */
                    },
                    error: function () {
                        $("#message").html("No data available");
                        $("#message").show();
                        $("#message").fadeOut(4000);
                    }
                });
            }

            function plot(json) {
                $("#infovis").width(getWidth(350));
                $("#infovis").height(getHeight(600));

                $("#infovis").empty();
                var tm = new $jit.TM.Squarified({
                    //where to inject the visualization  
                    injectInto: 'infovis',
                    //parent box title heights  
                    titleHeight: 15,
                    //enable animations  animate: animate,
                    //box offsets  
                    offset: 1,
                    //Attach left and right click events  
                    Events: {
                        enable: true,
                        onClick: function (node) {
                            if (node) tm.enter(node);
                        },
                        onRightClick: function () {
                            tm.out();
                        }
                    },
                    duration: 1000,
                    //Enable tips  
                    Tips: {
                        enable: true,
                        //add positioning offsets  
                        offsetX: 20,
                        offsetY: 20,
                        //implement the onShow method to  
                        //add content to the tooltip when a node  
                        //is hovered  
                        onShow: function (tip, node, isLeaf, domElement) {
                            var html = "<div class=\"tip-title\">" + node.name + "</div><div class=\"tip-text\">";
                            var data = node.data;
                            if (data.value) {
                                html += "Size value: " + data.value;
                            }
                            //if (data.image) {html += "<img src=\"" + data.image + "\" class=\"album\" />";}
                            tip.innerHTML = html;
                        }
                    },
                    //Add the name of the node in the correponding label  
                    //This method is called once, on label creation.  
                    onCreateLabel: function (domElement, node) {
                        domElement.innerHTML = node.name;
                        var style = domElement.style;
                        style.display = '';
                        style.color = 'yellow',
                    style.border = '1px solid transparent';
                        domElement.onmouseover = function () {
                            style.border = '1px solid #9FD4FF';
                        };
                        domElement.onmouseout = function () {
                            style.border = '1px solid transparent';
                        };
                    }
                });
                tm.loadJSON(json);
                tm.refresh();

                var back = $jit.id('back');
                $jit.util.addEvent(back, 'click', function () {
                    tm.out();
                });
            }
        });
        function getWidth(minWidth) {
            var width = $("#leftPanel").width();
            //var width = $(window).width() - $("#rightPanel").outerWidth(true) - 70;
            if (width < minWidth) { width = minWidth; }
            return width;
        }
        function getHeight(minHeight) {
            var height = $(window).height() - $("#controls").outerHeight(true) - $("#footer").outerHeight(true) - 75;
            if (height < minHeight) { height = minHeight; }
            return height;
        }
    </script>
