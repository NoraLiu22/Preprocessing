<script>
    var tvVideoString = "https://players.brightcove.net/6057940551001/default_default/index.html?videoId=6351502093112";
    var sbjtvModal = document.getElementById("mySBJTVVideoModal");
    var sbjtvButton = document.getElementById("videoButton-tv");
    var sbjtvClosebutton = document.getElementsByClassName("SBJTV-close-video")[0];

    sbjtvButton.onclick = function () {
        sbjtvModal.style.display = "block";
        document.getElementById("SBJTV-modal-video").setAttribute("src", tvVideoString);
    }

    sbjtvClosebutton.onclick = function () {
        sbjtvModal.style.display = "none";
        document.getElementById("SBJTV-modal-video").setAttribute("src", "");
    }

    window.addEventListener("click", function (event) {
        if (event.target == sbjtvModal) {
            sbjtvModal.style.display = "none";
            document.getElementById("SBJTV-modal-video").setAttribute("src", "");
        }
    });

    document.addEventListener("keydown", e => {
        if (e.keyCode == 27) {
            sbjtvModal.style.display = "none";
            document.getElementById("SBJTV-modal-video").setAttribute("src", "");
        }
    })
</script>    <div class="clone-properties hidden-tablet">
