<script type="text/javascript">

    function Entity(name, id) {
        this.name = name;
        this.id = id;
    }

    function pop(id) {
        const videoString = 'https://stub.sbjatlas.com/listing/preview?id=';
        var windowH = $(window.top).height();
        const allId = id.split('-');
        const actualId = allId[2];
        const entityName = allId[3];
        const modalId = 'entity-modal-' + actualId;
        let atlasModal = document.getElementById(modalId);
        const elem = document.getElementById(id);
        const rect = elem.getBoundingClientRect();

        const contentId = 'entity-content-' + actualId;
        let contentEl = document.getElementById(contentId);
        let position = parseInt(windowH) - parseFloat(rect['y'])

        atlasModal.setAttribute("src", videoString + actualId + "&tag=" + entityName);

        contentEl.classList.toggle('entity-content-hidden');

        if (position >= windowH / 2) {
            contentEl.style.removeProperty('bottom');
            contentEl.style.top = '1%';
        }
        else {
            contentEl.style.removeProperty('top');
            contentEl.style.bottom = '1%';
        }
    }


    $(document).ready(function () {
        let keys = [];
        let values = [];
        let entities = [];
        let usedEntities = [];
        var model = [{"Key":"7265c7c48b8a48f8b2223e2327f47780","Value":"NFL"},{"Key":"5866e7009e2945faa56f098d37eb4b3e","Value":"ESPN"},{"Key":"e7d6522609374407b2b132914fdae311","Value":"NCAA"},{"Key":"7265c7c48b8a48f8b2223e2327f47780","Value":"NFL"},{"Key":"987d94d539e14cd98adeabfa61dad119","Value":"Amazon"},{"Key":"8e3255cc646842ec94aea9fbaeded79f","Value":"Comcast"},{"Key":"5866e7009e2945faa56f098d37eb4b3e","Value":"ESPN"},{"Key":"9ee4f0a844654ff39f3e996c8c11250b","Value":"MLB"},{"Key":"b96a87bde0134ff1b2640d9de16ee7c0","Value":"NBA"},{"Key":"7265c7c48b8a48f8b2223e2327f47780","Value":"NFL"},{"Key":"1925d2008b654e0a964ff67cab5e1383","Value":"TNT Sports"},{"Key":"241e651eccf34671a3d233a98582fe5e","Value":"WNBA"},{"Key":"460994a50ee94fbe9e7ab0dcd5cc7ec3","Value":"Yankees"},{"Key":"50ef74e95d1543558037a4fe10927928","Value":"Bears"},{"Key":"95dc33471a534886865c5abeec0e4b5b","Value":"Chiefs"},{"Key":"5866e7009e2945faa56f098d37eb4b3e","Value":"ESPN"},{"Key":"6912946f971c4faea9b0751c5e288f03","Value":"Royals"},{"Key":"ab2839f4cf83456ea2d386cce192d4c5","Value":"Galaxy"},{"Key":"3f328da420d943efb43962dd635ff92b","Value":"Gillette Stadium"},{"Key":"bafbaacc911542c0be7fcb446c10a371","Value":"MLS"},{"Key":"200e621ba8d3427fad32242dbbb58317","Value":"Revolution"},{"Key":"e0365f05197a40fd80717293efd8f187","Value":"Astros"},{"Key":"529706ab6b0c4aa0b392bd9e5c0f08aa","Value":"Rockies"},{"Key":"b9c177a75290491a89e7ebdc70133701","Value":"Ian Poulter"},{"Key":"596c28e578274ac7bc579cf365406e75","Value":"PGA Tour"},{"Key":"88c5a05aa9db481b8be11f201926e26b","Value":"Josef Newgarden"},{"Key":"5866e7009e2945faa56f098d37eb4b3e","Value":"ESPN"},{"Key":"35e9d6ec97b9480789d6b1c63a05b882","Value":"UFC"},{"Key":"786f1e0b665d47a3891b74d1e9c9afab","Value":"American Airlines Center"},{"Key":"c1851a9bd62c46098fc7f1147a109164","Value":"Chevrolet"},{"Key":"4f4b1b92c9bd426d96c753fae8e1dcf3","Value":"Jordan Brand"},{"Key":"760ec622fb5f4b39a382602cf7a1cc0f","Value":"Luka Doncic"},{"Key":"c20e5bc11e814c6d