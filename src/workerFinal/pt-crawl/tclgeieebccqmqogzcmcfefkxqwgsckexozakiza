			<script>
				// Require the Dialog class
				dojo.require("dijit.Dialog");
				// Show the dialog
				function showdefinitions() {
					dijit.byId("definitions").show();
					gtag('event','ui', {'type': 'building links','value':'definition','site':'desktop','version':appVersion});
				}
				// Hide the dialog
				function hidedefinitions() {
					dijit.byId("definitions").hide();
