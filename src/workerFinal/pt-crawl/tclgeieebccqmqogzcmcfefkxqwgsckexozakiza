				<script>
					// Require the Dialog class
					dojo.require("dijit.Dialog");
					// Show the dialog
				   function showWelcomeDialog() {
						dijit.byId("Welcome").show();
					}
					// Hide the dialog
					function hideWelcomeDialog() {
						setTimeout(function() {
							document.getElementById('Welcome').style.display = 'none';
							$('#blackscreen').hide();
						}, 500);
					}
					$('#shareform>div>object')[0].data = window.location.origin+window.location.pathname.split('index.html')[0]+"share.html";
				</script>

