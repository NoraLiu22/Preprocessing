<div id="google-calendar"><div style="height:calc(5 * 100px);"></div></div>
<script>
/* use a function for the exact format desired... */
function ISODateString(d){
 function pad(n){return n<10 ? '0'+n : n}
 return d.getUTCFullYear()+'-'
      + pad(d.getUTCMonth()+1)+'-'
      + pad(d.getUTCDate())+'T'
      + pad(d.getUTCHours())+':'
      + pad(d.getUTCMinutes())+':'
      + pad(d.getUTCSeconds())+'Z'}

var d = new Date();

var maxResults  = "5";
var calGroup    = "4qk9vprcil262e3t2r1cnaiqu8@group.calendar.google.com";
var calendarUrl = "https://www.googleapis.com/calendar/v3/calendars/" + calGroup + "/events?key=AIzaSyClbI-VxJ01Mxli6YMmoq0D0JVBNXLHjtw&orderBy=startTime&singleEvents=true&timeMin=" + d.toISOString() + "&maxResults=" + maxResults + "&callback=?";
var eventLinks = "true";

function getDayName(dateStr, locale)
{
    var date = new Date(dateStr);
    return date.toLocaleDateString(locale, { weekday: 'long' });        
}

function getAmPmTime(time_string){
  var time_array = time_string.split(":");
  var hour_int = parseInt(time_array[0]);
  var minute_string =  time_array[1];
  var time_string;
  if (hour_int - 12 >= 0 ){
    if ( hour_int == 12 ){ 
      hour_int = 24;
    }
    time_string = (hour_int - 12) + ":" + minute_string + " PM";
  } else {
    time_string = hour_int + ":" + minute_string + " AM";
  }
  return time_string;
}

function buildCalendar(url){
    var calContent  = "";
	var months = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
  	var full_months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
    $.getJSON(url, function (data){
        var entries = data.items;//feed.entry;
        if(entries.length == 0){
        	calContent += "<p>No Events Available</p>";
        	$("#google-calendar").html(calContent);
            return;
        }
        $.each(entries, function(i, loc){
          //console.log(i,loc);
            var title       = loc.summary;//title.$t;
          	var description = "";
            if(loc.description){
            	description = loc.description;//content.$t;
              	description = description.split(" ").slice(0, 20).toString().replace(/,/g, " ") + "...";
          	}
          var timezone = loc.start.timeZone;
          if (timezone == undefined){ timezone = 'America/New_York'; }
          if(loc.start.date){
				var date = loc.start.date;//content.$t;
          		var full_date = loc.start.date;//content.$t;
            	var start_time = "";
          }
          else{
				var date = loc.start.dateTime;
          		var full_date = loc.start.dateTime;
				var start_time = " @ " + getAmPmTime(loc.start.dateTime.split("T")[1].split("-")[0]);
          }
          date = date.substring(0, 10);
          full_date = full_date.substring(0, 10);
          
			var num_date = months[date.substring(5,7) - 1] + " " + date.substring(8,10) + " " + date.substring(0,4);
          
          var day_of_week = getDayName(num_date, "En")

          
          if (parseInt(date.substring(8,10)) < 10){
            //date is 1 digit, omit leading 0
            date = "<span class='date_month'>" + months[date.substring(5,7) - 1] + "</span><br><div class='date_day'>" + date.substring(9,10) + "</div>";
            full_date = full_months[full_date.substring(5,7) - 1] + " " + full_date.substring(9,10);
            //year:  + ", " + full_date.substring(0,4)
          } else {
            date = "<span class='date_month'>" + months[date.substring(5,7) - 1] + "</span><br><div class='date_day'>" + date.substring(8,10) + "</div>";
            full_date = full_months[full_date.substring(5,7) - 1] + " " + full_date.substring(8,10);
            //year:   + ", " + full_date.substring(0,4)
          }
          	
            var link        = loc.htmlLink;
            
            calContent += "<div class='feedsb'>";
            if(eventLinks == "false"){
              calContent += "<div class='row'><div class='large-2 medium-3 small-3 columns'><div id='event_date'><h4><strong>" + date + "</strong></h4></div></div><div class='large-10 medium-9 small-9 columns' id='event_title_container'><div class='event_title'>" + title + "<BR>" + day_of_week + " " + full_date + " " + start_time + "</div></div>";
              
            }
            else{
              //link is enabled
              calContent += "<div class='row'><div class='large-2 medium-3 small-3 columns'><div id='event_date'><h4><strong><a href='" + link + "&ctz=America/New_York' target='_blank'>" + date + "</a></strong></h4></div></div><div class='large-10 medium-9 small-9 columns' id='event_title_container'><div class='event_title'>" + title + "<BR>" + day_of_week + " " + full_date + " " + start_time + "</div></div>";
              
             }
            calContent += "</div>";
        })
        $("#google-calendar").html(calContent);
    })
};

$(document).ready(function(){
	buildCalendar(calendarUrl);
});
</script><p>This calendar is dedicated to Extension system-wide meetings and events, district meetings and in-services. For help in adding or promoting an event, please contact Darryl Palmer at <a href="mailto:golem@ufl.edu">golem@ufl.edu.</a>&nbsp;</p></div> 

     <a id="FullCalendarLink" href="https://ifas.ufl.edu/extension-calendar-events/"><button class="uf-dotted-blue-border-button button large hollow" style="margin-top: 1rem;font-size: 1rem!important;text-transform: uppercase;color: #0021a5;padding: .6rem 1rem .6rem 1rem; border: 3px solid #0021a5;float:right;margin-right:25px;margin-bottom:0;" type="submit">View Full Calendar</button></a>
      </div>
    </div>
