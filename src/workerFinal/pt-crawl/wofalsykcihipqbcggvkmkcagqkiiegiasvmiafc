      }
      
      jQuery('#player1004195').click(function(){
        jQuery('div#yticon_1004195').hide();
        
        window.YT.ready(function() {
        
        if ( vid_id_element1004195.includes('list') ){
          var list_id1004195 = vid_id_element1004195.split("=")[1];
          
          var tag = document.createElement('script');
          tag.src = "https://www.youtube.com/iframe_api";
          var firstScriptTag = document.getElementsByTagName('script')[0];
          firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
            
          player1004195 = new window.YT.Player('player1004195', {
            height: '390',
            width: '640',
            playerVars: {
              'playsinline': 1,
              'listType':'playlist',
              'rel':0,
              'list': list_id1004195
            },
            events: {
              'onReady': onPlayerReady1004195,
              'onStateChange': onPlayerStateChange1004195
            }
          });// end player constructor
        } else {
          
          var tag = document.createElement('script');
          tag.src = "https://www.youtube.com/iframe_api";
          var firstScriptTag = document.getElementsByTagName('script')[0];
          firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
          
          player1004195 = new window.YT.Player('player1004195', {
            height: '390',
            width: '640',
            videoId: vid_id_element1004195,
            playerVars: {
              'playsinline': 1,
              'rel':0,
            },
            events: {
              'onReady': onPlayerReady1004195,
              'onStateChange': onPlayerStateChange1004195
            }
          });// end player constructor
        } // end if else
          
        });//end window.YT.ready(function() {
          
      });//end function
      

      // The API will call this function when the video player is ready.
      var pauseFlag1004195 = false;
      function onPlayerReady1004195(event) {
        event.target.playVideo();
      }
      
      function convert_min_decimal_to_secs(time){
		var time_array = time.split(".");
		var dec = time_array[1];
		var sec = dec * .6;
		var new_time = time_array[0] + ':' + sec.toFixed(2);
		return new_time;
	  }
      
      function onPlayerStateChange1004195(event) {
        var state = player1004195.getPlayerState();
        var title = player1004195.getVideoData().title;
        var time = player1004195.getCurrentTime()
        if (time > 59){
            time = time/60;
     		time = time.toFixed(2);
    		time = convert_min_decimal_to_secs(time);
	     	//console.log(time);
        } else {
          time = time.toFixed(2) + ' sec ';
        }
        var label = title + ' -- ' + time;

        switch(state) {
          case 0:
            ga('send', {
              hitType: 'event',
              eventCategory: 'Videos',
              eventAction: 'Finished',
              eventLabel: label
            });
            break;
          case 1:
            ga('send', {
              hitType: 'event',
              eventCategory: 'Videos',
              eventAction: 'Play',
              eventLabel: label
            });
            break;
          case 2:
            ga('send', {
              hitType: 'event',
              eventCategory: 'Videos',
              eventAction: 'Pause',
              eventLabel: label
            });
    
		}//end switch
      }// end state change function

      function stopVideo1004195() {
        // alert('stop');
        player.stopVideo();
        ga('send', {
          hitType: 'event',
          eventCategory: 'Videos',
          eventAction: 'Stop',
          eventLabel: document.title + '1004195'
        });
      }
      
