<script type="text/javascript">
jQuery(document).ready(function($) {	   		
	
    $('#slider').nivoSlider({
		directionNav: false,
		directionNavHide: false,
		effect: 'slideInLeft',
		pauseTime: 10000,		
		captionOpacity: 1
	});	
	
	$('#tabNavigation a').click(function(event) {
		event.preventDefault();			
		var rel = $(this).attr('rel');			
		$('.tabcontent').hide();
		$('#'+rel).show();			
		// Set on status
		$('#tabNavigation a').removeClass('on');
		$('.'+rel).addClass('on');		
	});		
	
	$('a.tab').click(function(event) {
		event.preventDefault();			
		var rel = $(this).attr('rel');			
		$('#'+rel).show();			
		$('.'+rel+'hide').hide(); // Optional	
		$('.'+rel).addClass('on');		
	});	
	$('a.innertab').click(function(event) {
		event.preventDefault();			
		var rel = $(this).attr('rel');			
		$('#'+rel).show();			
		$('.'+rel+'hide').hide(); // Optional	
		$('.'+rel).addClass('on');		
	});			
	
	$('a.minimise').click(function(event) {
		event.preventDefault();			
		var rel = $(this).attr('rel');			
		$('#'+rel).hide();			
		$('.'+rel+'hide').show(); // Optional	
		$('.'+rel).removeClass('on');		
	});			
	
	$('a.close').click(function(event) {
		event.preventDefault();		
		var rel = $(this).attr('rel');	
		$('#'+rel).hide();			
		$('.'+rel+'hide').show(); // Optional
		$('.'+rel).removeClass('on');		
	});
	
	
	
	$('#countryselect').change(function() {
		if( $('#countryselect').val() ){	
			ga('send', 'event', 'DropDown', 'CountrySelect');
			window.location = "https://www.commonwealthofnations.org/country/"+$('#countryselect').val();
		}
	});	
	
	$('#browseformcountry').change(function() {
		if( $('#browseformcountry').val() && $('#browseformsection').val()  ){
			ga('send', 'event', 'DropDown', 'CountrySectorSelect');
			window.location = "https://www.commonwealthofnations.org/" + $('#browseformsection').val() + "/" + $('#browseformcountry').val();
		}
	});	
	$('#browseformsection').change(function() {
		if( $('#browseformcountry').val() && $('#browseformsection').val()  ){
			ga('send', 'event', 'DropDown', 'CountrySectorSelect');
			window.location = "https://www.commonwealthofnations.org/" + $('#browseformsection').val() + "/" + $('#browseformcountry').val();
		}
	});	
	
	$('#sectiontoselect1').change(function() {
		if( $(this).val() ){
			ga('send', 'event', 'DropDown', 'Section1Select');			
			window.location = $(this).val();
		}
	});	
	$('#sectiontoselect2').change(function() {
		if( $(this).val() ){
			ga('send', 'event', 'DropDown', 'Section2Select');			
			window.location = $(this).val();
		}
	});	
	$('#sectiontoselect3').change(function() {
		if( $(this).val() ){
			ga('send', 'event', 'DropDown', 'Section3Select');			
			window.location = $(this).val();
		}
	});			
		
	$("a.continent").hover(
	  function () {
		 var rel = $(this).attr('rel');	
		 $(this).removeClass('continentslink');		  
		 $('#'+rel).addClass('on');		  
		 if(old_ie !== true){
		 	 $('a.continentslink').stop().fadeTo('fast',0.2);
			 $('#basemap').stop().fadeTo('fast', 0.3);
			 $(this).stop().fadeTo('slow', 1);
		 }
	  },
	  function () {
		 var rel = $(this).attr('rel');	
		 $('#'+rel).removeClass('on');		  
 	 	 $(this).addClass('continentslink');
		 if(old_ie !== true){		 		  
			 $('a.continentslink').stop().fadeTo('slow',1);
			 $('#basemap').stop().fadeTo('slow',1);
		 }
	  }
	);


	$("a.legend").hover(	  
	  function () {
		 var rel = $(this).attr('rel');	
		 $(this).addClass('on');
		 $('#'+rel).removeClass('continentslink');		
		 if(old_ie !== true){  
			 $('a.continentslink').stop().fadeTo('fast',0.2);
			 $('#basemap').stop().fadeTo('fast', 0.3);
			 $('#'+rel).stop().fadeTo('slow', 1);
		 }
	  },
	  function () {
		 var rel = $(this).attr('rel');	
		 $(this).removeClass('on');
	 	 $('#'+rel).addClass('continentslink');		  
 		 if(old_ie !== true){
			 $('a.continentslink').stop().fadeTo('slow',1);
			 $('#basemap').stop().fadeTo('slow',1);
		 }
	  }
	);	
			
	if( !$('body').hasClass('vold_ie') ){
		var tallesttab = 0;
		
		$('.scrolling .tabcontent').each(function() {
			
			if( $(this).height() > tallesttab ){
				tallesttab = $(this).height();
			}            
		});        
		
		if( tallesttab > 230 ){
			$('.scrolling').addClass('tall');
			$('.scrolling .tabcontent table').css('width','640px');		
			$('.scrolling').append('<a href="#" class="readmore"><span>Read more</span></a>');		
			ga('send', 'event', 'ReadMore', 'Display');
		}   	
	
		$('.scrolling a.readmore').click(function(event) {
			event.preventDefault();       
			$('.scrolling').removeClass('tall');
			$('.scrolling a.readmore').hide();
			ga('send', 'event', 'ReadMore', 'Click');
		});			
	}
});
</script>



