(function() {
    var xhr = new(XMLHttpRequest || ActiveXObject)('MSXML2.XMLHTTP.3.0');
    xhr.open('GET', '/blaize/datalayer', true);
    xhr.onreadystatechange = function() {
        if (xhr.readyState === 4) {
            var response;
            try {
                response = JSON.parse(xhr.response);
            } catch (e) {
                response = xhr.response;
            }
            if (xhr.status === 200) {
                for (var dataLayerFieldName in response) {
                    if (! (dataLayerFieldName in window)) {
                        window[dataLayerFieldName] = [];
                    }
                    var blaizeUserData = response[dataLayerFieldName] || {};

                    blaizeUserData.userStatus = blaizeUserData.loggedIn ? 'authenticated' : 'anonymous';

                    if (blaizeUserData.platinum_bundle) {
                        blaizeUserData.subscriptionType = 'platinum';
                    } else if (blaizeUserData.network_bundle) {
                        blaizeUserData.subscriptionType = 'network';
                    } else if (blaizeUserData.gold_bundle) {
                        blaizeUserData.subscriptionType = 'gold';
                    } else if (blaizeUserData.platinum_trial_bundle) {
                        blaizeUserData.subscriptionType = 'platinum_trial';
                    } else if (blaizeUserData.gold_trial_bundle) {
                        blaizeUserData.subscriptionType = 'gold_trial';
                    } else if (blaizeUserData.loggedIn) {
                        blaizeUserData.subscriptionType = 'bronze';
                    }

                    // if ('loggedIn' in blaizeUserData) { delete blaizeUserData.loggedIn; }
                    // if ('platinum_bundle' in blaizeUserData) { delete blaizeUserData.platinum_bundle; }
                    // if ('gold_bundle' in blaizeUserData) { delete blaizeUserData.gold_bundle; }
                    // if ('platinum_trial_bundle' in blaizeUserData) { delete blaizeUserData.platinum_trial_bundle; }
                    // if ('gold_trial_bundle' in blaizeUserData) { delete blaizeUserData.gold_trial_bundle; }
                    
                    for (var key in blaizeUserData) {
                        if (blaizeUserData[key] === null || blaizeUserData[key] === undefined) {
                            delete blaizeUserData[key];
                        }
                    }
                    

                    window[dataLayerFieldName].push(blaizeUserData);
                    
                    // Trigger custom event
		            setTimeout(function() { document.dispatchEvent( new CustomEvent( 'pei_datalayer_ready' ) ) }, 1500 );
                }
            }
        }
    }
    xhr.send({});
})();
