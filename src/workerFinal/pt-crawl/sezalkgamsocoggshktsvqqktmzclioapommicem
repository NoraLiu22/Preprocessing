<script>
jQuery( document ).ready(function($) {
    $('#phoneNumber').attr('title', '01234567890');
    var emailRegex = /^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()\[\]\.,;:\s@\"]+\.)+[^<>()\[\]\.,;:\s@\"]{2,50})$/;
    const checkCorrectEmailAfterChange = function () {
        removeIncorrectCharsFromEmail()
        var email = $('#reg_email').val();
        if (emailRegex.test(email)) {
            $('#reg_email').removeClass('wrong-email');
        } else {
            email.length > 0 ? $('#reg_email').addClass('wrong-email') : $('#reg_email').removeClass('wrong-email');
        }
    }

    const removeIncorrectCharsFromEmail = function () {
        var email = $('#reg_email').val();
        var email = email.replace(/[\\\/]/g, '');
        $('#reg_email').val(email);
    }   

    $('#reg_email')
    .attr('pattern','^(([^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+(\\.[^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+)*)|(\\x22.+\\x22))@(([^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+\\.)+[^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]{2,50})$')
    .attr('type','text')
    .attr('title','Work E-mail')
    .attr('placeholder', 'Work E-mail')
    .keypress(removeIncorrectCharsFromEmail)
    .keyup(removeIncorrectCharsFromEmail)
    .change(checkCorrectEmailAfterChange);

    $('#login_email')
    .attr('pattern','^(([^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+(\\.[^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+)*)|(\\x22.+\\x22))@(([^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]+\\.)+[^\\<\\>\\(\\)\\[\\]\\.,;:\\s@\\x22]{2,50})$')
    .attr('title','Work Email')
    .attr('type','text')
    .attr('title','Work E-mail')
    .attr('placeholder', 'Work E-mail')
    .keypress(removeIncorrectCharsFromEmail)
    .keyup(removeIncorrectCharsFromEmail)
    .change(checkCorrectEmailAfterChange);

    $('#first_name')
    .attr('pattern',$('#first_name').attr('pattern').replace('\\u0080-\\uFFFF-','\\u0080-\\uFFFF\\-'));

    $('#last_name')
    .attr('pattern',$('#last_name').attr('pattern').replace('\\u0080-\\uFFFF-','\\u0080-\\uFFFF\\-'));

    $('#organisation')
    .attr('pattern',$('#organisation').attr('pattern').replace('[a-zA-Z0-9-&()\\[\\],. ]{1,150}$','[a-zA-Z0-9\\-\\x26\\(\\)\\[\\],\\. ]{1,50}$'));

    $('#phoneNumber')
    .attr('pattern',$('#phoneNumber').attr('pattern').replace('{9,100}$','{9,50}$'));

    $('.blaize-theme-default.blaize-form.blaize-form-login .blaize-title').text("Sign in to your account");
    $('#sourceRegistration').val('pfm');
    document.getElementById('sourceRegistration').style.display="none";
    
    var jobTitlesToHide = [
        'Chairman',
        'Chief Executive',
        'Controller',
        'Counsel',
        'Finance',
    ];
    
    var select = document.getElementById('jobTitle');

    for(var i =0;i<select.options.length;i++){
        if(jobTitlesToHide.indexOf(select.options[i].value) >= 0){
            select.remove(i)
            i--;
        }
    }
    function alphabetizeList() {
            var sel = $(select);
            var s = sel.val();
            var opts_list = sel.find('option');
            var first_option =  opts_list[0];
            opts_list = opts_list.slice(1);
            opts_list.sort(function (a, b) {
                return $(a).text() > $(b).text() ? 1 : -1;
            });
            sel.html('').append([first_option, ...opts_list]);
            sel.val(s);
        }
    alphabetizeList();
    
    var US_COUNTRY_CODE = '03c11ff0-072b-e011-aaa9-00155dc21b03'
    var stateSelectField = '<label for="nav-state">' + 
        'State' + 
        '<select name="state" id="nav-state" placeholder="State (US Only)" required>' + 
            '<option selected disabled value="">State</option>' + 
            '<option value="AL">Alabama</option>' + 
            '<option value="AK">Alaska</option>' + 
            '<option value="AS">American Samoa</option>' + 
            '<option value="AZ">Arizona</option>' + 
            '<option value="AR">Arkansas</option>' + 
            '<option value="CA">California</option>' + 
            '<option value="CO">Colorado</option>' + 
            '<option value="CT">Connecticut</option>' + 
            '<option value="DE">Delaware</option>' + 
            '<option value="DC">District of Columbia</option>' + 
            '<option value="FL">Florida</option>' + 
            '<option value="GA">Georgia</option>' + 
            '<option value="GU">Guam</option>' + 
            '<option value="HI">Hawaii</option>' + 
            '<option value="ID">Idaho</option>' + 
            '<option value="IL">Illinios</option>' + 
            '<option value="IN">Indiana</option>' + 
            '<option value="IA">Iowa</option>' + 
            '<option value="KS">Kansas</option>' + 
            '<option value="KY">Kentucky</option>' + 
            '<option value="LA">Louisiana</option>' + 
            '<option value="ME">Maine</option>' + 
            '<option value="MD">Maryland</option>' + 
            '<option value="MA">Massachusetts</option>' + 
            '<option value="MI">Michigan</option>' + 
            '<option value="MN">Minnesota</option>' + 
            '<option value="MS">Mississippi</option>' + 
            '<option value="MO">Missouri</option>' + 
            '<option value="MT">Montana</option>' + 
            '<option value="NE">Nebraska</option>' + 
            '<option value="NV">Nevada</option>' + 
            '<option value="NH">New Hampshire</option>' + 
            '<option value="NJ">New Jersey</option>' + 
            '<option value="NM">New Mexico</option>' + 
            '<option value="NY">New York</option>' + 
            '<option value="NC">North Carolina</option>' + 
            '<option value="ND">North Dakota</option>' + 
            '<option value="MP">Northern Mariana Islands</option>' + 
            '<option value="OH">Ohio</option>' + 
            '<option value="OK">Oklahoma</option>' + 
            '<option value="OR">Oregon</option>' + 
            '<option value="PA">Pennsylvania</option>' + 
            '<option value="PR">Puerto Rico</option>' + 
            '<option value="RI">Rhode Island</option>' + 
            '<option value="SC">South Carolina</option>' + 
            '<option value="SD">South Dakota</option>' + 
            '<option value="TN">Tennessee</option>' + 
            '<option value="TX">Texas</option>' + 
            '<option value="UT">Utah</option>' + 
            '<option value="VT">Vermont</option>' + 
            '<option value="VI">Virgin Islands</option>' + 
            '<option value="VA">Virginia</option>' + 
            '<option value="WA">Washington</option>' + 
            '<option value="WV">West Virginia</option>' + 
            '<option value="WI">Wisconsin</option>' + 
            '<option value="WY">Wyoming</option>' + 
        '</select>' + 
      '</label>'
    var observer = new MutationObserver(function(mutations) {
      mutations.forEach(function(mutation) {
        if (mutation.type === 'childList' && mutation.addedNodes[0] && mutation.addedNodes[0].textContent === 'Something went wrong! Please try again later.') {
            document.querySelector('.blaize-error').textContent = 'Please select US State';
        }
      });    
    });      
    var countryField = $('#country');
    $('#country').change(function (e) {
        var country = e.currentTarget.value;
        var stateEl = document.querySelector('#nav-state');
        if (country === US_COUNTRY_CODE) {
            countryField.after(stateSelectField);
        } else if (stateEl) {
            stateEl.value = "";
            stateEl.parentNode.removeChild(stateEl);
        }
    });
    window.Blaize.registerFormPreProcessor('registration-form', function (formData) {
        var navState = document.querySelector('#nav-state');
        formData.attributes.state = "";
        if (navState) formData.attributes.state = navState.value;
        if (document.querySelector('#country').value === US_COUNTRY_CODE && !Boolean(navState.value)) {
            observer.observe(document.querySelector('.blaize-error'), { attributes: true, childList: true, subtree: true });
            return 'US State field is required';            
        } else {
            observer.disconnect();
            return formData;
        }
    });  
     window.Blaize.registerFormPostProcessor('registration-form', function(formData, response) {
        var BLAIZE_SUCCESS_RESPONSE = 'Registration successful'
        function addParselyTrackPageView() {
            var PARSELY_BRAND = 'privatefundsmanagement.net'
            let el = document.createElement('script');
            el.src = 'https://cdn.parsely.com/keys/' + PARSELY_BRAND + '/p.js'
            el.onload = function(script) {
                console.log('parsely loaded')
                PARSELY.beacon.trackPageView({
                    url: location.href,
                    urlref: location.href,
                    action: "NewUserRegistration"
                });
            }
            document.body.appendChild(el);
        }
        function addGAEvent() {
            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                'event': 'visitor interaction',
                'interaction_name': 'registration',
            });
        }
        if (response.message === BLAIZE_SUCCESS_RESPONSE && response.tracking_id) {
            addParselyTrackPageView()
            addGAEvent()
        } else {
            console.log('registration error ', response.message)
        }
    });
});
</script>
