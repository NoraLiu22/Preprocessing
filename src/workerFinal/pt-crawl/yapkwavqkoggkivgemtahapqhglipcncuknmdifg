	</script>


	<script id="attachements-template" type="text/x-attachementsTemplate">
		<tr>
				<td>
						<div class="sidebar__item sidebar__link"><a href='/attachments/@FileName_rst' id="aDownloadable" target="_blank">@Title_rst</a></div>
				</td>
		</tr>
	</script>

	<script id="breadCrumb-template-first" type="text/x-breadCrumbTemplate">
		<span class="breadcrumb__item ">
				<a data-LinkType="Breadcrumb" href="@URL_RST" class="breadcrumb__item breadcrumb__link">@Text_rst</a>
		</span>
	</script>

	<script id="breadCrumb-template" type="text/x-breadCrumbTemplate">
		<span class="breadcrumb__item breadcrumb__seperator"></span>
		<span class="breadcrumb__item ">
				<a data-LinkType="Breadcrumb" href="@URL_RST" class="breadcrumb__item breadcrumb__link">@Text_rst</a>
		</span>
	</script>

	<script type="text/javascript">
		// console.log("News start json processing.");
		if (window.location.href.split("?").length > 1) { window.history.pushState({}, document.title, window.location.pathname) };	

		var tagsList = "";
		var hasSidebar = false;
		//***
		//*** load json
		//***
		var pathname = ""
		pathname = window.location.pathname;
		pathname = (pathname.endsWith(".html")) ? "/JSON" + pathname.replace(".html", ".json") : "/JSON" + pathname + ".json";


		SiteNameSpace.ajaxCall({
			url: pathname,
			type: "GET",
			parameters: "",
			WebServiceResult: false,
			errorMessage: "Error: Unable to get " + pathname,
			success: function (results) {

				if (results != null) {
					//***set the site data

					var NewsItem = results;
					// console.log("Boo = " + NewsItem.Name_rst);

					//***
					//*** merge into tempate onto page
					//***

					//*** merge meta into tempate onto page
					if (NewsItem.Metadata_Title_rst.length > 0) $("#Head").append("<meta name=\"Title\" content=\"" + NewsItem.Metadata_Title_rst + "\" >");
					if (NewsItem.Metadata_Description_rst.length > 0) $("#Head").append("<meta name=\"Description\" content=\"" + NewsItem.Metadata_Description_rst + "\" >");
					if (NewsItem.Keywords_rst.length > 0) $("#Head").append("<meta name=\"Keywords\" content=\"" + NewsItem.Keywords_rst + "\" >");
					$("#Head").append("<meta name=\"ContentType\" content=\"News\" >");
					$("#Head").append("<meta name=\"TypeOfContent\" content=\"News\" >");

					//*** BreadCrumb list in top main column
					for (var i = 0; i < NewsItem.BreadCrumbs_rls.length; i++) {
						var relatedItem;

						if (i == 0) relatedItem = $('#breadCrumb-template-first').text(); //for the first item, the template is slightly differnt (no seperator)
						else relatedItem = $('#breadCrumb-template').text();

						relatedItem = relatedItem.replace("@Text_rst", NewsItem.BreadCrumbs_rls[i].bcname_rst);
						relatedItem = relatedItem.replace("@URL_RST", NewsItem.BreadCrumbs_rls[i].bcurl_rst);
						$("#lblBreadCrumbs").append(relatedItem);
					}


					//*** merge main content into tempate onto page
					$("#pageTitle").text(NewsItem.Name_rst);
					$("#h1_detail__header").text(NewsItem.Name_rst);
					$("#h2_detail__subtitle").text(NewsItem.Subtitle_rst);
					$("#div_detail__body").html(NewsItem.Body_rst);

					var date_lob = new Date(NewsItem.PublishDate_rdt);
					var date_lst = (date_lob.getMonth() + 1) + "/" + date_lob.getDate() + "/" + date_lob.getFullYear();

					$("#div_detail__date").text(date_lst);
					$("#div_detail__positionOffice").text(NewsItem.Byline_rst);
					//$("#div_detail_time").text(NewsItem.Event_DateLine2_rst);
					//$("#div_detail__location").text(NewsItem.Location_Name_rst);
					//$("#div_detail__address").text(NewsItem.Location_Address_rst);

					//	Let's send the page view to Google Analytics
					gtag('event', 'page_view', {
						page_title: NewsItem.Name_rst
					})


					//***hide the sidebar and other optional sections.  if data is found for a given sidebar, .show() will be called below.
					$("#mySideBarType1").hide();
					$("#mySideBarType2").hide();
					$("#mySideBarType3").hide();
					$("#mySideBarType4").hide();
					$("#mySideBarType5").hide();
					$("#divMedia").hide();



					//*** merge main image into tempate onto page
					var PageImage = NewsItem.Image_rob;
					if (PageImage !== null) {
						var imageString = "<img src=\"/DDM_Gallery/" + PageImage.URL_rst + "\" id=\"img_detail__image\" alt=\"" + PageImage.AlternateText_rst + "\">";
						$("#div_detail__image").append(imageString);
                        $("#div_detail__caption").text(PageImage.Credits_rst);
						$("#Head").append("<meta name=\"image\" content=\"/DDM_Gallery/" + PageImage.Thumbnail_rst + "\" >");

					};

					//*** merge tags list into tempate onto page
					for (var i = 0; i < NewsItem.Tags_rls.length; i++) {
						var tagsItem = $('#tagsItem-template').text();
						tagsItem = tagsItem.replace("@title", NewsItem.Tags_rls[i].shortName_rst);
						tagsItem = tagsItem.replace("@url", NewsItem.Tags_rls[i].url_rst);
						tagsItem = tagsItem.replace("@text", NewsItem.Tags_rls[i].name_rst);
						$("#divTagsList").append(tagsItem);
						$("#Head").append("<meta name=\"Topic\" content=\"" + NewsItem.Tags_rls[i].name_rst + "\" >");
					}


					//*** merge related list into tempate onto page
					for (var i = 0; i < NewsItem.RelatedContent_rls.length; i++) {
						var relatedItem = $('#related-template').text();
						relatedItem = relatedItem.replace("@url", NewsItem.RelatedContent_rls[i].url_rst);
						relatedItem = relatedItem.replace("@text", NewsItem.RelatedContent_rls[i].content_name_rst);
						$("#myRelatedContent_dlItems").append(relatedItem);
					}

					//*** merge related media list into tempate onto page
					for (var i = 0; i < NewsItem.Media_rls.length; i++) {
						var relatedItem = $('#relatedMedia-template').text();
						relatedItem = relatedItem.replace("@Filename_rst", NewsItem.Media_rls[i].Filename_rst);
						relatedItem = relatedItem.replace("@AlternateText_rst", NewsItem.Media_rls[i].AlternateText_rst);
						relatedItem = relatedItem.replace("@Thumbnail_rst", NewsItem.Media_rls[i].Thumbnail_rst);
						relatedItem = relatedItem.replace("@Title_rst", NewsItem.Media_rls[i].Title_rst);
						relatedItem = relatedItem.replace("@Parent_url_rst", NewsItem.Media_rls[i].parent_url_rst);
						$("#ulMedia").append(relatedItem);
					}

					if (NewsItem.Media_rls.length == 0) {
						$("#divMedia").hide();
					} else {
						$("#divMedia").show();
					}





					//*** program list in main content column
					for (var i = 0; i < NewsItem.Programs_rls.length; i++) {
						//OfficePrograms.Visible = True
						//CType(OfficePrograms, OfficeProgramsList).listProgram = _lPrograms
					}



					//*** office list in main content column
					for (var i = 0; i < NewsItem.Offices_rls.length; i++) {
						//OfficesList.Visible = True
						//CType(OfficesList, OfficesList).listOffices = _lOffices
					}


					//*** Attachements list in right content column
					for (var i = 0; i < NewsItem.Attachments_rls.length; i++) {
						var relatedItem = $('#attachements-template').text();
						relatedItem = relatedItem.replace("@FileName_rst", NewsItem.Attachments_rls[i].FileName_rst);
						relatedItem = relatedItem.replace("@Title_rst", NewsItem.Attachments_rls[i].Title_rst);
						$("#tbodyAttachments").append(relatedItem);
						$("#mySideBarType3").show();
						hasSidebar = true;
					}



					if (!hasSidebar) {
						$("#content__wrapper").removeClass("sidebar-second");
					}

				}

			},
			error: function (xhr) {
				location = "/not-found.html";
			}






		}); //end ajaxCall
	</script>

</body>
