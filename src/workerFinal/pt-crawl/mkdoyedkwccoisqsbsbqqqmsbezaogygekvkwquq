<script>
	$(document).ready(function ()
	{
		// Ajax - 마이셀럽스
		$.ajax(
		{
			url: "https://gaonchart.api.mycelebs.com/admin/sum_list?weeks=202417",
			type: "GET",
			dataType: "json",
			headers: { "x-api-key": "JJEENnAM5F2Q5MNF8wywc8gh8JQiZ1LKaJL0iRV4" },
			async: false,
			success: function (data)
			{
				if (typeof data !== 'undefined')
				{
					var img = data['data'][0]['cit_url'];
					if (isEmptyStr(img) === false) { 
						img = img.replace("240@240", "573@372");
