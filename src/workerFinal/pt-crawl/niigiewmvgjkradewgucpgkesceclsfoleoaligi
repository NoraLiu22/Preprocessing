<article data-history-node-id="495" about="/about/privacy" typeof="schema:WebPage" class="node page page--full">
  
    <span property="schema:name" content="Duke University Libraries Privacy Statement" class="hidden"></span>


      
    
    <div class="node__content">
      
<div  class="panel-display dul-asidebox-option clearfix">

