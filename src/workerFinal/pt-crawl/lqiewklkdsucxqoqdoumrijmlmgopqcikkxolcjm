                var AdButler = AdButler || {}; AdButler.ads = AdButler.ads || [];
                var abkw = window.abkw || '';
                console.log("emp-cba8a-size-"+divsize);
                if(divsize >= 728){
                    var plc551849 = window.plc551849 || 0;
