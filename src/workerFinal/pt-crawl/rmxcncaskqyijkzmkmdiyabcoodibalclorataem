<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.1800.1200.jpg" media="(min-width: 600px) and (max-width: 899px) and (-webkit-min-device-pixel-ratio: 2.0)"  />
<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.900.600.jpg" media="(min-width: 600px) and (max-width: 899px)"  />
<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.3840.2560.jpg" media="(min-width: 900px) and (max-width: 1199px) and (-webkit-min-device-pixel-ratio: 2.0)"  />
<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.1200.800.jpg" media="(min-width: 900px) and (max-width: 1199px)"  />
<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.3840.2560.jpg" media="(min-width: 1200px) and (-webkit-min-device-pixel-ratio: 2.0)"  />
<source srcset="https://assets.ey.com/content/dam/ey-sites/ey-com/en_gl/topics/strategy-transactions/ey-factory-shot-of-shiseido-cosmetics-contains-being-filled.jpg.rendition.1800.1200.jpg" media="(min-width: 1200px) and (max-width: 1799px)"  />
