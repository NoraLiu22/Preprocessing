                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Information about HIDOE" class="ms-sitemapdirectional" href="/ConnectWithUs/Pages/home.aspx">Connect with Us</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/ConnectWithUs/Organization/Pages/home.aspx">Organization</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/ConnectWithUs/Organization/Offices/Pages/home.aspx">Offices</a></span><span> &gt; </span><span class="current">Civil Rights Compliance Branch (CRCB)</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">Civil Rights Compliance Branch (CRCB)</h2>
                
    



				
				<p class="intro">
					The CRCB, in the Office of Talent Management, is committed to the Hawaii State Department of Education&#39;s overall compliance with Federal, State, departmental Administrative Rules, and Policies, which strictly prohibit any form of discrimination.
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

