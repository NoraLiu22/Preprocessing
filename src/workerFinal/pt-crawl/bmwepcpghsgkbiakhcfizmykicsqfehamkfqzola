<script>
$(function(){
    $('#logout').attr("href", "/ac/logout/?next=/portal/dt/");
    $('#login').attr("href", "/ac/login/?next=/portal/dt/");
    $('#signup').attr("href", "/ac/signup/?next=/portal/dt/");
    var urlHash = window.location.hash;
    var found = false;
    if(urlHash){
        $('#accordion .panel-title a').each(function(){
            console.log($( this ).text());
            if("#"+$( this ).text() == urlHash){
                $( this ).parent().parent().next().collapse('show');
                found = true;
            }
        });
    }
    if(!found){
        $('#accordion .collapse').first().collapse('show');
    }
});
</script>
 
    <script>
      var lastScrollTop = 0;
      var height;
      var trigger = 350;
      $(function () {
        $(window).scroll(function (event) {
          var st = $(this).scrollTop();
          if (st > lastScrollTop) {
            $(".navbar-gplates").hide();
          } else {
            $(".navbar-gplates").show();
          }
          lastScrollTop = st;

          height = $(document).height() - $(window).height();
          //console.log(height+" "+$(this).scrollTop());
          if ($(this).scrollTop() > height - trigger) {
            $(".footer").slideDown(300);
          } else {
            $(".footer").slideUp(300);
          }

          //if ( ($(window).height() + 100) < $(document).height() ) {
          //    $('#top-link-block').removeClass('hidden').affix({
          //    });
          //};
        });
        $("#top-link-block")
          .removeClass("hidden")
          .affix({ offset: { top: 100 } });
      });
    </script>
    
