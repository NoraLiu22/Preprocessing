			var v3 = $("#wipoVideo3").get(0);

			var playVideo = function(obj) {
				if (obj.paused){
					obj.autoplay = true;
					obj.play();
				}
			}
			var stopVideo = function(obj) {
				if (!obj.paused){
					obj.pause();
					obj.currentTime = 0;
				}
			}
			$('#wipoLnk1').click(function (){
				playVideo (v1);
			});
			$('#wipoBtn1').click(function (){
				stopVideo(v1);
			});
			$('#wipoModal1').bind('hide', function (){
				stopVideo(v1);
			});
			$('#wipoLnk2').click(function (){
				playVideo (v2);
			});
			$('#wipoBtn2').click(function (){
				stopVideo(v2);
