    <script>
        $(function () {            
            var pageNo = 2;
            $(window).scroll(function () {
                if ($(this).scrollTop() + $(window).height() >= $(document).height() - $('footer').height()) {
                    var fetched = false;
                    if (!fetched) {
                        $.ajax({
                            async: true,
                            cache: false,
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            url: "/webService/SearchService.asmx/GetPostList",
                            data: "{ 'pPostType': '2', 'pWebsite':'" + 48 + "', 'pLangId':'" + 1 + "', 'pPageNo':'" + pageNo + "' }",
                        processData: false,
                        dataType: "json",
                        success: function (result) {
                            var obj = jQuery.parseJSON(result.d);
                            $.each(obj, function (i, item) {
                                var litem = '<div class="col-xl-6">' +
                                                '<div class="d-flex position-relative mb-3 item">' +
                                                    '<div class="flex-shrink-0 me-3">' +
                                                        '<div class="type">' + item.type + '</div>' + 
                                                        '<div>' + item.date + '</div>' +
                                                    '</div>' +
                                                    '<div>' +
                                        '<a href="/alcicek/tr/etkinlikTakvimi/' + item.uri + '" class="title">' +
                                        '<h6>' + item.title + '</h6>' +
                                        '</a>' +
                                        '<p class="location"><i class="fas fa-street-view"></i> ' + item.location + '</p>' +
                                        '</div>' +
                                        '</div>' +
                                        '</div>';
                                    $('.all-list').append(litem);
                                });
                                fetched = true;
                            },
                            error: function () {
                                $('.all-list').append("Kayıt bulunamadı!");
                            }
                        });
                    }
                }
            });
            $.ajax({
                async: true,
                cache: false,
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "/webService/SearchService.asmx/GetPostList",
                data: "{ 'pPostType': '2', 'pWebsite':'" + 48 + "', 'pLangId':'" + 1 + "', 'pPageNo':'" + 1 + "' }",
                processData: false,
                dataType: "json",
                success: function (result) {
                    var obj = jQuery.parseJSON(result.d);
                    $.each(obj, function (i, item) {
                        var litem = '<div class="col-xl-6">' +
                                        '<div class="d-flex position-relative mb-3 item">' +
                                            '<div class="flex-shrink-0 me-3">' +
                                                '<div class="type">' + item.type + '</div>' + 
                                                '<div>' + item.date + '</div>' +
                                            '</div>' +
                                            '<div>' +
                                                '<a href="/alcicek/tr/etkinlikTakvimi/' + item.uri + '" class="title">' +
                                                    '<h6>' + item.title + '</h6>' +
                                                '</a>' +
                                                '<p class="location"><i class="fas fa-street-view"></i> ' + item.location + '</p>' +
                                            '</div>' +
                                        '</div>' +
                                    '</div>';
                        $('.all-list').append(litem);
                    });
                },
                error: function () {
                    $('.all-list').append("Kayıt bulunamadı!");
                }
            });
        });
    </script>
