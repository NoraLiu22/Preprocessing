rif;--typography-subheading-standard-s-font-weight:300;--typography-subheading-standard-s-text-transform:capitalize;--typography-subheading-serif-m-font-size:20px;--typography-subheading-serif-m-line-height:28px;--typography-subheading-serif-m-font-family:Exchange,Georgia,serif;--typography-subheading-serif-m-font-weight:400;--typography-subheading-serif-s-font-size:17px;--typography-subheading-serif-s-line-height:24px;--typography-subheading-serif-s-font-family:Exchange,Georgia,serif;--typography-subheading-serif-s-font-weight:400;--typography-summary-font-size-m:16px;--typography-summary-font-size-s:14px;--typography-summary-font-line-height-m:24px;--typography-summary-font-line-height-s:20px;--typography-summary-standard-m-font-size:16px;--typography-summary-standard-m-line-height:24px;--typography-summary-standard-m-font-family:Exchange,Georgia,serif;--typography-summary-standard-m-font-weight:400;--typography-summary-standard-s-font-size:14px;--typography-summary-standard-s-line-height:20px;--typography-summary-standard-s-font-family:Exchange,Georgia,serif;--typography-summary-standard-s-font-weight:400;--typography-timestamp-font-size-m:14px;--typography-timestamp-font-size-s:13px;--typography-timestamp-font-size-xs:13px;--typography-timestamp-font-line-height-m:16px;--typography-timestamp-font-line-height-xs:16px;--typography-timestamp-standard-xs-font-size:13px;--typography-timestamp-standard-xs-line-height:16px;--typography-timestamp-standard-xs-font-family:Retina;--typography-timestamp-standard-xs-font-weight:300;--typography-timestamp-standard-m-font-size:14px;--typography-timestamp-standard-m-line-height:16px;--typography-timestamp-standard-m-font-family:Retina Narrow,Helvetica,Arial,sans-serif;--typography-timestamp-standard-m-font-weight:400;--typography-timestamp-standard-m-text-transform:uppercase;--typography-timestamp-standard-s-font-size:13px;--typography-timestamp-standard-s-line-height:16px;--typography-timestamp-standard-s-font-family:Retina Narrow,Helvetica,Arial,sans-serif;--typography-timestamp-standard-s-font-weight:400;--typography-timestamp-standard-s-text-transform:uppercase;--box-shadow-card-dark:0px 1px 2px 1px rgba(255, 255, 255, 0.5);--box-shadow-card-light:0px 1px 2px 1px rgba(0, 0, 0, 0.1);--box-shadow-dropdown-dark:0px 3px 7px -4px rgba(255, 255, 255, 0.5);--box-shadow-dropdown-light:0px 3px 7px -4px rgba(0, 0, 0, 0.1);--z-header-nav:51;--z-header-edition-picker:52;--z-search-sector:51;--z-user-menu:52;--z-slim-header:60;--z-uds-popover:59;--z-skip:61;--z-top-hat:59;--z-market-data-strip:59;--z-share-tools:50;--z-media-layout:49;--z-media-layout-bleed:51;--z-image-backdrop:49;--z-header-lockup:49;--z-text-resize:49;--z-newsletter-dialog:61;--z-nav-overlay:-1;--z-nav-overlay-open:62;--z-overlay-open:61;--z-search-panel:62;--z-search-dialog:61;--z-share-dialog:61;--z-factiva-modal:61;--z-job-scrim-modal:61;--z-resume-sub-modal:61;--z-mobile-modal:61;--z-email-scrim:62;--z-slimline-header:60;--z-xbrand-slimline-header:60;--z-full-bleed-ads:51;--z-embed-expand:52;--focus-outline:var(--color-blue) solid 3px;--focus-outline-offset:1px;--article-base-font-size:17;--primary-background-color:var(--color-white);--secondary-background-color:var(--color-snow);--tertiary-background-color:var(--color-smoke);--quaternary-background-color:var(--color-silver);--interactive-background-color:var(--color-blue);--article-caption-font-family:var(--font-font-stack-retina);--article-font-family:var(--font-font-stack-exchange);--article-font-weight:var(--font-weight-regular);--article-subhed-font-family:var(--font-font-stack-escrow-condensed);--primary-text-color:var(--color-jet);--secondary-text-color:var(--color-nickel);--interactive-text-color:var(--color-blue);--opinion-text-color:var(--color-gold);--priority-text-color:var(--color-red);--interactive-hover-color:var(--color-dark-blue);--article-text-size-scale:1;--color-buyside-green:#336666;--color-deloitte-insight-divider:#cccccc;--color-deloitte-insight-main:#f4f4f4;--color-deloitte-insight-disclaimer:#6f6f6f;--color-deloitte-light-green:#7fc241;--color-deloitte-dark-green:#046a38;--font-family-deloitte-sans-serif:Open Sans,Arial,Helvetica,sans-serif;--font-family-deloitte-serif:'PT Serif',Georgia,Times,serif;}*:focus{outline:var(--color-blue) solid 3px;outline-offset:1px;}*:focus:not(:focus-visible){outline:none;}@supports not selector(:focus-visible){.js-focus-visible *:focus:not(.focus-visible){outline:none;}}*{box-sizing:border-box;}body{font-family:var(--font-font-stack-retina);background-color:var(--primary-background-color);}h1,h2,h3,h4,h5,h6{--headline-font-color:var(--primary-text-color);--headline-link-hover-color:var(--color-blue);}p{--summary-font-color:var(--secondary-text-color);--summary-bullet-small-font-color:var(--primary-text-color);}button{margin:0;}figure{margin:0;}@media print{body{color:var(--true-black);display:block;-webkit-transform:none;-moz-transform:none;-ms-transform:none;transform:none;zoom:1;text-align:left;width:100%;}}.pg-element{z-index:var(--z-media-layout);}</style><script>
  (function() {
    window.googletag = window.googletag || { cmd: [] };

    function yieldToMain() {
  return new Promise(resolve => {
    setTimeout(resolve, 0);
  });
}
    async function getCurrentUser(endpointURL, encryptedDocumentKey = '', additionalHeaders = {}) {
  const autoPricingOffers = window?.localStorage?.getItem('ca_offers');
  let offersParam = '';

  try {
    if (autoPricingOffers) {
      const offers = JSON.parse(autoPricingOffers);

      if (!offers || offers.exp < Date.now()) {
        offersParam = 'offers=true';
      }
    } else {
      offersParam = 'offers=true';
    }
  } catch (e) {
    offersParam = 'offers=true';
  }

  function yieldToMain() {
    return new Promise(resolve => {
      setTimeout(resolve, 0);
    });
  }

  const options = {
    cache: 'no-cache',
    credentials: 'include',
    headers: Object.assign({
      'X-ENCRYPTED-DOCUMENT-KEY': encryptedDocumentKey,
      // the following headers are necessary for the cxense dynamic paywall logic
      // which helps determine whether a user should see an article or not
      'x-original-host': window.location.host,
      'x-original-referrer': window.document.referrer,
      'x-original-url': window.location.pathname + window.location.search + window.location.hash
    }, additionalHeaders),
    timeout: 3000
  };

  try {
    const response = await fetch(`${endpointURL}${endpointURL.includes('?') ? '&' : '?'}${offersParam}`, options);
    const userResponse = await response.json();
    await yieldToMain();
    window?.entitlementsAsyncData?.resolve(userResponse);
    return userResponse;
  } catch (error) {
    window?.entitlementsAsyncData?.reject(error);
    return {};
  }
}
    const volatilityScore = quote => {
  if (quote) {
    return calculateVolatility(quote?.highValue, quote?.lowValue, quote?.previousPriceValue);
  }

  return '';
}
    const getVolatilityTargeting = n => {
  if (n >= 0.0 && n < 0.25) return 'volatility000';
  if (n >= 0.25 && n < 0.5) return 'volatility025';
  if (n >= 0.5 && n < 0.75) return 'volatility050';
  if (n >= 0.75 && n < 1.0) return 'volatility075';
  if (n >= 1.0 && n < 1.25) return 'volatility100';
  if (n >= 1.25 && n < 1.5) return 'volatility125';
  if (n >= 1.5 && n < 1.75) return 'volatility150';
  if (n >= 1.75 && n < 2.0) return 'volatility175';
  if (n >= 2.0 && n < 2.25) return 'volatility200';
  if (n >= 2.25 && n < 2.5) return 'volatility225';
  if (n >= 2.5 && n < 2.75) return 'volatility250';
  if (n >= 2.75 && n < 3.0) return 'volatility275';
  if (n >= 3.0 && n < 3.25) return 'volatility300';
  if (n >= 3.25 && n < 3.5) return 'volatility325';
  if (n >= 3.5 && n < 3.75) return 'volatility350';
  if (n >= 3.75 && n < 4.0) return 'volatility375';
  if (n >= 4) return 'volatilitymax';
  return '';
}
    const marketVolatility = quote => {
  if (quote) {
    return getVolatilityTargeting(volatilityScore(quote));
  }

  return '';
}
    const calculateVolatility = (compositeHigh, compositeLow, financialPV) => (compositeHigh - compositeLow) / financialPV * 100
    const marketStatus = quote => {
  if (quote) {
    return getMarketStatus(calculateStatus(quote.lastPrice, quote.previousPriceValue));
  }

  return '';
}
    const getMarketStatus = n => {
  if (n > 0) return 'green';
  if (n === 0) return 'black';
  if (n < 0) return 'red';
  return '';
}
    const calculateStatus = (currentMarket, openingMarket) => (currentMarket - openingMarket) / openingMarket

    async function getDJIA() {
      try {
        const res = await fetch(
          "https://api.wsj.net/api/dylan/quotes/v2/comp/quoteByDialect?needed=Trading&id=INDEX/US//DJIA&dialect=charting&ckey=57494d5ed7&EntitlementToken=57494d5ed7ad44af85bc59a51dd87c90&maxInstrumentMatches=1&accept=application/json"
        );
        const quoteData = await res.json();

        const instrumentData = quoteData.InstrumentResponses?.[0];
        if (instrumentData.Matches?.length < 1) return [];
        const matchData = instrumentData.Matches?.[0];

        const {
          Trading: {
            Last: { Price: { Value: lastPrice } } = {},
            Open: { Value: previousPriceValue } = {},
            High: { Value: highValue } = {},
            Low: { Value: lowValue } = {},
          } = {},
        } = matchData;

        if (!lastPrice || !previousPriceValue || !highValue || !lowValue) return [];

        const quote = {
          highValue,
          lastPrice,
          lowValue,
          previousPriceValue
        };

        const marketVol = [marketVolatility(quote), marketStatus(quote), volatilityScore(quote)]; //e.g [volatility050,green,0.6255902687930917]
        window.__ace('dataLayer', 'setData', [{alert: marketVol.join()}])
      } catch (error) {
        console.log(error);
      }
    }

    function setMarketVolatility() {
      try {
        if (!true) getDJIA();
        else {
          window.__ace('dataLayer', 'setData', [{alert: 'volatility050,red,0.7497928149677442'}]);
        }
      } catch (error) {
        console.log(error);
      }
    }

    function deferredPromise() {
      let _resolve;
      let _reject;

      const promise = new Promise((resolve, reject) => {
        _resolve = resolve;
        _reject = reject;
      });

      promise.resolve = _resolve;
      promise.reject = _reject;

      return promise;
    }

    /**
     * Make a request to get the entitlements of the current user
     */
    async function requestEntitlements() {
      window.entitlementsAsyncData = deferredPromise();

      // Get entitilements (user data)
      const queryParams = new URLSearchParams(window.location.search);
      const hasAdPageType = false;
      const isHomepage = 'hightouch' === 'mobilehomepage';
      const isL1orL2 = 'hightouch' === 'autofeed' || 'hightouch' === 'hightouch';
      const isSnippetView = 'false' === 'true';
      window.__isSnippetView = isSnippetView;

      const targeting = {
        msrc: queryParams.get('mod') || '',
        application_stack: "gcn",
        breakpoint: "at16units",
        page: "hightouch",
      };

      if (hasAdPageType) {
        targeting.pagetype = 'undefined';
      }

      if(isSnippetView) {
        const userTargeting = {
          circ: 'nonsubscriber',
          // AdTech renamed circ to usertype in data layer
          usertype: 'nonsubscriber'
        };

        window.utag_data.profiles = '';
        window.utag_data.user_exp = 'default';
        window.utag_data.user_ref = '';
        window.utag_data.user_tags = '';
        window.utag_data.user_type = 'nonsubscriber';

        if(!isHomepage && !isL1orL2){
          window.utag_data.article_template = 'preview';
        }

        try {
          window.__ace('dataLayer', 'setData',[{...targeting, ...userTargeting}]);
          await yieldToMain();
        } catch(e) {}
      }

      const entitlementsUrl = 'https://www.wsj.com/client';
      const encryptedDocumentKey = '';

      if (true) {
        performance.mark('client-request-entitlements');
        getCurrentUser(entitlementsUrl, encryptedDocumentKey);
      }


          const retrievedUser = await window.entitlementsAsyncData;
          await yieldToMain();
          performance.mark('ads-receive-entitlements');

          if (retrievedUser.redirect) {
            window.location.href = retrievedUser.redirect;
            return;
          }

          const {
            companyDetails: { company_codes: companyCodes, name: companyName = '' } = {},
            documentKey,
            execEdition,
            experience,
            offers,
            tags,
            type,
            vxid,
            paywall,
            permissions: { wsjPausedSub } = {},
            emailHashes,
            profiles
          } = retrievedUser || {};

          if (window.newrelic) {
            if (window?.requestIdleCallback) {
              window.requestIdleCallback(() => {
                newrelic.setCustomAttribute('user-type', type);
              });
            } else {
                await yieldToMain();
                newrelic.setCustomAttribute('user-type', type);
            }
          }

          // Set emailHashes property in DL and call Liveramp via GMS
          if(emailHashes){
            window.__ace('dataLayer', 'setData',[{emailHashes}]);
            await yieldToMain();
            window.__ace('liveramp','setEmailHashes',[emailHashes]);
            await yieldToMain();
          }

          if(!isHomepage && !isL1orL2){
            window.utag_data.cx_shield = paywall;
          }

          if(offers) {
            window.localStorage.setItem('ca_offers', JSON.stringify(offers));
          } else {
            window.localStorage.removeItem('ca_offers');
          }

          if(!isSnippetView) {
            const userTargeting = {
              circ: type,
              // AdTech renamed circ to usertype in data layer
              usertype: type
            };

            window.utag_data.profiles = profiles || '';
            window.utag_data.user_exp = experience || '';
            window.utag_data.user_ref = (vxid && `V2-${vxid}`) || '';
            window.utag_data.user_tags = (tags && tags.join) ? tags.join(',') : '';
            window.utag_data.user_type = type || '';

            if (!isHomepage && !isL1orL2) {
              window.utag_data.article_template = !documentKey ? 'preview' : 'full';
            }

            if (wsjPausedSub) {
              window.utag_data.sub_paused = 'true';
            }

            if (execEdition) {
              const companyId = (companyCodes && companyCodes.join('|')) || '';

              window.utag_data.company_id = companyId;
              window.utag_data.company_name = companyName;
              window.utag_data.page_site = 'WSJ Executive';
              window.utag_data.page_site_product = 'WSJEXEC';
              window.utag_data.page_content_region = 'Global';
            }

            try {
              await yieldToMain();
              window.__ace('dataLayer', 'setData',[{...targeting, ...userTargeting}]);
            } catch(e) {}
          }
    }

    requestEntitlements();
    if (window?.requestIdleCallback) {
      window.requestIdleCallback(()=>setMarketVolatility());
    } else {
      setTimeout(()=>setMarketVolatility(), 0);
    }

  })();
</script><style data-emotion="css w5ikmy">.css-w5ikmy{display:grid;grid-template-areas:'top' 'main' 'footer';grid-template-columns:minmax(0px, 1fr);--scrollbar-width:0px;}@media print{.css-w5ikmy{display:block;}.css-w5ikmy [data-block='doNotPrint']{display:none;}}</style><div class="css-w5ikmy egirysi14"><style data-emotion="css ittdge">.css-ittdge{grid-area:top;}@media print{.css-ittdge{display:none;}}</style><div class="css-ittdge egirysi11"><style data-emotion="css t7drxh-SkipContainer">.css-t7drxh-SkipContainer{position:fixed;top:-1000px;left:100px;-webkit-transition:top 0.3s;transition:top 0.3s;z-index:var(--z-skip);width:320px;}.css-t7drxh-SkipContainer:focus-within{top:0;height:auto;width:320px;background-color:var(--color-white);padding:10px;box-shadow:0 2px 3px -1px #888;}@media (max-width: 639px){.css-t7drxh-SkipContainer:focus-within{position:absolute;max-width:100%;left:50%;-webkit-transform:translateX(-50%);-moz-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);}}@media print{.css-t7drxh-SkipContainer{display:none;}}</style><div tabindex="-1" class="css-t7drxh-SkipContainer e15pmaxw3"><style data-emotion="css mexjld-SkipMainButton">.css-mexjld-SkipMainButton{font-family:var(--font-font-stack-retina);width:100%;display:inline-block;padding:15px 10px;font-size:14px;font-weight:var(--font-weight-medium);letter-spacing:0.5px;text-transform:var(--font-case-uppercase);text-align:center;-webkit-appearance:none;-moz-appearance:none;background-color:#027ebe;color:var(--color-white);margin-bottom:10px;}</style><button class="css-mexjld-SkipMainButton e15pmaxw2">Skip to Main Content</button></div><nav-hat mode="dark">
