<!-- Container in which the privacy notice will be rendered -->

<div id="otnotice-8c748169-0c9b-46bc-98bf-5c40b6e4b11f" class="otnotice"></div>
    <script src="https://privacyportalde-cdn.onetrust.com/privacy-notice-scripts/otnotice-1.0.min.js" type="text/javascript" charset="UTF-8" id="otprivacy-notice-script">
      settings=""
    </script>

    <script>
        const url = new URL(window.location.href);
        if (url.searchParams.has('lang')) {

// if URL parameter present, use it to display the respective language

        function getParameterByName(name, url=window.location.href) {
            name = name.replace(/[\[\]]/g, '\\$&');
        var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)')
            , results = regex.exec(url);
            if (!results)
            return null;
            if (!results[2])
            return '';
            return decodeURIComponent(results[2].replace(/\+/g, ' '));
            }

        OneTrust.NoticeApi.Initialized.then(function() {
        let lang = getParameterByName('lang') || 'en-us'
        OneTrust.NoticeApi.LoadNotices(["https://privacyportalde-cdn.onetrust.com/storage-container/70b0083d-d519-4ad2-84ca-96b7c5f8e1a9/privacy-notices/8c748169-0c9b-46bc-98bf-5c40b6e4b11f/published/privacynotice.json"], true, lang)
                        })
                    }
            else {

// if URL parameter not present, use automated language detection                

        var oneTrustMappings = [["de", "de-de", "de-ch", "de-at"], ["en-us","en", "en-ca"], ["en-ie"], ["en-gb"], ["sv", "sv-se"], ["it", "it-it", "it-ch"], ["en-au"], ["fr", "fr-fr", "fr-ch", "fr-be"], ["es", "es-es"], ["cs", "cs-cz"], ["en-nz"], ["nb", "no", "no-no", "nb-no"], ["th", "th-th"], ["ja", "ja-jp"], ["es-us", "es-mx"], ["ga", "ga-ie"], ["pl", "pl-pl"], ["da", "da-dk"], ["pt", "pt-pt", "pt-br"], ["zh-tw", "zh", "zh-cn", "zh-hk", "zh-hans", "zh-hant"]];
	function getLanguageFile(lan) {
        var desiredLang = lan.toLowerCase();
            for (var loop = 0; loop < oneTrustMappings.length; loop = loop + 1) {
            for (var loopI = 0; loopI < oneTrustMappings[loop].length; loopI = loopI + 1) {
            if (oneTrustMappings[loop][loopI].indexOf(desiredLang) > -1) {
            return oneTrustMappings[loop][0];
                            }
                        }
                    }

             return "en-us";
                    }

        OneTrust.NoticeApi.Initialized.then(function() {
        let lang = getLanguageFile(navigator.language || navigator.userLanguage || 'en-us')
        OneTrust.NoticeApi.LoadNotices(["https://privacyportalde-cdn.onetrust.com/storage-container/70b0083d-d519-4ad2-84ca-96b7c5f8e1a9/privacy-notices/8c748169-0c9b-46bc-98bf-5c40b6e4b11f/published/privacynotice.json"], true, lang)
                    });
                }
    </script>
