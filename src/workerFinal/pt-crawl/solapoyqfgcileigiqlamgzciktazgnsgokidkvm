                            const newFrameHeight = event.data.slice(lastIndex + 1) + "px";

                            frameWrapper.style.height = newFrameHeight;
                            frameWrapper.querySelector('div').style.height = newFrameHeight;
                            frameWrapper.querySelector('iframe').height = newFrameHeight;
                        }
                    }
