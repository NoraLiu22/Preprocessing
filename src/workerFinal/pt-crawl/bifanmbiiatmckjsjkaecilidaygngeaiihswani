<script>
	require(['jquery'], function($) {
        // Clone to mobile header
        $('[data-curated-nav-cont]').replaceWith($('[data-guid="324032ab-1657-4ade-afe7-c2b928fd03e4"]').clone());

        const checkScreenSize = function() {
            $('[data-nav-trigger]').each(function() {
                if(window.matchMedia("(min-width: 64em)").matches) {
                    $(this).on('mouseenter', function() {  
                        const navList = $('.curated-nav-links');
                        const navTrigger = $('.nav-trigger');

                        navList.addClass('active');
                        navTrigger.addClass('active');
                    });

                    $('.curated-nav-cont').on('mouseleave', function() {
                        const navList = $('.curated-nav-links');
                        const navTrigger = $('.nav-trigger');

                        navList.removeClass('active');
                        navTrigger.removeClass('active');
                    })
                } else {
                    $(this).on('click', function() {
                        const navList = $('.curated-nav-links');
                        const navTrigger = $('.nav-trigger');

                        navList.toggleClass('active');
                        navTrigger.toggleClass('active');
                    });
                }
            });
        }

        checkScreenSize();
 
        window.addEventListener('resize', () => {
            if(document.querySelector('.curated-nav-links').classList.contains('active')) {
                document.querySelector('.curated-nav-links').classList.remove('active');
            }
            checkScreenSize();
        });
	});
</script></div>
