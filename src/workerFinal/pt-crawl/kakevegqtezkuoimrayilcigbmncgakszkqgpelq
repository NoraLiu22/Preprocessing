    <script>
        // make all the headers for shop items cards the same height
        function setMinHeight(){
            $(".subscribe-card h2").css("min-height","0px");
            if(window.matchMedia("(min-width: 767px)").matches){
                var heights = $(".subscribe-card h2").map(function(){
                    return $(this).height();
                }).get();
                maxHeight = Math.max.apply(null, heights);
                $(".subscribe-card h2").css("min-height",maxHeight);
            }
        };
        setMinHeight();
        window.onresize = setMinHeight;
    </script>

