    <script>
        $(document).ready(function () {

            getForumResponsesNumber();

            function getForumResponsesNumber() {
                var request = $.ajax(
                    {
                        type: "GET",
                        url: "/getForumResponsesNumber",
                        data: {

                        },
                        success: function (response) {
                            var forumresponses = response.forumresponses
                            if (forumresponses > 0) {
                                $("#forumHeader").html('<a class="nav-link main" href="/forum">FORUM<sup>' + forumresponses + '</sup></a>');
                            }
                        }
                    }
                );
            }

        });
    </script>

    <!-- Google Analytics -->
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-139349847-1', 'auto');
        ga('send', 'pageview');
    </script>
