            <script type="text/javascript">

        /*******************************************************************************
         * JS here is only being used to assign variables from values in the model
         * logic should be implemented in external JS files, like client.script.js
         *******************************************************************************/

        var SCM = SCM || {};
        var accessIcons = [{"id":297128,"icon":"icon-availability_free","title":"Free"},{"id":297321,"icon":"icon-availability_free","title":"Free"},{"id":306259,"icon":"icon-availability_free","title":"Free"},{"id":306260,"icon":"icon-availability_free","title":"Free"},{"id":306261,"icon":"icon-availability_free","title":"Free"},{"id":297127,"icon":"icon-availability_free","title":"Free"},{"id":297322,"icon":"icon-availability_free","title":"Free"},{"id":307072,"icon":"icon-availability_free","title":"Free"},{"id":307084,"icon":"icon-availability_open","title":"Open Access"},{"id":307085,"icon":"icon-availability_free","title":"Free"},{"id":307100,"icon":"icon-availability_free","title":"Free"},{"id":307122,"icon":"icon-availability_free","title":"Free"},{"id":307123,"icon":"icon-availability_free","title":"Free"},{"id":307147,"icon":"icon-availability_free","title":"Free"},{"id":307149,"icon":"icon-availability_free","title":"Free"},{"id":306218,"icon":"icon-availability_open","title":"Open Access"},{"id":307071,"icon":"icon-availability_open","title":"Open Access"},{"id":307101,"icon":"icon-availability_free","title":"Free"},{"id":307124,"icon":"icon-availability_free","title":"Free"},{"id":307125,"icon":"icon-availability_free","title":"Free"},{"id":307133,"icon":"icon-availability_free","title":"Free"},{"id":286588,"icon":"icon-availability_free","title":"Free"},{"id":286770,"icon":"icon-availability_free","title":"Free"},{"id":286825,"icon":"icon-availability_open","title":"Open Access"},{"id":287009,"icon":"icon-availability_free","title":"Free"},{"id":287121,"icon":"icon-availability_open","title":"Open Access"},{"id":286615,"icon":"icon-availability_free","title":"Free"},{"id":307097,"icon":"icon-availability_free","title":"Free"}];
        if (SCM.AccessIcons) {
            SCM.AccessIcons = SCM.AccessIcons.concat(accessIcons);
        } else {
            SCM.AccessIcons = accessIcons;
        }
        var accessAttributes =  [{"id":297128,"availableforpurchase":false},{"id":297321,"availableforpurchase":false},{"id":306259,"availableforpurchase":false},{"id":306260,"availableforpurchase":false},{"id":306261,"availableforpurchase":false},{"id":297127,"availableforpurchase":false},{"id":297322,"availableforpurchase":false},{"id":307072,"availableforpurchase":false},{"id":307084,"availableforpurchase":false},{"id":307085,"availableforpurchase":false},{"id":307100,"availableforpurchase":false},{"id":307122,"availableforpurchase":false},{"id":307123,"availableforpurchase":false},{"id":307147,"availableforpurchase":false},{"id":307149,"availableforpurchase":false},{"id":306218,"availableforpurchase":false},{"id":307071,"availableforpurchase":false},{"id":307101,"availableforpurchase":false},{"id":307124,"availableforpurchase":false},{"id":307125,"availableforpurchase":false},{"id":307133,"availableforpurchase":false},{"id":286588,"availableforpurchase":false},{"id":286770,"availableforpurchase":false},{"id":286825,"availableforpurchase":false},{"id":287009,"availableforpurchase":false},{"id":287121,"availableforpurchase":false},{"id":286615,"availableforpurchase":false},{"id":307097,"availableforpurchase":false}];
        if (SCM.AccessAttributes) {
            SCM.AccessAttributes = SCM.AccessAttributes.concat(accessAttributes);
        } else {
            SCM.AccessAttributes = accessAttributes;
        }
        
    </script>
 
