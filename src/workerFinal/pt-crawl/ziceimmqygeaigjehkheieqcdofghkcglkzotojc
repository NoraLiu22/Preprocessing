    <div class="embed">
    <div data-testid='embed-component' id="aem-embed-jsp-/content/acs/en/membership/jcr:content/articleContent/embed" class="embed left" style="height:auto;  width:auto; " data-ishtl="true">
        
        <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js" integrity="sha512-16esztaSRplJROstbIIdwX3N97V1+pZvV33ABoG1H2OyTttBxEGkTsoIVsiP1iaTtM8b3+hu2kB6pQ4Clr5yug==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js" integrity="sha512-Ic9xkERjyZ1xgJ5svx3y0u3xrvfT/uPkV99LBwe68xjy/mGtO+4eURHZBW2xW4SZbFrF1Tf090XqB+EVgXnVjw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://cdn.jsdelivr.net/npm/split-type@0.3.4/umd/index.min.js"></script>

<script type="text/javascript">

(function () {
document.addEventListener("DOMContentLoaded", function () {

    console.log(window.location.href);

    if (window.location.href.includes('wcmmode=disabled') || window.location.href.includes('acs.org') ||
    window.location.href.includes('cmswwwdev.acs.org')
    || window.location.href.includes('author-p99509-e917506.adobeaemcloud.com/content/acs/en/membership')) {
        if (document.getElementById('exc')) {
            console.log('detected AEM Auth via #exc!');
            return
        } else {
            console.log('Running True Page JS!');
            gsap.registerPlugin(ScrollTrigger);

            const acsNavLinks = document.querySelectorAll('.acsnav a');

            acsNavLinks.forEach(link => {
                link.addEventListener('click', function(event) {
                    if (this.title === 'Common Questions') {
                        event.preventDefault(); 
        
                        const targetElement = document.querySelector('.herocontainer > .common-questions');
        
                        if (targetElement) {
                            const offsetPosition = targetElement.getBoundingClientRect().top + window.pageYOffset - 48;
        
                            window.scroll({
                                top: offsetPosition,
                                behavior: 'smooth'
                            });
                        } else {
                            console.log('Target element not found');
                        }
                    }
                });
            });

            const communityRowImages = document.querySelectorAll('.community-row img');
    
            communityRowImages.forEach(img => {
                img.removeAttribute('loading');
                const src = img.src;
                img.src = '';
                img.src = src;
            });

            function preloadHeroImage() {
                const targetElement = document.querySelector('.hero-bg > div');
                if (!targetElement) {
                    console.error('Element not found');
                    return;
                }
            
                const style = window.getComputedStyle(targetElement);
                let backgroundImage = style.backgroundImage;
                if (!backgroundImage || backgroundImage === 'none') {
                    console.error('No background image found');
                    return;
                }
            
                // Remove quotes from URL if present
                backgroundImage = backgroundImage.slice(4, -1).replace(/"/g, "");

                const pseudoElementStyle = `.hero-bg > div:after {
                    content: '';
                    background: url('${backgroundImage}')!important;
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    z-index: -1;
                }`;
                const styleSheet = document.createElement("style");
                styleSheet.type = "text/css";
                styleSheet.innerText = pseudoElementStyle;
                document.head.appendChild(styleSheet);
            
                // Set original element background to none
                targetElement.style.backgroundImage = 'none';
            
                // Preload image
                const img = new Image();
                img.onload = function () {
                    targetElement.classList.add('loaded');
                };
                img.onerror = function () {
                    console.error('Error loading the image');
                };
            
                img.src = backgroundImage;
            }
            
            preloadHeroImage();
            


            function initializeScrollTriggers() {
                const images = document.querySelectorAll('.community-row img');
                let loadedImages = 0;
            
                ScrollTrigger.create({
                    trigger: ".intro-cards-wrapper",
                    start: "middle 75%",
                    toggleClass: "active",
                    once: true,
                    scrub:1
                });

                const quantityH3 = document.querySelectorAll('h3.quantity');
                quantityH3.forEach(h3 => {
                    ScrollTrigger.create({
                        trigger: h3,
                        start: "top bottom",
                        toggleClass: "active",
                        once: true,
                        scrub:1
                    });
                });

                function onAllImagesLoaded() {

                    ScrollTrigger.create({
                        trigger: ".good-company",
                        start: "top 100%",
                        toggleClass: "active",
                        once: true
                    });

                    //Pricing Table Initialization
                    const pricingTable = document.getElementById('main-pricing-table');
                    pricingTable.classList.add('initialized');
                    var buttons = document.querySelectorAll('.toggle-wrapper li');
                    var pricingHeadings = document.querySelectorAll('.table-column.premium .pricing-wrapper h4');
                    
                    buttons.forEach(function (button, index) {
                        button.addEventListener('click', function () {

                            buttons.forEach(btn => btn.classList.remove('active'));
                            pricingHeadings.forEach(header => header.classList.remove('active'));

                            button.classList.add('active');
                            if (pricingHeadings[index]) {
                                pricingHeadings[index].classList.add('active');
                            }
                        });
                    });

                    ScrollTrigger.refresh();

                }
            
                images.forEach(img => {
                    img.onload = function() {
                        let parent = img;
                        for (let i = 0; i < 12; i++) {
                            parent = parent.parentElement;
                            if (!parent) break;
                        }
                        if (parent && parent.classList.contains('community-row')) {
                            parent.classList.add('loaded');
            
                            ScrollTrigger.create({
                                trigger: parent,
                                start: "top 85%",
                                onEnter: () => setTimeout(() => parent.classList.add('active'), 180),
                                once: false,
                                scrub:1
                            });
                        }
            
                        loadedImages++;
                        if (loadedImages === images.length) {
                            onAllImagesLoaded();
                        }
                    };
            
                    if (img.complete && img.naturalHeight !== 0) {
                        img.onload();
                    }
                });
            }
            
            initializeScrollTriggers();
            

            //SplitType Animations
            function splitH1() {
                var heroH1 = document.querySelectorAll('.hero-bg h1');
                if (heroH1) {
                    heroH1.forEach(function (element) {
                        new SplitType(element, { types: 'words' });
                    });
                }
            }
            splitH1();

            function splitH3Quantity() {
                var quantityH3 = document.querySelectorAll('h3.quantity');
                if (quantityH3) {
                    quantityH3.forEach(function (element) {
                        new SplitType(element, { types: 'chars' });
                    });
                }
            }
            splitH3Quantity();

            var charDivs = document.querySelectorAll('div.char');
            charDivs.forEach(function (div) {
                var text = div.textContent.trim();
                div.setAttribute('data-char', text);
            });

        }

    } else {
        console.log("In AEM Editor/Authoring - GSAP and ScrollTrigger not initialized");
    }

    ///
});

})()


</script>
        <div class="clear"></div>
    </div>

    

</div>


</div>

                    </div>
                </div><!-- /row-fluid -->
            </div>
        
        
        
        
    </div><!--=== main well end ===-->
    

    <!--====== end  page content ====== -->
