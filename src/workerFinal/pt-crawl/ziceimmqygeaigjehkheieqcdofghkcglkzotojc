    <div data-testid='embed-component' id="aem-embed-jsp-/content/acs/en/membership/references/pricing-table/jcr:content/articleContent/acscontainer/containerPar/embed_750909669" class="embed left" style="height:auto;  width:auto; " data-ishtl="true">
        
        <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js" integrity="sha512-16esztaSRplJROstbIIdwX3N97V1+pZvV33ABoG1H2OyTttBxEGkTsoIVsiP1iaTtM8b3+hu2kB6pQ4Clr5yug==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/ScrollTrigger.min.js" integrity="sha512-Ic9xkERjyZ1xgJ5svx3y0u3xrvfT/uPkV99LBwe68xjy/mGtO+4eURHZBW2xW4SZbFrF1Tf090XqB+EVgXnVjw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://cdn.jsdelivr.net/npm/split-type@0.3.4/umd/index.min.js"></script>

<script>
(function () {
    
document.addEventListener("DOMContentLoaded", function () {

    if (window.location.href.includes('wcmmode=disabled') || window.location.href.includes('acs.org') ||
        window.location.href.includes('cmswwwdev.acs.org')
        || window.location.href.includes('author-p99509-e917506.adobeaemcloud.com/content/acs/en/membership')) {
        if (document.getElementById('exc')) {
            console.log('detected AEM Auth via #exc!');
            return
        } else {
            gsap.registerPlugin(ScrollTrigger);

            // Pricing Table Initialization
            const pricingTable = document.getElementById('main-pricing-table');
            pricingTable.classList.add('initialized');

            var tabLinks = document.querySelectorAll('.toggle-wrapper li');
            var pricingHeadings = document.querySelectorAll('.pricing-wrapper h4');

            var allColumnDescriptions = document.querySelectorAll('.column-description p');
            var standardDescription = document.querySelector('.column-description p:nth-of-type(1)');
            var retiredDescription = document.querySelector('.column-description p.retired');
            var emeritusDescription = document.querySelector('.column-description p.emeritus');

            tabLinks.forEach(function (li, index) {
                li.addEventListener('click', function () {

                    tabLinks.forEach(function (lnk) {
                        lnk.classList.remove('active');
                    });
                    pricingHeadings.forEach(function (header) {
                        header.classList.remove('active');
                    });

                    li.classList.add('active');

                    if (pricingHeadings[index]) {
                        pricingHeadings[index].classList.add('active');
                    }

                    const anchor = li.querySelector('a');
                    const ariaControls = anchor ? anchor.getAttribute('aria-controls') : null;

                    if (ariaControls === 'graduates' || ariaControls === 'undergraduates' || ariaControls === 'emeritus' || ariaControls === 'retired') {
                        document.getElementById('main-pricing-table').classList.add('premium-only');
                    } else {
                        document.getElementById('main-pricing-table').classList.remove('premium-only');
                    }
                    if (ariaControls === 'retired') {
                        allColumnDescriptions.forEach(element => {
                            element.classList.remove('active');
                        });
                        retiredDescription.classList.add('active');
                    } else if (ariaControls === 'emeritus') {
                        allColumnDescriptions.forEach(element => {
                            element.classList.remove('active');
                        });
                        emeritusDescription.classList.add('active');
                    } else {
                        allColumnDescriptions.forEach(element => {
                            element.classList.remove('active');
                        });
                        standardDescription.classList.add('active');
                    }
                });
            });

            ScrollTrigger.create({
                trigger: pricingTable,
                start: "top 75%",
                toggleClass: "active",
                once: true
            });

            function debounce(func, wait) {
                let timeout;
                return function (...args) {
                    const later = () => {
                        clearTimeout(timeout);
                        func.apply(this, args);
                    };
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                };
            }

            function adjustColumnHeights() {
                const columns = document.querySelectorAll('.table-column.premium, .table-column.standard');
                columns.forEach(element => {
                    element.style.height = '';
                });
                if (window.innerWidth < 992) {

                    let tallestHeight = 0;

                    columns.forEach(element => {
                        tallestHeight = Math.max(tallestHeight, element.offsetHeight);
                    });

                    columns.forEach(element => {
                        element.style.height = `${tallestHeight}px`;
                    });
                }
            }
            adjustColumnHeights();
            const debouncedAdjustColumnHeights = debounce(adjustColumnHeights, 100);

            window.addEventListener('resize', debouncedAdjustColumnHeights);

            function addDataAttributes() {
                var buttons = document.querySelectorAll('#main-pricing-table .btn-join');

                buttons.forEach(function (button) {
                    button.setAttribute('data-toggle', 'modal');
                    button.setAttribute('data-target', '#createidModal');
                });
            }
            addDataAttributes();

            //CENTERING FOR PREMIUM-ONLY
            function premiumColumnCentering() {
                const tableWrapper = document.querySelector('#main-pricing-table > div > .section');
                const premiumColumn = document.querySelector('.table-column.premium');
                const toggleElements = document.querySelectorAll('.toggle-wrapper li');
                let isCentered = false;
                let initialTableWrapperHeight;
                let initialWidth;
                let initialHeight;

                function storeDimensions() {
                    initialWidth = premiumColumn.offsetWidth;
                    initialHeight = premiumColumn.offsetHeight;
                    initialTableWrapperHeight = tableWrapper.offsetHeight;
                }

                function getElementXPosition(element) {
                    const rect = element.getBoundingClientRect();
                    return rect.left + window.scrollX;
                }

                function centerPremiumColumn() {
                    if (!isCentered || premiumColumn.classList.contains('resizing')) {
                        const xPosition = getElementXPosition(premiumColumn);
                        const viewportWidth = window.innerWidth;
                        const centeredXPosition = (viewportWidth - initialWidth) / 2;
                        const translateX = centeredXPosition - xPosition;

                        if (!premiumColumn.classList.contains('resizing')) {
                            premiumColumn.style.transition = 'transform 0.8s';
                        }
                        tableWrapper.style.height = `${initialTableWrapperHeight}px`;
                        premiumColumn.style.opacity = '';
                        premiumColumn.style.position = 'absolute';
                        premiumColumn.style.width = `${initialWidth}px`;
                        premiumColumn.style.height = `${initialHeight}px`;

                        premiumColumn.style.transform = `translateX(${translateX}px)`;
                        premiumColumn.classList.add('centered');
                        isCentered = true;
                        premiumColumn.classList.remove('resizing');
                    }
                }

                function resetPosition() {
                    if (!premiumColumn.classList.contains('resizing')) {
                        premiumColumn.style.transition = 'transform 0.8s';
                        premiumColumn.style.transform = '';

                        premiumColumn.addEventListener('transitionend', function resetAfterTransition() {
                            premiumColumn.style.position = 'relative';
                            premiumColumn.style.width = '';
                            premiumColumn.style.height = '';
                            tableWrapper.style.height = '';
                            premiumColumn.removeEventListener('transitionend', resetAfterTransition);
                        });

                        premiumColumn.classList.remove('centered');
                        isCentered = false;
                    } else {
                        premiumColumn.style.opacity = '0';
                        premiumColumn.style.transform = '';
                        premiumColumn.style.transition = '';
                        premiumColumn.style.width = '';
                        premiumColumn.style.height = '';
                        tableWrapper.style.height = '';
                        premiumColumn.style.position = 'relative';
                    }

                }

                function handleResize() {
                    if (window.innerWidth <= 991) {
                        tableWrapper.style.height = '';
                        premiumColumn.style.position = '';
                        premiumColumn.style.transform = '';
                        premiumColumn.style.transition = '';
                        premiumColumn.style.width = '';
                        premiumColumn.style.height = '';
                        isCentered = false;
                        return;
                    } else {
                        premiumColumn.classList.add('resizing');
                        if (isCentered) {
                            resetPosition();
                            storeDimensions();
                            centerPremiumColumn();
                        } else {
                            storeDimensions();
                        }
                    }
                }

                toggleElements.forEach(toggleElement => {

                    toggleElement.addEventListener('click', (event) => {
                        if (window.innerWidth >= 992) {
                            premiumColumn.classList.remove('resizing');
                            storeDimensions();
                            if (event.currentTarget.classList.contains('premium-only')) {
                                if (!isCentered) {
                                    centerPremiumColumn();
                                }
                            } else {
                                if (isCentered) {
                                    resetPosition();
                                }
                            }
                        }
                    });
                });
                const debouncedHandleResize = debounce(handleResize, 90);
                window.addEventListener('resize', debouncedHandleResize);

                let currentUrl = window.location.href;
                if (currentUrl.includes("student")) {
                    console.log('student is part of url, so fixing the table to start as one column!');
                    if (window.innerWidth >= 992) {
                        premiumColumn.classList.remove('resizing');
                        storeDimensions();
                        if (!isCentered) {
                            document.getElementById('main-pricing-table').classList.add('premium-only');
                            centerPremiumColumn();
                        } else if (isCentered) {
                            resetPosition();
                        }
                    }
                }

            }

            premiumColumnCentering();

            const modal = document.querySelector('.embed .embed .modal');
            const articleContent = document.querySelector('.articleContent');

            if (modal && articleContent) {
                articleContent.parentNode.insertBefore(modal, articleContent);
            }

        }

    } else {
        console.log("In AEM Editor/Authoring - GSAP and ScrollTrigger not initialized");
    }

    ///
});
})()
</script>
        <div class="clear"></div>
    </div>

    

</div>
