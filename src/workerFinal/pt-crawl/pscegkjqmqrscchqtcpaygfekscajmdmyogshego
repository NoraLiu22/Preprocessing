    <script>
<!-- Track publicaton pdf titles for download buttons -->  
	var downloadBtns = $('a.btn');
	
	var btns = $(downloadBtns).filter( function () {
		var filteredDownloadBtns = $(this).text().toLowerCase().indexOf('download') >= 0;		
		return filteredDownloadBtns			
	})
	
	btns.each(function(){
		$(this).addClass('publication-pdf').prepend('<span class="sr-only"></span>');
		
		var pdfTitlesComponent = $(this).parent().parent().parent().find('h3.small-title').text().trim();
		$(this).find('span').html(pdfTitlesComponent);
		
		var pdfTitlePage = $(this).parent().parent().parent().parent().find('h1.title').html();
		$(this).find('span').html(pdfTitlePage);
	});

 
<!-- to initialise resizer -->	

	iFrameResize({}); 

<!-- To add 20px padding in the iframe buttons -->
	 $(window).load(function(){

 $('.comp_iframe_iframe').css("height","+=20");

 }); 

</script>
    
    
