<script>
$('body').on('DOMSubtreeModified', "div#events", function() {
  var new_height = getHeightDiv('google-calendar');
  document.getElementById("events").style.height = new_height + "px";
  function getHeightDiv(idElem) {
    return document.getElementById(idElem).getBoundingClientRect().height
  }
});
  
/*
 $('#events').on('DOMSubtreeModified', "div#events", function(){
  Foundation.reInit('equalizer');
});
$('#highlights').on('DOMSubtreeModified' , "div#events", function(){
  Foundation.reInit('equalizer');
}); 
  */
  
jQuery('div#events-highlights').on('load','img',function(){
    jQuery('window').trigger('resize');
  	jQuery('body').trigger('DOMSubtreeModified');
  	jQuery('div#events').trigger('DOMSubtreeModified');
  var new_height = getHeightDiv('google-calendar');
  document.getElementById("events").style.height = new_height + "px";
  function getHeightDiv(idElem){
    return document.getElementById(idElem).getBoundingClientRect().height
  }
});
  
jQuery('document').ready(function(){
 console.log('333 ++++++++++++');// --------------------
    jQuery('window').trigger('resize');
  jQuery('body').trigger('DOMSubtreeModified');
  jQuery('div#events').trigger('DOMSubtreeModified');
	var enableLink = "false";
	if(enableLink == "true"){
    	var head = document.getElementById("right_head");
    	jQuery(head).html("<a href=''>Career Development Blog</a>");
  	}
});
  
</script>
