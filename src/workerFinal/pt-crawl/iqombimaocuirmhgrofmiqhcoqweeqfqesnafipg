<div  about="/about/faqs/museum-collection" typeof="schema:WebPage" class="c-page-hero c-page-hero--no-spacing">
  <div class="c-page-hero__inner c-page-hero__inner--no-image">
        <div class="c-page-hero__body">
      <h1 class="c-page-hero__title">
<span property="schema:name">FAQ—The Collection</span>
</h1>
      
      
    </div>
  </div>
</div>



  <div class="l-constrain l-constrain--large l-constrain--no-padding-left">
    








<div  class="l-sidebar">

  
      <aside  class="l-sidebar__sidebar">
      




                                              <ul  id="block-vertical-menu-bottom" class="c-block c-block--provider-menu-block c-block--id-menu-blockmain c-menu c-menu--main c-vertical-menu">
        
                                
                                                  
                                            
      
      
