                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Resources and Tools" class="ms-sitemapdirectional" href="/ParentsAndStudents/Pages/home.aspx">Parents and Students</a></span><span> &gt; </span><span class="current">Grade-Level Overview</span></span>				 
			</div>
            <h2 class="standalone-page-head">&nbsp;Grade-Level Overview</h2>
            <div class="two-part-header">
                <div class="top-article">
                    
<div id="ctl00_PlaceHolderMain_ctl01_EditModePanel2">
	
    <div>
        <div class="story-image">
                <img alt="" src="/PublishingImages/2-TEACHLEARN/1-STUDENTLEARN/1-LearnerOutcomes/LearnBThumb.jpg" style="BORDER: 0px solid; ">
        </div>
        <div class="story-content">
            <h3>
                General Learner Outcomes
            </h3>
            <p>
                The six goals we set for all students of public schools.
            </p>
