  <script>
    {
      const target = document.currentScript.closest('.jobs-widget');
      const jobs = target.querySelector('.jobs-widget__jobs');

      const loadJobs = () => {
        const q = new URLSearchParams();
q.append('company[]', "Asmodee");q.append('company[]', "Coffee Stain Studios");q.append('company[]', "Embracer Group");
        fetch('/data/gi-jobs/latest?' + q.toString(), {
credentials: 'omit',        })
          .then(response => {
            if (!response.ok) throw new Error();
            return response.text();
          })
          .then(text => {
            jobs.innerHTML = text;
            target.dataset.loaded = true;
          })
          .catch(error => {
            jobs.innerHTML = '';
          });
      };

        if ('IntersectionObserver' in window) {
          const observer = new IntersectionObserver((entries) => {
            if (!entries[0].isIntersecting) return;
            loadJobs();
            observer.disconnect();
          }, {
            rootMargin: '0px 0px 1200px 0px',
          });
          observer.observe(target);
        } else loadJobs();
    }
  </script>
</section>



    


