le.small{margin-right:12px}}.shelf .view_archive{width:calc(100% - 64px)}@media screen and (max-width:500px){.shelf .view_archive{width:100%}}.shelf .summary_list{display:grid;gap:32px 12px}.shelf .carousel,.shelf .summary_list{margin-top:32px}.shelf .details,.shelf .metadata{padding-right:32px}body[data-version=hd] .shelf .summary_list{grid-template-columns:repeat(4,1fr)}.tabbed_wrapper{display:block;margin:16px 0}.tabbed_wrapper .archive_list{margin-left:0}.tabbed_wrapper.homepage{margin:0}.tabbed_nav{background:#fff;background-attachment:local,local,scroll,scroll;background-image:linear-gradient(90deg,#fff,#fff),linear-gradient(90deg,#fff,#fff),linear-gradient(90deg,rgba(0,0,0,.2),#fff),linear-gradient(270deg,rgba(0,0,0,.2),#fff);background-position:0,100%,0,100%;background-repeat:no-repeat;background-size:20px 100%,20px 100%,10px 100%,10px 100%;border-bottom:1px solid #d1d1d1;box-shadow:none;margin-bottom:16px;overflow-x:auto;padding-top:16px;position:sticky;top:0;transition:background-image .15s ease-in-out;z-index:50}.tabbed_nav .tabbed_buttons{display:inline-flex;justify-content:space-between;white-space:nowrap}.tabbed_nav .tabbed_buttons .tabbed_button_count{margin-left:4px}.tabbed_nav .tabbed_buttons .tabbed_button{align-items:center;border-bottom:4px solid transparent;color:#172425;display:inline-flex;line-height:1;padding:0 16px 10px}.tabbed_nav .tabbed_buttons .tabbed_button.active,.tabbed_nav .tabbed_buttons .tabbed_button:hover{border-bottom:4px solid #d1d1d1;font-weight:700}.tabbed_nav .tabbed_buttons .tabbed_button .count{font-size:13px;margin-left:4px}.tabbed_nav.small .tabbed_buttons{display:flex}.tabbed_nav.small .tabbed_buttons .tabbed_button{display:flex;justify-content:center;width:100%}@media screen and (min-width:769px){.tabbed_nav.small .tabbed_buttons{display:inline-flex}.tabbed_nav:not(.small) .tabbed_buttons{display:flex}.tabbed_nav:not(.small) .tabbed_buttons .tabbed_button{display:flex;justify-content:center;width:100%}}.tabbed_nav.homepage{margin-bottom:32px}@media screen and (min-width:769px){.tabbed_nav.homepage{margin-right:-32px}}figcaption{font-family:Bronkoh,fallback-font-sans-serif,sans-serif}._comments{line-height:1;margin:0 0 0 16px}._comments a{color:var(--metadata-color);font-family:Bronkoh,fallback-font-sans-serif,sans-serif;font-size:15px}._comments a:before{background:currentColor;content:"";display:inline-block;float:left;height:1em;-webkit-mask-image:url(/static/solid/comment.svg);mask-image:url(/static/solid/comment.svg);-webkit-mask-repeat:no-repeat;mask-repeat:no-repeat;padding-right:4px;width:1em}._page_subtitle{color:#e02f32;font-family:Bronkoh,fallback-font-sans-serif,sans-serif;font-size:21px;font-weight:400;line-height:1}  </style>

