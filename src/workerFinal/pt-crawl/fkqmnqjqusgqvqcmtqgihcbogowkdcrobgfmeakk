  <script>
    (function(){
      const target = document.currentScript;
      const section = target.closest('.newsletter_promo');
      const csrfToken = section.querySelector('input[name="csrfmiddlewaretoken"]');
      const inputs = section.querySelectorAll('.newsletter');
      const submitButton = section.querySelector('.button');
      const email = section.querySelector('input[name="EMAIL"]');
      const featuredNewsletter = !inputs.length && submitButton.dataset.endpoint;
      let inputCheckedCounter = Array.from(inputs).filter((input) => input.checked).length;

      inputs.forEach(input => input.addEventListener('change', () => {
        if (input.checked) inputCheckedCounter++;
        else inputCheckedCounter--;

        submitButton.disabled = inputCheckedCounter <= 0;
      }));

      submitButton.addEventListener('click', (e) => {
        if (email && !email.checkValidity()) return;
        e.preventDefault();

        submitButton.classList.add('loading');
        hideAlerts();

        if (featuredNewsletter) {
          signUpRequest(submitButton, email, csrfToken);
        } else {
          inputs.forEach(input => {
            if (input.checked) {
              signUpRequest(input, email, csrfToken);
            }
          });
        }

        submitButton.classList.remove('loading');
      });

      const signUpRequest = (el, email, csrfToken) => {
        const { endpoint } = el.dataset;
        const data = {};
        if (email) data['email'] = email.value;
        const headers = {
          'X-Requested-With': 'XMLHttpRequest',
        };
        if (csrfToken) headers['X-CSRFToken'] = csrfToken.value;
        fetch(endpoint, {
          method: 'POST',
          body: JSON.stringify(data),
          headers,
        })
          .then(response => {
            if (!response.ok) throw new Error();
            return response.json();
          })
          .then(json => {
            el.disabled = true;
            submitButton.disabled = true;
            if (json.success === true) {
              updateAlert('You have successfully subscribed', 'success');
            } else throw new Error();
          })
          .catch(error => {
            updateAlert(`We weren\'t able to subscribe you, please <a href="/contact-us">contact us</a> for more information.`, 'error');
          });
      };

      const updateAlert = (text, classList) => {
        const alert = section.querySelector('.alert');
        alert.classList = `alert ${classList}`;
        alert.innerHTML = text;
        alert.classList.remove('hidden');
      }

      const hideAlerts = () => {
        const alerts = section.querySelectorAll('.alert');
        alerts.forEach((alert) => {
          alert.classList.add('hidden');
        });
      }
    })()
  </script>
</section>  </footer>
