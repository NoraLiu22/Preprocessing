  <script type="text/javascript">
  	function addCommas(nStr){
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;}	
	
	$.getJSON('https://vertnet.carto.com/api/v2/sql?q=select count(icode) as publishercount, sum(recordtotal) as recordscount, sum(collectionscount) as collcount, sum(urlcount) as resourcecount from (SELECT icode, sum(count) as recordtotal, count(url) as urlcount, sum(collectioncount) as collectionscount FROM resource where ipt=TRUE AND networks LIKE %27%25VertNet%25%27 GROUP BY icode) AS ResourceList', function(resources) {
    $.each(resources.rows, function(key, val) {
        $("#reccount").append(addCommas(val.recordscount + " "));
        $("#rescount").append(addCommas(val.resourcecount + " "));
        $("#pubcount").append(addCommas(val.publishercount + " "));
        $("#colcount").append(addCommas(val.collcount + " "));
      });
    });
  </script>
