    <div class="widget-DynamicWidgetLayout widget-instance-Home_MainContentB0">
        <div class="widget widget-dynamic theme-homepage-layout cob gateway" data-count="3">
    

    <div class="widget-dynamic-inner-wrap">

    <div class="widget-DynamicWidgetLayout widget-instance-Home_MainContentB0B0">
