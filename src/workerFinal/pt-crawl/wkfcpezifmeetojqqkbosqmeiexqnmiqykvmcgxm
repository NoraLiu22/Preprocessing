                        e.preventDefault();

                        var searchTerm = encodeURI($("#txtGenericSearchTermMobile").val());

                        if (SearchTermIsValid(searchTerm)) {
                            if (window.location.href.indexOf("parlview.aph.gov.au") > -1) {
