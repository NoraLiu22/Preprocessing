    jQuery('> ul.sub-menu',this).stop().slideDown('slow');
}
},function () {
	if (jQuery('> ul.sub-menu',this).length > 0) {
		jQuery('> ul.sub-menu',this).stop().slideUp('slow');
	}
});
});

