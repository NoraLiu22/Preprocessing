        $(function() {
            $('[data-ajax-component-banner]').each(function() {
                var $this = $(this),
                    url = $this.data('url'),
                    queryParams = $this.data('ajax-query-parameters');
                url = url+"?";
                if (queryParams) {
                    url +=  queryParams;
                }
                $.get(url).success(function(data) {
                    $this.replaceWith(data);
                });
            });
 		});
