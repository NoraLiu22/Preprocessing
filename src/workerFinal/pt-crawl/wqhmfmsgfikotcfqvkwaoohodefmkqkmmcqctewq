        $(document).ready(function () {
            $(window).scroll(function(){
		        if ($(this).scrollTop() > 2000) {
			        $('.scrollToTop').fadeIn();
		        } else {
			        $('.scrollToTop').fadeOut();
		        }
	        });
	
	        $('.scrollToTop').click(function(){
		        $('html, body').animate({scrollTop : 0},800);
		        return false;
	        });
            var $container = $('.masonry-container');
                $container.imagesLoaded( function () {
                  $container.masonry({
                    columnWidth: '.item',
                    itemSelector: '.item'
                  });   
            });
