        });
        function checkContent() {
            vpw = $(document).width();
            if(vpw>992){
                    $('.scrolled').show();
                    $('.moreSpan').show();
                    $('#tickerDivMain2').show();
                    $('#tickerBanners').show();
                    vph = $(document).height();
                    vph=vph-200;
                    vph=vph/2.5;
                    $('#newsScroll').css({'height': 7260 + 'px'});
                    $('#newsScroll').mCustomScrollbar({
                        axis: "y",
                        theme: "dark-2",
                        scrollButtons: {
                            enable: true
                        },
                        setHeight: true,
                        mouseWheel: { scrollAmount: 150 }
                    });
                    $('#reviewsScrolled').css({'height': 3795 + 'px'});
                    $('#reviewsScrolled').mCustomScrollbar({
                        axis: "y",
                        theme: "dark-2",
                        scrollButtons: {
                            enable: true
                        },
                        setHeight: true,
                        mouseWheel: { scrollAmount: 150 }
                    });
                    $('#reportScrolled').css({'height': 375 + 'px'});
                    $('#reportScrolled').mCustomScrollbar({
                        axis: "y",
                        theme: "dark-2",
                        scrollButtons: {
                            enable: true
                        },
                        setHeight: true,
                        mouseWheel: { scrollAmount: 150 }
                    });
                    $('#interviewsScrolled').css({'height': 150 + 'px'});
                    $('#interviewsScrolled').mCustomScrollbar({
                        axis: "y",
                        theme: "dark-2",
                        scrollButtons: {
                            enable: true
                        },
                        setHeight: true,
                        mouseWheel: { scrollAmount: 150 }
                    });
            }
            else{
                $('#newsScroll').css({'height': 'auto'});
                $('#reviewsScrolled').css({'height': 'auto'});
                $('#tickerDivMain2').hide();
                $('#tickerBanners').hide();
                $('.scrolled').show();
                $('.moreSpan').hide();
            }
        }
    </script>
