<script>
    $(document).ready(function () {
        $('#FiguresTableBody tr:first').addClass('highlight-row');
    });
    $(document).on('click', '#FiguresTableBody tr', function () {
        var selected = $(this).hasClass("highlight-row");
        if (selected) {
            return;
        }
        $("#FiguresTableBody tr").removeClass("highlight-row");
        if (!selected)
            $(this).addClass("highlight-row");
    });

    window.onload = function () {
        function change_pic() {
            // var button = document.getElementById("button");
            var button = $(".button");
            console.log(button);
            // var picts = document.getElementById("picts");
            $.each(button, function () {
                var pict = $(this).children(".picts:first");
                var picts = pict[0];
                console.log("you clicked me!");

                $(this).on('click', function () {
                    //window.console.log(picts.getAttribute("src", 2));
                    if (picts.getAttribute("src", 2) == "/Content/Shared/icons/down.png") {
                        // button.innerText = "Hide legend";
                        picts.setAttribute("title", "Hide legend");
                        picts.setAttribute("src", "/Content/Shared/icons/up.png");
                    } else {
                        picts.setAttribute("title", "Show legend");
                        // button.innerText = "Show legend";
                        picts.setAttribute("src", "/Content/Shared/icons/down.png");

                    };
                });

            })

        }

        change_pic();
    }
    function alignTableHeaderFooter(e) {
        //console.log($(e).parent().find(".main-table-body").width())
        $('.scroll-table-tr>td:nth-child(1)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td').css("padding-left", '0');
        $('.scroll-table-tr>td').css("padding-right", '0');
        $('.scroll-table-tr>td:nth-child(6)').css("padding-left", '5px');
        //$('.scroll-table-tr>td:nth-child(7)').css("padding-left", '5px');
        //$('.scroll-table-tr>td').css('word-break', 'break-all');
        //$('.scroll-table-tr>td').css('white-space', 'pre-line')
        //$('.scroll-table-tr>td:nth-child(1)').css("padding-left", '5px');
        //$('.scroll-table-tr>td:nth-child(1)').css("padding-right", '4px');
        $('.scroll-table-tr>td:nth-child(2)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(3)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(4)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(5)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(6)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(7)').width($(e).parent().find(".main-table-body").width() * 0.10)
        $('.scroll-table-tr>td:nth-child(8)').width($(e).parent().find(".main-table-body").width() * 0.12)
        //$('.scroll-table-tr>td:nth-child(9)').width($(e).parent().find(".main-table-body").width() * 0.18)
        $(e).width($(e).parent().find(".main-table-body").width())
    }
    $(document).ready(function () {
        $(".headerAndFooter").each(function (e, t) { alignTableHeaderFooter(t) })
    });


    // Throttle

    function alignTable() {

        $(".headerAndFooter").each(function (e, t) { alignTableHeaderFooter(t) })
    };
    function throttle(fn, delay) {
        var timer = null;
        console.log("Throttle")
        return function () {
            clearTimeout(timer);
            timer = setTimeout(function () {
                fn()
            }
            );
        }
    };
    window.onresize = throttle(alignTable, 100, 1000);
</script>

