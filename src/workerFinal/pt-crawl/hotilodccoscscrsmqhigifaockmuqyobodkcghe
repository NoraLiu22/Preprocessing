                                    <a class="c-navigation__url c-navigation__url--level-1 c-navigation__url--level-1-dropdown" data-bind="
                                    html: title,
                                    attr: {
                                        href: url,
                                        target: open_in_new_window ? '_blank' : '',
                                    },
                                    event: {
                                        focus: function() { $parent.openMenu(); },
                                        blur:  function() { $parent.closeMenu(); }
                                    }">
                                </a>
