                                    click: toggleMenuItem,
                                    css: {
                                        'is-open': isItemOpen()
                                    },
                                    attr: {
                                        'aria-expanded': isItemOpen() ? true : false,
                                        'aria-haspopup': items && items.length,
                                        tabindex: $parent.isMenuOpen() ? 0 : -1
                                    },
                                    event: {
                                        focus: function() { $parent.openMenu(); },
                                        blur:  function() { $parent.closeMenu(); }
                                    }"></a>
                                </span>
                            <!-- /ko -->
    
