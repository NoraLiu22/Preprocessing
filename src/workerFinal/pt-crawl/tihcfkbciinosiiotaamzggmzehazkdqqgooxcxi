    <section class="single-content">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="widget-title widget-component">
            <h1 class="page-title">
<p>Upcoming events</p>            </h1>
</div>    <section id="events-37e29d92-30ad-46f9-8eff-28594831f142" class="widget-events widget-component">
        <div class="list-events">

<form action="/kentico.pagebuilder/widgets/en-us/upu.widget.eventsfilterbycategory/filter" class="d-flex row" data-ajax="true" data-ajax-method="POST" data-ajax-mode="replace" data-ajax-update="#events-container-37e29d92-30ad-46f9-8eff-28594831f142" id="form0" method="post"><input name="__RequestVerificationToken" type="hidden" value="rW0vWRXv-2dNYGfdHwr096GCxDkxF6XmnSi93iJxcmMTK633ScERhBmrrPUp0OKvkT7An17c2k0MYb7KlDIBLl0oNPC3D5fgiQaeRpJpAHHjQvKCHCd4EuloFzitDq4NFqMn_ofb6Yy2Xt2AqLS7Pg2" />                    <div id="list-events-filter" class="list-events-filter">
                        <div class="row js-postback mb-3 filter-row">
    <div class="col-md-8">
        <div class="dropdown-filter form-inline justify-content-start">
            <div class="input-group">
                <select name="SelectedCategory" id="events-category" class="selectpicker select-categories" tabindex="-98" data-dropup-auto="false">
