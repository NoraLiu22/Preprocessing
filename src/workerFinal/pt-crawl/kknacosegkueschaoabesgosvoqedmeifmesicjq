  if ((ck == null) || (ck == 'mb')) { 
   var r = "https://www.shellac.net/cgi-shellac/sb/ss_mb.cgi?storeid=*1249fea48a520871965071&ss_parm=Ae1ef549a24ba862037197652bf7e5f12";   if(window.location.search){r += '&' + window.location.search.replace('?', '');}
   window.location.replace(r);
  }
