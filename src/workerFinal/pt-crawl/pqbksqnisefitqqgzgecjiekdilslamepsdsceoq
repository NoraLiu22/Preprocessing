<script>   

    function showRegisterPopup() {
        $("#register-modal-pop-up").show();
        $("#register-modal-container").show();
    }

    function registerUserModalInit() {
        $('.registerModalError').hide();
    }

    $(document).ready(function () {
        $('#registerCloseBtn').click(function () {
            $("#register-modal-pop-up").hide();
            $("#register-modal-container").hide();
        });

        $('#registerModalSubmit').click(function () {
            registerModalSubmit();
        });

        registerUserModalInit();
    });
</script>
