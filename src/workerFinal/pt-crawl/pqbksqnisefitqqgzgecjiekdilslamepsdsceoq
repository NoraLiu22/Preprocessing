        .complete-profile-modal-btn {
            margin: 20px 0 !important;
        }
    }

    .login-captcha, .whiteText {
        color: white;
    }

    .completeProfileModalError {
        border: solid 1px red;
        color: red;
        background-color: white;
    }

    .completeProfileModalSuccess {
        border: solid 1px green;
        color: green;
        background-color: white;
    }
</style>

<div id="complete-profile-modal-container" style="display: none;">
