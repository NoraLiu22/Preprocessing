                </div><!-- end modal-body -->
                <div class="modal-footer">
                    <a id="close" href="#" class="btn btn-default" data-dismiss="modal">Cancel</a>
                    <button id="add-wiki-submit" type="submit" class="btn btn-success">Add</button>
