<section class="region  region--single  region--search">      

    <form action="/search/" 
            id="advanced-search-form" 
            class="search row search__form--advanced" 
            role="search"
            data-placement="Search - Advanced">
       
        <div class="col-md-7">
            
            <fieldset>
                <legend>Search Information <em>About</em> Items</legend>
