        function checkShortcut() {
            if (event.keyCode == 8 || event.keyCode == 13) {
                return false;
            }
        }

        function popHelp() {
            var popurl = "docs/Help.htm"
            winpops = window.open(popurl, "", "width=700,height=400,scrollbars,resizable,")
        }

        function popDocument(doc) {
            winpops = window.open(doc, "", "width=700,menubar,toolbar,height=400,scrollbars,resizable,")
        }

        function popSample() {
            var popurl = "images/SampleLicense.jpg"
            winpops = window.open(popurl, "", "width=700,menubar,toolbar,height=400,scrollbars,resizable,")
        }

        function doTooltip(e, ar) {
            if (typeof Tooltip == "undefined" || !Tooltip.ready) return;
            var cntnt = wrapTipContent(ar);
            var tip = document.getElementById(Tooltip.tipID);
            if (ar[2]) tip.style.width = ar[2] + "px";
            else tip.style.width = Tooltip.defaultWidth + "px";
            Tooltip.show(e, cntnt);
        }

        function hideTip() {
            if (typeof Tooltip == "undefined" || !Tooltip.ready) return;
            Tooltip.hide();
        }

        function wrapTipContent(ar) {
            var cntnt = "";
            if (ar[0]) cntnt += '<div class="img"><img src="' + ar[0] + '" /></div>';
            if (ar[1]) cntnt += '<div class="txt">' + ar[1] + '</div>';
            return cntnt;
        }
    </script>

