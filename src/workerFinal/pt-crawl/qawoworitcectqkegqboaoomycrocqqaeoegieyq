<script type="text/javascript">
            function currentDate() {
            var today = new Date();
            var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December']
            today.setTime(today.getTime());
            return today.getDate() + " " + months[today.getMonth()] + " " + today.getFullYear();
            }
            var citation = 'Cruz, Márcia N. S. de la et al. Antimicrobial Diterpene from the Brazilian Termite Nasutitermes macrocephalus (Isoptera: Termitidae: Natutitermitinae). Journal of the Brazilian Chemical Society [online]. 2018, v. 29, n. 3 [Accessed CURRENTDATE], pp. 509-514. Available from: &lt;https://doi.org/10.21577/0103-5053.20170163&gt;. ISSN 1678-4790. https://doi.org/10.21577/0103-5053.20170163.'.replace('CURRENTDATE', currentDate());
            document.getElementById('citation').innerHTML = citation;
            document.getElementById('citationCut').value = citation.replace('&lt;', '<').replace('&gt;', ">");
        </script>
