                    require(['jquery', 'tsdist/cbs-surround'], function ($) {
                        $(function () {
                            var overrideUnit = ((location.href.split('google_force_ad_unit=')[1]||'').split('&')[0] ||'');var kvpString = '';
                            var simpleUrlTag = null;
                            if (window.cbs_ads_activated) {
                                var pageTargetting = PageTargetting.getPageLevel();
                                var slot_level = pageTargetting.forUnit('outofpage','outofpage');
                                simpleUrlTag = slot_level.toSimpleUrlTag(Date.now().toString());
                      		}

                        makeInterstitialCall();
                        
                        function makeInterstitialCall() {
                            var cookie = (document.cookie.split('; ').filter(function(value){ return value.substring(0,7) === '__gads='; })[0] || '').substring(7);
                            
                            // we need to wait for the gpt cookie, for freq. caps
                            if (cookie == '') {
                                setTimeout(makeInterstitialCall, 1000);
                                return;
                            }
                            
                            var tcString = toQueryString(window.targetting_criteria);
                            var query = { 
                                iu: (overrideUnit || '/21708449227/DUKE/out_of_page_default1'),
                                ists: 1,
                                sz: '1x1',
                                cookie: cookie,
                                t: tcString,
                                c: Date.now()
                            };

                            $.get(simpleUrlTag || ('https://pubads.g.doubleclick.net/gampad/adx?' + toQueryString(query)))
                                    .then(function(html) {
                                        if (html) document.write(html); 
                                    });
                        }

                        function toQueryString(tc){ 
                            return Object.keys(tc).map(function(key) { return key + '=' + encodeURIComponent(tc[key]); }).join('&'); 
                        }
                        });
                    }); 
                    </script></body>
</html>
