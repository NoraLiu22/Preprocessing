                    //$(".c-navigation__offcanvas").removeClass("is-open");

                });
                
                $(document).on("focusin focusout", ".s-videos .c-videos--grid .c-videos__play-btn", function(e){
                    
                    if (e.type === "focusin") {

                        $(this).closest(".c-videos__item").addClass("sidearm-hover");

                    } else if (e.type === "focusout") {

                        $(this).closest(".c-videos__item").removeClass("sidearm-hover");
                        
                    }
                    
                });

                $(document).on("keyup keydown", ".c-search__toggle, .is-open .c-search__close", function (e) {
                    
                    var $this = $(this),
                        $search = $this.closest(".c-search--overlay"),
                        $overlay = $search.find(".c-search__overlay"),
                        key = e.which,
                        shift = e.shiftKey,
                        type = e.type;
                    
                    if (type === "keyup" && (key === 32 || key === 13)) {
                        
                        e.preventDefault();
                        
                    }
                    
                    if (type === "keydown" && (key === 32 || key === 13)) {
                        
                        if ($this.hasClass("c-search__toggle")) {
                        
                            //$this.click();
                            
                            setTimeout(function(){
                                $overlay.find(".c-search__text").focus();
                            }, 100);
                        
                        } else if ($this.hasClass("c-search__close")) {
                            
                            $search.find(".c-search__toggle").focus();
                            
                        }
                        
                    }
                    
                });
                
                $(document).on("click", ".c-search__toggle", function (e) {
                    
                    var $this = $(this),
                        $search = $this.closest(".c-search--overlay"),
                        $overlay = $search.find(".c-search__overlay");
                        
                    if ($this.hasClass("c-search__toggle")) {
                        
                        setTimeout(function(){
                            $overlay.find(".c-search__text").focus();
                        }, 100);
                        
                    } else {
                        
                        //console.log("hasNoClass");
                        
                    }
                    
                });
                
                $(document).on("keyup keydown", ".is-open .c-search__text, .is-open .c-search__submit, .is-open .c-search__close", function (e) {

                    var $this = $(this),
                        $overlay = $this.closest(".c-search__overlay"),
                        key = e.which,
                        shift = e.shiftKey,
                        type = e.type;
                        
                    if (key === 9) {

                        e.preventDefault();

                        if (type === "keydown") {
                            
                            if ($this.hasClass("c-search__text")) {
                                
                                if (shift) {
                                    $overlay.find(".c-search__close").focus();
                                } else {
                                    $overlay.find(".c-search__submit").focus();
                                }
                                
                            } else if ($this.hasClass("c-search__submit")) {
                                
                                if (shift) {
                                    $overlay.find(".c-search__text").focus();
                                } else {
                                    $overlay.find(".c-search__close").focus();
                                }
                                
                            } else if ($this.hasClass("c-search__close")) {
                                
                                if (shift) {
                                    $overlay.find(".c-search__submit").focus();
                                } else {
                                    $overlay.find(".c-search__text").focus();
                                }
                                
                            }

                        }
                    
                    }
                    
                });
                
                $(document).on("focusin focusout", ".c-search__toggle", function (e) {
                    
                    var $this = $(this),
                        type = e.type,
                        $mobile_nav = $(".c-navigation--main.c-navigation--mobile .c-navigation__offcanvas");
                    
                    if (type === "focusin") {
                        
                        if ($mobile_nav.length) {
                            
                            console.log("$mobile_nav", $mobile_nav);
                            
                            if ($mobile_nav.hasClass("is-open")) {
                                $(".c-navigation--main.c-navigation--mobile .c-navigation__toggle").click();
                            }
                            
                        }
                        
                        //$(".c-navigation__toggle").click();
                        
                    } else if (type === "focusout") {
                        
                        //is_open = $this.hasClass("is-open")
                        
                    }
                    
                });
                
                var nav_selectors_click = [
                    ".c-navigation--main.c-navigation--mobile .c-navigation__url--level-1",
                    ".c-navigation--main.c-navigation--mobile .c-navigation__url--level-2",
                    ".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__toggle",
                    '.sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__parent .c-navigation__url--level-1'
                ];

                $(document).on("keyup keydown", nav_selectors_click.join(", "), function(e) {
                    
                    var $this = $(this),
                        $overlay = $this.closest(".c-search__overlay"),
                        key = e.which,
                        shift = e.shiftKey,
                        type = e.type,
                        $mobile_nav = $(".c-navigation--mobile");
                        
                    if (key === 32 || key === 13) {
                        
                        if (type === "keyup") {
                            e.preventDefault();
                        }
                        
                        if (type === "keydown") {
                            
                            if ($this.hasClass("c-navigation__url--level-1") && ($this.closest(".main-header").length) && ($this.closest(".c-navigation__parent").length <= 0)) {
                                
                                console.log("elifvbwepiaof");
                                
                                var is_open = $this.hasClass("is-open"),
                                    $level_2_links = $this.parent().find(".c-navigation__level-2 a");
                                
                                if (is_open) {
                                    
                                    $level_2_links.attr("tabindex", "-1");
                                    
                                } else {
                                    
                                    setTimeout(function(){
                                        
                                        $level_2_links.attr("tabindex", "0");
                                        
                                        $level_2_links.first().focus();
                                        
                                    }, 100);
                                    
                                }
                                
                            } else if ($this.hasClass("c-navigation__toggle")) {
                                
                                setTimeout(function(){
                                    
                                    $(".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__url--level-1").first().focus();
                                    
                                }, 100);
                                
                            } else if ($this.hasClass("c-navigation__url--level-1") && ($this.closest(".c-navigation__parent").length)) {
                                
                                e.preventDefault();
                                e.stopPropagation();
                                
                                console.log("Bah >:-(");
                                
                                //$this.click();
                                
                                if ($this.hasClass("is-open")) {
                                    
                                    $this.closest("li").removeClass("is-open");
                                    $this.closest("li").find(".c-navigation__level-2").removeClass("is-open");
                                    $this.removeClass("is-open");
                                    $this.closest("li").find(".c-navigation__level-2").find("a").attr("tabindex", "-1");
                                    
                                    $this.focus();
                                    
                                } else {
                                    
                                    $this.closest("li").addClass("is-open");
                                    $this.closest("li").find(".c-navigation__level-2").addClass("is-open");
                                    $this.addClass("is-open");
                                    $this.closest("li").find(".c-navigation__level-2").find("a").attr("tabindex", "0");
                                    
                                    setTimeout(function(){
                                        $this.closest("li").find(".c-navigation__level-2").find(".c-navigation__item--level-2").first().focus();
                                    }, 100);
                                    
                                }
                                    
                                
                            }
                            
                        }
                        
                    }
                    
                });

                var nav_selectors_tab = [
                    ".main-header__logo-holder",
                    ".c-search__toggle", 
                    ".c-navigation--main.c-navigation--mobile .c-navigation__toggle",
                    ".c-navigation--main.c-navigation--mobile .c-navigation__url--level-1:last",
                    ".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__sport-link",
                    ".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__toggle"
                ];

                $(document).on("keyup keydown", nav_selectors_tab.join(", "), function(e) {
                    
                    var $this = $(this),
                        $overlay = $this.closest(".c-search__overlay"),
                        key = e.which,
                        shift = e.shiftKey,
                        type = e.type,
                        $mobile_nav = $(".c-navigation--mobile");
                    
                    //Enter/Space
                    if (key === 32 || key === 13) {
                        
                        if (type === "keyup") {
                            e.preventDefault();
                        }
                        
                        if (type === "keydown") {
                            
                            if ($mobile_nav.length && $this.hasClass("c-navigation__toggle")) {
                                
                                if (!$this.hasClass("is-toggled")) {
                                    
                                    $this.click();
                                    $(".c-navigation--main.c-navigation--mobile .c-navigation__parent .c-navigation__url--level-1:first").focus();
                                    
                                }
                                
                            }   
                            
                        }
                    
                    }
                    
                    //Tab
                    if (key === 9) {

                        if (type === "keyup") {
                            
                            e.preventDefault();
                            
                            if ($mobile_nav.length) {
                            
                                if ($this.hasClass("c-navigation__toggle") && ($this.closest(".main-header").length) && ($this.closest(".sport-navigation").length <= 0)) {
                                    
                                    console.log("Meh!");
                                    
                                    if ( $this.hasClass("is-toggled") ) {
                                        
                                        $this.click();
                                        
                                    }
                                    
                                }
                            
                            }
                            
                            //.c-navigation--main.c-navigation--mobile .c-navigation__toggle
                            
                            
                        }
                        
                        if (type === "keydown") {
                            
                            if ($mobile_nav.length) {
                                
                                e.preventDefault();

                                if ($this.hasClass("main-header__logo-holder")) {
                                    
                                    e.preventDefault();
                                    
                                    if (shift) {
                                        $(".sidearm-skip-link-main").focus();
                                    } else {
                                        $(".c-search__toggle").focus();
                                    }
                                    
                                } else if ($this.hasClass("c-search__toggle")) {
                                    
                                    e.preventDefault();
                                    
                                    if (shift) {
                                        $(".main-header__logo-holder").first().focus();
                                    } else {
                                        $(".c-navigation--main.c-navigation--mobile .c-navigation__toggle").focus();
                                    }
                                    
                                } else if ($this.hasClass("c-navigation__toggle") && ($this.closest(".main-header").length)) {
                                    
                                    e.preventDefault();
                                    
                                    if (shift) {
                                        
                                        $(".c-search__toggle").first().focus();
                                        
                                    } else {
                                        
                                        //var tabindex = $.trim($(".c-navigation--main.c-navigation--mobile .c-navigation__parent .c-navigation__url--level-1:first").attr("tabindex"));
                                        
                                        //console.log("tabindex", tabindex);
                                        
                                        //if (tabindex != "-1") {
                                            //$(".c-navigation--main.c-navigation--mobile .c-navigation__parent .c-navigation__url--level-1:first").focus();
                                        //} else {
                                            //$("main a:not(.sidearm-skip-landing):first").first().focus();
                                        //}
                                        
                                        var $sport_nav_sport_link = $(".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__sport-link");
                                        
                                        if ($sport_nav_sport_link.length) {
                                            $sport_nav_sport_link.first().focus();
                                        } else {
                                            $("main a:not(.sidearm-skip-landing):first").first().focus();
                                        }
                                        
                                    }
                                    
                                } else if ($this.hasClass("c-navigation__url--level-1") && ($this.closest(".main-header").length)) {
                                    
                                    e.preventDefault();
                                    
                                    if (shift) {
                                    
                                        if ($this.closest(".sport-navigation").length) {
                                            
                                            if ($mobile_nav.length) {
                                                
                                                $(".c-navigation--main.c-navigation--mobile .c-navigation__toggle").focus();
                                                
                                            } else {
                                                
                                                $(".c-search--overlay .c-search__toggle").first().focus();
                                                
                                            }
                                            
                                        }
                                        
                                    } else {

                                        //$(".c-navigation--main.c-navigation--mobile .c-navigation__toggle").click();

                                        if ($(".c-navigation__sport-link").length) {

                                            $(".c-navigation__sport-link").focus();

                                        } else {

                                            $("main a:not(.sidearm-skip-landing):first").first().focus();

                                        }
                                        
                                        //$(".c-navigation__toggle").click();
                                        //$("main a:not(.sidearm-skip-landing):first").first().focus();
                                        
                                    }
                                    
                                } else if ($this.hasClass("c-navigation__sport-link") && $this.closest(".sport-navigation").length) {

                                    console.log(".c-navigation__sport-link");
                                    
                                    e.preventDefault();
                                    e.stopPropagation();
                                    
                                    if (shift) {
                                        
                                        $(".c-navigation--main.c-navigation--mobile .c-navigation__toggle").focus();
                                        
                                    } else {

                                        $(".sport-navigation .c-navigation--sport.c-navigation--mobile .c-navigation__toggle").focus();
                                        
                                        //$("main a:not(.sidearm-skip-landing)")
                                            .not("[tabindex='-1']")
                                            .not(".c-navigation__url")
                                            .not(".c-navigation__sport-link").first().focus();
                                        
                                    }
                                    
