        <script>

            require(["jquery"], function () {

                //ROSTER TWEAKS (so they do not need to use the First Name field)
                //roster view - default
                $('.sidearm-roster-default-template .sidearm-roster-player-name a').each(function(){
                    var roster_name = $(this).html();
                    var nbsp = '&nbsp;';
                    roster_name_trimmed = roster_name.replace(nbsp,"");
                    $(this).html(roster_name_trimmed);
                });
                // roster view - grid
                $('.sidearm-roster-grid-template-1 .sidearm-table-player-name a').each(function(){
                    var roster_name = $(this).html();
                    var nbsp = '&nbsp;';
                    var roster_name_trimmed = roster_name.replace(nbsp,"");
                    $(this).html(roster_name_trimmed);
                });
