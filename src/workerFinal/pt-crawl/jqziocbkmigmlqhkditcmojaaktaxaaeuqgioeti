    $(document).ready(function() {
        GLOTTOLOG3.Slider.init(0);
        $("#macroarea").change(GLOTTOLOG3.LangdocStatus.reload);
        $("#msfamily").on("change", GLOTTOLOG3.LangdocStatus.reload);
    });
</script>



