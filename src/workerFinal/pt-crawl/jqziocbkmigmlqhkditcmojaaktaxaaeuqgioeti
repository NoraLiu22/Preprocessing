        <a style="cursor: pointer;"
           title="click to open list of languages"
           onclick="GLOTTOLOG3.LangdocStatus.loadLanguages(9, 9)"
           id="cell-total-total"> </a>
    </td>

                </tr>
                </tbody>
            </table>
        
    </div>

    </div>
</div>

<div class="row-fluid">
    <div class="span12" id="languages">
    </div>
</div>

