<!-- JS to open and close sidebar with overlay effect -->
<script>
function w3_open_languages() {
  document.getElementById("SidebarLanguage").style.display = "block";
  document.getElementById("overlay_languages").style.display = "block";
}
function w3_open_sections() {
  document.getElementById("SidebarSection").style.display = "block";
  document.getElementById("overlay_sections").style.display = "block";
}
function w3_close_languages() {
  document.getElementById("SidebarLanguage").style.display = "none";
  document.getElementById("overlay_languages").style.display = "none";
}
function w3_close_sections() {
  document.getElementById("SidebarSection").style.display = "none";
  document.getElementById("overlay_sections").style.display = "none";
}
function open_accordion(id) {
  var x = document.getElementById(id);
  if (x.className.indexOf("w3-show") == -1) {
    x.className += " w3-show";
  } else { 
    x.className = x.className.replace(" w3-show", "");
  }
}
function w3_close_accordion(id) {
  var x = document.getElementById(id);
  x.className = x.className.replace(" w3-show", "");
}
</script>

