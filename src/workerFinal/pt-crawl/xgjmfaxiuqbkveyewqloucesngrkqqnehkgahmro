<script>
        function getAPLocalStorage(key) {
            try {
                var json = window.localStorage.getItem(key);
                if (!json) {
                    return null;
                }
                var data = JSON.parse(json);
                return data;
            }
            catch (err) {
                window.localStorage.removeItem(key);
            }
        }

