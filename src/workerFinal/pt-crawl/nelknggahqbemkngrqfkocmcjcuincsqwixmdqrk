) != -1 ) {		menuLayer			+= "imperialIconMenu";	}		menuLayer = eval( menuLayer + ".document.images['" + object + "']" );	return ( menuLayer );}//	_______________________________________________________________ setMarkerOnfunction setMarkerOn( imageName ) {	if ( document.images ) {		if ( microsoftDOM ) {			document.images[imageName].src = markerOn.src;		}/*		if ( netscapeDOM ) {	//		obj = document.layers['content'].document.layers['fiftyAndBeyondMenu'].document.layers;			obj = document.menuContainer.document.fiftyAndBeyondMenu.document.images;	//		obj = document.layers['content'].layers;			msg = "";			for ( var i in obj ) {				msg += i + "=" + obj[i] + "\n";			}			alert( msg );			//			indicator		= determineMenuLayer( imageName );//			indicator.src	= markerOn.src;		} else {			document.images[imageName].src = markerOn.src;		}*/	}}//	______________________________________________________________ setMarkerOfffunction setMarkerOff( imageName ) {	if ( document.images ) {		if ( microsoftDOM ) {			document.images[imageName].src = markerOff.src;		}/*		if ( netscapeDOM ) {			indicator		= determineMenuLayer( imageName );			indicator.src	= markerOff.src;		} else {			document.images[imageName].src = markerOff.src;		}*/	}}/* ************************************************************************* *///	___________________________________________________ getObjectStyleReferencefunction getObjectStyleReference( object ) {	var objectStyleReference;		if ( typeof object == "string" ) {		objectStyleReference = eval( container + "document." + collection + object + styleObject );	} else {		objectStyleReference = object;	}		return ( objectStyleReference );}//	________________________________________________________________ showObjectfunction showObject( object ) {	var theObject = getObjectStyleReference( object );		theObject.visibility = VISIBLE;}//	______________________________________________________________________ showfunction show( object, parent ) {	if ( browserSupportsDHTML ) {		if ( object != currentMenu ) {			if ( currentMenu != "" ) {				hide( currentMenu, currentParent );			}						container = ( parent && netscapeDOM ) ? parent : "";			showObject( object );			currentMenu = object;			currentParent = ( parent ) ? parent : "";		}//	} else {//		document.location.hash = ( object.substring( 0, object.indexOf( "Layer" ) ) );	}}//	________________________________________________________________ hideObjectfunction hideObject( object ) {	var theObject = getObjectStyleReference( object );		theObject.visibility = HIDDEN;}//	______________________________________________________________________ hidefunction hide( object, parent ) {	if ( browserSupportsDHTML ) {		container = ( parent && netscapeDOM ) ? parent : "";		hideObject( object );	}}//--></script><style type="text/css"><!--.fontStyle {	font-family:		verdana;	font-size:			10px;	padding:			4px;}.listStyle {	font-family:		verdana;	font-size:			10px;}.container {	position:			relative;}.menu {	position:			absolute;	padding:			4 4 4 6;	z-index:			15;	font-family:		arial,helvetica;	font-size:			10px;	font-weight:		bold;	visibility:			hidden;}.menuItem {	font-family:		arial,helvetica;	font-size:			10px;	font-weight:		bold;}.menuSection {	font-family:		arial,helvetica;	font-size:			10px;	font-weight:		bold;	color:				#FFCC66;}#fiftyAndBeyondMenu {	background-color:		#663300;	layer-background-color:	#663300;	top:	0;}#insideChinaMenu {	background-color:		#663366;	layer-background-color:	#663366;	top:	85;}#redGiantMenu {	background-color:		#CC0000;	layer-background-color:	#CC0000;	top:	175;}#asianSuperpowerMenu {	background-color:		#333300;	layer-background-color:	#33330