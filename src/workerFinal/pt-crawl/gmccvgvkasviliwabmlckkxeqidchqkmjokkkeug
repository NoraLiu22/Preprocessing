<script type="text/javascript">
$((function () {
  $(".scroll_arrow").on("click", (function (event) {
      event.preventDefault(), $("html, body").animate({
          scrollTop: $("#scroll_to_section").offset().top
      }, 450)
  }));
  var custom_lab_block_distance = $(".custom_left_aligned_block").offset().top;

  function autoAdjustHeight() {
      var waitForImg = setInterval((function () {
          var custom_la_img = document.getElementById("custom_left_aligned_image");
          if (custom_la_img) {
              function autoUpdateHeight() {
                  var custom_left_aligned_block_ref = document.querySelector("#custom_left_aligned_block"),
                      custom_left_aligned_gradient = document.querySelector("#custom_left_aligned_gradient") ? document.querySelector("#custom_left_aligned_gradient").offsetHeight : 0,
                      custom_la_block_height = document.querySelector("#custom_left_aligned_image").offsetHeight;
                  window.innerWidth >= 1400 ? custom_left_aligned_block_ref.style.height = custom_la_block_height + 1 + "px" : window.innerWidth > 1200
                   && window.innerWidth < 1400 ? custom_left_aligned_block_ref.style.height = custom_la_block_height + custom_left_aligned_gradient + "px" : window.innerWidth > 1024 
                   && window.innerWidth <= 1200 ? custom_left_aligned_block_ref.style.height = custom_la_block_height + custom_left_aligned_gradient / 1.6 + "px" : window.innerWidth > 767 
                   && window.innerWidth <= 1024 ? custom_left_aligned_block_ref.style.height = custom_la_block_height + custom_left_aligned_gradient / 1.8 + "px" : custom_left_aligned_block_ref.style.height = "auto"
              }
              clearInterval(waitForImg), autoUpdateHeight(), window.addEventListener("orientationchange", autoUpdateHeight), window.addEventListener("resize", autoUpdateHeight), custom_la_img.onload = function () {
                  autoUpdateHeight()
              }
          }
      }), 50)
  }
  $("section.tt-strap").css({
      "z-index": "12",
      "background-color": "transparent"
  }), $(window).scrollTop() >= custom_lab_block_distance ? $("section.tt-strap").css({
      "z-index": "14",
      "background-color": custom_tt_strap_background
  }) : $("section.tt-strap").css({
      "z-index": "10",
      "background-color": "transparent"
  }), $(window).on("scroll", (function () {
      var custom_lab_block_distance = $(".custom_left_aligned_block").offset().top;
      $(this).scrollTop() >= custom_lab_block_distance ? ($("section.tt-strap").css({
          "z-index": "12",
          "background-color": custom_tt_strap_background
      }), $(".custom_left_aligned_image").css({
          "z-index": "8;"
      }), $(".custom_left_aligned_image_mobile").css({
          "z-index": "8;"
      }), $(".inner_block").css({
          "z-index": "8;"
      })) : ($("section.tt-strap").css({
          "z-index": "10",
          "background-color": "transparent"
      }), $(".custom_left_aligned_image").css({
          "z-index": "12;"
      }), $(".custom_left_aligned_image_mobile").css({
          "z-index": "12;"
      }), $(".inner_block").css({
          "z-index": "12;"
      }))
  })), autoAdjustHeight()
}));
</script>

