	<script>
		// Toggle citations.
		function toggleCitations(id) {
			btn = $(id);
			cites = $(id + ' ~ span');
			if (cites.is(':visible')) {
				btn.text(' (View Citations)');
				cites.hide();
			}
			else {
				btn.text(' (Hide Citations)');
				cites.show();
			}
		}
		// Trending.
        $( function() {
            $.getJSON("/data/trending.json", function(data) {
                var links = '';
                $.each(data, function(key, val) {
                    links += '<li><a href="/entry/' + key + '.html">' + val + '</a></li>';
                });
                $("#trending").html(links);
            });
        });
	</script>
