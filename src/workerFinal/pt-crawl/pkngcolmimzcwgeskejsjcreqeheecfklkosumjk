  <div class="views-element-container settings-tray-editable block block--views-block--glimps-homepage-webtv-homepage-webtv" data-drupal-settingstray="editable">

  
    

      <div>

<div class="view view--glimps-homepage-webtv view-display--homepage-webtv js-view-dom-id-785038c1dd8ae4fdc44739a6b122501dcc35a3b5e18f569decdb7d9bdb915625 mb-5 mb-md-7">
  
  
  

        
        
  
  

      
    <div class="view-content">
      
  

  
  <div class="views-row-wrapper row">
          <div class="views-row"><article class="node media-asset media-asset--video-player-home">
      
  <div class="field field--name-field-proxy field--type-entity-reference-revisions field--label-hidden field__items media-asset__field-proxy">
        <div class="field__item">  <section class="video w-100 bg-primary">
    <div id="un3-video-player" class="un3-video-player"></div>
    <script type="text/javascript" src="https://cdnapisec.kaltura.com/p/2503451/embedPlaykitJs/uiconf_id/49754663"></script>
<script type="text/javascript">
// This script might execute before KalturaPlayer or jQuery loaded, so we need to
// make sure that we have them.
function waitForDependencies() {
	if(typeof KalturaPlayer !== "undefined" && typeof jQuery !== "undefined") {
		kalturaScript();
	} else {
		setTimeout(waitForDependencies, 50);
	}
}
waitForDependencies();
// Wait for jQuery to load.
function kalturaScript() {
	// Initialize Kaltura Player
	var kalturaPlayer;
	const scheduledValue = 'Live';
	const mediaScheduled = scheduledValue === 'Scheduled';
    const kentryID = '1_gb6tjmle';

	function getPlayer(playerConfig, mediaInfo) {
		try {
			kalturaPlayer = KalturaPlayer.setup(playerConfig);
			kalturaPlayer.loadMedia(mediaInfo);
			return kalturaPlayer;
		} catch(e) {
			console.error(e.message);
		}
	}
	// Get Kaltura player config ready
	// See https://github.com/kaltura/playkit-js/blob/master/docs/configuration.md
	// 1_j294ho01, 1_zn0lt3x0, 1_fblzvysa (Live), 1_qhuse5bi, 1_sh7yz72p (Multi Audio)
	var mediaInfo = {
		entryId: '1_gb6tjmle'
	};
	var playerConfig = {
		targetId: "un3-video-player",
		provider: {
			uiConfId: 49754663,
			partnerId: 2503451
		},
		playback: {
			audioLanguage: 'eng'
		},
		plugins: {
			"kaltura-live": {
				checkLiveWithKs: false,
				isLiveInterval: 2,
				bufferingFailoverTimeout: 60
			}
		},
		customLabels: {
			audio: function(audioTrack) {
				switch(audioTrack.language) {
					case 'es':                                             
						return audioTrack.label = "Español";
						break;
					case 'spa':
                        if (kentryID == "1_rm37rrmf") {
                            return audioTrack.label = "English";
                        } else {
							return audioTrack.label = "Español";
                        }
						break;
					case 'fr':
						return audioTrack.label = "Français";
						break;
					case 'fre':
						return audioTrack.label = "Français";
						break;
					case 'ar':
						return audioTrack.label = "العربية";
						break;
					case 'ara':
						return audioTrack.label = "العربية";
						break;
					case 'ru':
						return audioTrack.label = "Русский";
						break;
					case 'rus':
						return audioTrack.label = "Русский";
						break;
					case 'zh':
						return audioTrack.label = "中文";
						break;
					case 'chi':
						return audioTrack.label = "中文";
						break;
					case 'ia':
						return audioTrack.label = "Original";
						break;
					case 'ina':
						return audioTrack.label = "Original";
						break;
					case 'en':
						return audioTrack.label = "English";
						break;
					case 'eng':
                        if (kentryID == "1_rm37rrmf") {
                            return audioTrack.label = "Español";
                        } else {                                                  
							return audioTrack.label = "English";
                        }
						break;
					case 'de':
						return audioTrack.label = "Deutsch";
						break;
					case 'ger':
						return audioTrack.label = "Deutsch";
						break;
					case 'pl':
						return audioTrack.label = "Polski";
						break;
					case 'pol':
						return audioTrack.label = "Polski";
						break;
					case 'sw':
						return audioTrack.label = "kiswahili";
						break;
					case 'swa':
						return audioTrack.label = "kiswahili";
						break;
					case 'ja':
						return audioTrack.label = "日本語";
						break;
					case 'jpn':
						return audioTrack.label = "日本語";
						break;
					case 'pt':
						return audioTrack.label = "Português";
						break;
					case 'por':
						return audioTrack.label = "Português";
						break;
					case 'hi':
						return audioTrack.label = "हिन्दी";
						break;
					case 'hin':
						return audioTrack.label = "हिन्दी";
						break;
					case 'tr':
						return audioTrack.label = "Türkçe";
						break;
					case 'tur':
						return audioTrack.label = "Türkçe";
						break;
					case 'az':
						return audioTrack.label = "Azərbaycan";
						break;
					case 'aze':
						return audioTrack.label = "Azərbaycan";
						break;
					default:
						return;
				}
			}
		}
	};
	// Screen Aspect ratio changes what needs to be hidden or not
	var screenRatio = jQuery(window).width() / jQuery(window).height();
	var screenWidth = jQuery(window).width();
	jQuery(window).resize(function() {
		screenRatio = jQuery(window).width() / jQuery(window).height();
		screenWidth = jQuery(window).width();
		checkNav();
	});

	function checkNav() {
		if(screenWidth < 992) {
			if(screenRatio < 1) {
				jQuery('.navbar').show();
			} else {
				jQuery('.navbar').hide();
			}
		} else {
			if(screenRatio > 1) {
				jQuery('.navbar').show();
			} else {
				jQuery('.navbar').hide();
			}
		}
	}

	function showNav() {
		if((screenRatio >= 1) || (screenWidth >= 992)) {
			jQuery('.navbar').fadeIn();
		}
	}

	function hideNav() {
		if((screenRatio >= 1) || (screenWidth >= 992)) {
			jQuery('.navbar').fadeOut();
		}
	}
	var player = getPlayer(playerConfig, mediaInfo);
	var iframeval = '';
	jQuery(document).on({
		click: function() {
			iframeval = jQuery("div.playkit-input-copy-url input.playkit-form-control").val();
			if(iframeval.includes("iframe")) {
				var matches = iframeval.match(/\b(https|http)?:\/\/\S+\b/gi);
				var res = matches + '&config[playback]={"audioLanguage":"en"}' + '&config[ui]={"locale":"en"}';
				iframeval = iframeval.replace(matches, res);
				jQuery("div.playkit-input-copy-url input.playkit-form-control").val(iframeval);
			}
		}
	}, 'i.playkit-icon-embed, label#start-from-label');
	jQuery(document).on('change', 'input[type="text"]', function() {
		if(jQuery('div.playkit-video-start-options-row input#start-from').is(":checked")) {
			iframeval = jQuery("div.playkit-input-copy-url input.playkit-form-control").val();
			if(iframeval.includes("iframe")) {
				var matches = iframeval.match(/\b(https|http)?:\/\/\S+\b/gi);
				var res = matches + '&config[playback]={"audioLanguage":"en"}' + '&config[ui]={"locale":"en"}';
				iframeval = iframeval.replace(matches, res);
				jQuery("div.playkit-input-copy-url input.playkit-form-control").val(iframeval);
			}
		} else if(iframeval.length > 0) {
			jQuery("div.playkit-input-copy-url input.playkit-form-control").val(iframeval);
		}
	});
	jQuery(document).ready(function() {
			if(mediaScheduled) {
				//showNav();
				jQuery('#un3-video-overlay .un3-video-overlay-content').removeAttr("style");
			}
		})
		//   if (!mediaScheduled) {
		// Menu appears on mouse move
	var timeout = null;
	jQuery(document).on('mousemove', function() {
		clearTimeout(timeout);
		if(!((screenRatio >= 1) && (screenWidth < 992))) {
			showNav();
		}
		timeout = setTimeout(function() {
			var scroll = jQuery(window).scrollTop();
			if(((scroll == 0) && (!player.paused)) || ((scroll == 0) && mediaScheduled)) {
				hideNav();
			}
		}, 2000);
	});
	// Menu appears on scroll
	jQuery(window).scroll(function(event) {
		var scroll = jQuery(window).scrollTop();
		if(scroll == 0) {
			if(!player.paused || mediaScheduled) {
				hideNav();
			}
		} else {
			showNav();
		}
	});
	// Hide and show page elements on load
	var isWebTVHome;
	if(mediaScheduled) {
		isWebTVHome = true;
	}
	jQuery(() => {
			if(!isWebTVHome) {
				checkNav();
			}
			jQuery('.un3-video-cover').css('opacity', '0');
			// jQuery('.un3-video-overlay-content').slideDown(500)
			jQuery('.un3-video-overlay-content').css('opacity', '1');
			player.ready().then(() => {
                                 jQuery('i.playkit-icon-share').click(() => {
					jQuery('.playkit-gui-area .overlay-portal').css({'z-index': '2000'});
				});
                                 jQuery('.playkit-bottom-bar').css({'max-height':'70px'});
                                 jQuery('.live-tag__live-tag___UmfOr').css({'margin':'10px 12px'});
				// Show chapters on first play.
				player.addEventListener(player.Event.Core.FIRST_PLAY, event => {
					jQuery('.cue__chapters_container').removeClass('d-none');
				});
				jQuery('.right-upper-bar-wrapper-container').prependTo('.playkit-bottom-bar .playkit-right-controls');
				// Make player bar same width as the rest of the UI.
				jQuery('.playkit-bottom-bar').addClass("container");
				jQuery('.playkit-bottom-bar').after("<div class='playkit-bottom-bar playkit-bottom-bar-background'></div>");
				jQuery('.playkit-player').after("<div class='un-video-player-watermark'></div>");
				jQuery('.playkit-time-display').before("<div class='playkit-webtv-logo'><span>UN Web TV</span></div>");
				player.addEventListener(player.Event.Core.PAUSE, event => {
					jQuery('.player').removeClass('player-play');
					jQuery('#un3-video-overlay').fadeIn();
					showNav();
				});
				player.addEventListener(player.Event.Core.PLAY, event => {
					jQuery('.player').addClass('player-play');
					hideNav();
					jQuery('#un3-video-overlay').fadeOut("normal", () => {
						if(isWebTVHome) {
							jQuery('.un3-video-overlay').removeClass('un-video-overlay-index-fresh');
						}
					});
				});
				// Manage Chapters.
				let chapterTimestamps = jQuery('.cue__item').map(function() {
					return jQuery(this).data('start-time');
				}).get();
				if(chapterTimestamps.length > 0) {
					let currentChapter = 0;
					let nextChapter = 0;
					jQuery.fn.scrollTo = function(elem, speed) {
						jQuery(this).animate({
							scrollLeft: jQuery(this).scrollLeft() - jQuery(this).offset().left + jQuery(elem).offset().left
						}, speed === undefined ? 1000 : speed);
						return this;
					};
					player.addEventListener(player.Event.Core.TIME_UPDATE, event => {
						if(player.currentTime >= currentChapter && player.currentTime < nextChapter) {
							return;
						}
						jQuery('.cue__item').removeClass('cue__item_active');
						jQuery.each(chapterTimestamps, function(index, value) {
							if(player.currentTime >= value) {
								let nextKey = index + 1;
								if(!chapterTimestamps.hasOwnProperty(nextKey) || player.currentTime < chapterTimestamps[nextKey]) {
									currentChapter = value;
									nextChapter = !chapterTimestamps.hasOwnProperty(nextKey) ? Number.MAX_VALUE : chapterTimestamps[nextKey];
									jQuery('.cue__item[data-start-time="' + currentChapter + '"]').addClass('cue__item_active');
									jQuery(".cue__items").scrollTo('.cue__item[data-start-time="' + currentChapter + '"]', 2000);
									return false;
								}
							}
						});
					});
				}
			});
		})
		//   }
		// For UN Web TV Home, clicking on videos on live schedule replaces top video.
	jQuery(document).ready(function() {               
		// Onclick function for the overlay		
		jQuery('.un3-video-play-button').click(() => {
			//      console.log('PLAYING');
			player.play();                                      
			if(mediaScheduled) {
				hideNav();
				jQuery('#un3-video-overlay').fadeOut("normal", () => {
					if(isWebTVHome) {
						jQuery('.un3-video-overlay').removeClass('un-video-overlay-index-fresh');
					}
				});
			}
		});
		jQuery('.cue__chapters_toggle').removeClass('d-none').appendTo('.playkit-bottom-bar .playkit-right-controls');
		jQuery('.cue__item').click(function() {
			const startTime = jQuery(this).data('start-time');
			player.currentTime = startTime;
		}).find('.cue__show, .cue__hide').click(function() {
			return false;
		});
		jQuery('.cue__chapters_toggle').click(function() {
			jQuery('.cue__chapters_container').toggleClass('d-none');
		});
		jQuery('.cue__chapters_close').click(function() {
			jQuery('.cue__chapters_container').addClass('d-none');
		});
		jQuery('.cue__show').click(function() {
			jQuery(this).closest('.cue__collapse').addClass('show');
		});
		jQuery('.cue__hide').click(function() {
			jQuery(this).closest('.cue__collapse').removeClass('show');
		});
	})
};
</script>
  </section>

  <section id="un3-video-overlay" class="un3-video-overlay un3-video-play-button un3-video-overlay-index-fresh w-100 align-items-center ratio ratio-16x9" style="display: flex; background-image: linear-gradient(180deg, rgba(0, 0, 0, 0.5), black), url(//cfvod.kaltura.com/p/2503451/sp/250345100/thumbnail/entry_id/1_gb6tjmle/width/1200/height/822/type/4);">
    <div class="un3-video-overlay-text w-100 d-flex align-items-center">
      <div class="un3-video-overlay-content container px-2 px-sm-3" style="opacity: 0; transition: opacity 1s;">
        <div class="row">
                      <div class="col-12">
              <h3 class="border-0 mb-2"><span class="badge badge-danger bg-danger text-uppercase">Live</span></h3>
            </div>
                    <div class="col-12">
                          <h1 class="jumbotron-heading display long-texts" data-bs-toggle="tooltip" data-bs-placement="top" title="24 Hour Live and pre-recorded Programming">
                <i class="fas fa-play"></i>
                24 Hour Live and pre-recorded Programming
              </h1>
                      </div>
