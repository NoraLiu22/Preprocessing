<article data-history-node-id="17731" class="node node--type-landing-page node--promoted node--view-mode-full">

			<div style="margin-top: -1.5rem"></div>
			  <div class="grid-container element-relative">
    <div class="grid-x grid-padding-x">
		<div class="cell large-8">
		  <div id="readspeaker_button1" class="rs_skip rsbtn rs_preserve">
			<a rel="nofollow" class="rsbtn_play" title="Listen to this page using ReadSpeaker webReader" href="https://app-oc.readspeaker.com/cgi-bin/rsent?customerid=14232&amp;lang=en_au&amp;readclass=rs-readclass&amp;url=/2023-australian-human-rights-awards?page=8">
				<span class="rsbtn_left rsimg rspart"><span class="rsbtn_text"><span>Listen</span></span></span>
				<span class="rsbtn_right rsimg rsplay rspart"></span>
			</a>
		  </div>
