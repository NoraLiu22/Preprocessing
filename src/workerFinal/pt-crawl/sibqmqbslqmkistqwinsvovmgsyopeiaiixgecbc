        <script type="text/javascript">
            $(function() {
                var event_id = "4d98d405\u002D379c\u002D49fc\u002Daae1\u002D4f8725bc7fb9\/par\/parsyscolumncontrol_\/col0\/tabs_51f1\/1480337841\/parsyscolumncontrol_\/col1\/eventlist_s5bt".replace(/\//g,"\\/");
                console.log(event_id);
                if (document.getElementById("showExtendedDisplay_4d98d405\u002D379c\u002D49fc\u002Daae1\u002D4f8725bc7fb9\/par\/parsyscolumncontrol_\/col0\/tabs_51f1\/1480337841\/parsyscolumncontrol_\/col1\/eventlist_s5bt").value == 'true') {
