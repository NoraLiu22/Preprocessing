<section class="contacts">
  <div class="wfp-wrapper ph3 ph0-lg">
    <h2 class="page-stripe__title contacts__title">Contacts</h2>
          <section class="contacts__group">
                <div class="contacts__items">
                      <div class="contacts__item">
