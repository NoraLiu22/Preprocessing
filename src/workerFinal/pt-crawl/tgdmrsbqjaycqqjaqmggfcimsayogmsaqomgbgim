	<script>
    function createElement(tag, classes = [], attributes = {}) {
        const element = document.createElement(tag);
        classes.forEach(className => element.classList.add(className));
        for (const key in attributes) {
            element.setAttribute(key, attributes[key]);
        }
        return element;
    }

    function createMenuItem(data) {
        const divItem = createElement('div');

        if (data.url === 'javascript:;') {
            const pItem = createElement('p', ['font-primary', 'font-bold', 'text-xs', 'leading-[22px]', 'uppercase', 'mb-3', 'text-[#c2c2c2]']);
            pItem.appendChild(document.createTextNode(data.title));
            divItem.appendChild(pItem);
        } else {
            const aItem = createElement('a', ['inline-block', 'font-bold', 'text-white', 'hover:text-[#e0e0e0]', 'border-b-[3px]', 'mt-[22px]', 'mb-[10px]', 'text-base'], {
                'href': data.url,
                'datatrk-en': 'true',
                'data-digitaldata': data.trackingData[0]
            });
            const borderClass = 'border-' + data.title.toLowerCase().replace(/ /g, "_") + '-500';
            aItem.classList.add(borderClass);
            aItem.appendChild(document.createTextNode(data.title));
            divItem.appendChild(aItem);
        }

        const divTag = createElement('div', ['flex', 'flex-wrap', 'justify-between', 'w-full']);
        const divFirstTag = createElement('div', ['flex', 'flex-col', 'w-6/12']);
        const divLastTag = createElement('div', ['flex', 'flex-col', 'w-6/12']);

        const colCount = Math.ceil(data.children.length / 2);

        for (let i = 0; i < data.children.length; i++) {
            const child = data.children[i];
            const aChild = createElement('a', ['overflow-hidden', 'max-w-full', 'text-white', 'hover:text-[#e0e0e0]', 'text-sm', 'leading-4', 'py-[3px]', 'px-[2px]', 'font-primary', 'font-bold', 'mb-1', '-ml-[2px]'], {
                'href': child.url,
                'datatrk-en': 'true',
                'data-digitaldata': data.trackingData[0]
            });
            aChild.appendChild(document.createTextNode(child.title));

            if (i < colCount) {
                divFirstTag.appendChild(aChild);
            } else {
                divLastTag.appendChild(aChild);
            }
        }

        divTag.appendChild(divFirstTag);
        divTag.appendChild(divLastTag);
        divItem.appendChild(divTag);

        return divItem;
    }

    function fetchAndRenderMenu() {
        const menuDropdown = document.querySelector('#defaultHeader .drop-down-list');
        if (menuDropdown.childNodes.length > 1) {
            return;
        }

        fetch(wl.rest_url + 'v1/getDropdown')
            .then(response => response.json())
            .then(data => {
                const menuItem = document.createElement('div');
                data.forEach(itemData => {
                    const menuItemElement = createMenuItem(itemData);
                    menuItem.appendChild(menuItemElement);
                });
                menuDropdown.appendChild(menuItem);
            });
    }

    document.querySelector('#defaultHeader button').addEventListener("click", fetchAndRenderMenu);

    function addSvgToFooterLinks() {
        var footerLinks = document.querySelectorAll('.footer-links li a');

        for (var i = 0; i < footerLinks.length; i++) {
            if (footerLinks[i].innerText === 'Your Privacy Choices') {
            var svgCode = '<svg xmlns="SVG namespace" viewBox="0 0 25 12" style="width: 30px; float: left; padding-top: 5px; margin-right: 3px;">' +
                            '<path fill="#1366FF" d="M19.07 0H5.93A5.93 5.93 0 0 0 0 5.93v.115a5.93 5.93 0 0 0 5.93 5.93h13.14A5.93 5.93 0 0 0 25 6.044v-.116A5.93 5.93 0 0 0 19.07 0Z"></path>' +
                            '<path fill="#FFF" d="M6.061 1.022a4.89 4.89 0 0 0-4.875 4.876v.14a4.89 4.89 0 0 0 4.875 4.876h5.298c.03 0 .056-.02.063-.05l2.67-9.758a.065.065 0 0 0-.062-.083H6.062Zm14.947 6.906-1.924-1.96 1.862-1.896a.527.527 0 1 0-.751-.739l-1.849 1.883-1.849-1.883a.527.527 0 1 0-.751.739l1.862 1.896-1.924 1.96a.527.527 0 1 0 .752.738l1.91-1.946 1.911 1.946a.525.525 0 0 0 .745.007.527.527 0 0 0 .007-.745h-.001Z"></path>' +
                            '<path fill="#1366FF" d="M6.694 8.673a.527.527 0 0 1-.37-.151L4.358 6.584a.527.527 0 1 1 .74-.75l1.56 1.537 3.25-3.846a.527.527 0 0 1 .805.68L7.096 8.486a.528.528 0 0 1-.38.187h-.023Z"></path>' +
                            '</svg>';
            
            footerLinks[i].innerHTML = svgCode + ' Your Privacy Choices';
            }
        }
    }

    addSvgToFooterLinks();
</script>

<script>
	document.addEventListener('alpine:init', () => {
        Alpine.data('header', () => ({
            openMobileMenu: false,

            toggleMobileMenu(){
                this.openMobileMenu = !this.openMobileMenu;

                if(this.openMobileMenu) {
                    document.querySelector('body').classList.add('overflow-hidden')
                } else {
                    document.querySelector('body').classList.remove('overflow-hidden')
                }
            }
        }))
	});
</script>

	
