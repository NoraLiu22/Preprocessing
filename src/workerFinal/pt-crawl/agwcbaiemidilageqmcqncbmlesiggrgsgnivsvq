    </a>
    <!-- alt in wl -->
    <a class="mpwl alt wishlisted-msg" title="Remove this item to your wishlist">
        <span class="icon"><svg width="15px" height="13px" viewBox="0 0 20 20"><use xlink:href="#collect-control-wishlisted"></svg></span>
        <!--span class="txt">in wishlist</span-->
    </a>
    <a class="mpgo" data-bind="attr: {'href': currentTrack().tralbumUrl }" data-clickthrough="true">
        <span data-bind="visible: currentTrack().parentTralbumType === 'a'">go to album</span>
        <span data-bind="visible: currentTrack().parentTralbumType === 't'">go to track</span>
    </a>
    <a class="mpgo alt" data-bind="attr: {'href': currentTrack().tralbumUrl }" title="Go to album/track" data-clickthrough="true">
        <span data-bind="visible: currentTrack().parentTralbumType === 'a'">go to album</span>
        <span data-bind="visible: currentTrack().parentTralbumType === 't'">go to track</span>
    </a>

