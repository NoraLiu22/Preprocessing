                IsArticleIdsToHideRelatedArticleBox: 'False',
                RectifiedPillText: ''
            };
        })();
        var FRArticleRecaptchaSettings = (function() {
            return {
                RecaptchaSiteKey: '6LdG3i0UAAAAAOC4qUh35ubHgJotEHp_STXHgr_v'
            };
        })();

    </script>

