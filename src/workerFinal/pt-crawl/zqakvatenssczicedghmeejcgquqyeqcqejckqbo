        <script>
            AOS.init({
                easing: 'ease-out-back',
                duration: 1000
            });
        </script>
        <script>
            hljs.initHighlightingOnLoad();

            $('.hero__scroll').on('click', function (e) {
                $('html, body').animate({
                    scrollTop: $(window).height()
                }, 1200);
            });
        </script>
        <script type="text/javascript">
            $(".question1").click(function () {
                $(".question1").removeClass("question--selected");
                //$(".tab").removeClass("selected");
                $(this).addClass("question--selected");

                var selectedTab = $(this).attr("data-img");
                $(".question-image").css('background-image', 'url(' + selectedTab + ')');
            });

            $(".tab-selector").click(function () {
                $(".tab-selector").removeClass("selected");
                $(".tab").removeClass("selected");
                $(this).addClass("selected");
                var selectedTab = $(this).attr("data-tab");
                $("." + selectedTab).addClass("selected");
            });

            $(function () {
                $('.question1.question1').click(function () {

                    if ($('.question1.question1').hasClass('question--selected')) {

                        $('.question1.question-selected').addClass('active');
                    } else {
                        $('.question1.question-selected').addClass('active');
                    }

                });
            });
        </script>
        <script type="text/javascript">
            $(document).ready(function () {
                $('#watch-video').click(function () {
                    $('#background-video1').css("display", "block");
                    var video = document.getElementById("bgvideo");
                    video.currentTime = 0;

                });
                $('#close-video1').click(function () {

                    $('#background-video1').css("display", "none");
                    var video = document.getElementById("bgvideo");
                    video.currentTime = 0;
                });

            });
        </script>
        <script type="text/javascript">
            $(".question").click(function () {
                $(".question").removeClass("question--selected");
                //$(".tab").removeClass("selected");
                $(this).addClass("question--selected");

                var selectedTab = $(this).attr("data-img");
                $(".question-image").css('background-image', 'url(' + selectedTab + ')');
            });

            $(".tab-selector").click(function () {
                $(".tab-selector").removeClass("selected");
                $(".tab").removeClass("selected");
                $(this).addClass("selected");
                var selectedTab = $(this).attr("data-tab");
                $("." + selectedTab).addClass("selected");
            });
            var $window = $(window);
            var $header = $('.header-banner-step1');
            var $headerUnit = $('.header-unit-step1');
            $(window).resize(function () {
                var factor = 0.55;
                var width = $window.width();

                var height = factor * width;

                $header.css({
                    'height': height,
                    'border-width': '0px'
                });
                $headerUnit.css({
                    'height': height,
                    'border-width': '0px'
                });
            }).resize();
        </script>
