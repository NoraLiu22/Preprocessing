        var isAdSticky = 'false';
        var adHeaderHeight = 0;

        /* Show Ghost block on homepage when MPU is shown */
        function showHPGhostBlock() {
            document.getElementById('js-ad-text').classList.add('js-padding');
        };


        var gptadslots = []; var googletag = googletag || {}; googletag.cmd = googletag.cmd || [];
        (function (r) {
            var gads = document.createElement('script');
            gads.async = true; gads.type = 'text/javascript';
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);

            var isObj = function (v) { return (typeof v === 'object' && v !== null) },
                isArr = function (v) { return (isObj(v) && typeof v.length === 'number') },
                cxVal = function (v) { return (motors.getContext('vehicle:' + v) || '') };

            function getTarg(n) {
                var d = JSON.parse(getMotorsCookie(n)), makes = cxVal('make'), models = cxVal('model'), dObj = isObj(d);
                if (makes === '' && makes === '' && dObj) {
                    var mm = d.MakeModels, item, mkarr = [], moarr = [];
                    if (isArr(mm)) {
                        for (var i = mm.length; i--;) {
                            item = mm[i];
                            mkarr.push(item.Value);
                            if (isArr(item.Models))
                                moarr = moarr.concat(item.Models);
                        }
                        makes = mkarr.join(','); models = moarr.join(',');
                    }
                }
                return { make: makes.toLowerCase(), model: models.toLowerCase(), area: getArea(d) };
            }
            function setTarg(d) {
                var i; if (!('area' in d)) d.area = getArea(getMotorsCookie('motSeaPanBas'));
                for (var p in d) {
                    if (d.hasOwnProperty(p)) {
                        i = d[p];
                        if (i && i !== "")
                            googletag.pubads().setTargeting(p, i.toLowerCase());
                    }
                }
            }

            googletag.cmd.push(function () {
                var leaderMap = googletag.sizeMapping()
                                .addSize([1245, 600], [[970, 250], [970, 90], [728, 90]])
                                .addSize([768, 200], [[728, 90]])
                                .addSize([320, 480], [[320, 50], [300, 100]])
                                .build();
                                                                                                                                                                
                gptadslots[0] = googletag.defineSlot('/3048/driving/used-cars', [320, 50], 'div-gpt-ad-leaderboard')
                    .defineSizeMapping(leaderMap)
                    .set('ad_position', 'leaderboard')
                    .addService(googletag.pubads());

                gptadslots[1] = googletag.defineSlot('/3048/driving/used-cars', [[300, 250], [300, 600]], 'div-gpt-ad-mpu')
                    .set('ad_position', 'mpu')
                    .addService(googletag.pubads());

                gptadslots[2] = googletag.defineSlot('/3048/driving/used-cars', [300, 300], 'div-gpt-ad-mpu-2')
                    .set('ad_position', 'mpu')
                    .addService(googletag.pubads());

                googletag.pubads()
                    .setTargeting('pos', ['mpu'])
                    .setTargeting('section', '/used-cars')
                    .setTargeting('path', '/used-cars/')
                    .setTargeting('sec_id', null)
                    .setTargeting('cont_type', 'sec')
                    .setTargeting('kw', 'null')
                    .setTargeting('slug', 'null')
                    .setTargeting('topics', '')
                    .setTargeting('aid', 'null')
                    .setTargeting('article_type', 'null')
                    .setTargeting('testmode', 'null');

                googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                    // The slot letter changed? for some reason
                    if (event.slot.get('ad_position') === 'leaderboard') {
                        adHeaderHeight = event.size[1]+10;
                        document.getElementById('js-gpt-header-ad').style.height = adHeaderHeight+"px";
                    }

                    // Show ghost block if MPU, rather than a double MPU
                    if (event.slot.get('ad_position') === 'mpu') {
                        if (event.size[1] == 250) {
                            showHPGhostBlock();
                        }
                    }
                });
                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableAsyncRendering();
                googletag.enableServices();
            });
        }(this));
