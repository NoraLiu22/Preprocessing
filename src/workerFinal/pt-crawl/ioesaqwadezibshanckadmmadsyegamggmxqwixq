<!-- End Two Column --><script>
// Must execute inline after the geomaps element to avoid load order issues
// this shared content block prevents an error on NHS location and Getting here pages. 
// Place icons in the geomap data table should have a type of '2' but some are being assigned a type of '4', preventing the map from loading
// Investigation of this issue is ongoing

//  let geomapTD = document.querySelector('#addParksCan tbody tr td:nth-of-type(1)')
//  geomapTD.textContent = "2"

</script>

<script>
// Must execute inline after the geomaps element to avoid load order issues
// this shared content block prevents an error on NHS location and Getting here pages. 
// Places in the geomap data table should have a set type but some are being assigned a type of '4', preventing the map from loading
// Parks should be 1
// NHS should be 2
// NMCA should be 3
// Investigation of this issue is ongoing

  let placeType = document.URL.match("(.ca\/)([^\/]+)"),
    placeCat;
  switch (placeType[2]) {
    case "lhn-nhs" :
      placeCat = 2;
      break;
    case "amnc-nmca" :
      placeCat = 3;
      break;
    default :
      placeCat = 1;
  }

  let geomapCatCell = document.querySelector('#addParksCan tbody tr td:nth-of-type(1)');
  geomapCatCell.textContent = placeCat;

</script>
