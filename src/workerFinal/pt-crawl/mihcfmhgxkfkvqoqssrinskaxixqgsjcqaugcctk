        App.suppressGtm = App.suppressGtm || !!document.getElementsByClassName('suppress-gtm').length;
    </script>

    

        <div id="ad-interstitial" class="ad-interstitial">
            <script>
      googletag.cmd.push(function() {
        if (App.suppressAds) { return; }
        App.Ads.slots[13] = googletag.defineOutOfPageSlot(App.Ads.adPath + '/collections', 'ad-interstitial')
          .setTargeting('pos', ['interstitial'])
          .addService(googletag.pubads());
        googletag.display('ad-interstitial');
        googletag.pubads().refresh([App.Ads.slots[13]], {changeCorrelator: false});
      });
            </script>
