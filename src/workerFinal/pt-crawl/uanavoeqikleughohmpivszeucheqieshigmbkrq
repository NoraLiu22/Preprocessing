<script type="text/javascript" language="JavaScript">
function ActionDeterminator()
{
if(document.bibsearch.sorting[0].selected == true) {
document.bibsearch.action = 'http://homepages.bw.edu/bachbib/script/bach1.pl';
}
if(document.bibsearch.sorting[1].selected == true) {
document.bibsearch.action = 'http://homepages.bw.edu/bachbib/script/bach1c.pl';
document.bibsearch.method = 'get';
}
if(document.bibsearch.sorting[2].selected == true) {
document.bibsearch.action = 'http://homepages.bw.edu/bachbib/script/bach1ct.pl';
document.bibsearch.method = 'get';
}
return true;
}
// -->
