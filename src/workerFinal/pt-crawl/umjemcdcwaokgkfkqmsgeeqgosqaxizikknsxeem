    id="_applications-open"
  
  >
  <div class="container--desktop">
    <div class="featured_text_block ">
      
      <div class="container--narrow">
        <div class="featured_text_block__content">
          <div class="featured_text_block__textArea" dir="auto">
            <div class="featured_text_block__textContent">
              
              <div class="block_slide__textContent__body body-text">
                <div style="position:relative;">
<div style="background-image: url('https://images.ctfassets.net/uo17ejk9rkwj/im7nX5AOn7lgKiD99acwF/e27a00b3c7a670d66f49f05c0c9adbb9/applications.svg'); background-size: 100% 100%; height: 500px; background-position: center; background-repeat: no-repeat;">
<div style="top: 60%; text-align: left; position: absolute; width: 100%;"><p style="font-size:18px; font-weight:700;">Application</p></div>
<div style="padding-top: 10%; display:flex; justify-content:center; text-align:center; "><p style="font-size:28px; width:100%; font-weight:700;">Applications are welcome anytime!</p></div>
<div style="top: 60%; left: 0%; text-align: right; position: absolute; width: 100%;"><p style="font-size:18px; font-weight:700;">Participation</p></div>
<div style="top: 85%; left: 30%; position: absolute; width: 280px;"><p style="font-size:18px; font-weight:700;">Review and selection</p></div>
</div>
