      flowInitiated: { ga4DiStatus: 'flow_initiated' },
      diDisabledByMeta: { ga4DiStatus: 'disabled_by_meta' },
      diHasBeenShown: { ga4DiStatus: 'has_been_shown' },
      diHasNotPriority: { ga4DiStatus: 'has_not_priority' },
      diDisabledForPageType: { ga4DiStatus: 'disabled_for_page_type' },
      diDisabledInvalidPublisherConfig: { ga4DiStatus: 'disabled_invalid_publisher_config' },
      gettingContext: { ga4DiStatus: 'getting_context' },
      diErrorRenderingRootElement: { ga4DiStatus: 'error_rendering_root_element' },
      diSetModalVisible: { ga4DiStatus: 'set_modal_visible' },
      diPreRenderDone: { ga4DiStatus: 'pre_render_done' },
      close: { name: 'Close', ga4Event: 'di_click', ga4Clicked: 'close' },
      continue: { name: 'ClickContinue', ga4Event: 'di_click', ga4Clicked: 'continue' },
      display: { name: 'Show', ga4Event: 'di_show' },
      downloadRecommended: { name: 'ClickProgram', ga4Event: 'di_download', downloadType: 'download' },
      downloadLandingPage: { name: 'ClickExternalProgram', ga4Event: 'di_download', downloadType: 'download_visit_website' },
      home: { name: 'ClickHome', ga4Event: 'di_click', ga4Clicked: 'home' },
      getCampaign: { ga4Event: 'di_request' },
      support: { name: 'ClickSupportPage', ga4Event: 'di_click', ga4Clicked: 'support' },
      error: {
        name: 'Error',
        labelFn: (ev) => ev.error || 'unknown error',
        nonInteractive: true,
      }
    };
    
    function trackGA4Event(diEvent) {
      const eventMapped = eventMapping[diEvent.kind];
      if (diEvent.kind === 'error') {
        window.dataLayer.push({
          event: 'di_error',
          error_message: diEvent.error || 'unknown error',
          di_status: 'error',
        });
      } else if (diEvent.kind === 'getCampaign') {
        const suitableCampaigns = (diEvent.campaignsResult?.suitableCampaigns || []).map(({ id }) => id);
        const winnerCampaign = diEvent.campaignsResult?.winnerCampaign?.id ? 'has_winner_campaign' : 'has_not_winner_campaign';

        window.dataLayer.push({
          event: eventMapped.ga4Event,
          detected_country: diEvent.countryCode,
          suitable_campaigns: suitableCampaigns,
          di_status: suitableCampaigns.length === 0 ? 'non_suitable_campaign' : winnerCampaign,
        });
      } else if (['revampLoaded', 'flowInitiated', 'diDisabledByMeta', 'diHasBeenShown', 'diHasNotPriority', 'diDisabledForPageType', 'diDisabledInvalidPublisherConfig', 'gettingContext', 'diErrorRenderingRootElement', 'diPreRenderDone', 'diSetModalVisible' ].includes(diEvent.kind)) {

        window.dataLayer.push({
          di_status: eventMapped.ga4DiStatus,
        });
      } else {
        window.dataLayer.push({
          event: eventMapped.ga4Event,
          ...(eventMapped.ga4Clicked && { element_clicked: eventMapped.ga4Clicked }),
          ...(eventMapped.downloadType && { di_download_type: eventMapped.downloadType }),
          di_campaign_name: diEvent.campaign?.id ? diEvent.campaign.id : 'n/a',
          campaign_delivery_logic: diEvent.campaign.logic,
          detected_country: diEvent.countryCode,
        });
      }
    }

    Object.keys(eventMapping).forEach((eventName) => {
      document.addEventListener(`rv:di:${eventName}`, (ev) => {
        const diEvent = ev.detail;
        const eventMapped = eventMapping[diEvent.kind];
        if (!eventMapped) {
          return;
        }
        const campaignIdLabelFn = (ev) => ev.campaign?.id ? ev.campaign?.id : 'n/a';
        const labelFn = eventMapped.labelFn ? eventMapped.labelFn : campaignIdLabelFn;
        if (diEvent.campaign) {
          window.dataLayer.push({
            cpiSelectionLogic: diEvent.campaign.logic,
            diCampaign: diEvent.campaign.id
          });
        }
       
        trackGA4Event(diEvent);
        
        if (eventMapped.name){
          const gtmEvent = {
            event: 'diRevAmp',
            diRevAmpAction: eventMapped.name,
            diRevAmpLabel: labelFn(diEvent),
            diRevNonInteractiveHit: !!eventMapped.nonInteractive,
          };
          window.dataLayer.push(gtmEvent);
      }
      });
    });
  })();
