                var App = App || {};
                App.ArticleSplitScreen = App.ArticleSplitScreen || {};
                App.ArticleSplitScreen.siteName = 'jamaneurology';
                App.ArticleSplitScreen.siteId = 16;
                App.ArticleSplitScreen.journalId = 72;
