<script>

function submit_search_annual_form() { 
    var global_qvalue = document.getElementById('qterm_id').value;

    if(global_qvalue==""){

        global_qvalue = document.getElementById('qterm_idmob1').value;

    }

    if(global_qvalue==""){
        return false;
    }
    document.getElementById("hiddensearchId1").action = "https://www.worldbank.org/en/meetings/splash/annual/searchlist" + "?q=" + global_qvalue;
    document.getElementById("hiddensearch_id_txt1").value = global_qvalue;
    document.getElementById("hiddensearchId1").submit();
    return false;
}



    var data = {};
    data["messageFolder"] = "inbox";
    var messagecount;
    $.ajax({
        type: "GET",
        url: "/content/meetings/en/meetingslogin/asmeeting/messaging/jcr:content/par/supergrid/par/gridlayout/grid_9_par_1/messagebox_5ab3.social.0.0.json",
        async: true,
        data: data,
        success: function(json) {
        messagecount = json["messageCounts"].nonDeletedUnreadCount;
        $(".messagecount").html("("+messagecount+")");
    }
           });
    
    

</script>
