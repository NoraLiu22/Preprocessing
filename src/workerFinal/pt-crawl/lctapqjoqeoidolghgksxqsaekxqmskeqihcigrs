                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Information about HIDOE" class="ms-sitemapdirectional" href="/ConnectWithUs/Pages/home.aspx">Connect with Us</a></span><span> &gt; </span><span><a class="ms-sitemapdirectional" href="/ConnectWithUs/MediaRoom/Pages/home.aspx">Media Room</a></span><span> &gt; </span><span class="current">Media Kit</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">Media Kit</h2>
                
    



				
				<p class="intro">
					Key facts about the Hawai‘i State Department of Education (HIDOE), contact list, fact sheets, reports, data, and other items to assist the media and interested parties.
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

