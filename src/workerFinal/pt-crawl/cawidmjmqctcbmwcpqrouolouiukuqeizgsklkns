    <script>
        function acticateBtn(activeId) {
            if (activeId != "2")
                activeId = parseInt(activeId) + 1;
            else
                activeId = parseInt(1);
            $(".grayBtnB").removeClass('grayBtnBFrc');
            $(".circleBlue").removeClass("active");
            $(".leftTabs").removeClass("active");
            var str = '.circleBlue[data-id="' + activeId + '"]';
            var strTab = '.leftTabs[data-id="' + activeId + '"]';
            var grayBtnB = '.grayBtnB[data-id="' + activeId + '"]';
            $(str).addClass('active');
            $(strTab).addClass('active');

            $(grayBtnB).addClass('grayBtnBFrc');

        }
        $(document).ready(function () {
            timer = setInterval(function () {
                var activeId = $(".circleBlue.active").attr("data-id");

                //class="circleBlue active " data-id="1"
                acticateBtn(activeId);

            }, 4000);

            $('.circleBlue').click(function (e1) {
                activeId = $(this).attr("data-id");
                if (activeId != 1)
                    acticateBtn(activeId - 1);
                else
                    acticateBtn(2);
            });
            $('.um-list-li-content').click(function (e1) {
                var circle = $(this).prev();
                circle.trigger('click');
            });

        });
    </script>
	
<script>
        $(document).ready(function () {
            $('*[data-target="Qbank"]').trigger('click');
        });

    </script>
	
