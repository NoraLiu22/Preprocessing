        String.prototype.format = function () {
          var s = this,
              i = arguments.length;

          while (i--) {
              s = s.replace(new RegExp('\\{' + i + '\\}', 'gm'), arguments[i]);
          }
          return s;
        }

        master.setPageTitle = function(title) {
          if (title !== '') {
            if (this.globalTitle !== '') {
              document.title = this.pageTitleFormat.format(title, this.globalTitle);
            } else {
              document.title = title;
            }
          }
        } 
      
