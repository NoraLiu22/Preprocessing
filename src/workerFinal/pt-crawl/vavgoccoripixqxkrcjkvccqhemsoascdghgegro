  <script>
    var dataLayerOb = {
    		"event": "pageView",
    		"page": {"path":"/discover/dinosaurs.html","site":"main site","siteSearchResultPage":false,"errorPage":false,"articlePage":false,"domain":"www.nhm.ac.uk","transactionPage":false,"title":"Dinosaurs | Natural History Museum","url":"https://www.nhm.ac.uk/discover/dinosaurs.html"},
    		"user": {"id2":"","id1":""}
    };

    const pollForAdobeAnalytics = () => {
      let interval;
      let timer = 0;
      const TIME_LIMIT = 60000;
      interval = setInterval(() => {
        timer += 10;
        if (timer > TIME_LIMIT) {
          clearInterval(interval);
          console.log("adobe analytics failed to load within one minute")
          return;
        }
        if (window.__satelliteLoaded) {
          const dataLayerEvent = new CustomEvent("dataLayerEvent", { detail: dataLayerOb });
          document.querySelector("body").dispatchEvent(dataLayerEvent);
          
          clearInterval(interval);
        } 
      }, 10)
    }

    window.addEventListener("DOMContentLoaded", () => {
      pollForAdobeAnalytics();
    } );
  </script>


    

	
	
