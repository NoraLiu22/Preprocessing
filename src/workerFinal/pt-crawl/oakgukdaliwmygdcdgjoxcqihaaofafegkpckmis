<script type="text/javascript">

    $(document).ready(function () {
        $('.local_nav_wrapper').css('opacity', 1);
        /* Initiate priorityNav */
        var nav = priorityNav.init({
            mainNavWrapper: ".local_nav_wrapper", // mainnav wrapper selector (must be direct parent from mainNav)
            mainNav: ".local_nav-ul", // mainnav selector. (must be inline-block)
            navDropdownLabel: 'More',
            navDropdownClassName: "local_dropdown", // class used for the dropdown.
            navDropdownToggleClassName: "dropdown-toggle", // class used for the dropdown toggle.
        });
    });

        
            var lastScrollPosition = 0;
            var nav = document.querySelector('.sub-nav');
            nav.classList.add('sub-nav--fixed');
            window.addEventListener("scroll", function () {
                document.querySelector('.priority-nav__dropdown').classList.remove('show');
                var currentScrollPostition = window.pageYOffset;
                if (currentScrollPostition > nav.offsetHeight && currentScrollPostition > lastScrollPosition) {
                    nav.classList.add('scroll-down');
                }
                if (currentScrollPostition < lastScrollPosition) {
                    nav.classList.remove('scroll-down');
                }
                lastScrollPosition = currentScrollPostition;
            });
        
</script>



