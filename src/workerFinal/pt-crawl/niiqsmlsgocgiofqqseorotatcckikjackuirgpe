<script>
    if (window.location.search.indexOf("&searchType=advanced") >= 0) {
        $('#btnReturnAdvancedSearch').removeClass('hide');
        $('.querybuilder-wrap').addClass('hide');
    }

    $('#btnReturnAdvancedSearch').click(function () {
        window.location.href = location.protocol + "//" + $('#hfSiteURL').val() + "/advanced-search";
    });

</script> 
