                <div class="grid grid--33-33-33" id="c11216">
                    
    
        
            
                    
<div class="grid__col">
    <div class="grid__item">
        <article class="teaser t-white ">
            
                    
                        <div class="teaser__image">
                            <figure class="figure">
                                <div class="figure__media">
                                    <div class="image loading" style="padding-top: 56.25%;">
                                        <noscript>
                                            <img class="image__fallback" alt="" height="558" src="/fileadmin/_processed_/b/d/csm_zeitreise_gross_ceb95746f2.jpg" width="992" />
