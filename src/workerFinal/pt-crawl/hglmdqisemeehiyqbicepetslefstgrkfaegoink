rcset="https://assets-global.website-files.com/62a7bb6e97112a609a58427b/64b60e876c8a2733e0a18bd0_Graph_2-p-500.webp 500w, https://assets-global.website-files.com/62a7bb6e97112a609a58427b/64b60e876c8a2733e0a18bd0_Graph_2.webp 665w" class="solutions-tabs_lottie"/></div></div><div id="tabs-menu-1" class="tabs__menu w-tab-menu"><a data-w-tab="Tab 1" data-w-id="276e5ac6-a7ea-65f0-05ac-5fc1a557a632" class="tabs__link is-blue w-inline-block w-tab-link w--current"><div class="tabs_dot-wrapper"><div class="tabs_dot"></div><div class="tabs-dotted-line"><div class="tabs-solid-line default"></div></div></div><div class="tabs_content-wrap"><div class="tabs-link__title"><h3 class="caption-1">Reporting tools</h3></div><div class="tabs__link-body"><p class="paragraph">Get a snapshot of your audience’s behaviours, like recent sign ups, trending content, transactions, total streams and more. </p><img alt="" loading="lazy" src="https://assets-global.website-files.com/plugins/Basic/assets/placeholder.60f9b1840c.svg" class="solutions-tabs-image mobile"/></div></div></a><a data-w-tab="Tab 2" data-w-id="276e5ac6-a7ea-65f0-05ac-5fc1a557a63d" class="tabs__link is-blue w-inline-block w-tab-link"><div class="tabs_dot-wrapper"><div class="tabs_dot"></div></div><div class="tabs_content-wrap"><div class="tabs-link__title"><h3 class="caption-1 text-color-black">Promo codes for high engagement</h3></div><div class="tabs__link-body"><p class="text-color-black">Reward return attendees or fill last minute seats with promo codes that can reward your most loyal patrons.</p><img alt="" loading="lazy" src="https://assets-global.website-files.com/plugins/Basic/assets/placeholder.60f9b1840c.svg" class="solutions-tabs-image mobile"/></div></div></a></div></div></div></div></div></div><img src="https://assets-global.website-files.com/62a7bb6e97112a609a58427b/6303f9d3432d8b6e02701338_Frame%201653%20(1).svg" loading="eager" alt="" class="background-image"/><div class="w-embed w-script"><script>
  var Webflow = Webflow || [];
  Webflow.push(function () {

    function isElementInViewport(el) {

      // Special bonus for those using jQuery
      if (typeof jQuery === "function" && el instanceof jQuery) {
        el = el[0];
      }

      var rect = el.getBoundingClientRect();

      return (
        rect.bottom >= 0 &&
        rect.right >= 0 &&
        rect.top <= (window.innerHeight || document.documentElement.clientHeight) &&
        rect.left <= (window.innerWidth || document.documentElement.clientWidth));
    }

    var tabTimeout1;
    var tabMenus = [[$('#tabs-menu-1'), false]];

    function tabLoop1(tabMenu, timeout = 10000) {
      tabTimeout1 = setTimeout(function () {
        var $next = tabMenu.children('.w--current:first').next();
        if ($next.next().length == 0) {
          $(tabMenu).find('.tabs__link:first').removeAttr("href").click();
          return;
        }
        if ($next.length) {
          if ($(".menu-button").hasClass("w--open")) {
            tabLoop1(tabMenu);
          } else {
            $next.removeAttr("href").click();
          }
        } else {
          if ($(".menu-button").hasClass("w--open")) {
            tabLoop1(tabMenu);
          } else {
            $(tabMenu).find('.tabs__link:first').removeAttr("href").click();
          }
        }
      }, timeout);
    }

    $('#tabs-menu-1 .tabs__link').click(function () {
      clearTimeout(tabTimeout1);
      for (let i = 0; i < tabMenus.length; i++) {
        if (tabMenus[i][1] == true) {
          var tabToLoop = tabMenus[i][0];
          tabLoop1(tabToLoop);
        }
      }
    });

    // On any change to the window we check to see if any tabs are in the viewport
    $(window).on("load resize scroll", function (e) {
      for (let i = 0; i < tabMenus.length; i++) {
        // if the tab is in the viewport and it wasn't previously (its visibility state was false)
        // then we switch its visibility state to true and run the loop
        if (isElementInViewport(tabMenus[i][0]) && tabMenus[i][1] == false) {
          tabMenus[i][1] = true;
          tabLoop1(tabMenus[i][0], 0);
          // if it isn't visible and last time we checked it was visible we now switch its visibility state to false
        } else if (!isElementInViewport(tabMenus[i][0]) && tabMenus[i][1] == true) {
          tabMenus[i][1] = false;
          clearTimeout(tabTimeout1);
        }
      }
    });
  });
</script></div></div><section class="section-engagement-bottom"><img src="https://assets-global.website-files.com/62a7bb6e97112a609a58427b/64b61630f4775a8c6ad720fd_page_curve_down_events.svg" loading="eager" alt="" class="engagement-background-image"/></section><div class="section-solutions-four-grid"><div class="page-padding z-index-1"><div class="container-large"><div class="padding-vertical padding-massive no-padding-top-mobile"><div class="festival-monetisation_component"><div class="title-block max-width-medium"><div class="subtitle-2 text-color-blue-dark">MONETISATION</div><h2 class="margin-bottom margin-small heading-small">Turn your content into revenue</h2><div class="margin-bottom margin-large"><p class="text-size-large">Create a new revenue stream with flexible platform features designed for audiences and sponsors alike.</p></div></div></div><div class="festival-monetisation_4-options is-monetisation"><div class="festival-monetisation_item is-4-wide"><div class="margin-bottom margin-xsmall"><div class="caption-1">Rent or sell content</div></div><div class="margin-bottom margin-xsmall"><p class="text-size-large">Control access windows and set pricing for purchase to give audiences the content they’ll enjoy. </p></div><a href="/features/ecommerce" class="button-tertiary deletable w-inline-block"><div class="text-color-blue-dark">Learn more about ecommerce</div><div class="button-tertiary_icon w-embed"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
