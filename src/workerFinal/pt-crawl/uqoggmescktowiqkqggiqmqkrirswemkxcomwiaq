<script>
function dismiss() {
    createCookie('dismissed','Y','1');
    hasDismissed();
}

function hasDismissed() {
    var dismissed = readCookie('dismissed');
    if (dismissed != 'Y') {
        document.getElementById('fundraiser').style.display = "inherit";
    } else {
        document.getElementById('fundraiser').style.display = "none";
    }
}

document.addEventListener('DOMContentLoaded', hasDismissed);
