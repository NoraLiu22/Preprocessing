        /*******************************************************************************
         * JS here is only being used to assign variables from values in the model
         * logic should be implemented in external JS files, like client.script.js
         *******************************************************************************/

        var SCM = SCM || {};
        var accessIcons = [{"id":347105,"icon":"icon-availability_open","title":"Open Access"},{"id":346894,"icon":"icon-availability_open","title":"Open Access"},{"id":346560,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":346526,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":347030,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":347032,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":346561,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":347001,"icon":"icon-availability_open","title":"Open Access"},{"id":346876,"icon":"icon-availability_open","title":"Open Access"},{"id":347023,"icon":"icon-availability_open","title":"Open Access"},{"id":346504,"icon":"icon-availability_cart","title":"Available to Purchase"},{"id":346562,"icon":"icon-availability_open","title":"Open Access"},{"id":347031,"icon":"icon-availability_open","title":"Open Access"},{"id":346893,"icon":"icon-availability_cart","title":"Available to Purchase"}];
        if (SCM.AccessIcons) {
            SCM.AccessIcons = SCM.AccessIcons.concat(accessIcons);
        } else {
            SCM.AccessIcons = accessIcons;
        }
        var accessAttributes =  [{"id":347105,"availableforpurchase":false},{"id":346894,"availableforpurchase":false},{"id":346560,"availableforpurchase":true},{"id":346526,"availableforpurchase":true},{"id":347030,"availableforpurchase":true},{"id":347032,"availableforpurchase":true},{"id":346561,"availableforpurchase":true},{"id":347001,"availableforpurchase":false},{"id":346876,"availableforpurchase":false},{"id":347023,"availableforpurchase":false},{"id":346504,"availableforpurchase":true},{"id":346562,"availableforpurchase":false},{"id":347031,"availableforpurchase":false},{"id":346893,"availableforpurchase":true}];
        if (SCM.AccessAttributes) {
            SCM.AccessAttributes = SCM.AccessAttributes.concat(accessAttributes);
        } else {
            SCM.AccessAttributes = accessAttributes;
        }
        
    </script>
 
    </div>

        </div>

        <div id="Sidebar" class="page-column page-column--right issue-sidebar">
<div class="sidebar-widget_wrap">

    <div class="widget-DynamicWidgetLayout widget-instance-ArticlesByGroup_Sidebar">
        <div class="widget widget-dynamic " data-count="1">
    

    <div class="widget-dynamic-inner-wrap">

    <div class="widget-DynamicWidgetLayout widget-instance-ArticlesByGroup_SidebarB0">
        <div class="widget widget-dynamic issue-self-serve" data-count="1">
    

    <div class="widget-dynamic-inner-wrap">

    <div class="widget-SelfServeContent widget-instance-dev_pap-self-serve">
        



    <input type="hidden" class="SelfServeContentId" value="pap-self-serve" />
    <input type="hidden" class="SelfServeVersionId" value="0" />

<div class="rail-widget_wrap mt-150">
<h3>About Accepted manuscripts</h3>
<div class="widget-links_wrap">
<p>Accepted manuscripts are peer-reviewed author manuscripts posted online in manuscript form soon after article acceptance and in advance of publication of the online issues in which they will appear – providing authors and readers with rapid access to the journal's latest research. Each article is subsequently enhanced by copy-editing, proofreading, typesetting and article layout, and will be published in printable PDF format, with a fully linked full-text version.  </p>
<p><a href="/dev/pages/alerts" class="base-font" target="_blank">Receive an email alert</a> when new Accepted manuscripts are posted.<br />
Learn more <a href="/dev/pages/information-about-accepted-manuscripts" class="base-font" target="_blank">about Accepted manuscripts</a>, including how to cite them.</p>
</div>
</div>



 
    </div>

    </div>

</div> 
    </div>

    </div>

</div> 
    </div>

