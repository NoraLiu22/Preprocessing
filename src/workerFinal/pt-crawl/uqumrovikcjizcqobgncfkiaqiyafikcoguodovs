    <div data-drupal-messages-fallback class="hidden"></div><div id="block-awm-content">
  
    
      <article id="page-top"  role="article" about="/about/organisation/corporate" typeof="schema:WebPage" class="node--published">

<div class="">
  <div class="container ">
    <div class="page-title">
      <h1 aria-level="1" role="heading">
<span property="schema:name">Corporate Documents</span>
</h1>
    </div>
      </div>
  
      <div class="container">
          </div>
  
  <div >
    
      <div>
              <div>


  

<div class="container ">
  <div class="paragraphs-item-grid">
    <div class="grid-container flat flexible">
        <div class="grid-wrap flat flexible">
          
      <div>
              <div>


<div class="paragraph--type--grid-item">
      <div class="image">
                <a class="grid-link" href="https://www.awm.gov.au/sites/default/files/AWM_Year%20in%20Review_2022-23_Web_0.pdf">
              <img data-object-fit="contain" class="image-contain" src="https://www.awm.gov.au/sites/default/files/styles/main_content/public/203459/gridimage/capture.png?itok=xwIwOURX" alt="Year In Review 2022-2023">
              </a>
            <a class="lightbox" href="/sites/default/files/203459/gridimage/capture.png" data-lightbox="grid-194866" data-title="">
        <img src="https://www.awm.gov.au/sites/default/files/styles/main_content/public/203459/gridimage/capture.png?itok=xwIwOURX" alt="Year In Review 2022-2023">
      </a>
    </div>       <div class="caption">
                              <h2 class="eyebrow"> 
            <div>2022-2023</div>
      </h2>
          
                      <h2> 
            <div>Year In Review</div>
      </h2>
                    

                      <a class="cta" href="https://www.awm.gov.au/sites/default/files/AWM_Year%20in%20Review_2022-23_Web_0.pdf">Download<span class="arrow"></span></a>
                        </div> </div></div>
              <div>


