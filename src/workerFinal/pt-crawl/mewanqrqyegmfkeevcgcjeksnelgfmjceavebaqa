<div class="content-block content-block--center" style="max-width: None;">
    <h1>Working Papers</h1>
    <!-- Text -->
    
    <div class="article-summary">
    <table id="example" class="display article-summary" cellspacing="0" width="100%">
        <thead><tr><th>&nbsp;</th><th>WP #</th><th>Title</th><th>Author(s)</th><th>Citation</th><th>Abstract</th></tr></thead>
