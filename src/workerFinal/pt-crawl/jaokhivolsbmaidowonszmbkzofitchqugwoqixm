          <div class="container">
    
      
            
                        <h2  class="mb-7 xl:mb-14 context--slice-title" data-js-split-lines="" data-js-animation="splitLineAnimation" id="paragraph-41244-title">
              Ask us a question 

          </h2>
              
            
            
          

<div  class="c-grid c-teaser-group | gap-y-7 [ xl:gap-y-gutter-xl ] c-teaser-group--1">
      
    <div class="c-teaser-group__item" data-js-animation="teaserAnimation" >
      



  
    <article  class="c-teaser c-teaser--featured">
    <a href="/study/chat-student" class="c-teaser__link [ c-underline ]" data-js-underline>
              <div class="c-teaser__image">
          <div class="absolute top-0 left-0 w-full h-full" data-js-teaser-animation-image-reveal-wrapper>
            <span data-js-teaser-animation-image-reveal-overlay class="bg-color-theme-contrast"></span>
            <div data-js-teaser-animation-image-reveal-content>
                

  <div  class="media media-teaser_landscape media-image" data-media-wrapper-class='figure-media-image'>
    

                <img loading="lazy" class="lazyload absolute w-full h-full top-0 left-0 object-cover" width="750" height="562" data-sizes="auto" data-src="/sites/default/files/styles/16_9_media_small/public/2023-05/4.png?h=026830cb&amp;itok=bOIa7G1V" data-srcset="/sites/default/files/styles/1_1_media_tiny/public/2023-05/4.png?h=026830cb&amp;itok=ggg6S6Js 400w, /sites/default/files/styles/4_3_small_750x562_/public/2023-05/4.png?h=026830cb&amp;itok=KzstpjS3 750w, /sites/default/files/styles/16_9_media_medium/public/2023-05/4.png?h=026830cb&amp;itok=qhNlcuYL 1000w, /sites/default/files/styles/16_9_media_large/public/2023-05/4.png?h=026830cb&amp;itok=vpYO4I6N 1300w" alt="" />

