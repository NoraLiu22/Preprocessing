$(document).ready(function(){
  $(window).bind('scroll', function() {
    var distance = 200;
    if ($(window).scrollTop() > distance) {
      $('body').addClass('scrolled');
    }
    else {
      $('body').removeClass('scrolled');
    }
  });
});
