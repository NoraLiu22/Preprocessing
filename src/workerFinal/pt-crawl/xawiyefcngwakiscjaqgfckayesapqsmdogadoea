					(function ($) {
						$(function () {
							$('#296_303_addToCart:submit').click(function (e) {
								$('#quantity-validation-message').remove();
								if ($('#quantity-303').val() == '0') {
									var message = "<div id='quantity-validation-message' class='form-validation-message field-validation-error'>" +
										"Please enter a valid quantity" +
										"</div>";
									$('#quantity-303').after(message);
									$('#quantity-303').attr("aria-describedby", "quantity-validation-message");
									$('#quantity-303').focus();
									e.preventDefault();
								}
							});

							$('#quantity-303').change(function () {
								$('#quantity-validation-message').remove();
							});
						});
					})(adnsf$);
