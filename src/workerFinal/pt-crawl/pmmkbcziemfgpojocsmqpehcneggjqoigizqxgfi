    <script>
        //获取数据
        $Common.Popup.LoadingShow('ulimg');
        $Common.Popup.LoadingShow('ulcontent');
        GetData_PicList()
        function GetData_PicList() {
            $Common.Ajax.PostAPI("BoxOffice_GetMovieData_HotList", "topNum=15", function (data) {
                if (data && data["Table"]) {
                    $Common.Data.InitTemplate("TP_ProPic", data['Table'], function (html) {
                        $("#ulimg").html(html);
                    });
                }
            }, function (msg) { });
        }
    </script>
