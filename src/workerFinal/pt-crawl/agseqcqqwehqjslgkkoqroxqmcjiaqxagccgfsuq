    jQuery('div.post-text').hover(function(){
      //enter
      var post_id = jQuery(this).attr('id');
      jQuery('div#over-'+ post_id).css('opacity','.5');
    },function(){
      //leave
      var post_id = jQuery(this).attr('id');
      jQuery('div#over-'+ post_id).css('opacity','0');
    });
