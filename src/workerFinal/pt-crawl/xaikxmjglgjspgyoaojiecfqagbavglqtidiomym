        <script>
          function callWebtru(e, t) {
            var n = document.getElementById(e);
            n.addEventListener(
              'toggle',
              (e) => {
                if (n.open) {
                  const e = document.createElement('script');
                  (e.src = t),
                    (e.async = !0),
                    (e.id = '__datasign_consent_manager__'),
                    (e.dataset.embedDisclosureMode = 'true'),
                    document.head.append(e);
                }
              },
              { once: !0 }
            );
          }
        </script>
