            jQuery(document).ready(function($){
                if(!__tnt.user.services && window.falconOffer){
                    try {
                        var sSubUrl = 'https://subscriberservices.lee.net/subscriberservices/Content/paymentpagesingle.aspx?Domain=missoulian.com&SubscriberLevel={{level}}&Return=https%3A%2F%2Fmissoulian.com%2Fnews%2Fstate-regional%2F'.replace('{{level}}', window.falconOffer.level);
                        sSubUrl = `${sSubUrl}#tracking-source=header&ir=true`;
                        
                        if($('.lee-featured-subscription-head').length > 0){
                            $('.lee-featured-subscription-head').each(function(){
                                $(this).attr('href', sSubUrl)
                                    .html(`<span>Subscribe</span> <span class="term">${window.falconOffer.display_title}</span>`)
                                    .addClass('has-falcon-package');
                            });
                        }
                        if($('#join-btn-mobile-link').length > 0){
                            $('#join-btn-mobile-link').attr('href', sSubUrl);
                        }
                    } catch (e) {
                        if(window.console) console.warn(e);
                    }
                }
            });
