      <label for="edit-species-topic-filter">Find a Regulation by Species</label>
        <select data-drupal-selector="edit-species-topic-filter" id="edit-species-topic-filter" name="species_topic_filter" class="form-select form-control"><option value="All" selected="selected">- Any -</option><option value="67">Frog</option><option value="153">Temperate Bass</option><option value="155">Black Bass</option><option value="157">Bowfin</option><option value="158">Buffalo</option><option value="165">Bullhead</option><option value="166">Carp</option><option value="167">Carpsucker &amp; Quillback</option><option value="168">Catfish</option><option value="169">Crappie</option><option value="170">Freshwater Drum</option><option value="171">Gar</option><option value="172">Goggle-eye (Rock Bass) and Warmouth</option><option value="173">Muskellunge</option><option value="175">Paddlefish</option><option value="176">Pickerel</option><option value="177">Shovelnose Sturgeon</option><option value="178">Suckers</option><option value="179">Sunfish</option><option value="180">Trout</option><option value="181">Walleye &amp; Sauger</option><option value="182">Mussels &amp; Clams</option><option value="183">Northern Pike</option><option value="184">Gizzard Shad</option><option value="185">Turtle</option></select>
