  submitButton = $('button[name=LoginSubmit]');
  submitButton.attr('disabled', true);
  submitButton.css('opacity', 0.5);
  $('input[name=ldap_access]').click(function() {
    if (this.checked) {
      submitButton.removeAttr('disabled');
      submitButton.css('opacity', 1);
    } else {
      submitButton.attr('disabled', true);
      submitButton.css('opacity', 0.5);
    }
  });
});
