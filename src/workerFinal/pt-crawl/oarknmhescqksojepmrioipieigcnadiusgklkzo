<script type="text/javascript">
    //******
    // email_Updates
    //******
    $(function () {
        "use strict";
        $("#btnSubscribeSubmit").click(function (event) {
            event.preventDefault();
            
            var txtSubscribeEmail = $("#txtSubscribeEmail").val();
            var txtSubscribeFname = $("#txtSubscribeFname").val();
            var txtSubscribeLname = $("#txtSubscribeLname").val();
            if (txtSubscribeEmail === "") return;

            $.ajax({
                type: "POST",
                url: "/Conferences/Readers/Subscribe.asmx/AddSubscriberWithName",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                
                  data: "{ 'emailAddress': '" + txtSubscribeEmail + "', 'FirstName': '" + txtSubscribeFname + "', 'LastName': '" + txtSubscribeLname + "'}",
              
                success: function () {
                    $("#EmailSubscribeHolder").html("Successfully Subscribed");
                },
                error: function (response) {
                    $("#EmailSubscribeHolder").html("<span style=\"color:red;\">" + response.responseJSON.Message + "</span>");
                }
            });
        });
    });
</script> <!--module:Custom.Feature-Speaker-->

                
        <div class="colocated">
<h5>A Free Virtual Event Featuring Sessions From</h5>
