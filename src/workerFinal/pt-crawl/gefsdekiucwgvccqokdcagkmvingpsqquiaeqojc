                <!-- ko foreach: linkedNonLabelMemberBands() -->
                    <li class="submenu-item" data-bind="template: {
                        'name': 'band-menuitem-template'
                    },
                    css: {
                        'hidden': ($data.id == $root.artistsMenu.loggedInBand().id)
