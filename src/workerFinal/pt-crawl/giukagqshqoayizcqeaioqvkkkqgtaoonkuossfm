		var J = jQuery.noConflict();

		J(document).ready(function(){

					//Lightbox

					J("a[rel^='prettyPhoto']").prettyPhoto({theme: 'light_rounded',counter_separator_label: ' of '});
						

					//Portfolio hover

					J(".ad img, .portfolio-large img").hover(

						function() {

							J(this).animate({"opacity": ".8"}, "fast");

							},

						function() {

							J(this).animate({"opacity": "1"}, "fast");

					});			

					//Portfolio hover

					J(".fade img, .blog-content h2, .portfolio-items h2, h2.entry-title, .portfolio-items-full li h2 a, .gallery-item img, #sidebar h2 a").hover(

						function() {

							J(this).animate({"opacity": ".7"}, "fast");

							},

						function() {

							J(this).animate({"opacity": "1"}, "fast");

					});
								

					/* Sets the testimonials accordion functionality. */

						J('.testimonial-container').each(function(){

							J(this).find('div.testim-pane:first').addClass('first');

							J(this).tabs(jQuery(this).find('div.testim-pane'), {

								tabs: 'img', 

								effect: 'horizontal'

							})

						});									

					// Tabs

					J(function() {

						// setup ul.tabs to work as tabs for each div directly under div.panes

					J("ul.tabs").tabs("div.panes > div");

					});


					/*toggle script*/

						J(".toggle-container").hide();

						 

						J(".trigger").toggle(function(){

							J(this).addClass("active");

							}, function () {

							J(this).removeClass("active");

						});

						J(".trigger").click(function(){

							J(this).next(".toggle-container").slideToggle();

						});

						

						J('.trigger a').hover(function() {

						J(this).stop(true,false).animate({color: '#666'},50);

							}, function () {

							J(this).stop(true,false).animate({color: '#888'},150);

					});

					/*accordion script*/

					J('.accordion').hide();

		

					J('.trigger-button').click(function() {

						J(".trigger-button").removeClass("active")

						J('.accordion').slideUp('normal');

						if(J(this).next().is(':hidden') == true) {

							J(this).next().slideDown('normal');

							J(this).addClass("active");

						 } 

					 });


