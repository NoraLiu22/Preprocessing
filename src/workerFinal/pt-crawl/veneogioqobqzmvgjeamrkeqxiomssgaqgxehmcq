        <h2  class="h2 section__title">
        
            Belgium
      
  
    </h2>

<div class="field field-name-field-pt-block"><section class="views-element-container block block-views block-views-blockvw-dynamic-list-standard clearfix" id="block-views-block-vw-dynamic-list-standard--2">
  
    

      <div><div class="view view-vw-dynamic-list view-id-vw_dynamic_list view-display-id-standard js-view-dom-id-106968096e655c0f1f5ae913d3a66d3af7f0369af650ad5c3cb0eaf0cb68382a">
  
    
    
      <div class="view-content row">
          <div class="col-xs-12"><article  class="ct--view-20 node node--type-ct-person node--view-mode-vm-view20 ct">

        <div class="ct__image-container ct__image-container--fallback ct__image-container--person"></div>

  
  <div class="ct__content">
                      
