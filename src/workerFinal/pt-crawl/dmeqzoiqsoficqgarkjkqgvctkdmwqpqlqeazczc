            gusto.CATEGORY_PATH = "/news/";
            gusto.PROJECT_NAME = "smooth";
            gusto.PLAYER_INFO = {"localise_url": "/localise/", "play_overlay": false, "player_url_ukrp": "https://radioplayer.musicradio.com/radio/smooth/uk/", "is_localised": false, "twin_stations": null, "player_url": "https://www.globalplayer.com/live/smooth/uk", "global_player_enabled": true, "localisation_light_enabled": false, "localisations_enabled": true, "player_url_template": ""};
            gusto.DEBUG = false;
            gusto.SETTINGS = {
                'COOKIE_STATION_MAX_AGE_SECONDS': 63072000
            };
            gusto.ad_settings = {
                'banner_visibility_delay': 3000,
            };
            gusto.ad_switch = {
                'enable_ads_dac': true,
                'enable_ads_related': true,
                'enable_ads_ukrp': false,
                'enable_ads_preroll': true,
                'enable_ads_related_videos': false,

            };
            gusto.STATION = {
                'name': "Smooth",
                'slug': ""
            };
            gusto.breakpoints = {
                limits: {
                    'small': [0, 640],
                    'medium': [641, 1023],
                    'large': [1024, 1440],
                    'xlarge': [1441, 1920],
                    'xxlarge': [1921, 9001]
                },
                check: function(target) {
                    
                    var target_bits = target.split('-'),
                        brkpoint = target_bits[0],
                        dir = target_bits[1],
                        win = window.innerWidth,
                        ref = this.limits[brkpoint];
                    if (!ref) return false;
                    if (dir == 'up') return (win >= ref[0]);
                    else if (dir == 'down') return (win <= ref[1]);
                    else return (win >= ref[0] && win <= ref[1]);
                }
            };

            gusto.ABSOLUTE_URL = "/news/";
            gusto.station_twitter = 'smoothradio';
            gusto.FACEBOOK_APP_ID = '233524320165894';
            gusto.category_tree = {"384": {"full_slug": "/artists/boy-george/", "name": "Boy George", "parent": 76}, "1": {"full_slug": "/", "name": "smoothradio.com", "parent": null, "children": [231, 126, 123, 76, 77, 79, 203, 361, 202, 57, 399, 400]}, "131": {"full_slug": "/artists/stevie-wonder/", "name": "Stevie Wonder", "parent": 76}, "388": {"full_slug": "/radio/podcasts/", "name": "Podcasts", "parent": 231}, "134": {"full_slug": "/artists/adele/", "name": "Adele", "parent": 76}, "136": {"full_slug": "/artists/elton-john/", "name": "Elton John", "parent": 76}, "137": {"full_slug": "/artists/lionel-richie/", "name": "Lionel Richie", "parent": 76}, "399": {"full_slug": "/country-2/", "name": "Country", "parent": 1}, "400": {"full_slug": "/chill-2/", "name": "Chill", "parent": 1}, "286": {"full_slug": "/artists/ed-sheeran/", "name": "Ed Sheeran", "parent": 76}, "287": {"full_slug": "/artists/take-that/", "name": "Take That", "parent": 76}, "289": {"full_slug": "/artists/celine-dion/", "name": "Celine Dion", "parent": 76}, "294": {"full_slug": "/artists/robbie-williams/", "name": "Robbie Williams", "parent": 76}, "57": {"full_slug": "/localise/", "name": "Choose your station", "parent": 1}, "188": {"full_slug": "/artists/whitney-houston/", "name": "Whitney Houston", "parent": 76}, "317": {"full_slug": "/artists/queen/", "name": "Queen", "parent": 76}, "191": {"full_slug": "/artists/phil-collins/", "name": "Phil Collins", "parent": 76}, "202": {"full_slug": "/singles/", "name": "Dating", "parent": 1}, "203": {"full_slug": "/charity/", "name": "Charity", "parent": 1}, "76": {"full_slug": "/artists/", "name": "Artists", "parent": 1, "children": [85, 134, 84, 286, 136, 118, 113, 137, 83, 382, 384, 80, 191, 122, 131, 287, 188, 289, 294, 317]}, "77": {"full_slug": "/events/", "name": "Tickets", "parent": 1}, "79": {"full_slug": "/win/", "name": "Win", "parent": 1}, "80": {"full_slug": "/artists/michael-jackson/", "name": "Michael Jackson", "parent": 76}, "83": {"full_s