        <script type="text/javascript">
            var $window = $(window);
            var $header = $('.header-banner-step1');
            var $headerUnit = $('.header-unit-step1');
            $(window).resize(function () {
                var factor = 0.55;
                var width = $window.width();

                var height = factor * width;

                $header.css({
                    'height': height,
                    'border-width': '0px'
                });
                $headerUnit.css({
                    'height': height,
                    'border-width': '0px'
                });
            }).resize();
        </script>

        <script type="text/javascript">
            var out = 1;
            var inside = 1;
            $(window).scroll(function () {
                var top_of_element = $("#crcle").offset().top;
                var bottom_of_element = $("#crcle").offset().top + $("#crcle").outerHeight();
                var bottom_of_screen = $(window).scrollTop() + $(window).innerHeight();
                var top_of_screen = $(window).scrollTop();

                if ((bottom_of_screen > top_of_element) && (top_of_screen < bottom_of_element) && inside == 1) {
                    // the element is visible, do something
                    $(".circle1path").removeClass('circleanimation');
                    inside = 2;
                    setTimeout(function () {
                        $(".circle1path").addClass('circleanimation');
                    }, 0);

                }
            });
        </script>

        <script type="text/javascript">
            var video1 = document.getElementById("why-UMock");
            //var spinner = document.getElementById("spinner");
            var delayMillis = 4000;
            var spinnerIsHere = 1;
            video1.volume = 0;

            var playVid = setTimeout(function () {
                if (spinnerIsHere == 1) {
                    // Delete element DOM
                    // spinner.parentNode.removeChild(spinner);
                    //spinner.style.visibility = "hidden";
                    spinnerIsHere = 0;
                }
                video1.play();
            }, delayMillis);
        </script>
