<script>
    var App = App || {};
    App.MagazineView = App.MagazineView || {} ;
    App.MagazineView.siteName = 'jama';
    App.MagazineView.siteId = 3;
    App.MagazineView.journalId = 67;
    App.MagazineView.adDisplay = 'DualBoxMobile';
    App.MagazineView.adSection = "magazine";
    App.MagazineView.adPosition = "bta";
    App.MagazineView.controlId = "AMA_AdTag_magazine_bta_dualMobile";
    App.MagazineView.fromGoogle = 'False';
</script>

 
