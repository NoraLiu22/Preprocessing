<nav id="p-bw-sidebar-projects" class="mw-portlet mw-portlet-bw-sidebar-projects vector-menu-portal portal vector-menu" aria-labelledby="p-bw-sidebar-projects-label" role="navigation"  >
	<h3
		id="p-bw-sidebar-projects-label"
		
		class="vector-menu-heading "
	>
		<span class="vector-menu-heading-label">Projects</span>
	</h3>
