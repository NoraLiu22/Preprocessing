    <div class="widget-ArticleListAccess widget-instance-ArticleListAccess">
            <script type="text/javascript">

        /*******************************************************************************
         * JS here is only being used to assign variables from values in the model
         * logic should be implemented in external JS files, like client.script.js
         *******************************************************************************/

        var SCM = SCM || {};
        var accessIcons = [{"id":11945,"icon":"icon-availability_free","title":"Free"},{"id":11946,"icon":"icon-availability_free","title":"Free"},{"id":11950,"icon":"icon-availability_free","title":"Free"},{"id":11952,"icon":"icon-availability_free","title":"Free"},{"id":11956,"icon":"icon-availability_free","title":"Free"},{"id":11958,"icon":"icon-availability_free","title":"Free"},{"id":11961,"icon":"icon-availability_free","title":"Free"},{"id":11964,"icon":"icon-availability_free","title":"Free"},{"id":11968,"icon":"icon-availability_free","title":"Free"},{"id":11970,"icon":"icon-availability_free","title":"Free"},{"id":11973,"icon":"icon-availability_free","title":"Free"},{"id":11975,"icon":"icon-availability_free","title":"Free"},{"id":11982,"icon":"icon-availability_free","title":"Free"},{"id":11986,"icon":"icon-availability_free","title":"Free"},{"id":11991,"icon":"icon-availability_free","title":"Free"},{"id":11993,"icon":"icon-availability_free","title":"Free"},{"id":11995,"icon":"icon-availability_open","title":"Open Access"},{"id":11947,"icon":"icon-availability_free","title":"Free"}];
        if (SCM.AccessIcons) {
            SCM.AccessIcons = SCM.AccessIcons.concat(accessIcons);
        } else {
            SCM.AccessIcons = accessIcons;
        }
        var accessAttributes =  [{"id":11945,"availableforpurchase":false},{"id":11946,"availableforpurchase":false},{"id":11950,"availableforpurchase":false},{"id":11952,"availableforpurchase":false},{"id":11956,"availableforpurchase":false},{"id":11958,"availableforpurchase":false},{"id":11961,"availableforpurchase":false},{"id":11964,"availableforpurchase":false},{"id":11968,"availableforpurchase":false},{"id":11970,"availableforpurchase":false},{"id":11973,"availableforpurchase":false},{"id":11975,"availableforpurchase":false},{"id":11982,"availableforpurchase":false},{"id":11986,"availableforpurchase":false},{"id":11991,"availableforpurchase":false},{"id":11993,"availableforpurchase":false},{"id":11995,"availableforpurchase":false},{"id":11947,"availableforpurchase":false}];
        if (SCM.AccessAttributes) {
            SCM.AccessAttributes = SCM.AccessAttributes.concat(accessAttributes);
