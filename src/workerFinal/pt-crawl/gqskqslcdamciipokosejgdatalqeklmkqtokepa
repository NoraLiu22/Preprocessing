  document.getElementById("").onclick = function() {showRows('');};
  }
  
  function showRows(str) {
    var array = str.split(",");
    var x = document.getElementsByClassName("complex-layout");
    for (i = 0; i < x.length; i++) {
      x[i].style.display = "none";
    }
    array.forEach(function(i) {
     if (x[i] != undefined){
      	x[i].style.display = "inline";
     }
    });
  }
  function showAll() {
    var x = document.getElementsByClassName("complex-layout");
    for (i = 0; i < x.length; i++) {
      if (x[i] != undefined){
      	x[i].style.display = "inline";
      }
    }
  }
  
  var ids_array = ['Bio', 'Education', 'Publications', 'Bio', 'Recent Services and Honors', '','', '', ''];
    
  jQuery(document).ready(function(){
    if ('' != '' && jQuery.inArray( '', ids_array ) != -1 ){
      var default_view = '';
      jQuery('#breadcrumb-navigation > ul.breadcrumbs > li#' + default_view).trigger('click');
    } else {
        showRows('0,1');
    }
  });
</script>
<style>
/* Style Breadcrumb Links as A's */
  #breadcrumb-navigation > ul.breadcrumbs > li{
    font-family: gentona_medium!important;
    font-size: 1rem;
    color: #343741;
    cursor: pointer;
  }
  #breadcrumb-navigation > ul.breadcrumbs > li:hover{
    color: #0021A5;
  }
</style>  </div>
