			isiChartData[5] = (adata.yearTotalMap[''+year]!=null ? adata.yearTotalMap[''+year] : 0);
			drawChart();
		}).always(function(adata){
			//console.log('end isiUpdateCitation');
			runningExternal=false;
		});
	}
