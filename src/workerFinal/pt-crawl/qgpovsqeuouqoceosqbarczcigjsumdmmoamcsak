							J(this).addClass("active");

							}, function () {

							J(this).removeClass("active");

						});

						J(".trigger").click(function(){

							J(this).next(".toggle-container").slideToggle();

						});

						

						J('.trigger a').hover(function() {

						J(this).stop(true,false).animate({color: '#666'},50);

