<script>
    $(window).ready(function () {
        var h = $('.wrapper-box-b').height();
        $(".top-svg-5").css({"height": h+"px"});
        $(".close").on("click",function () {
            $(".hidden-box--a-menu").removeClass("show");
            $(".hidden-box--a-menu-bg").removeClass("show");
            $(".wrapper-box-b").removeClass("blur");
        });

        $(".burger").on("click",function () {
            $(".hidden-box--a-menu").addClass("show");
            $(".hidden-box--a-menu-bg").addClass("show");
            $(".wrapper-box-b").addClass("blur");
        });

        $(".hidden-box--a-menu-bg").on("click",function () {
            $(".hidden-box--a-menu").removeClass("show");
            $(".hidden-box--a-menu-bg").removeClass("show");
            $(".wrapper-box-b").removeClass("blur");
        });

        $(".form-item button").on("click", function () {
            var form = $("form");
            var val = 0;
            var name = $(this).closest('form').find('input[name="name"]');
            var email = $(this).closest('form').find('input[name="email"]');
            var text = $(this).closest('form').find('textarea');

            if(EmptyField(email) && VerificationEmail(email,'')){
                val = val + 1;
            }

            if(EmptyField(name)){
                val = val + 1;
            }
            if(EmptyField(text)){
                val = val + 1;
            }

            if(val==3){
                form.remove();
                $(this).closest('form').find('input[name="name"]').val(' ');
                $(this).closest('form').find('input[name="email"]').val(' ');
                $(this).closest('form').find('textarea').val(' ');
                $(".message").html("Thank you! <br> Your message has been sent for moderation.");
            }

            return false;
        });

        function VerificationEmail(el, event){
            var tmpEmail = /^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,8}|[0-9]{1,3})(\]?)$\.?$/i;
            var result = tmpEmail.test($(el).val())
            if (!result) {
                $(el).removeClass('verefy');
                $(el).addClass('error');
                return false;
            }
            else {
                $(el).addClass('verefy');
                $(el).removeClass('error');
                return true;
            }
        }
        function EmptyField(el){
            if ($(el).val() == '') {
                $(el).removeClass('verefy');
                $(el).addClass('error');
                return false;
            }
            else{
                $(el).addClass('verefy');
                $(el).removeClass('error');
                return true;
            }
        }
    });
    $(window).resize(function () {
        var h = $('.wrapper-box-b').height();
        $(".top-svg-5").css({"height": h+"px"});
    });
</script>
