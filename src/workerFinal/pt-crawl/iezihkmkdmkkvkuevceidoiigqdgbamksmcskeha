<article class="node node--type-staff-profile node--view-mode-full">
  <div class="media-wrapper">
          
            <div class="field field--name-field-staff-contact-image field--type-entity-reference field--label-hidden field__item"><figure class="media media--view-mode-circular_large_download media--type-image">
  
      
  <div class="field field--name-field-media-image field--type-image field--label-visually_hidden">
    <h2 class="field__label visually-hidden">Image</h2>
              <div class="field__item">  <img loading="lazy" src="/assets/styles/circular_l/public/2022-05/frank-rusco.jpg.webp?itok=h7tt9uc8" width="320" height="320" alt="Frank Rusco" />


</div>
          </div>

  <div class="image-photo-download">
    <a href="https://www.gao.gov/assets/2022-05/frank-rusco.jpg" target="_blank"><span class="fas fa-download"></span> Download Hi-Res Photo</a>
  </div>


  </figure>
