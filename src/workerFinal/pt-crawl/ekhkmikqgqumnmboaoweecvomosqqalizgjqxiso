		var event = $(this);
		var event_id = $(this).attr('id');
		var list_id = $(this).attr('tag');
		$.ajax({
			url: 'https://igoevent.com/api/dates?id='+event_id+'&list_id='+list_id,
			dataType : "json",
			success: function (dates) {
				$(event).html(dates);
			}
		});
	});

