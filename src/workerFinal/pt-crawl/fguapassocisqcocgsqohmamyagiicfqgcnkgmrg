            <script>
                const rootElement = document.querySelector('[data-js-inspirationcalendar-intro=root]');

                if (document.cookie.includes('inspiration-calendar-intro-seen=true')) {
                    rootElement.remove();
                } else {
                    const templateElement = rootElement.querySelector('[data-js-inspirationcalendar-intro=template]')
                    const content = templateElement.content.cloneNode(true);
                    rootElement.append(content);

                    window.requestAnimationFrame(() => {
                        const contentElement = rootElement.querySelector('[data-js-inspirationcalendar-intro=content]');
                        contentElement.addEventListener('animationend', () => {
                            document.cookie = 'inspiration-calendar-intro-seen=true; path=/; samesite=strict';
                            rootElement.remove();
                        });
                    });
                }
            </script>
        </div>
