        //form element AUT_fname required check
        if( !_CF_hasValue(_CF_this['AUT_fname'], "TEXT", false ) )
        {
            _CF_onError(_CF_this, "AUT_fname", _CF_this['AUT_fname'].value, "You must enter a First Name");
            _CF_error_exists = true;
        }

        //form element AUT_lname required check
        if( !_CF_hasValue(_CF_this['AUT_lname'], "TEXT", false ) )
        {
            _CF_onError(_CF_this, "AUT_lname", _CF_this['AUT_lname'].value, "You must enter a Last Name");
            _CF_error_exists = true;
        }

        //form element AUT_email required check
        if( _CF_hasValue(_CF_this['AUT_email'], "TEXT", false ) )
        {
            //form element AUT_email 'EMAIL' validation checks
            if (!_CF_checkEmail(_CF_this['AUT_email'].value, true))
            {
