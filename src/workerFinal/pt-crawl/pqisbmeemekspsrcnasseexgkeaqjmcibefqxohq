<section class="banner-section first-section horizontal-cards-section">
    <a id="cultural-education" class="anchor-link first-anchor-link"></a>
    <div class="row">
        <div class="card horizontal-card full-feature-card col-xs-12">
            <div class="row">
                <div class="image col-sm-7">
                    <a href="/expand-your-cultural-education/smithsonian"><img src="/images/education/virginia-dancer-large.jpg" alt="Expand Your Cultural Education" /></a>
                </div>
                <div class="details col-sm-5">
                    <div class="details-inner no-underline">
                        <a href="/expand-your-cultural-education/smithsonian" class="title">
                            <span class="light">
                                <span class="er">
                                    <h2>Expand Your Cultural Education</h2>
                                </span>
                            </span>
                        </a>
                        <div class="description link-underline">
                            <p>
                                Download lesson plans and learning guides, watch videos, order books and recordings, browse online exhibitions, and link to a world of cultural heritage educational materials.
                            </p>
                            <p>
                            </p>
                        </div>
                        <div class="content">
                            <a href="/expand-your-cultural-education/smithsonian" class="high more-link ">
                                <div class="light">
