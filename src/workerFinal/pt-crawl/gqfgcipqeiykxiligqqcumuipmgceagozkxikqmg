    <select data-drupal-selector="edit-dogs-not" id="edit-dogs-not" name="dogs_not" class="form-select"><option value="All" selected="selected">- Any -</option><option value="no_dogs_permitted">No dogs permitted</option><option value="dogs_permitted">Dogs permitted</option><option value="on_a_lead">On a lead</option><option value="under_effective_control">Under effective control</option><option value="guide_dogs_only">Assistance dogs only</option></select>
    <span class="validation--icon"></span>
