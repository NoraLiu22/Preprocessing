<script type="text/javascript">
    $(document).ready(function () {
        var playerVersion = infosoftglobal.FusionMapsUtil.getPlayerVersion();
        if (playerVersion.major < 6) {
            google.setOnLoadCallback(drawChart);
        } else {
            renderFlashMap();
        }
    });
