<!-- form_tree_checkbox_bootstrap5.it -->
<script type="text/javascript">
domReady(function() {
    newsletterParams_checkForParentNodesIndeterminate()
})

function newsletterParams_checkForParentNodesIndeterminate() {
    const parentNodes = Array.from(document.querySelectorAll('.has-children'))
    parentNodes.forEach((parentNode) => {
        const parentNodeInput = parentNode.querySelector('input[type="checkbox"]')
        const parentNodeId = parentNode.id;
        const childNodes = Array.from(document.querySelectorAll(`[data-parent="${parentNodeId}"]`))
        const childNodesCount = childNodes.length
        let childNodesChecked = 0;
        childNodes.forEach((childNode) => {
            const childNodeInput = childNode.querySelector('input[type="checkbox"]')
            if (childNodeInput.checked) {
                childNodesChecked += 1
            }
        })
        if (childNodesChecked < childNodesCount && childNodesChecked !== 0) {
            parentNodeInput.indeterminate = true
        }
    })
}

function newsletterParams_uncheck(node) {
    if(node) {
        node.classList.remove('checked')
        node.checked = false;
        node.removeAttribute('checked');
    }
}

function newsletterParams_check(node) {
    if(node) {
        node.classList.add('checked')
        node.checked = true;
        node.setAttribute('checked', 'checked');
    }
}

function newsletterParamsToggleArrow(inputNode) {
    var isExpanded = inputNode.getAttribute('aria-expanded') === 'true';
    var ariaLabel = isExpanded ? 'Collapse' : 'Expand';
    var itemLabel = inputNode.getAttribute('data-itemlabel');
    inputNode.setAttribute('aria-label', ariaLabel + ' ' + itemLabel);
}

/**
 * In base all'elemento flaggato il metodo lavora sui nodi figli per abilitare o disabilitare
 * i check sull'input checkbox
 */
function newsletterParamsToggleInputChildren(inputNode) {

    const parentNode = inputNode?.parentNode?.parentNode

    let childInputNodes = [];
    if (parentNode) {
        childInputNodes = Array.from(document.querySelectorAll(`[data-parent="${parentNode.id}"] input[type="checkbox"]`));
    }

    if (inputNode.checked) {
        newsletterParams_check(inputNode)
        // abilito tutti i check dai nodi figli
        if (childInputNodes.length) {
            childInputNodes.forEach((childInputNode) => newsletterParams_check(childInputNode))
        }
    } else {
        newsletterParams_uncheck(inputNode)
        // disabilito tutti gli input dei nodi figli
        if (childInputNodes.length) {
            childInputNodes.forEach ((childInputNode) => newsletterParams_uncheck(childInputNode))
        }
    }

    newsletterParamsCheckParentOnAllChildrenChecked(inputNode, inputNode.checked);
}

function newsletterParamsCheckParentOnAllChildrenChecked(inputNode, isAChildChecked) {
    const parentNode = document.getElementById(inputNode?.parentNode?.parentNode?.dataset?.parent);
    // nodo padre dell'albero
    let branchChildNodes = [];
    let branchParentNode;
    if (parentNode) {
        branchParentNode = document.getElementById(parentNode.id)
        branchChildNodes = Array.from(document.querySelectorAll(`[data-parent="${parentNode.id}"]`))
    }
    let allChildrenAreChecked = true
    let allChildrenAreUnChecked = true

    if (branchChildNodes.length) {
        // ciclo i nodi dello stesso livello del checkbox flaggato per capire quale stato impostare al nodo branch padre
        branchChildNodes.forEach((branchChildNode) => {

            const inputBranchChildNode = branchChildNode.querySelector('input[type="checkbox"]');
            const isChecked = inputBranchChildNode.checked
            if (!isChecked && allChildrenAreChecked) {
                allChildrenAreChecked = false;
            }
            if ((isChecked && allChildrenAreUnChecked) || isAChildChecked) {
                allChildrenAreUnChecked = false;
            }
        })

        const inputBranchParentNode = branchParentNode.querySelector('input[type="checkbox"]')
        if (allChildrenAreChecked) {
            // tutti i nodi dello stesso livello del nodo checkato sono stati flaggati quindi devo flaggagre
            // solo il nodo branch padre
            inputBranchParentNode.indeterminate = false
            newsletterParams_check(inputBranchParentNode)
        } else if (allChildrenAreUnChecked) {
            // nessun check sui sotto nodi del branch padre
            if (inputBranchParentNode.indeterminate) {
                inputBranchParentNode.indeterminate = false
            }
            newsletterParams_uncheck(inputBranchParentNode)
        } else {
            // check parziali sui sotto nodi del branch padre
            inputBranchParentNode.indeterminate = true
            newsletterParams_uncheck(inputBranchParentNode)
        }
    }
    // controllo se il nodo branch padre proviene da un ramo superiore, in questo caso lancio una
    // chiamata ricorsiva passando come argomento il nodo branch padre
    if (branchParentNode) {
        const idSuperBranchParent = branchParentNode.dataset.parent
        const superBranchParentNode = document.getElementById(idSuperBranchParent)
        if (superBranchParentNode) {
            const inputBranchParentNode = branchParentNode.querySelector('input[type="checkbox"]')[0]
            newsletterParamsCheckParentOnAllChildsChecked(inputBranchParentNode, isAChildChecked)
        }
    }
}
</script>

<div class="enabu-form-tree-checkbox-content tree-id-newsletterParams" id="tree-id-newsletterParams">
    <!-- elenco elementi -->
    <div class="block items">
                                                    <div class="parent-wrapper">
                <div class="toggle-branch">
                    <button class="btn btn-link p-0 mx-1"
                            type="button"
                            data-itemlabel="Arts and music"
                            data-bs-toggle="collapse"
