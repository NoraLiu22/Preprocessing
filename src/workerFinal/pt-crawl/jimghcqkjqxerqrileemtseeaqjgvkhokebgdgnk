<script>
    function userForm_submit(action) {
        const userAgree = checkLicenseAgreements()
        if (userAgree) {
            const form = document.getElementById('userForm');
            form.action = '/mulino/Userform/' + action;
            form.submit();
        }
    }

    function checkLicenseAgreements() {
        const licenseAgreementCheckbox = document.getElementById('licenseAgreement')
        const licenseAgreement2Checkbox = document.getElementById('licenseAgreement2')
        return (
                licenseAgreementCheckbox === null
                && licenseAgreement2Checkbox === null
            ) || !!(
                licenseAgreementCheckbox
                && licenseAgreement2Checkbox
                && licenseAgreementCheckbox.checked
                && licenseAgreement2Checkbox.checked
            )
    }

    function userForm_selectCountry() {
    var form = document.getElementById('userForm');
    var state = form.elements['userForm-state'];
    var provinceRow = document.getElementById('ctrlhld-userForm-province');
    if (provinceRow == null) {
        return;
    } else {
       if (state.value == '1') {
           provinceRow.style.display = '';
       } else {
           provinceRow.style.display = 'none';
       }
    }
    return;
}

dojo.addOnLoad(function() {
    userForm_selectCountry();
});
</script>
        </main>

    
