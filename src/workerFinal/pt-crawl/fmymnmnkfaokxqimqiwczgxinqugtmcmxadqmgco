            if (typeof val == 'number') {
                if (!format) {
                    format = '%.2f';
                }
                return numberWithCommas($.jqplot.sprintf(format, val));
            }
            else {
                return String(val);
            }
        };
        function numberWithCommas(x) {
            return x.toString().replace(/\B(?=(?:\d{3})+(?!\d))/g, ",");
        }

