          <div class="item">


            <div class="top-col">                        
              <div id="hs_cos_wrapper_module_1632344833215114" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_module" style="" data-hs-cos-general-type="widget" data-hs-cos-type="module"><div class="Featured-Graphic">

  <!-- Vimeo -->
  

  <!-- Youtube -->
  

  <!-- Images -->
  
  <div class="Photo"> 
    
    
    
    
    
    
    <img src="https://www.artscommons.ca/hubfs/MicrosoftTeams-image.png" alt="MicrosoftTeams-image" loading="lazy" style="max-width: 100%; height: auto;">
      
