        mounted() {
          console.log("Tess Dev. module mounted!");
          var _this = this;
          if(this.tessituraID){
            /* Get Event Details by ID */
            // var data = { "action" : "getTessitura", "procedure": "LP_GET_UMBRACO_DATES", "procedureID": "145", "tessitura_id": this.tessituraID };            
            const _data = { "action" : "getAllTessitura", "procedure": "", "tessitura_id": this.tessituraID };
            const options = {
              url: baseUriServer,
              method: 'POST', 
              data: _data
            };
            console.log("axios method() here!");
            axios(options).then(response => {
              console.log(response.status);              
              /* 1. Data for the Main Show */
              this.mainShow = response.data[0][0];
              /* 2. Data for Genres */
              this.genres = response.data[1];            
              var val_genres = JSON.stringify(response.data[1]);
              /* Method: Compare if Tessitura's Genres match Blog Post's Topics */
              this.checkTopicsWithGenres();

              /* 3. Data for the performances */
              console.log("performances are here!");
              this.performances = response.data[2];
              var val_performances = JSON.stringify(response.data[2]);

              /* Method: Update GetTicketCTAs */
              if(this.performances[0]) {
                this.updateGetTicketCTA(this.tessituraID, this.performances[0].perf_no);
                var _isSinglePerformance = this.isSinglePerformance ? 1 : 0;
                var _ticketURL = this.ticketURL + this.tessituraID + '/' + this.performances[0].perf_no;      
                console.log("format time data1 : " + this.mainShow.first_perf_timestamp);

                this.timestamp.first_perf_timestamp = this.mainShow.first_perf_timestamp ? Number(this.mainShow.first_perf_timestamp) * 1000 : 0;
                this.timestamp.last_perf_timestamp = this.mainShow.last_perf_timestamp ? Number(this.mainShow.last_perf_timestamp) * 1000 : 0;
                this.timestamp.onsale_timestamp = this.mainShow.onsale_timestamp ? Number(this.mainShow.onsale_timestamp) * 1000 : 0;
                this.timestamp.presale_timestamp = this.mainShow.presale_timestamp ? Number(this.mainShow.presale_timestamp) * 1000  : 0;
                this.timestamp.off_sale_timestamp = this.mainShow.off_sale_timestamp ? Number(this.mainShow.off_sale_timestamp) * 1000 : 0;
                console.log("format time data2 : " + this.timestamp.first_perf_timestamp);

                var val_presale_dt = this.mainShow.presale_dt ? this.mainShow.presale_dt : "",
                    val_onsale_dt = this.mainShow.onsale_dt ? this.mainShow.onsale_dt : "",
                    val_first_perf_dt = this.mainShow.first_perf_dt ? this.mainShow.first_perf_dt : "",
                    val_last_perf_dt = this.mainShow.last_perf_dt ? this.mainShow.last_perf_dt : "",
                    val_min_price = this.mainShow.min_price ? this.mainShow.min_price : "",
                    val_max_price = this.mainShow.max_price ? this.mainShow.max_price : "",
                    val_show_promo = this.mainShow.show_promo ? this.mainShow.show_promo : "",
                    val_first_perf_time = this.mainShow.first_perf_time ? this.mainShow.first_perf_time : "",
                    val_last_perf_time = this.mainShow.last_perf_time ? this.mainShow.last_perf_time : "",
                    val_presale_time = this.mainShow.presale_time ? this.mainShow.presale_time : "",
                    val_onsale_time = this.mainShow.onsale_time ? this.mainShow.onsale_time : "",   
                    val_isSaleOpen = this.isSaleOpen(_this.timestamp.onsale_timestamp) ? 1 : 0,
                    val_first_perf_timestamp = _this.timestamp.first_perf_timestamp,
                    val_last_perf_timestamp = _this.timestamp.last_perf_timestamp,
                    val_onsale_timestamp = _this.timestamp.onsale_timestamp,
                    val_presale_timestamp = _this.timestamp.presale_timestamp,
                    val_off_sale_timestamp = _this.timestamp.off_sale_timestamp,
                    _new_hubdb_data = {"name" : this.eventTitle,
                                       "postID" : this.postID, 
                                       "isSinglePerformance" : _isSinglePerformance, 
                                       "eventURL": _ticketURL, 
                                       "genres" : val_genres,
                                       "presale" : val_presale_dt, 
                                       "onsale" : val_onsale_dt, 
                                       "first_perf" : val_first_perf_dt, 
                                       "last_perf" : val_last_perf_dt, 
                                       "min_price" : val_min_price, 
                                       "max_price" : val_max_price,
                                       "show_promo" : val_show_promo,
                                       "first_perf_time" : val_first_perf_time,
                                       "last_perf_time" : val_last_perf_time,
                                       "presale_time" : val_presale_time,
                                       "onsale_time" : val_onsale_time, 
                                       "performances" : val_performances,
                                       "isSaleOpen" : val_isSaleOpen,                                       
                                       "first_perf_timestamp" : val_first_perf_timestamp,
                                       "last_perf_timestamp" : val_last_perf_timestamp,
                                       "onsale_timestamp" : val_onsale_timestamp,
                                       "presale_timestamp" : val_presale_timestamp,
                                       "off_sale_timestamp" : val_off_sale_timestamp};

                <!-- IF HubDB Table DATA for the Event matches the Tessitura Data, then all good. If NOT, then update the HubDB data for that specific event! -->
                  if(JSON.stringify(_this.current_hubdb_data) === JSON.stringify(_new_hubdb_data)) console.log("comparison --> true");
                else
                {
                  console.log("comparison --> FALSE --> Update HubDB DATA based on Tessitura DATA.");
                  _this.updateHubDB(this.tessituraID, _new_hubdb_data);
                }
              }
              else{
                console.log("NO NEED FOR a COMPARISON!");
              }

            }).catch(error => {
              this.errorMessage = error.message;
              console.error("There was an error!", error);
            });


          }


          if($(".il_hero .event-cta-w a.cta_button").length > 0){
            // Add this event only for the main CTA on the header ( not for the additional CTA )
            $(".il_hero .event-cta-w.main-cta a.cta_button").click(function(event){
              event.preventDefault();
              console.log("redirectionx... : " + $("#ghost-event-btn").length);    
              if($("#ghost-event-btn").length > 0){
                $("#ghost-event-btn")[0].click();         
                if(!_this.isSinglePerformance) {
                  $(".cta.event-cta-w.main-cta a").text("Show Description");                  
                  _this.scrollToEventDetails("event-listing-content");
                }
              }
              else{                

                //Check if it is with multiple perf. If so, then scroll
                if(!_this.isSinglePerformance) {
                  _this.scrollToEventDetails("event-description");
                  _this.showPerformances = !_this.showPerformances;
                  $(".cta.event-cta-w.main-cta a").text("Select Showtime");
                }
                else $(".cta.event-cta-w.main-cta a").text("Get Tickets");
              }
            });            
          }          
        },
          computed: {
            <!-- Show the event only if the date criteria is met -->
              isHubDObjectExists : function(){
                if(this.current_hubdb_data)
                  return true;
                else
                  return false;
              },
                isMainShowON : function(){
                  if(this.isPromotionValid) return true;
                  if(this.mainShow){                    
                    var _onSaleDate = this.timestamp.onsale_timestamp;
                    var _nowCanada = this.timestamp.now_canada_timestamp;
                    
                    /* We should reach the Sale date! */
                    if(_nowCanada >= _onSaleDate) return true;
                    else return false;
                  }
                  return false;
                },
                  isSaleOver : function(){
                    if(this.mainShow){
                      // Two-levels of Check
                      if(this.timestamp.off_sale_timestamp){                        
                        // Check 1 - By TimeStamp Data
                        var _offSaleDate = this.timestamp.off_sale_timestamp;                
                        var _nowCanada = this.timestamp.now_canada_timestamp;
                        console.log("isSaleOver | by timestamp , eventOffSale : " + _offSaleDate + " | vs | now: " + _nowCanada);
                        if(_nowCanada >= _offSaleDate && _offSaleDate == 0) return true;                              
                        else return false;
                      }
                      else
                        return true;
                    }
                  },
                    firstPerformanceNo : function(){
                      if(this.performances[0]) return this.performances[0].perf_no;
                      else return "";
                    },
                      isSinglePerformance : function(){
                        var newEventURL = this.ticketURL + this.tessituraID + '/' + this.firstPerformanceNo;
                        <!-- console.log("newEventURL : " + newEventURL); -->
                          if(this.performances.length > 1){
                            console.log("xIF");
                            if($(".il_hero .event-cta-w a.cta_button").length > 0) {
                              var _label = "Select Showtime";

                              console.log("this.mainCTALabel : " + this.mainCTALabel);
                              console.log("_label 1 : " + _label);
                              if(this.mainCTALabel){
                                _label = this.mainCTALabel;
                              }         
                              else {
                                this.mainCTALabel = _label;
                              }
                              console.log("_label : " + _label);


                              $(".il_hero .event-cta-w.main-cta a.cta_button").text(_label);
                              if(!this.isMainShowON || this.isSaleOver) this.disableCTAs();
                              else {
                                console.log("HERO CTA -> : " + newEventURL);
                                $(".il_hero .event-cta-w a.cta_button").removeClass("disabled");
                                $(".il_hero .event-cta-w.main-cta a.cta_button").attr("href", newEventURL);
                              }
                            }
                            return false;
                          }
                        else{                           
                          if(!this.isMainShowON  || this.isSaleOver) this.disableCTAs();
                          else{
                            console.log("HERO CTA -> : " + newEventURL);
                            $(".il_hero .event-cta-w a.cta_button").removeClass("disabled")
                            $(".il_hero .event-cta-w.main-cta a.cta_button").attr("href", newEventURL);
                          }
                          return true;                    
                        }
                      }
          },
            filters: {              
              formatDate: function (value) {            
                if(value){
                  let fdate = value.slice(0,-6);
                  var formattedDate = new Date(fdate).toLocaleDateString(
                    'en-us',
                    {
                      year: 'numeric',
                      month: 'long',
                      day: 'numeric'
                    }
                  );
                  console.log("formattedDate : " + formattedDate);
                  return formattedDate;
                }
                else{
                  console.log("formattedDate : Unknown exception!");                
                  return "";
                }
              },
                formatDateByTime: function (value) {
                  return new Date(value).toLocaleDateString(
                    'en-us',
                    {
                      year: 'numeric',
                      month: 'long',
                      day: 'numeric',
                      hour: 'numeric', 
                      minute: 'numeric'
                    }
                  );
                },
                  formatCurrency: function (amount) {
                    var amount = parseFloat(amount);
                    
                    return "$" + amount.toFixed(2).replace(/\d(?=(\d{3})+\.)/g, "$&,");
                  },
            }

      })
    </script>
    
