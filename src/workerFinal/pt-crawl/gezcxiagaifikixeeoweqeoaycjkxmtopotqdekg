<script>
    $(function () {
        $("#listNav li").on("click", function () {
            $(this).addClass("current").siblings().removeClass("current");
            GetData_TableList();//获取数据-表格
        }).eq(0).click();
    });


    //获取数据-表格
    function GetData_TableList() {
        $Common.Popup.LoadingShow('WrapRight');//Loading显示
        $Common.Ajax.PostAPI("BoxOffice_GetTvData_PlayIndexRank", "tvType=" + $("#listNav li.current").eq(0).attr("data-type"), function (data) {
            $Common.Popup.LoadingHide('WrapRight');//Loading关闭
            if (data && data["Table"] && data["Table"].length > 0) {
                SetChart_Line("ChartWrap_Line", data);//设置图表
                $Common.Data.InitTemplate("TP_TableList", data["Table"], function (html) {
                    $("#TableList").html(html);
                });
            }
            else $("#ChartWrap_Line").hide(), $("#TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        }, function (msg) {
            $Common.Popup.LoadingHide('WrapRight');//Loading关闭
            $("#ChartWrap_Line").hide(), $("#TableList").html('<p class="tac noData">暂无数据，请稍后再试！</p>');
        });
    }

    //设置图表
    function SetChart_Line(_ObjID, _ChartJson) {
        $("#" + _ObjID).show();
        var _ChartObj = new EChartsHelper(), _ParamArr = [];
        _ParamArr['smooth'] = true;//设置折线图属性

        var _ChartJsonArr = $Common.Json.GetByKey(_ChartJson['Table'], 'TvName,PlayIndex,MediaHot,UserHot,AnswerHot,PlayHot');

        _ChartObj.xAxis[0].data = _ChartJsonArr['TvName'];//X轴
        _ChartObj.legend.data = ['播映指数', '媒体热度', '用户热度', '好评度', '观看度'];//图例
        //数据
        _ChartObj.series.push(_ChartObj.GetSeries('line', '播映指数', _ChartJsonArr['PlayIndex'], _ParamArr));
        _ChartObj.series.push(_ChartObj.GetSeries('line', '媒体热度', _ChartJsonArr['MediaHot'], _ParamArr));
        _ChartObj.series.push(_ChartObj.GetSeries('line', '用户热度', _ChartJsonArr['UserHot'], _ParamArr));
        _ChartObj.series.push(_ChartObj.GetSeries('line', '好评度', _ChartJsonArr['AnswerHot'], _ParamArr));
        _ChartObj.series.push(_ChartObj.GetSeries('line', '观看度', _ChartJsonArr['PlayHot'], _ParamArr));

        //格式化提示内容
        _ChartObj.tooltip.formatter = function (params) {
            var res = params[0].name;
            for (var i = 0, l = params.length; i < l; i++) {
                res += '<br/>' + params[i].marker + params[i].seriesName + ' : ' + params[i].value;
            }
            return res;
        };
        _ChartObj.xAxis[0].axisLabel.formatter = function (value, index) { return value.length > 6 ? value.substring(0, 5) + '...' : value; };//x轴格式化

        _ChartObj.xAxis[0].axisLabel.interval = 0;//x轴始终显示

        _ChartObj.title[0].text = $("#listNav li.current").eq(0).attr("data-title") + '播映指数（' + (_ChartJson['Table1'][0]['MaxDate'] || '') + '）';//标题
        _ChartObj.DrawChart(_ObjID, '');//生成图片
    }
</script> contentType 9 text/html url 56 https://www.endata.com.cn:443/BoxOffice/Video/index.html responseCode 3 200 