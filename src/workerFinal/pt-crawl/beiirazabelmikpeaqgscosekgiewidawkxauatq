 <script language="javascript" type="text/javascript"> var cnnBrandingValue = ""; var cnnAuthor = "Parija Kavilanz"; var cnnSource = "CNN"; var cnnPublishDate = "2016-02-16"; var cnnOmniture_slidenum= 2; var cnnContentType = "gallery"; //global </script> <!-- ADSPACE: small_business/survey/survey.1x1 --><div id="ad-208568" align="center" style="padding: 0; margin: 0; border: 0;"><script type="text/javascript"> 


cnnad_createAd("208568","http://ads.cnn.com/html.ng/site=cnn_money&cnn_money_position=1x1_survey&cnn_money_rollup=small_business&page.allowcompete=no&params.styles=fs&page.allowcompete=yes","1","1");
















































