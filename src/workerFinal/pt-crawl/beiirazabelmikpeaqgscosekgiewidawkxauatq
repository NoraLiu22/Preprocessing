<script>
  var subnavToggles = document.getElementsByClassName('js-toggle-subnav');
  var flyout = document.querySelector('.js-toggle-flyout');
  var subNav = document.querySelector('.container.js-toggle-header-biz');
  var menuIcon = document.querySelector('.js-toggle-subnav .menu-icon');
  var subSections = document.querySelector('.nav-business-subsections.js-toggle-subnav');
  var desktopSearchForm = document.querySelector('.main-banner--search form');
  var desktopSearchInput = document.querySelector('.main-banner--search input[type="text"]');
  var desktopSearchToggle = document.querySelector('.js-toggle-search');
  var stickyHeader = document.querySelector('.js-sticky-header');
  var stickyAnchor = document.querySelector('.js-sticky-header-anchor');


  function toggleClass(elem, className) {
    if (document.querySelector('body').classList) {
      elem.classList.toggle(className);
    } else {
      // For IE9
      var classes = elem.className.split(" ");
      var i = classes.indexOf(className);

      if (i >= 0) {
        classes.splice(i, 1);
      } else {
        classes.push(className);
        elem.className = classes.join(" ");
      }
    }
  }

  /* Header Subnav Toggle */
  function handleSubNavToggle(e) {
    toggleClass(flyout, "open");
    toggleClass(subNav, "open");
    toggleClass(menuIcon, "open");
    toggleClass(subSections, "open");
  }

  for (var i = 0; i < subnavToggles.length; i++) {
    subnavToggles[i].addEventListener('click', handleSubNavToggle, false)
  }

  desktopSearchToggle.onclick = function() {
    toggleClass(desktopSearchForm, 'active');

    console.log(desktopSearchForm.className)
    if (desktopSearchForm.className.includes('active')) {
      desktopSearchInput.focus();
    }
  }

  function handleStickyHeader() {
    var headerHeight = stickyHeader.getBoundingClientRect().height;
    var anchorDistanceFromTop = stickyAnchor.getBoundingClientRect().top;

    if (anchorDistanceFromTop < 0) {
      console.log('duck')
      stickyAnchor.style.height = headerHeight + 'px';
      stickyHeader.classList.add('fixed')
    } else {
      stickyAnchor.style.height = 'auto';
      stickyHeader.classList.remove('fixed')
    }


  }

  if (WM && WM.UserConsent && WM.UserConsent.isInCcpaRegion()) {
    let doNotSellLink = document.getElementById('js-ccpa-do-not-sell-header');
    doNotSellLink.className += ' show';
    doNotSellLink.text = window.WM.UserConsent.getLinkTitle() || doNotSellLink.text;
  }

  document.addEventListener('scroll', handleStickyHeader)
</script>
