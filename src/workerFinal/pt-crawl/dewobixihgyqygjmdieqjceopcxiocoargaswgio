    <script type="text/javascript">
      $(function() {
        var wordcloud_data = []
        if (wordcloud_data.length > 0) {
            $("#wordcloud").jQCloud(wordcloud_data, {height: (wordcloud_data.length > 10 ? 500 : 300)});
