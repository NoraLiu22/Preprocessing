                $('a').click(function() {
                                var href = $(this).attr('href');
                                if (href.match(/^http/) && !href.match(document.domain)) {
                                                ga('send', 'event', 'Outbound Links', 'Click', href);
                                }
                                else if (href.match(/\.(doc|docx|pdf|ppt|pptx|xls|xlsx|zip)$/)) {
                                                ga('send', 'event', 'Downloads', 'Click', href);
                                }
																else{
