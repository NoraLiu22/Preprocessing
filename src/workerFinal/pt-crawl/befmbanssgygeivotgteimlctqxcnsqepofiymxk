                                                    function submitForm(button)
                                                    {
				                                                    var queryString = $("input[name='County'],select[name='County']").serialize();
				                                                    location.href = 'http://www.dhhr.wv.gov/COVID-19/pages/healthdept-results.aspx?'+queryString;
				                                                    return false;
                                                    }
                                                    $(document).ready(function() { 
 
												   $("input[name='County'],select[name='County']").bind("keydown", function(event) { 
												      var keycode = (event.keyCode ? event.keyCode : (event.which ? event.which : event.charCode)); 
												      if (keycode == 13) { 
												         document.getElementById('buttonhide').click(); 
												         return false; 
												      } else  { 
												         return true; 
												      } 
												   }); 
