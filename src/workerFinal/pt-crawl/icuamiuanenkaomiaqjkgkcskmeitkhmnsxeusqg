    <div class="widget-ArticleListAccess widget-instance-ArticleListAccess">
            <script type="text/javascript">

        /*******************************************************************************
         * JS here is only being used to assign variables from values in the model
         * logic should be implemented in external JS files, like client.script.js
         *******************************************************************************/

        var SCM = SCM || {};
        var accessIcons = [{"id":10137,"icon":"icon-availability_free","title":"Free"},{"id":10152,"icon":"icon-availability_free","title":"Free"},{"id":10156,"icon":"icon-availability_free","title":"Free"},{"id":10159,"icon":"icon-availability_free","title":"Free"},{"id":10167,"icon":"icon-availability_free","title":"Free"},{"id":10170,"icon":"icon-availability_free","title":"Free"},{"id":10173,"icon":"icon-availability_free","title":"Free"},{"id":10175,"icon":"icon-availability_free","title":"Free"},{"id":10177,"icon":"icon-availability_free","title":"Free"},{"id":10180,"icon":"icon-availability_free","title":"Free"},{"id":10133,"icon":"icon-availability_free","title":"Free"},{"id":10134,"icon":"icon-availability_free","title":"Free"},{"id":10138,"icon":"icon-availability_free","title":"Free"},{"id":10140,"icon":"icon-availability_free","title":"Free"},{"id":10143,"icon":"icon-availability_free","title":"Free"},{"id":10147,"icon":"icon-availability_free","title":"Free"},{"id":10149,"icon":"icon-availability_free","title":"Free"}];
        if (SCM.AccessIcons) {
            SCM.AccessIcons = SCM.AccessIcons.concat(accessIcons);
        } else {
            SCM.AccessIcons = accessIcons;
        }
        var accessAttributes =  [{"id":10137,"availableforpurchase":false},{"id":10152,"availableforpurchase":false},{"id":10156,"availableforpurchase":false},{"id":10159,"availableforpurchase":false},{"id":10167,"availableforpurchase":false},{"id":10170,"availableforpurchase":false},{"id":10173,"availableforpurchase":false},{"id":10175,"availableforpurchase":false},{"id":10177,"availableforpurchase":false},{"id":10180,"availableforpurchase":false},{"id":10133,"availableforpurchase":false},{"id":10134,"availableforpurchase":false},{"id":10138,"availableforpurchase":false},{"id":10140,"availableforpurchase":false},{"id":10143,"availableforpurchase":false},{"id":10147,"availableforpurchase":false},{"id":10149,"availableforpurchase":false}];
        if (SCM.AccessAttributes) {
            SCM.AccessAttributes = SCM.AccessAttributes.concat(accessAttributes);
        } else {
            SCM.AccessAttributes = accessAttributes;
        }
        
