			function searchMobCat(el, cat) {
				sMobCat = document.querySelectorAll('#search_mobile_cat a');
				for (i=0;i<sMobCat.length;i++) {
					sMobCat[i].classList.remove('active')
				}
				el.classList.add('active');
				document.querySelector('#search_mobile_input_cat').value=cat;
			}
		</script>
