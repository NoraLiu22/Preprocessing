<script>(()=>{function e(){let e,n=navigator.userAgent,r=n.match(/(opera|SamsungBrowser|criOS|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i)||[];if("CriOS"===r[1])return{name:"Chrome",version:r[2]};if("MSIE"===r[1])return{name:"IE",version:r[2]};if(/trident/i.test(r[1]))return e=/\brv[ :]+(\d+)/g.exec(n)||[],{name:"IE",version:e[1]||""};if("SamsungBrowser"===r[1])return{name:"Samsung",version:r[2]};if("Chrome"===r[1]&&(e=n.match(/\b(OPR|Edg|Edge)\/(\d+)/),null!=e)){if("Edg"==e[1])return{name:e[1].replace("Edg","Edge"),version:e[2]};if("Edge"==e[1])return{name:e[1],version:e[2]};if("OPR"==e[1])return{name:e[1].replace("OPR","Opera"),version:e[2]}}return r=r[2]?[r[1],r[2]]:[navigator.appName,navigator.appVersion,"-?"],null!=(e=n.match(/version\/(\d+)/i))&&r.splice(1,1,e[1]),{name:r[0],version:r[1]}}function n(){window.navigator.userAgent;var e=window.navigator.platform,n=null;return-1!==["Macintosh","MacIntel","MacPPC","Mac68K"].indexOf(e)?n="Mac OS":-1!==["Win32","Win64","Windows","WinCE"].indexOf(e)&&(n="Windows"),n}function r(e){window.dataLayer.push({cd:{Supported_Browser:e}})}window.__showBrowserDisclaimer=()=>{const i=document.querySelector(".browser-compat");!function(){const i={Desktop:{Chrome:49,Firefox:48,Edge:"Mac OS"===n()?81:18,Safari:9},Mobile:{Chrome:64,Firefox:83,Safari:10,Samsung:5}};let o=3===getDeviceTypeInNumber(window.deviceType)?"Mobile":"Desktop";return e().name in i[o]?parseInt(e().version)>=i[o][e().name]?(r(!0),!0):(r(!1),!1):"IE"===e().name?(r(!1),!1):(r(!0),!0)}()?i.style.display="table":i.style.display="none",document.querySelector(".browser-compat .close").addEventListener("click",(function(){i.style.display="none"}))},window.addEventListener("DOMContentLoaded",window.__showBrowserDisclaimer)})();</script>    <script>
        function lazyLoadBackgroundImage() {
            const observer = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.style.backgroundImage = `url(${entry.target.dataset.bgSrc})`;
                        observer.unobserve(entry.target);
                    }
                });
            }, {
                rootMargin: "500px",
            });
            const element = document.querySelectorAll('.lazy-load-bg');
            element.forEach((item) => {
                observer.observe(item);
            })
        }
        document.addEventListener('DOMContentLoaded', () => {
            lazyLoadBackgroundImage();
        })
    </script>
	
<script>
	document.addEventListener('DOMContentLoaded', () => {
                const accordianTab = document.querySelectorAll('.faq-question');
                accordianTab.forEach((item) => {
                    item.addEventListener('click', (e) => {
                        event.preventDefault();
                        var container = item.nextElementSibling;
                        if (!container.classList.contains('active')) {
                            item.classList.add('active');
                            item.closest('.faqs').classList.add('clicked');
                            container.classList.add('active');
                            container.style.height = 'auto';
                            var height = container.clientHeight + 'px';
                            container.style.height = '0px';
                            setTimeout(function() {
                                container.style.height = height;
                            }, 0);
                        } else {
                            item.classList.remove('active');
                            item.closest('.faqs').classList.remove('clicked');
                            container.style.height = '0px';
                            container.addEventListener('transitionend', function() {
                                container.classList.remove('active');
                            }, {
                                once: true
                            });
                        }
                    });
                })
            });
</script>

	
