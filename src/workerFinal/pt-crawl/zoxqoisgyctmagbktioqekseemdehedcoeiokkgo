<script type="text/javascript">	
var $j = jQuery.noConflict();	
galleryTarget = 0
galleryTargetVr = 0
//menu scrolls	 .offset().top },
$j("#ramMenu button").click(
	function(){
		var scrollTarget = $j(this).attr("data-menubtn")

		 $j('html, body').animate({ scrollTop: $j("#ram" + scrollTarget + "Mod").offset().top + -70}, 2000);	
})
//FAQ
$j(".ramFaqQ").click(
	function(){
		$j(this).siblings(".ramFaqA").slideToggle(500);
		$j(this).children("img").toggleClass("ramRotate")
	});
//mobile menu	
$j(".ramArrowRight").click(
	function(){
		$j("#ramMenu").animate({scrollLeft:"+=250px"}, "slow");
	});
$j(".ramArrowLeft").click(
	function(){
		$j("#ramMenu").animate({scrollLeft:"-=250px"}, "slow");
	});
//sticky menu
$j(document).scroll(function() {
    var pageTop = $j(document).scrollTop();
    var scollTarget = $j("#ramHeader").offset().top;
    if (pageTop > scollTarget) {
		$j("#ramMenuWrap").addClass("ramMenuToggle");		
    } else {
		$j("#ramMenuWrap").removeClass("ramMenuToggle");
    }
});		
//gallery pop ups
$j(".ramGal").click(
	function(){
		galleryTarget = $j(this).data('gallery');
		$j("#ramLightboxMod").fadeIn(1000).css("display","flex");
		$j(".ramLbGal[data-galleryLb="+ galleryTarget +"]").fadeIn(1000);
		$j(".ramLbThumbBtn").removeClass("ramLbThumbActive");
		$j(".ramLbThumbBtn[data-gallery="+ galleryTarget +"]").addClass("ramLbThumbActive");
});
$j(".ramLbThumbBtn").click(
	function(){
		galleryTarget = $j(this).data("gallery")
		$j(".ramLbGal").fadeOut(0)
		$j(".ramLbThumbBtn").removeClass("ramLbThumbActive");
		$j(".ramLbGal[data-galleryLb="+ galleryTarget +"]").fadeIn(0);
		$j(".ramLbThumbBtn[data-gallery="+ galleryTarget +"]").addClass("ramLbThumbActive");
		checkBtnState()
	});
$j("#ramNextBtnRight").click(
		function(){
			galleryTarget = parseInt(galleryTarget)
			galleryTarget = galleryTarget + 1

				if(galleryTarget <= 10 && galleryTarget > 0){

					$j(".ramLbGal").fadeOut(0)
					$j(".ramLbThumbBtn").removeClass("ramLbThumbActive");
					$j(".ramLbGal[data-galleryLb="+ galleryTarget +"]").fadeIn(0);
					$j(".ramLbThumbBtn[data-gallery="+ galleryTarget +"]").addClass("ramLbThumbActive");
				}
				if(galleryTarget == 10 ){
					$j("#ramNextBtnRight").fadeOut(100)
				}
				checkBtnState()
})
$j("#ramNextBtnLeft").click(
		function(){
			galleryTarget = parseInt(galleryTarget)
			galleryTarget = galleryTarget - 1

				if(galleryTarget >= 1){

					$j(".ramLbGal").fadeOut(0)
					$j(".ramLbThumbBtn").removeClass("ramLbThumbActive");
					$j(".ramLbGal[data-galleryLb="+ galleryTarget +"]").fadeIn(0);
					$j(".ramLbThumbBtn[data-gallery="+ galleryTarget +"]").addClass("ramLbThumbActive");
				}
				if(galleryTarget == 1 ){
					$j("#ramNextBtnLeft").fadeOut(100)
				}
				checkBtnState()
})
//vr gallery
$j(".ramVrGal").click(
	function(){
		galleryTargetVr = $j(this).data('gallery');
		$j("#ramLightboxVrMod").fadeIn(1000).css("display","flex");
		$j(".ramLbVrGal[data-galleryLb="+ galleryTargetVr +"]").fadeIn(1000);
		$j(".ramLbVrThumbBtn").removeClass("ramLbThumbActive");
		$j(".ramLbVrThumbBtn[data-gallery="+ galleryTargetVr +"]").addClass("ramLbThumbActive");
});
$j(".ramLbVrThumbBtn").click(
	function(){
		galleryTargetVr = $j(this).data("gallery")
		$j(".ramLbVrGal").fadeOut(0)
		$j(".ramLbVrThumbBtn").removeClass("ramLbThumbActive");
		$j(".ramLbVrGal[data-galleryLb="+ galleryTargetVr +"]").fadeIn(0);
		$j(".ramLbVrThumbBtn[data-gallery="+ galleryTargetVr +"]").addClass("ramLbThumbActive");
		checkVrBtnState()
	});
	$j("#ramNextVrBtnRight").click(
		function(){
			galleryTargetVr = parseInt(galleryTargetVr)
			galleryTargetVr = galleryTargetVr + 1

				if(galleryTargetVr <= 5 && galleryTargetVr > 0){

					$j(".ramLbVrGal").fadeOut(0)
					$j(".ramLbVrThumbBtn").removeClass("ramLbThumbActive");
					$j(".ramLbVrGal[data-galleryLb="+ galleryTargetVr +"]").fadeIn(0);
					$j(".ramLbVrThumbBtn[data-gallery="+ galleryTargetVr +"]").addClass("ramLbThumbActive");
				}
				if(galleryTargetVr == 5 ){
					$j("#ramNextVrBtnRight").fadeOut(100)
				}
				checkVrBtnState()
})
$j("#ramNextVrBtnLeft").click(
		function(){
			galleryTargetVr = parseInt(galleryTargetVr)
			galleryTargetVr = galleryTargetVr - 1

				if(galleryTargetVr >= 1){

					$j(".ramLbVrGal").fadeOut(0)
					$j(".ramLbVrThumbBtn").removeClass("ramLbThumbActive");
					$j(".ramLbVrGal[data-galleryLb="+ galleryTargetVr +"]").fadeIn(0);
					$j(".ramLbVrThumbBtn[data-gallery="+ galleryTargetVr +"]").addClass("ramLbThumbActive");
				}
				if(galleryTargetVr == 1 ){
					$j("#ramNextVrBtnLeft").fadeOut(100)
				}
				checkVrBtnState()
})

//close and reset
$j(".ramCloseLbBtn").click(
	function(){
		resetGallery()
});
//close on lb background click
$j(document).on('click', '#ramLightboxMod', function (e) {

if ($j(e.target).closest('.ramCloseLbBtn').length !== 0 )
{return true}
else if ($j(e.target).closest('#ramLightboxGallery').length !== 0 )
{return false;}
resetGallery()
});

$j(document).on('click', '#ramLightboxVrMod', function (e) {

if ($j(e.target).closest('.ramCloseLbBtn').length !== 0 )
{return true}
else if ($j(e.target).closest('#ramLightboxVrGallery').length !== 0 )
{return false;}
resetGallery()
});

function resetGallery(){
	galleryTarget = 0
	$j(".ramLbThumbBtn").removeClass("ramLbThumbActive");
	$j(".ramVrLbThumbBtn").removeClass("ramLbThumbActive");
	$j(".ramLbGal").fadeOut(0)
	$j(".ramLbVrGal").fadeOut(0)	
	$j("#ramLightboxMod").fadeOut(500)
	$j("#ramLightboxVrMod").fadeOut(500)
};

//check button state for scrolling on thumbnails
function checkBtnState(){
	if(galleryTarget > 1 && $j("#ramNextBtnLeft").css('display') == 'none'){
		$j("#ramNextBtnLeft").fadeIn(100)
	}
	if(galleryTarget < 10 && $j("#ramNextBtnRight").css('display') == 'none'){
		$j("#ramNextBtnRight").fadeIn(100)
	}	
};
function checkVrBtnState(){
	if(galleryTargetVr > 1 && $j("#ramNextVrBtnLeft").css('display') == 'none'){
		$j("#ramNextVrBtnLeft").fadeIn(100)
	}
	if(galleryTargetVr < 5 && $j("#ramNextVrBtnRight").css('display') == 'none'){
		$j("#ramNextVrBtnRight").fadeIn(100)
	}	
};
	
</script>	 
    <script type="text/javascript">
        window.onload = function() {
            if (TM.Ads && typeof TM.Ads.iframeAssets != "undefined" && TM.Ads.iframeAssets.length > 0){
               jQuery.each(TM.Ads.iframeAssets, function(i,k){
                   if( jQuery( k.name ).length > 0 && k.src ){
                      if ((this.name) == "#edp_leaderboard"){
                          this.src = appendTransID(this.src, true);
                      }
                      ( function(o){
                           setTimeout( function() {
                               jQuery( o.name ).attr("src", o.src )
                           } , 200 );
                       }(k) )
                   }
               })
            }
        }
