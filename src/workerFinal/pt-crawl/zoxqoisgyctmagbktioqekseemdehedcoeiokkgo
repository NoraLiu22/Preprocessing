<script type="text/javascript">
var searchForm = {
    inputDefaultText: '',
    setDefaultText:function(text,replaceEx,replaceItem){
        searchForm.inputDefaultText = text.replace(replaceEx,replaceItem);
    },
    validate: function(form){
        var sendForm = true;
        var inputValue = form.find('.js-search-input');
        if(jQuery.trim(inputValue.val()) == '' || inputValue.val() == searchForm.inputDefaultText ){
            alert(checkUni("Please enter search text."));
            inputValue.focus();
            inputValue.val('');
            sendForm = false;
        }
        return sendForm;
    },
    toggleInput: function(status){
        if(status == 'show'){
            jQuery('.js-header-logo').hide();
            jQuery('.js-header-search').show();
        }
        if(status == 'hide'){
            jQuery('.js-header-logo').show();
            jQuery('.js-header-search').hide();
        }
    },
    toggleMenu: function(status){
        if(status == 'show'){
            jQuery('.footer__mini-footer').removeClass('footer__mini-footer--collapse').addClass('footer__mini-footer--expand');
            jQuery('#mobile-disabled-background').removeClass('is-hidden').addClass('footer__disabled-background');
            jQuery('.footer__mobile-hamburger').addClass('js-hide-menu').removeClass('js-show-menu');
        }
        if(status == 'hide'){
            jQuery('.footer__mini-footer').removeClass('footer__mini-footer--expand').addClass('footer__mini-footer--collapse');
            jQuery('#mobile-disabled-background').removeClass('footer__disabled-background').addClass('is-hidden');
            jQuery('.footer__mobile-hamburger').removeClass('js-hide-menu').addClass('js-show-menu');
        }
    }
};

searchForm.setDefaultText('What are you looking for?',new RegExp('&eacute;','g'),'\u00E9');

jQuery(document).ready(function(){
    var isOpen = false;
    var responsive_size = 740;
    jQuery('.js-search-form').submit(function(e){
        return searchForm.validate(jQuery(this));
    });
    jQuery('.js-search-input').focus(function(){
        if (jQuery(this).val() == searchForm.inputDefaultText){
            jQuery(this).val('').attr('title', '').removeClass("placeholder");
        }
    });
    jQuery('.js-search-input').blur(function(){
        if (jQuery(this).val() == ''){
            jQuery(this).val(searchForm.inputDefaultText).attr('title',searchForm.inputDefaultText).addClass("placeholder");
        }
    });
    jQuery('.tm-header__search-toggle .js-search-submit').click(function(e){
        e.preventDefault();
        searchForm.toggleInput('show');
    });
    jQuery('.tm-header__search-mini .js-cancel-button').click(function(e){
        e.preventDefault();
        searchForm.toggleInput('hide');
    });
    jQuery('.js-show-menu').bind('click',function(){
        if (!isOpen) {
            searchForm.toggleMenu('show');
            isOpen = true;
        } else {
            searchForm.toggleMenu('hide');
            isOpen = false;
        }
        omniTracking.navigation_link('tm_hamburger_header');
    });

    jQuery('.page__wrapper-footer footer').delegate('div.footer__disabled-background', 'click', function (e) {
        e.preventDefault();
        searchForm.toggleMenu('hide');
        isOpen = false;
    });
    function onResize (size) {
    jQuery(window).resize(function () {
        if (jQuery('.page__content-header header .tm-header__content.js-header-logo').width() >= size && searchForm && searchForm.toggleInput){
            searchForm.toggleInput('hide');
        }
    });
};
onResize(responsive_size);

});
</script>
