		const script = document.createElement("script");
		script.src = "https://www.werstreamt.es/_resources/themes/wse/js/widget.v4.min.js";
		document.head.appendChild(script);

	</script>
		<script>
        $(document).ready(function () {

            $(".stream-provider .items").on("scroll", function () {
                const left = $(this).scrollLeft();

                if ($(this)[0].scrollWidth - $(this).width() - $(this).scrollLeft() === 0) {
                    $(this).parent().parent().removeClass("shadow-right");
                } else {
                    $(this).parent().parent().addClass("shadow-right");
                }

                if (left === 0) {
                    $(this).parent().parent().removeClass("shadow-left");
                } else if (left > 0) {
                    $(this).parent().parent().addClass("shadow-left");
                }
            });

            $(".arrow-left").on("click", function () {
                const el = $(".stream-provider .items");
                el.css("scroll-snap-type", "inherit");
                el.animate({scrollLeft: el.scrollLeft() - 500}, 400);
                window.setTimeout(function() {
                    el.css("scroll-snap-type", "x mandatory");
                }, 500);
            });

            $(".arrow-right").on("click", function () {
                const el = $(".stream-provider .items");
                el.css("scroll-snap-type", "inherit");
                el.animate({scrollLeft: el.scrollLeft() + 500}, 400);
                window.setTimeout(function() {
                    el.css("scroll-snap-type", "x mandatory");
                }, 500);
            });

            $(".is-grid:not(.no-slider)").each(function () {


                const items = $(this).find(".o-posts-grid-post").length;

                if (items > 1 && window.matchMedia("only screen and (max-width: 760px)").matches) {

                    const pagination = $("<ul>").addClass("pagination");

                    const grid = $(this).clone();
                    const slider = $("<div>");

                    $("<div>").addClass("o-posts-grid-post-blog o-posts-grid-post-plain").css({width: 1}).appendTo(grid);

                    $(this).before(slider.addClass("slider").html($("<div>").addClass("max-w-full overflow-auto mobile").html(grid))).remove();


                }
            })
        });

	</script>
	
