		jQuery(function( $ ){
			$('.scrollArrows a')
				.filter('.next').bind('click', 'next', moveR )
				.end()
				.filter('.prev').bind('click', 'prev', moveL );
				
			function moveR( e ){
				$('#featuredContent').scrollTo( '+=194px', {
					speed:800,
					axis:'x'
				});
				return false;
			};
			function moveL( e ){
				$('#featuredContent').scrollTo( '-=194px', {
					speed:800,
					axis:'x'
				});
				return false;
			};
		});
	</script>

