<script>
	function showHideAdBox(){
	var targetDiv 	= document.getElementById('moneySponsorBox');
	var containerDiv 	= document.getElementById('adContainer');
	if (typeof targetDiv != 'undefined'){
		var elementArray = containerDiv.getElementsByTagName('div');
		for (var i=0; i< elementArray.length; i++){
		elementArray[i].style.marginBottom = 15;
		}
		if (targetDiv.offsetHeight < 40)
			targetDiv.style.display = 'none';
		else{
			targetDiv.style.visibility = 'visible';
			targetDiv.style.marginBottom = 15;
			}
		}
	}
</script>
