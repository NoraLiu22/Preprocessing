		document.write('<' + 'scr' + 'ipt type="text/javascript" src="//i.cdn.turner.com/money/.element/ssi/sections/mag/fo_431x215ofie1209.js"><' + '/scr' + 'ipt>');
	else
	/* moneymag homepage */
	if (location.pathname.match('moneymag'))
		document.write('<' +'script type="text/javascript" src="//i.cdn.turner.com/money/.element/ssi/sections/mag/mo_431x215ofie1209.js"><' +'/scr' + 'ipt>');
