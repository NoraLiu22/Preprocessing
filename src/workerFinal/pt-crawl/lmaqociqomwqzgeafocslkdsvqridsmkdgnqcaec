        gptadslots.push(googletag.defineSlot('/21727259002/SHOWHUB_MID2_970x90', [[728,90],[970,90],[970,250],[300,250]], 'div-gpt-ad-19081523-MID2SHOW')
            .defineSizeMapping(mappingHorizontalBanners)
