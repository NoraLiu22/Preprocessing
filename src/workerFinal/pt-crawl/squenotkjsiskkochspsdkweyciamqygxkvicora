      function _sendNcsesEvent(_cat, _act, _lbl, _val, _nonInteraction) {
          analytics.sendEvent({
              category: _cat,
              action: _act,
              label: _lbl,
              value: _val,
              nonInteraction: _nonInteraction
          });
      }

      if(document.addEventListener){
        document.addEventListener('DOMContentLoaded', function() {
            analytics.initialize();
