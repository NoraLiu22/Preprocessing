<script>
    window.addEventListener("phx:scroll-to", (event) => {
        var nodes = document.querySelectorAll("#navbar-search-results div.search-result");
        // scroll and maintain window scroll pos
        var startY = window.scrollY;
        nodes[event.detail.index].scrollIntoView({ block: 'center', inline: 'center' })
        window.scrollTo(0, startY);
    });
    window.addEventListener("lsd:chat-toggle", (event) => {
        updateChatContainer();
    });
    window.addEventListener("lsd:chat-open", (event) => {
        document.cookie = `chat-collapse=false;path=/;samesite=lax;expires="Tue, 15 Jan 2041 19:13:33 GMT"`;
        load_autocomplete();
    });
    window.addEventListener("lsd:chat-close", (event) => {
        document.cookie = `chat-collapse=true;path=/;samesite=lax;expires="Tue, 15 Jan 2041 19:13:33 GMT"`;
    });
    window.addEventListener("phx:set-search-type", (event) => {
        document.cookie = `search-type=` + event.detail.type + `;path=/;samesite=lax;expires="Tue, 15 Jan 2041 19:13:33 GMT"`;
    });
    window.addEventListener("phx:drawer-update", (event) => {
        updateDrawer();
    });
    window.addEventListener("phx:drawer-on", (event) => {
        document.getElementById("kt_body").setAttribute("data-kt-drawer-chat", "on");
        document.getElementById("kt_body").setAttribute("data-kt-drawer", "on");
    });
    function updateChatContainer() {
        setTimeout(function() {
            var chatContainer = document.getElementById("chat-message-container");
            // redraw chat message container on mount because livesocket and drawer js don't play well together
            KTDrawer.getInstance($('#kt_drawer_chat')[0]).update();
            if (KTScroll.getInstance(chatContainer) != null) {
                KTScroll.getInstance(chatContainer).update();
                KTScroll.getInstance(chatContainer).update(); // this is intentional, otherwise height is weird
            } else {
                KTScroll.init(chatContainer);
            }
        }, 450);
    }

    function updateDrawer() {
        var drawerElement = document.querySelector(".header-menu"); 
        var drawer = KTDrawer.getInstance(drawerElement);
        drawer.hide();
        drawer.show();
        drawer.update();
    }
</script>

