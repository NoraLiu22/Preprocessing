<script type="text/javascript">

    function initStatusTable() {
        $('#status-table-dt').remove();
        $('#status-table').clone().attr('id', 'status-table-dt').removeClass('d-none').appendTo('#statuses-dt');
        var table = $('#status-table-dt').DataTable({
        "scrollX": true,
        "paging": false,
        dom: "<'row'<'col-sm-12 col-md-6'l><'col-sm-12 col-md-6'f>>" + "<'row'<'col-sm-12'tr>>" + "<'row'<'col-sm-12 col-md-5'i><'col-sm-12 col-md-7'p>>",
        "columnDefs": [
            // sorts pretty date by hidden column with raw date
            { 'orderData':[8], 'targets': [7] }, // sent date
            { 'orderData':[10], 'targets': [9] }, // received date
            { 'orderData':[12], 'targets': [11] }, // complete date
            { 'orderData':[14], 'targets': [13] }, // ur date
            { 'orderData':[16], 'targets': [15] }, // ur2 date
            { 'orderData':[18], 'targets': [17] }, // ii date
            { 'orderData':[20], 'targets': [19] }, // decision date
            {
                "targets": [ 0, 8,10,12,14,16,18,20 ],
                "visible": false,
                "searchable": false
            }
        ],
        "language": {
            "info": "Showing all _TOTAL_ of GiftedMagicalDragonfly's apps.",
        }
        });
        return table;
    };

    function getStatusTable() {
        return $('#status-table-dt').DataTable();
    };

    // this only works with liveview, which will reset the html to state at mount()
    function redrawStatusTable() {
        // $('#status-table').clone().attr('id', 'status-table-dt').removeClass('d-none').appendTo('#statuses-dt');
        var table = $('#status-table-dt').DataTable();
        table.destroy(true);
        initStatusTable();
        initExpandStatuses();
    };

    function initExpandStatuses() {

    };
</script>
