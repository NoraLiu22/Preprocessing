<script type="text/javascript">
    $(document).ready(function () {
        // Hide Contact Info if no contact information is published
        if ($('#contactInfo').html().trim() == '') {
            $('#contactInfo').hide();
            $('#campaignPromotionImage').attr("class", "rr-section first");
        }

        // Hide Campaign Promotion Image if no image is configured
        if ($('#campaignPromotionImage').html().trim() == '') {
            $('#campaignPromotionImage').hide();
            $('#howdoi').attr("class", "rr-section first");
        }
        
        // Hide Related Articles if no related articles exist
        if ($('.related-articles').is(":hidden")) {
            $('#relatedArticles').hide();
        }

        // Hide Related Downloads if no downloads are configured
        if ($('#relatedDownloads').html().trim() == '') {
            $('#relatedDownloads').hide();
        }
        
        // Hide Site Links if no site links are configured
        if ($('#siteLinks').html().trim() == '') {
            $('#siteLinks').hide();
        }
    });
