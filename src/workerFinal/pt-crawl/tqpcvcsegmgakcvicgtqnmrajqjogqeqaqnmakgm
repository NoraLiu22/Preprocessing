                <div id="Div1">
                    <!-- main body area -->
                    <div id="content-area">
                    	<div data-name="ContentPlaceHolderMain">
	                    	<span id="DeltaPlaceHolderMain">
		                        
<div class="article-main-content">
	<div class="content-wrapper">
		<div class="content-left">
	        <div id="site-breadcrumb">				
				<span><span><a title="Top level site" class="ms-sitemapdirectional" href="/Pages/Home.aspx">Hawaii State Department of Education</a></span><span> &gt; </span><span><a title="Student Programs and Services" class="ms-sitemapdirectional" href="/BeyondTheClassroom/Pages/home.aspx">Beyond the Classroom</a></span><span> &gt; </span><span class="current">Library Services</span></span>				 
			</div>
			<div class="article-summary">
				<h2 class="article-heading">Library Media Services</h2>
                
    



				
				<p class="intro">
					Our school library media programs and services are a valuable resource for students and teachers. They are evolving to meet needs in a digital age and serve as 1) Promoters of active learning; 2) Curators of resources; 3) Developers of responsible and compassionate citizenship; and 4) Leaders in forging local and global connections. 
					
				</p>
				
<script type="text/javascript">
    function DisplayEmailForm() {
        var encodedURL = "/_layouts/HIDOE.Internet/SharePageViaEmail.aspx?pageurl=" + encodeURIComponent(window.location.href);
        openCustomForm(encodedURL, 420, 240);
        _gaq.push(['_trackEvent', 'article', 'share', hidoePageTitle]);
    }

    function OnDialogClose(dialogResult, returnValue) {
	if (dialogResult == SP.UI.DialogResult.OK) {
        	SP.UI.Notify.addNotification('Operation Successfull!');
	}
        SP.UI.ModalDialog.RefreshPage(SP.UI.DialogResult.OK);
    }

    function openCustomForm(url, width, height) {
        var options = {
            url: url,
            title: "",
            width: width,
            height: height,
            showClose: true,
            allowMaximize: false,
            dialogReturnValueCallback: OnDialogClose
        };
        SP.SOD.execute('sp.ui.dialog.js', 'SP.UI.ModalDialog.showModalDialog', options);
    }

</script>

