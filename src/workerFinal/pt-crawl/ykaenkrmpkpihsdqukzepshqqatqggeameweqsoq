  require("app").initialize();
})
</script><script>(function() {
  require.register("views/scroller", function(exports, require, module) {
    var ScrollerView, easie, utils;
    easie = require("lib/easie");
    utils = require("lib/utils");
    ScrollerView = (function() {
      class ScrollerView extends Backbone.View {
        initialize() {
          var ref, timeout;
          this.data = this.$el.data();
          this.data.slideHeights = $(".scroll-item").map((i, e) => {
            return +e.dataset.nSlides;
          });
          this.data.support = (ref = window.CSS) != null ? ref.supports("scroll-snap-type: y mandatory") : void 0;
          this.$elements = {
            pager: $(this.$el.data("pager")),
            index: $(this.$el.data("index"))
          };
          timeout = null;
          $("#btn-prev").click((e) => {
            return this.onPrev(e);
          });
          $("#btn-next").click((e) => {
            return this.onNext(e);
          });
          this.$el.on("scroll", (e) => {
            this.onScroll();
            window.clearTimeout(timeout);
            return timeout = window.setTimeout(() => {
              return this.onScrollEnd();
            }, this.data.support ? 10 : 300);
          });
          return timeout = window.setTimeout(() => {
            return this.onScrollEnd();
          }, 1);
        }

        // sum heights array sequentially until sum > index
        // index should be set to the interator value minus 1
        // this is to allow for slides with over 100% height
        getIndex(index) {
          var aggregate, j;
          j = 0;
          aggregate = 0;
          while (aggregate <= index) {
            aggregate += this.data.slideHeights[j];
            j++;
          }
          return j - 1;
        }

        sum(arr) {
          if (arr.length) {
            return arr.reduce((a, b) => {
              return a + b;
            });
          } else {
            return 0;
          }
        }

        onScroll() {
          var cumulativeSlideHeights, i, i2, k, n, slideOffsetTop, t1, t2, w, x;
          i = Math.floor(this.el.scrollTop / this.el.offsetHeight);
          i2 = Math.floor(this.el.scrollTop / this.el.offsetHeight + 0.5);
          i = this.getIndex(i);
          i -= this.data.min;
          i2 = this.getIndex(i2);
          i2 = Math.max(Math.min(i2, this.data.max), this.data.min);
          n = this.data.slideHeights[i2];
          cumulativeSlideHeights = this.sum(this.data.slideHeights.get().slice(0, i2));
          slideOffsetTop = this.el.offsetHeight * cumulativeSlideHeights;
          if (slideOffsetTop > this.el.scrollTop) {
            cumulativeSlideHeights = this.sum(this.data.slideHeights.get().slice(0, i2 - 1));
            slideOffsetTop = this.el.offsetHeight * cumulativeSlideHeights;
            n = this.data.slideHeights[i2 - 1];
          }
          t1 = ((this.el.scrollTop - slideOffsetTop) / (n * this.el.offsetHeight)) % 1;
          t2 = Math.sin(Math.PI * t1);
          if (t1 < 0.5 && i < 0) {
            t2 = 1;
          }
          w = t2;
          x = t1 > 0.5 ? 1 - t2 : 0;
          k = 0;
          if (i < 0) {
            k = 2;
            x -= k;
            w += k;
          }
          this.$elements.index.html(i2);
          this.$elements.pager.css({
            "transform": `translateX(${i * 200}%)`
          });
          this.$elements.pager.children().eq(0).css({
            "transform": `translate3d(${x * 400 + k * (1 - t2) * 400}%, 0, 0)`
          });
          this.$elements.pager.children().eq(1).css({
            "transform": `translate3d(${-4 + x * 100 + k * (1 - t2) * 100}%, 0, 0) scale(${t2 * (k + 1) + 0.1}, 1)`
          });
          return this.$elements.pager.children().eq(2).css({
            "transform": `translate3d(${(x + w) * 400}%, 0, 0)`
          });
        }

        onScrollEnd() {
          var child, i, index, l, len, ref;
          index = Math.floor(this.el.scrollTop / this.el.offsetHeight + 0.5);
          index = this.getIndex(index);
          if (!this.data.support) {
            this.scrollTo(index);
          }
          if (index !== this.data.i) {
            $("body").addClass(`page-${index}`).removeClass(`page-${this.data.i}`);
            _.extend(this.data, {
              y: this.el.scrollTop,
              h: this.el.offsetHeight,
              i: index
            });
            ref = this.el.children;
            for (i = l = 0, len = ref.length; l < len; i = ++l) {
              child = ref[i];
              child.classList.toggle("active", i === this.data.i);
              $("[data-view]", child).trigger(i === this.data.i ? "show" : "hide");
            }
            return typeof window.ga === "function" ? window.ga("send", "event", "Scroller", "show", document.title, index) : void 0;
          }
        }

        onPrev() {
          var index;
          index = Math.floor(this.el.scrollTop / this.el.offsetHeight + 0.5);
          return this.scrollTo(index - 1);
        }

        onNext() {
          var index;
          index = Math.floor(this.el.scrollTop / this.el.offsetHeight + 0.5);
          return this.scrollTo(index + 1);
        }

        scrollTo(i) {
          this.$el.addClass("scrolling");
          return this.$el.scrollTo(i * this.el.offsetHeight, () => {
            return this.$el.removeClass("scrolling");
          });
        }

      };

      ScrollerView.prototype.events = {
        "show": "onShow",
        "hide": "onHide"
      };

      return ScrollerView;

    }).call(this);
    return module.exports = ScrollerView;
  });

}).call(this);
(function() {
  require.register("views/counter", function(exports, require, module) {
    var CounterView, easie, utils;
    easie = require("lib/easie");
    utils = require("lib/utils");
    CounterView = (function() {
      class CounterView extends Backbone.View {
        initialize() {
          var $el, html, i, j, len, ref, val, y;
          html = this.$el.html();
          this.$el.html(html.replace(/(\d)/g, "<span class='digit' data-value='$1'></span>"));
          this.$digits = this.$("span").map(function() {
            return $(this);
          }).toArray();
          ref = this.$digits;
          for (i = j = 0, len = ref.length; j < len; i = ++j) {
            $el = ref[i];
            val = parseInt($el.data("value"));
            y = val * 10 + i * 300;
            $el.attr("data-y", -y).html(`<ul>
  <li>0</li><li>1</li><li>2</li><li>3</li><li>4</li>
  <li>5</li><li>6</li><li>7</li><li>8</li><li>9</li><li>0</li>
</ul>`);
            $el.find("li").eq(val).addClass("active");
          }
          return this.onHide();
        }

        onShow() {
          var dur, now, repeat;
          now = Date.now();
          dur = 1500;
          this.playing = true;
          this.$el.addClass("playing");
          return (repeat = () => {
            var $el, i, j, len, ms, ref, results, t, y;
            if (!this.playing) {
              return;
            }
            t = (Date.now() - now) / dur;
            t = Math.min(t, 1);
            if (t < 1) {
              window.requestAnimationFrame(repeat);
            } else {
              this.$el.removeClass("playing");
            }
            ref = this.$digits;
            results = [];
            for (i = j = 0, len = ref.length; j < len; i = ++j) {
              $el = ref[i];
              ms = (this.$digits.length - i) * 100;
              t = (Date.now() - now - ms) / (dur - ms);
              t = Math.max(Math.min(t, 1), 0);
              y = easie.quintInOut(t) * $el.data("y") % 100;
              results.push($el.children().first().css({
                "transform": `translate3d(0, ${y}%, 0)`
              }));
            }
            return results;
          })();
        }

        onHide() {
          var $el, i, j, len, ref;
          this.playing = false;
          ref = this.$digits;
          for (i = j = 0, len = ref.length; j < len; i = ++j) {
            $el = ref[i];
            $el.children().first().css({
              "transform": ""
            });
          }
          return this.$el.removeClass("playing");
        }

      };

      CounterView.prototype.events = {
        "show": "onShow",
        "hide": "onHide"
      };

      return CounterView;

    }).call(this);
    return module.exports = CounterView;
  });

}).call(this);
const qs = (q) => document.querySelector(q)
const qsa = (q) => document.querySelectorAll(q)

const moveBackgroundSliders = (e) => {
  const scroller = qs("#scroller")
  const sliders = qsa(".background-slider")

  sliders.forEach(el => {
    // get the #scroll-item-x
    const slide = qs(`#scroll-item-${el.dataset.slide}`)
    const nSlides = slide.dataset.nSlides

    // calculate % it has scrolled through the scroller
    let percent = (25 + 100 / Math.min(nSlides - 1.3, 1)) * ((scroller.scrollTop + scroller.offsetTop) - slide.offsetTop) / (nSlides * scroller.offsetHeight)

    // clamp between 0 and 100
    percent = Math.max(Math.min(percent, 100), 0)

    // update background-position to 50% x%
    el.style.backgroundPosition = `${percent}% 50%`

    slide.classList[percent > 1 ? "add" : "remove"]("opacity-0")
  })
}


window.addEventListener("load", () => {
  const scroller = qs("#scroller")
  scroller.addEventListener("scroll", moveBackgroundSliders)
