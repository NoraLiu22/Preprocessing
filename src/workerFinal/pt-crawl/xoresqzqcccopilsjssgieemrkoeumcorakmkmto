		jQuery('select#cat').change(function(){
			var selected_cat = jQuery("#cat :selected").val();
			if (selected_cat != 'XXX'){
				location.href = 'https://blogs.ifas.ufl.edu/news/category/' + selected_cat;
					//'/category/'
			}
		});
