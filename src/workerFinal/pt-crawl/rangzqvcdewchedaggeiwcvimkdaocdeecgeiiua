                    window.fetchOCApi = function(url, params) {
                        return fetch(url, {
                            method: 'POST',
                            body: JSON.stringify(params),
                            headers: {
                                RequestVerificationFormToken: formToken,
                                'Content-Type': 'application/json'
                            }
