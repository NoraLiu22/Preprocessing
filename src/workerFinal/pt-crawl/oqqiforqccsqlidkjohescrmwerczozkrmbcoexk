<script type="text/javascript">
    var cbMember = document.getElementById("cbMember");
    var cbSenator = document.getElementById("cbSenator");

    if (cbMember != null) {
        cbMember.onclick = function () {
            checkType(this);
        }
        cbMember.onkeypress = function () {
            checkType(this);
        }
    }

    if (cbSenator != null) {
        cbSenator.onclick = function () {
            checkType(this);
        }
        cbSenator.onkeypress = function () {
            checkType(this);
        }
    }

    function checkType(cb) {
        var cbMember = document.getElementById("cbMember");
        var cbSenator = document.getElementById("cbSenator");

        if (!cbMember.checked && !cbSenator.checked)
            cb.checked = true;
    }
</script> 



