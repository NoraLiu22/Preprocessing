    <script>
        $(document).ready(function () {
            $('.home a').attr('aria-label', 'Return to frontpage');
            $('.has-dropdown > a').attr('aria-expanded', false);

            $('.has-dropdown > a').mouseover(function () {
                if ($(window).width() >= 1200) {
                    $(this).attr('aria-expanded', true);
                }

            });
            $('.has-dropdown > a').mouseleave(function () {
                if ($(window).width() >= 1200) {
                    $(this).attr('aria-expanded', false);
                }

            });
            $('.dropdown').mouseover(function () {
                if ($(window).width() >= 1200) {
                    $(this).siblings('a').attr('aria-expanded', true);
                }
            });
            $('.dropdown').mouseleave(function () {
                if ($(window).width() >= 1200) {
                    $(this).siblings('a').attr('aria-expanded', false);
                }
            });
            $('.has-dropdown > a').focus(function () {
                if ($(window).width() >= 1200) {
                    $(this).attr('aria-expanded', true);
                    $(this).parent().siblings().children('a').attr('aria-expanded', false);
                    $(this).keydown(function (e) {
                        if (e.which == 27) {
                            $(this).parent().removeClass('show-dropdown');
                            $(this).attr('aria-expanded', false);
                            $(this).blur();
                        }
                    });
                }

            });
            $('.dropdown a').focusin(function () {
                if ($(window).width() >= 1200) {
                    $(this).parents('.dropdown').siblings('a').attr('aria-expanded', true);
                    $(this).keydown(function (e) {
                        if (e.which == 27) {
                            $(this).parents('.dropdown').siblings('a').attr('aria-expanded', false);
                        }
                    });
                }

            });
            $('.has-dropdow > a').focusout(function () {
                if ($(window).width() >= 1200) {
                    $(this).attr('aria-expanded', false);
                }

            });

            $('.dropdown').focusout(function (e) {
                if ($(window).width() >= 1200 && !this.contains(e.relatedTarget)) {
                    $(this).siblings('a').attr('aria-expanded', false);
                }
            })

            $('.current.has-childs').addClass('active');
            $('.toggler').attr({ 'aria-expanded': 'false', 'aria-label': 'Expand menu'});
            $('.active').children('.toggler').attr({ 'aria-expanded': 'true', 'aria-label': 'Close menu'});

            $('.toggler').click(function () {
                if ($(this).attr('aria-expanded') === 'false') {
                    $(this).attr('aria-expanded', true);
                } else {
                    $(this).attr('aria-expanded', false);
                }
                if ($(this).attr('aria-label') === 'Expand menu') {
                    $(this).attr('aria-label', 'Close menu');
                } else {
                    $(this).attr('aria-label', 'Expand menu');
                }
            });

            $('.toggler').keydown(function (e) {
                if (e.which == 32 || e.which == 13) {
                    e.preventDefault();
                    $(this).parent('li').toggleClass('active');
                    if ($(this).attr('aria-expanded') === 'false') {
                        $(this).attr('aria-expanded', true);
                    } else {
                        $(this).attr('aria-expanded', false);
                    }
                    if ($(this).attr('aria-label') === 'Expand menu') {
                        $(this).attr('aria-label', 'Close menu');
                    } else {
                        $(this).attr('aria-label', 'Expand menu');
                    }
                }
            });
        });
    </script>
