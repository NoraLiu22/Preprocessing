<script type="text/javascript">
//<![CDATA[
//프린트팝업



	function Linksns(title, type){
	//title = encodeURIComponent('\"'+title+'\"');
	var linkurl = "";
	//url = encodeURIComponent('http://'+url);
	







	/****************  게시판 URL 시작*********************/
var page = "";
var width="100";
var height="100";
var scrapParam = "";
	var scrapMethod = "";	
	var urlinfo = "";

     	if(document.ifrm != null) 
	{	    
		if(document.ifrm['SCRAP_PARAM'] != null)
		{
			scrapParam = document.ifrm['SCRAP_PARAM'].innerHTML;
		}
	
		if(document.ifrm['SCRAP_METHOD'] != null)
		{
			scrapMethod = document.ifrm['SCRAP_METHOD'].innerHTML;
		}

		rgExp = eval("/&amp;/g");
		scrapParam = scrapParam.replace(rgExp, "&");
	}
	
	if( document.ifrm ) {
		if(scrapMethod=="POST ") {
			urlinfo = document.ifrm.location.href + "?" + scrapParam;
		} else {
			urlinfo = document.ifrm.location.href;
		}
	} else {
		urlinfo = document.location.href;
	}
       	 if(document.ifrm != null)
	 {  
			page = urlinfo;
			page = page.split("&").join("%26");
	page= page.replace("?","%3F");
	//page= page.replace("&","%26");

	boardPage=document.location+"%26sp=";

	boardPage = boardPage.replace("?","%3F");


	boardPage = boardPage.replace("&","%26");

	var iWords="";


		var leng = $('#print_area').find('#ifrm').length;

				if (leng >0){
					
							iWords = $('#ifrm').contents().find('#snsboardtitle').html();
							if (iWords == null)
							{
								iWords = "Latin America and Caribbean List of the Countries";
							}


				}else{
					iWords = "Latin America and Caribbean List of the Countries";
					
				}


		
	if(type == 'twitter'){ 
 
		var sendUrl = "https://web.archive.org/web/20140122012151/http://twitter.com/share?url="+boardPage+encodeURI(page)+"&text="+iWords;
		window.open(sendUrl);
 
 
	}else if(type == 'me2day'){


		var openNewWindow = window.open("about:blank");

		openNewWindow.location.href = "https://web.archive.org/web/20140122012151/http://me2day.net/posts/new?new_post[body]="+encodeURIComponent('\"'+iWords+'\"')+":"+boardPage+encodeURI(page);

		
	}else if(type == 'facebook'){

		var facebookurl =document.ifrm.location.href;
		facebookurl = facebookurl.split("&").join("^");



		var strImages = "https://web.archive.org/web/20140122012151/http://www.mofa.go.kr/2011_main/res/kor_mofat.gif";
		
		var strShareUrl = "https://web.archive.org/web/20140122012151/http://www.facebook.com/sharer.php";
		var strParam = "";
		


		strUrl = encodeURIComponent(document.location)+'%26sp='+encodeURIComponent(facebookurl);
		strTitle = encodeURIComponent("List of the Countries");
		strsummary = encodeURIComponent(iWords);
		strImages= encodeURIComponent(strImages);

		strParam += "?s=100";
		strParam += "&p[title]="+strTitle;
		strParam += "&p[url]="+strUrl;
		strParam += "&p[summary]="+strsummary;
		strParam += "&p[images][0]="+strImages;
		window.open(strShareUrl+strParam);

	}else if(type == 'yozm'){


		window.open('https://web.archive.org/web/20140122012151/http://yozm.daum.net/api/popup/post?prefix='+encodeURIComponent(title)+':'+boardPage,'sharer','toolbar=0,status=0,width=490,height=400');



	}





	}else{


	page=document.location;
if(type == 'twitter'){ 




		var sendUrl = "https://web.archive.org/web/20140122012151/http://twitter.com/share?url="+page+"&text="+title;
		window.open(encodeURI(sendUrl));    





	}else if(type == 'me2day'){




		var openNewWindow = window.open("about:blank");

		openNewWindow.location.href = "https://web.archive.org/web/20140122012151/http://me2day.net/posts/new?new_post[body]="+encodeURIComponent('\"'+title+'\"')+":"+encodeURI(page);



		
	}else if(type == 'facebook'){



		var strImages = "https://web.archive.org/web/20140122012151/http://www.mofa.go.kr/2011_main/res/kor_mofat.gif";
		
		var strShareUrl = "https://web.archive.org/web/20140122012151/http://www.facebook.com/sharer.php";
		var strParam = "";


		strUrl = encodeURIComponent(page);
		strTitle = encodeURIComponent("List of the Countries");
		strsummary = encodeURIComponent("Latin America and Caribbean List of the Countries");
		strImages= encodeURIComponent(strImages);

		strParam += "?s=100";
		strParam += "&p[title]="+strTitle;
		strParam += "&p[url]="+strUrl;
		strParam += "&p[summary]="+strsummary;
		strParam += "&p[images][0]="+strImages;
		window.open(strShareUrl+strParam);		     



	}else if(type == 'yozm'){


		window.open('https://web.archive.org/web/20140122012151/http://yozm.daum.net/api/popup/post?prefix='+encodeURIComponent(title)+':'+page,'sharer','toolbar=0,status=0,width=490,height=400');



	}


	}
	browser_name = navigator.appName ;

	if (browser_name == "Netscape" ) 
	{
	    	bannerWin.focus() ;
	}



	/*******************게시판 URL 끝**********************/
}
	






function OpenPrintWindow2(page, width, height)
 {
	var scrapParam = "";
	var scrapMethod = "";	
	var urlinfo = "";
	
 
     	if(document.ifrm != null) 
	{	    
		if(document.ifrm['SCRAP_PARAM'] != null)
		{
			scrapParam = document.ifrm['SCRAP_PARAM'].innerHTML;
		}
	
		if(document.ifrm['SCRAP_METHOD'] != null)
		{
			scrapMethod = document.ifrm['SCRAP_METHOD'].innerHTML;
		}
 
		rgExp = eval("/&amp;/g");
		scrapParam = scrapParam.replace(rgExp, "&");
	}
	
	if( document.ifrm ) {
		if(scrapMethod=="POST ") {
			urlinfo = document.ifrm.location.href + "?" + scrapParam;
		} else {
			urlinfo = document.ifrm.location.href;
		}
	} else {
		urlinfo = document.location.href;
	}
       	 if(document.ifrm != null)
	 {  
			page = urlinfo;
			page = page.split("&").join("|");
			


var bannerWin = window.open("/ENG/system/popup/index4.jsp?printUrl="+page,"popupWin","statusbar=yes,resizable=no,scrollbars=yes,width= 730px,height=600px") ;
 
	}else{

 
		var bannerWin = window.open(page , "popupWin","statusbar=yes,resizable=no,scrollbars=yes,width= 730px,height=600px") ;
	}
	browser_name = navigator.appName ;
 
	if (browser_name == "Netscape" ) 
	{
	    	bannerWin.focus() ;
	}
}








