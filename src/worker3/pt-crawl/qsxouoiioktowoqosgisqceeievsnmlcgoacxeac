<script type="text/javascript" language="javascript">
		//<![CDATA[

		/*
		var quick_r = $('#quick_r');
		var quick_top = 110;
		/* quick menu initialization */
		/*
		quick_r.css('top', $(window).height() );
		$(document).ready(function(){
		 quick_r.animate( { "top": $(document).scrollTop() + quick_top +"px" }, 500 );
		 $(window).scroll(function(){
		  quick_r.stop();
		  quick_r.animate( { "top": $(document).scrollTop() + quick_top + "px" }, 1000 );
		 });
		});
		*/

		/* 화면 확대 축소 시작 IE 전용 */
		var nowZoom = 100; // 현재비율
		var maxZoom = 200; // 최대비율(500으로하면 5배 커진다)
		var minZoom = 80; // 최소비율

		//화면 키운다.
		function zoomIn(){
			if(nowZoom < maxZoom){
				nowZoom += 10; //25%씩 커진다.
			}else{
				return;
			}
			//document.body.style.zoom = nowZoom + "%";
			document.getElementById('wrapper').style.zoom  = nowZoom + "%";
		}

		//기본화면
		function zoomEmty(){
			nowZoom = 100;
			document.getElementById('wrapper').style.zoom  = nowZoom + "%";
		}

		//화면 줄인다.
		function zoomOut(){
			if(nowZoom > minZoom){
				nowZoom -= 10; //25%씩 작아진다.
			}else{
				return;
			}
			//document.body.style.zoom = nowZoom + "%";
			document.getElementById('wrapper').style.zoom  = nowZoom + "%";
		}

		//프린트팝업
		//]]>
	</script>

