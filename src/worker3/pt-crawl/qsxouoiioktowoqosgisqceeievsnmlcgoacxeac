<script type="text/javascript">


function initMenus3(id) {

	$('ul.left_menu ul').hide();
	//$('#'+id).parent('ul').show();
	$('#'+id).parent('ul').slideDown('fast');
	$('#'+id).addClass("current"); 

	if($('#'+id).children("ul").length > 0){ //하위가 있다...
		$('#'+id).children('ul').show();
		$('#'+id).parent('ul').slideDown('fast');
	}else{
		$('#'+id).parent('ul').parent('li').addClass("current"); 
	}
}
$(document).ready(function() {initMenus3('m_30_30');});
</script>

