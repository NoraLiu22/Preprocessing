    <script>
	var pushnewsUrl = '//web.archive.org/web/20200712095924/https://cdn.pn.vg/c/r7/pushnews.min.js';

	var loadPushnews = function() {
		var $script = document.createElement('script');
		$script.src = pushnewsUrl;
		$script.async = true;

		document.getElementsByTagName('script')[0].appendChild($script);
	};

