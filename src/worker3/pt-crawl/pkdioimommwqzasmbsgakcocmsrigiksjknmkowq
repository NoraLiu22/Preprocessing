<script type="text/javascript">
    // configuração do prebid do intext mobile


    window.addEventListener('load', function() {


        // teads mobile
        googletag.cmd.push(function() {
            var teads = googletag
                .defineSlot(r7ad.hash, [1,1], 'r7ad-teads')
                .addService(window.googletag.pubads())
                .setTargeting('pos', 'teads');
                            
            window.googletag.enableServices();

            var container = $("#r7ad-teads");
            var lazy = new window.R7AdvLazy();
            lazy.registerAdMapValue(teads, container);
        });

        $('.content > p:eq(2)').after($('#r7ad-teads'));
        $('.content > p:eq(3)').after($('#r7ad-intext'));
    });
</script>

