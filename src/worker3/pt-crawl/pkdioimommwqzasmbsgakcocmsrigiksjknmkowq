    line-height: 32px;
    position: relative;
    top: 5px;
    left: 10px;
    font-weight: 900;
    font-style: italic;
    letter-spacing: -1px;
    text-transform: lowercase;
}

.mais_lidas_sidebar_trends a.mais-lidas-trends-title:hover {
  color: #556373 !important;
}

.mais-lidas-trends-box {
  width: 100%;
}
</style>
<script>
  //Desktop
  /* globals Handlebars*/
  window.addEventListener('load', function() {
    R7.component('MaisLidasDesktop', function() {
      'use strict';

      function MaisLidasDesktop(id, isMobile, color) {
        this.prepare(id, isMobile, color);
        this.start();
      }

      //GTM 'mais lidas desktop' link
      function CallGTMLinkMaisLidas() {
        var selectorLinkDropDonw = $('.mais_lidas_sidebar_trends a.mais-lidas-trends-title');
        if (selectorLinkDropDonw.length > 0) {
          clearInterval(executionInterval);
          selectorLinkDropDonw.click(function() {
            ga('send', 'event', 'click', this.href, 'Link Dropdonw Mais lidas Sidebar Desktop');
            ga('send', 'pageview');
          });
        }
      }

      var executionInterval = setInterval(CallGTMLinkMaisLidas, 500);

      //GTM mais lidas Desktop
      document.querySelector('.mais-lidas-trends-dropdown').addEventListener('click', function() {
        ga('send', 'event', 'click', window.location.href, 'Select Mais lidas Sidebar Desktop');
        ga('send', 'pageview');
      });

      MaisLidasDesktop.prototype = {
        prepare: function(id, isMobile, color) {
          this.loaded = false;
          this.utils = new R7.MaisLidasUtils(id);
          this.color = color;
        },

        start: function() {
          var firstSection = this.getFirstSection();
          this.events();
          this.utils.getData(firstSection, this.success.bind(this), this.utils.error);
        },

        getFirstSection: function() {
          return this.utils.selector('.mais-lidas-trends-dropdown-item')
            .first()
            .data().section;
        },

        success: function(media) {
          var newMedia = this.utils.formatData(media);

          this.loaded = true;
          this.appendTemplate(newMedia);
          this.recursiveFadeTitles();
          this.utils.fixIeTrouble();
        },

        appendTemplate: function(media) {
          var source = this.utils.selector('#mais-lidas-template').html(),
            template = Handlebars.compile(source);

          this.utils.selector(this.utils.prefix).append(template(media));
        },

        recursiveFadeTitles: function() {
          this.hideLoading();
          this.getTitles().each(function(index, element) {
            var time = index + '00';

            setTimeout(function() {
              $(element).removeClass('inative');
            }, time);
          });
        },

        hideLoading: function() {
          this.utils.selector(this.utils.prefix + ' .loading').fadeOut();
        },

        getTitles: function() {
          return this.utils.selector(this.utils.prefix + '-title');
        },

        events: function() {
          var dropdownItem = this.utils.prefix + '-dropdown-item',
            dropdownCurrent = this.utils.prefix + '-dropdown-current',
            title = this.utils.prefix + '-title';

          this.utils.wrapper.on('click', dropdownItem, this.dropdownTrigged.bind(this));
          this.utils.wrapper.on('click', dropdownCurrent, this.toggleDropdown.bind(this));
          this.utils.wrapper.on('mouseover', title, this.addColor.bind(this));
          this.utils.wrapper.on('mouseout', title, this.removeColor.bind(this));
        },

        addColor: function(event) {
          $(event.target).css('color', this.color);
        },

        removeColor: function(event) {
          $(event.target).css('color', '#333333');
        },

        dropdownTrigged: function(event) {
          var element = $(event.target),
            id = element.data().section;

          this.loaded = false;
          this.loadNewData(id);
          this.toggleDropdown();
          this.utils.selector(this.utils.prefix + '-dropdown-current').text(element.text());
        },

        toggleDropdown: function() {
          this.utils.selector(this.utils.prefix + '-dropdown-list').slideToggle();
        },

        loadNewData: function(id) {
          this.getTitles().addClass('inative');
          this.showLoading();

          setTimeout(function() {
            this.getTitles().remove();

            if (this.utils.hasCache(id)) {
              this.success(window.cacheMaisLidas[id]);

            } else {
              this.utils.getData(id, this.success.bind(this), this.utils.error);

            }
          }.bind(this), 700);
        },

        showLoading: function() {
          setTimeout(function showIfNotLoad() {
            if (!this.loaded) {
              this.utils.selector(this.utils.prefix + ' .loading').fadeIn();
            }
          }.bind(this), 1000);
        }

      };

      R7.register('MaisLidasDesktop', MaisLidasDesktop);
    });

    //Main
    R7.component('MaisLidasTrends', function() {
      'use strict';

      function MaisLidasTrends(id, isMobile, color) {
        this.start(id, isMobile, color);
      }

      MaisLidasTrends.prototype = {
        start: function(id, isMobile, color) {
          this.isMobile = (isMobile === 'true');

          if (this.isMobile) {
            this.mobile = new R7.MaisLidasMobile(id, isMobile, color);

          } else {
            this.desktop = new R7.MaisLidasDesktop(id, isMobile, color);
          }
        }
      };

      R7.register('MaisLidasTrends', MaisLidasTrends);
    });

    // Util
    R7.component('MaisLidasUtils', function() {
      'use strict';

      function MaisLidasUtils(id) {
        this.prepare(id);
      }

      MaisLidasUtils.prototype = {
        prepare: function(id) {
          this.wrapper = $('#mais-lidas-trends-' + id);
          this.prefix = '.mais-lidas-trends';
        },

        selector: function(selector) {
          return this.wrapper.find(selector);
        },

        getData: function(id, success, error) {
          var baseUrl = 'https://web.archive.org/web/20200712095924/https://cms-media-api.r7.com/most_read_news',
            amountOfPosts = 7,
            jsonpCallback = this.removeHyphen(id);

          $.ajax({
            context: this,
            url: this.madeUrl(id, baseUrl, amountOfPosts),
            dataType: 'jsonp',
            processData: false,
            cache: true,
            jsonpCallback: jsonpCallback,
            success: function(data) {
              success(data, id);
              this.cacheData(data, id);
            },

            error: error
          });
        },

        error: function(jqXHR, textStatus, errorThrown) {
          console.log('Error', {
            jqXHR: jqXHR,
            textStatus: textStatus,
            errorThrown: errorThrown
          });
        },

        madeUrl: function(id, baseUrl, limit) {
          if (id) {
            id = '/' + id;
          }

          return baseUrl + id + '?limit=' + limit;
        },

        removeHyphen: function(string) {
          return string.replace(/-/g, '');
        },

        formatData: function(media) {
          var $this = this;

          media.forEach(function(current, index) {
            current.position = index + 1;
            delete current.views;
            delete current.id;

            // add target blank if is external url
            current.target = $this.setTarget(current.url);
          });

          return media;
        },

        setTarget: function(url) {
          return /r7\.com/.test(url) ? '_self' : '_blank';
        },

        cacheData: function(data, id) {
          window.cacheMaisLidas = window.cacheMaisLidas || {};
          window.cacheMaisLidas[id] = data;
        },

        hasCache: function(id) {
          return window.cacheMaisLidas.hasOwnProperty(id);
        },

        fixIeTrouble: function() {
          if (window.navigator.msPointerEnabled) {
            this.selector(this.prefix + '-title')
              .addClass('mais-lidas-trends-title-no-bg');
          }
        }
      };

      R7.register('MaisLidasUtils', MaisLidasUtils);
    });
  });
</script>
  <script type="text/javascript">

  var options = {
    boxId: "5e9905e84b495584bc00023b",
    showMobile: false,
    color: "#556373"
  }

  if ( document.readyState == 'complete' ) {
    new R7.MaisLidasTrends(options.boxId, options.showMobile, options.color);

  } else {
    window.addEventListener('load', function(){
      new R7.MaisLidasTrends(options.boxId, options.showMobile, options.color);
    });
  }
</script>
</div>
                </div>
                <div class="box publicidade">
                  


    
