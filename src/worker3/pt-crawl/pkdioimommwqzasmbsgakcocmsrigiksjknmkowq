      <script>
    (function() {
        new R7.StickyAd();

      new R7.Header();
      new R7.Share();

      var buttonShared = $('.get-ga');
      buttonShared.on('click', postGa);
      function postGa(){
        var info   = $(this).data('info'),
            social = $(this).data('social'),
            action = $(this).data('action'),
            url    = $(this).data('url');
        ga('send', info, social, action, url);
      }

    })();
