<script type="text/javascript">
  var box_key = "publicidade-5b02d5aad363c6a3cc00007e";

  (function(){

    var config = {
      code: 'box-publicidade-island-ad-5b02d5aad363c6a3cc00007e',
      sizes: [300, 250],
      bids: [{
        bidder: 'criteo',
        params: {
          zoneId: '1209596'
        }
      }]
    }

    var options = {
      sizes: [
        300,
        600
      ],
      id: 'box-publicidade-island-ad-5b02d5aad363c6a3cc00007e',
      pos: 'halfpage',
      context: 'sidebar',
      safeFrame: false
    };

    r7adv.incrementUnit(config);
    r7adv.defineAd(options);

    window[box_key] = options;
  })();

  window.addEventListener('load', function() {
    if (typeof BoxPublicidade !== 'undefined') {
      BoxPublicidade.sidebarSetHeight();
    }    
  });

</script>
