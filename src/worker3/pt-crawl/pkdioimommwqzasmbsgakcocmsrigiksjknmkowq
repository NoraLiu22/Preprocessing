		$boxPublic.addClass('publicidade-vertical-layout');

	});
</script>

        <script> 
  function BoxPublicidade() {}

  /**
   * Ajusta a altura do último box  Publicidade para que chegue até o final da sidebar
   */
  BoxPublicidade.boxPublicidadeSetHeight = function($sidebar, containerHeight) {
    var _boxes = $sidebar.find('.box');
    var height = 0;

    var boxesFullheight = function(_boxes) {
      var h = 0;
      $.each(_boxes, function() { h += $(this).outerHeight(); });
      return h;
    };

    // height = auto
    _boxes.last().css('height', '');

    // arrumando altura sidebar
    $sidebar.css('height', containerHeight + 'px');

    height = boxesFullheight(_boxes);

    // espaço vazio na sidebar
    height = $sidebar.height() - height;

    // espaço vazio na sidebar + altura do próprio box
    height += parseInt( _boxes.last().outerHeight() );

    _boxes.last().css('height', height + 'px');
  };

  /**
   * Deixa a sidebar com a mesma altura da íntegra
   */
  BoxPublicidade.sidebarSetHeight = function() {


    var $sidebar = $('header.type-integra ~ .container aside.sidebar');
    var $content = $sidebar.siblings('.r7cl5');


    function domChanged($sidebar) {
      setTimeout(function() {
        var heightHalfPage = $('.publicidade-vertical-layout > div').height();
        var containerHeight = $content.find('.container').height() + heightHalfPage;
        $sidebar.css('height', containerHeight + 'px');
        BoxPublicidade.boxPublicidadeSetHeight($sidebar, containerHeight);
      }, 0);      
    };

    domChanged($sidebar);

    $content.bind('DOMSubtreeModified DOMNodeInserted DOMNodeRemoved', function() {
      domChanged($sidebar);
    });

    setInterval(function() {
      domChanged($sidebar);
    }, 5000);
  };
</script>




