  <script>
  (function() {
    var internal = new R7.Internal({
      url: 'http://' + window.location.host,
      sectionURL:'https://web.archive.org/web/20200712095924/http://noticias.r7.com/brasil',
      twitterUrl: 'https://web.archive.org/web/20200712095924/https://twitter-count.r7.com'
    });

      var urlPreview = window.location.href.split('?')[0].split('/');
      var hasPreview = urlPreview.indexOf('preview') === -1

      if(hasPreview){
        var listInfinitWrapper;
        var objIntegraListInfinity = new R7.IntegraListInfinite({
            urlServiceWebb: 'https://web.archive.org/web/20200712095924/https://cms-media-api.r7.com',
            idSection: '5086bd181d42065064001e5c',
            device: 'false',
            idMedia: '5d017dde19d224cfb6000d14'
          });
        var showListInfinity = function(){
          var isInViewport = LazyLoad.isInViewport(listInfinitWrapper);
          if (isInViewport) {
            objIntegraListInfinity.start();
            LazyLoad.start();
            setTimeout(LazyLoad.start, 1000);
            setTimeout(LazyLoad.start, 2000);
            setTimeout(LazyLoad.start, 4000);
            setTimeout(LazyLoad.start, 6000);
            $(window).unbind('.listInfinityTemplate');
          }
        };

        // elemento que ocorrera lazy
        $(window).load(function() {
          listInfinitWrapper = document.getElementById('wrapper-list-infinite');
        });
        $(window).bind('scroll.listInfinityTemplate', function () {
          showListInfinity();
        });
      }
  })();
</script>

      <script>
	$(function() {
		var $container = $('#box-publicidade-island-5b02d5aad363c6a3cc00007e');
		var $boxPublic = $container.closest('.publicidade');

		$boxPublic.attr('id', 'publicidade-5b02d5aad363c6a3cc00007e');


		$boxPublic.addClass('publicidade-vertical-layout');

	});
</script>

        <script> 
  function BoxPublicidade() {}

  /**
   * Ajusta a altura do último box  Publicidade para que chegue até o final da sidebar
   */
  BoxPublicidade.boxPublicidadeSetHeight = function($sidebar, containerHeight) {
    var _boxes = $sidebar.find('.box');
    var height = 0;

    var boxesFullheight = function(_boxes) {
      var h = 0;
      $.each(_boxes, function() { h += $(this).outerHeight(); });
      return h;
    };

    // height = auto
    _boxes.last().css('height', '');

    // arrumando altura sidebar
    $sidebar.css('height', containerHeight + 'px');

    height = boxesFullheight(_boxes);

    // espaço vazio na sidebar
    height = $sidebar.height() - height;

    // espaço vazio na sidebar + altura do próprio box
    height += parseInt( _boxes.last().outerHeight() );

    _boxes.last().css('height', height + 'px');
  };

  /**
   * Deixa a sidebar com a mesma altura da íntegra
   */
  BoxPublicidade.sidebarSetHeight = function() {


    var $sidebar = $('header.type-integra ~ .container aside.sidebar');
    var $content = $sidebar.siblings('.r7cl5');


    function domChanged($sidebar) {
      setTimeout(function() {
        var heightHalfPage = $('.publicidade-vertical-layout > div').height();
        var containerHeight = $content.find('.container').height() + heightHalfPage;
        $sidebar.css('height', containerHeight + 'px');
        BoxPublicidade.boxPublicidadeSetHeight($sidebar, containerHeight);
      }, 0);      
    };

    domChanged($sidebar);

    $content.bind('DOMSubtreeModified DOMNodeInserted DOMNodeRemoved', function() {
      domChanged($sidebar);
    });

    setInterval(function() {
      domChanged($sidebar);
    }, 5000);
  };
</script>

      <script>
	$(function() {
