<script type="text/javascript">
	jQuery(document).ready(function($) {
      $('a[rel*=facebox]').facebox() 
    })
    
    function muestraGale(x){
 		jQuery.facebox(document.getElementById("cargandoDiv").innerHTML) ;
	    J.get(x, function(data) { J.facebox(data) })
    }
    
	var currPhoto=1;
	var currPage=1;
	var currMedTab = 0;
	var tabMedOFF=0;
	function verLeyenda(campo,id)
	{
		document.getElementById(campo).innerHTML = document.getElementById(id).innerHTML;
	}
	
	var pageSize=50;
	function AnImage(complete, thumb){
	 	this.complete=complete;
	 	this.thumb=thumb;
	}
	
	function previous(total){
	  	currPhoto--;
		if(currPhoto==0){
			currPhoto=total;
		} 
		if ((currPhoto+1)%pageSize==0 || currPhoto==total){
			aux=currPhoto;
			previousPage(1);
			currPhoto=aux;			
		}
		document.getElementById('fotoa').src=images[currPhoto-1].thumb;
		document.getElementById('fotoam').src=images[currPhoto-1].complete;
		document.getElementById('total').innerHTML = currPhoto + " de " + 2;
		return;		
	}
  		
	function next(total){
		currPhoto++;
		if(currPhoto>total){
			currPhoto=1;
		}
		
		if(currPhoto%pageSize==0){
			alert("entra aqui");
			nextPage(1);
			return true;
		}		
		document.getElementById('fotoa').src=images[currPhoto-1].thumb;
		document.getElementById('fotoam').src=images[currPhoto-1].complete;
		document.getElementById('total').innerHTML = currPhoto + " de " + 2;
		return;
	}
	
	function previousPage(total){
		currPage--;
		if(currPage<=1){
			currPage=total;
		} 
		return;
	}
  		
	function nextPage(total){
		currPage++;
		if(currPage>total){
			currPage=1;
		}
		return;
	}
	
	function cambiarImagen(id)
	{
		document.getElementById('fotoGale').innerHTML = document.getElementById('fotoGale_'+id).innerHTML;;
	}
	
</script>	



