			function resetR_lead1Div(id){
				if(lead1R_timerId!=null){
					clearTimeout(lead1R_timerId);
				}
			
				document.getElementById('contR_'+lead1R_divIndex).style.display='none';
				if(document.getElementById('contR_'+id)!=null){
					lead1R_divIndex = id;
				}else{
					lead1R_divIndex = 0;
				}
				document.getElementById('contR_'+lead1R_divIndex).style.display='';
				
				if(lead1R_timerId!=null){
					lead1R_timerId = setTimeout("lead1R_ChangeDiv()", lead1R_frequency);
				}
			}
			
			
			
