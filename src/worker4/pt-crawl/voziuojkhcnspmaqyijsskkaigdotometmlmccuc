		function diffDay(date,today){
			return Math.ceil((today.getTime()- date.getTime())/(1000 * 60 * 60 * 24)) - 1
		}
		
		function diffHour(date,today){
			return Math.ceil((today.getTime()-date.getTime())/(1000 * 60 * 60)) - 1
		}
		
		function diffMinute(date,today){
			return Math.ceil((today.getTime()-date.getTime())/(1000 * 60)) - 1
		}
		
		function getPubTime(date,today){
			total =0;
			total = diffDay(date,today);
			if (total>0){
				return total+' dia'+(total>1?'s':'');
			}
			total = diffHour(date,today);
	
			if (total>0){
				return total+' hora'+(total>1?'s':'');
			}
			total = diffMinute(date,today);
	
			if (total>0){
				return total+'&rsquo;';
			}
	
			return '1 &rsquo;';
		}
	</script>
	<strong>CARACAS</strong>, Thursday September 03, 2009 | Update <strong><span><span>






<script>
	var publishDay = 27;
	var publishMount = 6;
	var publishYear = 2013;
	var publishHour = 23;
	var publishMinute = 5;	
	
	var publishDate = new Date(publishYear,publishMount,publishDay,publishHour,publishMinute);
	//var diff = 235 - publishDate.getTimezoneOffset();
	var diff = 0;
	publishDate.setMinutes(publishDate.getMinutes()+diff);
	var todayDate = new Date();	
		
	function diffDay(date,today){
		return Math.ceil((today.getTime()- date.getTime())/(1000 * 60 * 60 * 24)) - 1
	}
	
	function diffHour(date,today){
		return Math.ceil((today.getTime()-date.getTime())/(1000 * 60 * 60)) - 1
	}
	
	function diffMinute(date,today){
		return Math.ceil((today.getTime()-date.getTime())/(1000 * 60)) - 1
	}
	
	function getPubTime(date,today){
		total =0;
		total = diffDay(date,today);
		if (total>0){
			return total+' dia'+(total>1?'s':'');
		}
		total = diffHour(date,today);

		if (total>0){
			return total+' hora'+(total>1?'s':'');
		}
		total = diffMinute(date,today);

		if (total>0){
			return total+'&#39;';
		}

		return '1&#39;';
	}
</script>
