<script type="text/javascript">
    var isSERPLoaded = false;
    document.domain = "reference.com";
    function translateFrameOnLoad() {
        if (!isSERPLoaded) {
            isSERPLoaded = true;
            return;
        }
        if (isSERPLoaded) {
            self.scrollTo(0, 0);
        }
        isSERPLoaded = true;
        hideHeader("translateFrame");
        updateAnchorTarget("translateFrame");
        //find the height of the internal page
        var ifrm = document.getElementById("translateFrame");
        if(false){
        var main = window.frames["translateFrame"].document.getElementById("main");
        if (main){
            if((main.getElementsByTagName('iframe')==null || main.getElementsByTagName('iframe').length ==0)
                    && window.frames["translateFrame"].document.getElementById('transopts')==null){
                ifrm.style.paddingTop = "0px";
                }
        }
        }
         if(translateFrame.document.URL=="about:blank"){
            var mainBody=document.getElementById("mainbody");
            var footer=document.getElementById("foot");
            var mainResults=document.getElementById("main");
            if(mainResults){
                mainResults.style.display="block";
            }
            if(mainBody){
                mainBody.style.display="block";
            }
            if(footer){
                mainBody.style.display="block";
                document.getElementById("translateFrame").style.height="0px";
            }
        }else{
            var mainBody=document.getElementById("mainbody");
            var footer=document.getElementById("foot");
            var mainResults=document.getElementById("main");
            if(mainResults){
                mainResults.style.display="none";
            }
            if(mainBody){
                mainBody.style.display="none";
            }
            if(footer){
                mainBody.style.display="none";
            } 
         }
        if (ifrm) {
            var the_height = ifrm.contentWindow.document.body.scrollHeight;
            //change the height of the iframe
             if(a10.browser.isIE6()){
                the_height = the_height+100;
            }
            ifrm.style.height = (the_height) + "px";
        }
        var ifrm = document.getElementById("translateFrame"); 
        if (ifrm) {
            ifrm.style.display = "block";
            /* var main=document.getElementById("main");
            if(main){
                main.style.top = 110 +"px";
            }*/
        }                         

    }

</script>

    <style type="text/css">
                  #translateFrame
        {
            height:0px;

        }
  </style>
    <iframe name="translateFrame" id="translateFrame" scrolling="no" frameborder="0" width="100%" height="0px" onload="translateFrameOnLoad();"></iframe>



