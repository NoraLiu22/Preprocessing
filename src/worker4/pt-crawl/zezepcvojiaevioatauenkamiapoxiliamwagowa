<script type="text/javascript">
    function s_formcheck(frm){
        if(document.getElementById("email")){
           if(document.getElementById("email").value==""){
               alert("please enter an email id for subscription");
               return false;
           }
        }
        frm.target="translateFrame" ;
        document.domain = "reference.com"; 	 
         //hide existing ads and content
            var mainBody=document.getElementById("mainbody");
            var footer=document.getElementById("foot");
            var mainResults=document.getElementById("main");
            if(mainResults){
                mainResults.style.display="none";
            }
            if(mainBody){
                mainBody.style.display="none";
            }
            if(footer){
                mainBody.style.display="none";
            }
         var iframe=document.getElementById('translateFrame');
         if(iframe){
             iframe.style.paddingTop=100 +"px";
         }
        var email= document.getElementById('email').value;
        if(email){
            var username=email.split('@');
            document.getElementById('name').value= username[0];
         }
        self.scrollTo(0,0);
        return true;
    }
</script>

