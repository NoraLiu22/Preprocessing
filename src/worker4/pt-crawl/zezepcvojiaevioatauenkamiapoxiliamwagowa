<script type="text/javascript">
   function dic_header_linkOver(e) {
        if (e.className.indexOf("dic_header_current_dropitemoff")) {
      e.className = e.className.replace("dic_header_current_dropitemoff", "");
    }
   e.className = e.className + " dic_header_current_dropitem";
}
    function dic_header_linkOut(e) {
    if (e.className.indexOf("dic_header_current_dropitem")) {
      e.className = e.className.replace("dic_header_current_dropitem", "dic_header_current_dropitemoff");
    }
}
</script>

