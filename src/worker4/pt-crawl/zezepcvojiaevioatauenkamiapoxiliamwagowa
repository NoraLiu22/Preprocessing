    if (!a10) {var a10 = {}};
    a10.ss2 = {};

    a10.ss2.createSuggestions = function(suggestions, qsrc, origin, partnerID, siteID, country) {
        var return_str = '<ul id="suggestlist">';

        function createSuggestionList(suggestion, qsrc, origin, partnerID, siteID, country, idx) {
            highlight = "";
            if (highlight == "true") {
                query = a10.ss.lastSuccessfulQuery;
                re = new RegExp("^"+query);
                suggestion = suggestion.replace(re, "<span class='suggest'>"+query+"</span>");
            }
            if(_path=="homepage")
            {
                _path="dic";
            }
            var link = suggestion.replace(/(<([^>]+)>)/ig,"");

            return "<li><a href='"+_path+"?q="+escape(link)+"' class='suggestions' onmouseover='a10.ss.select(" + (idx) +");'>" + suggestion + "</a></li>"; 
        }

        
        var separate = "";

        prefix_suggestions = [];
        mixed_suggestions = [];

        if (separate == "true") {
            for (var i = 0; i < suggestions.length; i++) {
                suggestion = suggestions[i];
                if (suggestion.substring(0, 1) == "<") {
                    prefix_suggestions.push(suggestion);
                } else {
                    mixed_suggestions.push(suggestion);
                }
            }
        } else {
            mixed_suggestions = suggestions;
        }

        var suggestion_index = 0;

        for (var i = 0; i < prefix_suggestions.length; i++) {
            if (_maxItems > 0 && suggestion_index >= _maxItems) {
                break;
            }
            return_str += createSuggestionList(prefix_suggestions[i], qsrc, origin, partnerID, siteID, country, suggestion_index++);
        }

        if (prefix_suggestions.length && mixed_suggestions.length) {
            if (_maxItems > 0 && suggestion_index < _maxItems) {
                return_str += "<li class=\"separator\"></li>";
                suggestion_index++;
            }
        }

        for (var i = 0; i < mixed_suggestions.length; i++) {
            if (_maxItems > 0 && suggestion_index >= _maxItems) {
                break;
            }
            return_str += createSuggestionList(mixed_suggestions[i], qsrc, origin, partnerID, siteID, country, suggestion_index++);
        }
        return_str += "</ul>";
        return return_str;
    }
</script>
<script type="text/javascript">

    
    
    
    
    var _suggestUrls = {web : "https://web.archive.org/web/20090312065659/http://ss.ask.com/query?"};
    a10.ss2.getSuggestions = function(jsonData) {
        return jsonData["Suggestions"];
    };

    a10.ss2.getQuery = function(jsonData) {
        return jsonData["Query"];
    };

    a10.ss2.execute = function(query, country, language) {
        url = _suggestUrls['web'] + 'fn=suggestCallBack' +
                "&q=" + encodeURIComponent(query).toLowerCase() +
                "&cn=" + country +
                "&ln=" + language +
                "&ss=reply";
        jrObj = new JSONscriptRequest(url);
        jrObj.makeRequest();
    };

    
    


</script>



    
    
        
        <script type="text/javascript">
            a10.ss2.postRender = function() {
                if(a10.ss.bubble) {
                    a10.ss.bubble.redraw();
                } else {
                    a10.ss.bubble = new a10.Bubble($(a10.ss.uiId), $(a10.ss.uiBodyId), 220, 0, "up", false, null, 35);
                }
            };

        </script>

