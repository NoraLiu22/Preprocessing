    <script type="text/javascript">
        function openFeedback(URL, sender, args) {
            //Get the reference to window
            var windowFeedback = $("#windowFeedback");

            //Create window object if not created yet
            if (!windowFeedback.data("kendoWindow")) {
                windowFeedback.kendoWindow({
                    width: "300px",
                    height: "355px",
                    title: "Feedback",
                    content: URL,
                    modal: true,
                    resizable: false,
                    actions: ['close'],
                    visible: false,
                    iframe: true
                });
            }
            else {
                windowFeedback.data("kendoWindow").refresh(URL);
            }

            //Open and center the feedback window
            windowFeedback.data("kendoWindow").open();
            windowFeedback.data("kendoWindow").center();
        }

        //This is called from within the feedback window
        function closeFeedback() {
            $("#windowFeedback").data("kendoWindow").close();
        }
    </script>
    
