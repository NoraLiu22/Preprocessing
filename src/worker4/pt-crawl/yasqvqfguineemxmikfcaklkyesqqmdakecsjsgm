<script type="text/javascript">
<!-- //
/**
 * Dummy function
 */
navOn = function(nav) { }

/**
 * Define the mouseover/out functions for all the <li>'s in the navbar
 * The over/out functions simply add/remove an *additional* 'sfhover'
 * class to each <li>. The submenu show/hide effect is therefore a
 * result of the CSS moving the submenu on/off the canvas
 */
sfHover = function() {
  var sfEls = document.getElementById("nav").getElementsByTagName("li");
  for (var i=0; i<sfEls.length; i++) {
    sfEls[i].onmouseover=function() {
      this.className+=" sfhover";
    }
    sfEls[i].onmouseout=function() {
      this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
    }
  }
}

/**
 * Add the event handler onload
 */
addLoadEvent(sfHover);
addLoadEvent(function () { document.forms.vv_search.q.value = unescape(parseQs()['q'] || 'keyword');});

// -->
</script>
